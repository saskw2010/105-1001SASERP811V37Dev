	,"PJcntmf_Prj_"."Prj_Nm" "PJcntmf_Prj_Prj_Nm"
	,"PJcntmf_Prj_Cstm_"."Cstm_Nm" "PJcntmf_Prj_Cstm_Cstm_Nm"
	,"PJcntmf_Prj_Cstm_Area"."Area_name" "PJcntmf_Prj_Cstm_AreaArea_name"
	,"PJcntmf_Prj_Cstm_Evaluation"."Evaluation_name" "PJcntmf_Prj_Cstm_EvaluationEvaluation_name"
	,"PJcntmf_Prj_Cstm_acc_"."Acc_Nm" "PJcntmf_Prj_Cstm_acc_Acc_Nm"
	,"PJcntmf_Prj_Cstm_Cntry_"."Cntry_Nm" "PJcntmf_Prj_Cstm_Cntry_Cntry_Nm"
	,"PJcntmf_Prj_Cstm_curncy_"."Curncy_Nm" "PJcntmf_Prj_Cstm_curncy_Curncy_Nm"
	,"PJcntmf_Prj_Cstm_TSNEF"."TSNEF_name" "PJcntmf_Prj_Cstm_TSNEFTSNEF_name"
	,"PJcntmf_Prj_Cstm_TSNEFNO1"."TSNEF1_name" "PJcntmf_Prj_Cstm_TSNEFNO1TSNEF1_name"
	,"PJcntmf_Prj_Cstm_Slm_"."Slm_Nm" "PJcntmf_Prj_Cstm_Slm_Slm_Nm"
	,"SubCntItmd"."Qu" "Qu"
	,"SubCntItmd"."Pr_Mo" "Pr_Mo"
	,"SubCntItmd"."Itmval" "Itmval"
	,"SubCntItmd"."Pymval" "Pymval"
	,"SubCntItmd"."ItmBal" "ItmBal"
	,"SubCntItmd"."Itm_stdate" "Itm_stdate"
	,"SubCntItmd"."Itm_Endate" "Itm_Endate"
	,"SubCntItmd"."PymDt" "PymDt"
	,"SubCntItmd"."PymPer" "PymPer"
	,"SubCntItmd"."ItmDisc" "ItmDisc"
	,"SubCntItmd"."ModifiedBy" "ModifiedBy"
	,"SubCntItmd"."ModifiedOn" "ModifiedOn"
	,"SubCntItmd"."CreatedBy" "CreatedBy"
	,"SubCntItmd"."CreatedOn" "CreatedOn"
from "dbo"."SubCntItmd" "SubCntItmd"
	left join "dbo"."Prjbudgac1" "TheAc_No1" on "SubCntItmd"."Ac_No1" = "TheAc_No1"."Ac_No1"
	left join "dbo"."Subcntmf" "SubCnt_" on "SubCntItmd"."SubCnt_No" = "SubCnt_"."Sub_No"
	left join "dbo"."SubContrctr" "SubCnt_SubCnt_" on "SubCnt_"."SubCnt_no" = "SubCnt_SubCnt_"."Sub_No"
	left join "dbo"."PJcntmf" "PJcntmf_Prj_" on "SubCntItmd"."PJcntmf_Prj_No" = "PJcntmf_Prj_"."Prj_No"
	left join "dbo"."Stcustmr" "PJcntmf_Prj_Cstm_" on "PJcntmf_Prj_"."Cstm_No" = "PJcntmf_Prj_Cstm_"."Cstm_No"
	left join "dbo"."AreaList" "PJcntmf_Prj_Cstm_Area" on "PJcntmf_Prj_Cstm_"."AreaNo" = "PJcntmf_Prj_Cstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "PJcntmf_Prj_Cstm_Evaluation" on "PJcntmf_Prj_Cstm_"."EvaluationNO" = "PJcntmf_Prj_Cstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "PJcntmf_Prj_Cstm_acc_" on "PJcntmf_Prj_Cstm_"."acc_no" = "PJcntmf_Prj_Cstm_acc_"."Acc_No"
	left join "dbo"."PYCntry" "PJcntmf_Prj_Cstm_Cntry_" on "PJcntmf_Prj_Cstm_"."Cntry_No" = "PJcntmf_Prj_Cstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "PJcntmf_Prj_Cstm_curncy_" on "PJcntmf_Prj_Cstm_"."curncy_no" = "PJcntmf_Prj_Cstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "PJcntmf_Prj_Cstm_TSNEF" on "PJcntmf_Prj_Cstm_"."TSNEFNO" = "PJcntmf_Prj_Cstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "PJcntmf_Prj_Cstm_TSNEFNO1" on "PJcntmf_Prj_Cstm_"."TSNEFNO1" = "PJcntmf_Prj_Cstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "PJcntmf_Prj_Cstm_Slm_" on "PJcntmf_Prj_Cstm_"."Slm_No" = "PJcntmf_Prj_Cstm_Slm_"."Slm_No"
]]></text>
