  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"GSIlinedetail"."ID" "ID"
	,"GSIlinedetail"."MasterIDSEC" "MasterIDSEC"
	,"MasterIDSEC"."DetailData" "MasterIDSECDetailData"
	,"MasterIDSECMaster"."WORKORDERDSC" "MasterIDSECMasterWORKORDERDSC"
	,"MasterIDSECMasterGSIwork"."GSIworkDsc" "MasterIDSECMasterGSIworkGSIworkDsc"
	,"MasterIDSECMasterCstm_"."Cstm_Nm" "MasterIDSECMasterCstm_Cstm_Nm"
	,"GSIlinedetail"."DetailData" "DetailData"
	,"GSIlinedetail"."Req_Typcmpline" "Req_Typcmpline"
	,"Req_Typcmpline"."Req_NM" "Req_TypcmplineReq_NM"
	,"GSIlinedetail"."ID_compline" "ID_compline"
	,"ID_compline"."ID_USER" "ID_complineID_USER"
	,"ID_complineIDGROUP"."ID_USER" "ID_complineIDGROUPID_USER"
	,"ID_complineSup_"."Item_name" "ID_complineSup_Item_name"
	,"ID_complineid1"."DESCRP1" "ID_complineid1DESCRP1"
	,"ID_complineid2"."DESCRP1" "ID_complineid2DESCRP1"
	,"ID_complineid3"."DESCRP1" "ID_complineid3DESCRP1"
	,"ID_complineid4"."DESCRP1" "ID_complineid4DESCRP1"
	,"ID_complineid5"."DESCRP1" "ID_complineid5DESCRP1"
	,"ID_complineid6"."DESCRP1" "ID_complineid6DESCRP1"
	,"ID_complineid7"."DESCRP1" "ID_complineid7DESCRP1"
	,"GSIlinedetail"."brn_no" "brn_no"
	,"GSIlinedetail"."ModifiedBy" "ModifiedBy"
	,"GSIlinedetail"."ModifiedOn" "ModifiedOn"
	,"GSIlinedetail"."CreatedBy" "CreatedBy"
	,"GSIlinedetail"."CreatedOn" "CreatedOn"
from "dbo"."GSIlinedetail" "GSIlinedetail"
	left join "dbo"."GSIWORKORDERPRODUCTION" "MasterIDSEC" on "GSIlinedetail"."MasterIDSEC" = "MasterIDSEC"."ID"
	left join "dbo"."GSIWORKORDER" "MasterIDSECMaster" on "MasterIDSEC"."MasterID" = "MasterIDSECMaster"."ID"
	left join "dbo"."GSIworkList" "MasterIDSECMasterGSIwork" on "MasterIDSECMaster"."GSIworkid" = "MasterIDSECMasterGSIwork"."GSIworkid"
	left join "dbo"."Stcustmr" "MasterIDSECMasterCstm_" on "MasterIDSECMaster"."Cstm_No" = "MasterIDSECMasterCstm_"."Cstm_No"
	left join "dbo"."pyhldtypsl1" "Req_Typcmpline" on "GSIlinedetail"."Req_Typcmpline" = "Req_Typcmpline"."Req_Typ"
	left join "dbo"."ITEMS" "ID_compline" on "GSIlinedetail"."ID_compline" = "ID_compline"."ID_comp"
	left join "dbo"."GROUPStk" "ID_complineIDGROUP" on "ID_compline"."IDGROUP" = "ID_complineIDGROUP"."IDGROUP"
	left join "dbo"."ItemList" "ID_complineSup_" on "ID_compline"."Sup_No" = "ID_complineSup_"."ItemNo"
	left join "dbo"."PYHldtypsv1" "ID_complineid1" on "ID_compline"."id1" = "ID_complineid1"."ID1"
	left join "dbo"."PYHldtypsv2" "ID_complineid2" on "ID_compline"."id2" = "ID_complineid2"."ID2"
	left join "dbo"."PYHldtypsv3" "ID_complineid3" on "ID_compline"."id3" = "ID_complineid3"."id3"
	left join "dbo"."PYHldtypsv4" "ID_complineid4" on "ID_compline"."id4" = "ID_complineid4"."id4"
	left join "dbo"."PYHldtypsv5" "ID_complineid5" on "ID_compline"."id5" = "ID_complineid5"."id5"
	left join "dbo"."PYHldtypsv6" "ID_complineid6" on "ID_compline"."id6" = "ID_complineid6"."id6"
	left join "dbo"."PYHldtypsv7" "ID_complineid7" on "ID_compline"."id7" = "ID_complineid7"."id7"
]]></text>
    </command>
