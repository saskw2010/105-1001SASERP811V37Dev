  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
SELECT        Barcodeitem.BarcodeAutoid, Barcodeitem.autoid, autoBatch.Expiredate AS autoModifiedBy, autoBatchID_comp.ID_USER AS autoBatchID_compID_USER, autoID_Comp.ID_USER AS autoID_CompID_USER, 
                         autoID_CompIDGROUP.ID_USER AS autoID_CompIDGROUPID_USER, autoID_CompSup_.Item_name AS autoID_CompSup_Item_name, autoID_Compid1.DESCRP1 AS autoID_Compid1DESCRP1, 
                         autoID_Compid2.DESCRP1 AS autoID_Compid2DESCRP1, autoID_Compid3.DESCRP1 AS autoID_Compid3DESCRP1, autoID_Compid4.DESCRP1 AS autoID_Compid4DESCRP1, 
                         autoID_Compid5.DESCRP1 AS autoID_Compid5DESCRP1, autoID_Compid6.DESCRP1 AS autoID_Compid6DESCRP1, autoID_Compid7.DESCRP1 AS autoID_Compid7DESCRP1, autoBatchID_comp.ID_USER, 
                         autoBatch.Barcodubatch AS Batchcode, autoBatchID_comp.Itm_Sal1 AS price, Barcodeitem.Suplier, autoBatchID_comp.ITEM AS notes,Barcodeitem.ModifiedBy, autoBatch.Expiredate as  ModifiedOn, Barcodeitem.CreatedBy, 
                         Barcodeitem.CreatedOn, autoBatch.Expiredate, autoBatchID_comp.ITEM, autoBatchID_comp.Itm_Sal1, Barcodeitem.ID_Comp, Barcodeitem.Batchid, 
                         autoBatchID_comp.ID_USER + CAST(YEAR(autoBatch.Expiredate) AS nvarchar(4)) + CAST(MONTH(autoBatch.Expiredate) AS nvarchar(2)) + + CAST(DAY(autoBatch.Expiredate) AS nvarchar(2)) 
                         AS Barcodubatch
FROM            dbo.Barcodeitem AS Barcodeitem LEFT OUTER JOIN
                         dbo.BarcodeCreate AS auto ON Barcodeitem.autoid = auto.autoid LEFT OUTER JOIN
                         dbo.itembatch AS autoBatch ON auto.Batchid = autoBatch.Batchid LEFT OUTER JOIN
                         dbo.ITEMS AS autoBatchID_comp ON autoBatch.ID_comp = autoBatchID_comp.ID_comp LEFT OUTER JOIN
                         dbo.ITEMS AS autoID_Comp ON auto.ID_Comp = autoID_Comp.ID_comp LEFT OUTER JOIN
                         dbo.GROUPStk AS autoID_CompIDGROUP ON autoID_Comp.IDGROUP = autoID_CompIDGROUP.IDGROUP LEFT OUTER JOIN
                         dbo.ItemList AS autoID_CompSup_ ON autoID_Comp.Sup_No = autoID_CompSup_.ItemNo LEFT OUTER JOIN
                         dbo.PYHldtypsv1 AS autoID_Compid1 ON autoID_Comp.id1 = autoID_Compid1.ID1 LEFT OUTER JOIN
                         dbo.PYHldtypsv2 AS autoID_Compid2 ON autoID_Comp.id2 = autoID_Compid2.ID2 LEFT OUTER JOIN
                         dbo.PYHldtypsv3 AS autoID_Compid3 ON autoID_Comp.id3 = autoID_Compid3.id3 LEFT OUTER JOIN
                         dbo.PYHldtypsv4 AS autoID_Compid4 ON autoID_Comp.id4 = autoID_Compid4.id4 LEFT OUTER JOIN
                         dbo.PYHldtypsv5 AS autoID_Compid5 ON autoID_Comp.id5 = autoID_Compid5.id5 LEFT OUTER JOIN
                         dbo.PYHldtypsv6 AS autoID_Compid6 ON autoID_Comp.id6 = autoID_Compid6.id6 LEFT OUTER JOIN
                         dbo.PYHldtypsv7 AS autoID_Compid7 ON autoID_Comp.id7 = autoID_Compid7.id7
]]></text>
    </command>
    <command id="BarcodeAutoidIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="BarcodeAutoid" />
      </output>
    </command>
  </commands>
