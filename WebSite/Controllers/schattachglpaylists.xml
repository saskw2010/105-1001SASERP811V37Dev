    <command id="command1" type="Text">
      <text><![CDATA[
select
	"schattachglpaylists"."schattachglpayid" "schattachglpayid"
	,"schattachglpaylists"."schglpaydlyhdregid" "schglpaydlyhdregid"
	,"schglpaydlyhdreg"."payDelevername" "schglpaydlyhdregpayDelevername"
	,"schglpaydlyhdregsgm"."sgm_Nm" "schglpaydlyhdregsgmsgm_Nm"
	,"schglpaydlyhdregsgmOpco"."OpcoName" "schglpaydlyhdregsgmOpcoOpcoName"
	,"schglpaydlyhdregpayIDregorg"."NAME_1" "schglpaydlyhdregpayIDregorgNAME_1"
	,"schglpaydlyhdregacd"."AcademicYear" "schglpaydlyhdregacdAcademicYear"
	,"schglpaydlyhdregacdGlFinperiod"."Fin_period_info" "schglpaydlyhdregacdGlFinperiodFin_period_info"
	,"schglpaydlyhdregbranch"."Desc1" "schglpaydlyhdregbranchDesc1"
	,"schglpaydlyhdregbranchsgm"."sgm_Nm" "schglpaydlyhdregbranchsgmsgm_Nm"
	,"schglpaydlyhdregbranchGender"."Gender" "schglpaydlyhdregbranchGenderGender"
	,"schglpaydlyhdregbranchstage"."ShortDesc1" "schglpaydlyhdregbranchstageShortDesc1"
	,"schglpaydlyhdregbranchschtyp"."schtypDesc" "schglpaydlyhdregbranchschtypschtypDesc"
	,"schattachglpaylists"."schattachglpaytype" "schattachglpaytype"
	,"schattachglpaylists"."schattachglpayrefno" "schattachglpayrefno"
	,"schattachglpaylists"."schattachglpayrefdate" "schattachglpayrefdate"
	,"schattachglpaylists"."schattachglpayrefpagesno" "schattachglpayrefpagesno"
	,"schattachglpaylists"."notes" "notes"
	,"schattachglpaylists"."ExternalDocFileName" "ExternalDocFileName"
	,"schattachglpaylists"."ExternalDocContentType" "ExternalDocContentType"
	,"schattachglpaylists"."ExternalDocLength" "ExternalDocLength"
from "dbo"."schattachglpaylists" "schattachglpaylists"
	left join "dbo"."schglpaydlyhdreg" "schglpaydlyhdreg" on "schattachglpaylists"."schglpaydlyhdregid" = "schglpaydlyhdreg"."schglpaydlyhdregid"
	left join "dbo"."glmulcmp" "schglpaydlyhdregsgm" on "schglpaydlyhdreg"."sgm" = "schglpaydlyhdregsgm"."sgmid"
	left join "dbo"."OpCo" "schglpaydlyhdregsgmOpco" on "schglpaydlyhdregsgm"."OpcoID" = "schglpaydlyhdregsgmOpco"."OpcoIDidentity"
	left join "dbo"."REG_ORGANIZATIONS" "schglpaydlyhdregpayIDregorg" on "schglpaydlyhdreg"."payIDregorg" = "schglpaydlyhdregpayIDregorg"."IDIDregorg"
	left join "dbo"."schAcademicyear" "schglpaydlyhdregacd" on "schglpaydlyhdreg"."acdcode" = "schglpaydlyhdregacd"."Code"
	left join "dbo"."GlFinanceperiod" "schglpaydlyhdregacdGlFinperiod" on "schglpaydlyhdregacd"."GlFinperiodID" = "schglpaydlyhdregacdGlFinperiod"."GlFinperiodID"
	left join "dbo"."schBranch" "schglpaydlyhdregbranch" on "schglpaydlyhdreg"."branch" = "schglpaydlyhdregbranch"."branch"
	left join "dbo"."glmulcmp" "schglpaydlyhdregbranchsgm" on "schglpaydlyhdregbranch"."sgm" = "schglpaydlyhdregbranchsgm"."sgmid"
	left join "dbo"."PyGender" "schglpaydlyhdregbranchGender" on "schglpaydlyhdregbranch"."Genderid" = "schglpaydlyhdregbranchGender"."Genderid"
	left join "dbo"."schStage" "schglpaydlyhdregbranchstage" on "schglpaydlyhdregbranch"."stagecode" = "schglpaydlyhdregbranchstage"."Code"
	left join "dbo"."schtyp" "schglpaydlyhdregbranchschtyp" on "schglpaydlyhdregbranch"."schtypid" = "schglpaydlyhdregbranchschtyp"."schtypid"
]]></text>
    </command>
