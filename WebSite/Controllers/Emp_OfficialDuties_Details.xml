    </field>
    <field name="Emp_OfficialDuty_SEmp_SID" type="String" readOnly="true" label="Emp Official Duty S Emp SID" length="255" />
    <field name="Emp_OfficialDuty_SOffDuty_SOfficial_Code" type="String" readOnly="true" label="Emp Official Duty S Off Duty S Official Code" length="50" />
  </fields>
