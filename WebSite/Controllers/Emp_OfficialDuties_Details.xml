  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Emp_OfficialDuties_Details"."SID" "SID"
	,"Emp_OfficialDuties_Details"."TDate" "TDate"
	,"Emp_OfficialDuties_Details"."Day_SID" "Day_SID"
	,"Emp_OfficialDuties_Details"."Emp_OfficialDuty_SID" "Emp_OfficialDuty_SID"
	,"Emp_OfficialDuty_S"."Emp_SID" "Emp_OfficialDuty_SEmp_SID"
	,"Emp_OfficialDuty_SOffDuty_S"."Official_Code" "Emp_OfficialDuty_SOffDuty_SOfficial_Code"
from "dbo"."Emp_OfficialDuties_Details" "Emp_OfficialDuties_Details"
	left join "dbo"."Emp_OfficialDuties" "Emp_OfficialDuty_S" on "Emp_OfficialDuties_Details"."Emp_OfficialDuty_SID" = "Emp_OfficialDuty_S"."SID"
	left join "dbo"."Official_Duties" "Emp_OfficialDuty_SOffDuty_S" on "Emp_OfficialDuty_S"."OffDuty_SID" = "Emp_OfficialDuty_SOffDuty_S"."SID"
]]></text>
    </command>
    <command id="SIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="SID" />
      </output>
    </command>
  </commands>
