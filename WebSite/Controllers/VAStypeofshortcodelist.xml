  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"VAStypeofshortcodelist"."typeofshortcode" "typeofshortcode"
	,"VAStypeofshortcodelist"."typeofshortDescription" "typeofshortDescription"
	,"VAStypeofshortcodelist"."typeofshortcodemain" "typeofshortcodemain"
	,"typeofshortcodemain"."typeofshortDescription" "typeofshortcodemaintypeofshortDescription"
	,"typeofshortcodemainIncomeAcc_"."Acc_Nm" "typeofshortcodemainIncomeAcc_Acc_Nm"
	,"typeofshortcodemainIncomeAcc_Clsacc_"."Acc_Nm" "typeofshortcodemainIncomeAcc_Clsacc_Acc_Nm"
	,"typeofshortcodemainIncomeAcc_Acc_Bnd"."Acc_Nm" "typeofshortcodemainIncomeAcc_Acc_BndAcc_Nm"
	,"typeofshortcodemainCostAcc_"."Acc_Nm" "typeofshortcodemainCostAcc_Acc_Nm"
	,"typeofshortcodemainCostAcc_Clsacc_"."Acc_Nm" "typeofshortcodemainCostAcc_Clsacc_Acc_Nm"
	,"typeofshortcodemainCostAcc_Acc_Bnd"."Acc_Nm" "typeofshortcodemainCostAcc_Acc_BndAcc_Nm"
	,"VAStypeofshortcodelist"."IncomeAcc_no" "IncomeAcc_no"
	,"IncomeAcc_"."Acc_Nm" "IncomeAcc_Acc_Nm"
	,"IncomeAcc_Clsacc_"."Acc_Nm" "IncomeAcc_Clsacc_Acc_Nm"
	,"IncomeAcc_Acc_Bnd"."Acc_Nm" "IncomeAcc_Acc_BndAcc_Nm"
	,"IncomeAcc_Acc_Bndaccclass"."accountclass" "IncomeAcc_Acc_Bndaccclassaccountclass"
	,"IncomeAcc_Acc_BndAcc_Bab"."Acc_Nm" "IncomeAcc_Acc_BndAcc_BabAcc_Nm"
	,"VAStypeofshortcodelist"."CostAcc_no" "CostAcc_no"
	,"CostAcc_"."Acc_Nm" "CostAcc_Acc_Nm"
	,"CostAcc_Clsacc_"."Acc_Nm" "CostAcc_Clsacc_Acc_Nm"
	,"CostAcc_Acc_Bnd"."Acc_Nm" "CostAcc_Acc_BndAcc_Nm"
	,"CostAcc_Acc_Bndaccclass"."accountclass" "CostAcc_Acc_Bndaccclassaccountclass"
	,"CostAcc_Acc_BndAcc_Bab"."Acc_Nm" "CostAcc_Acc_BndAcc_BabAcc_Nm"
from "dbo"."VAStypeofshortcodelist" "VAStypeofshortcodelist"
	left join "dbo"."VAStypeofshortcodelist" "typeofshortcodemain" on "VAStypeofshortcodelist"."typeofshortcodemain" = "typeofshortcodemain"."typeofshortcode"
	left join "dbo"."GLmstrfl" "typeofshortcodemainIncomeAcc_" on "typeofshortcodemain"."IncomeAcc_no" = "typeofshortcodemainIncomeAcc_"."Acc_No"
	left join "dbo"."GLmfbab" "typeofshortcodemainIncomeAcc_Clsacc_" on "typeofshortcodemainIncomeAcc_"."Clsacc_No" = "typeofshortcodemainIncomeAcc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "typeofshortcodemainIncomeAcc_Acc_Bnd" on "typeofshortcodemainIncomeAcc_"."Acc_Bnd" = "typeofshortcodemainIncomeAcc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."GLmstrfl" "typeofshortcodemainCostAcc_" on "typeofshortcodemain"."CostAcc_no" = "typeofshortcodemainCostAcc_"."Acc_No"
	left join "dbo"."GLmfbab" "typeofshortcodemainCostAcc_Clsacc_" on "typeofshortcodemainCostAcc_"."Clsacc_No" = "typeofshortcodemainCostAcc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "typeofshortcodemainCostAcc_Acc_Bnd" on "typeofshortcodemainCostAcc_"."Acc_Bnd" = "typeofshortcodemainCostAcc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."GLmstrfl" "IncomeAcc_" on "VAStypeofshortcodelist"."IncomeAcc_no" = "IncomeAcc_"."Acc_No"
	left join "dbo"."GLmfbab" "IncomeAcc_Clsacc_" on "IncomeAcc_"."Clsacc_No" = "IncomeAcc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "IncomeAcc_Acc_Bnd" on "IncomeAcc_"."Acc_Bnd" = "IncomeAcc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "IncomeAcc_Acc_Bndaccclass" on "IncomeAcc_Acc_Bnd"."accclass" = "IncomeAcc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "IncomeAcc_Acc_BndAcc_Bab" on "IncomeAcc_Acc_Bnd"."Acc_Bab" = "IncomeAcc_Acc_BndAcc_Bab"."Acc_Bab"
	left join "dbo"."GLmstrfl" "CostAcc_" on "VAStypeofshortcodelist"."CostAcc_no" = "CostAcc_"."Acc_No"
	left join "dbo"."GLmfbab" "CostAcc_Clsacc_" on "CostAcc_"."Clsacc_No" = "CostAcc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "CostAcc_Acc_Bnd" on "CostAcc_"."Acc_Bnd" = "CostAcc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "CostAcc_Acc_Bndaccclass" on "CostAcc_Acc_Bnd"."accclass" = "CostAcc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "CostAcc_Acc_BndAcc_Bab" on "CostAcc_Acc_Bnd"."Acc_Bab" = "CostAcc_Acc_BndAcc_Bab"."Acc_Bab"
]]></text>
    </command>
