    <command id="command1" type="Text">
      <text><![CDATA[
select
	"callregister"."callid" "callid"
	,"callregister"."MarketplanID" "MarketplanID"
	,"Marketplan"."MarketplanDesc" "MarketplanMarketplanDesc"
	,"callregister"."customertel1" "customertel1"
	,"callregister"."customertel2" "customertel2"
	,"callregister"."Slm_No" "Slm_No"
	,"Slm_"."Slm_Nm" "Slm_Slm_Nm"
	,"Slm_masngerclass"."masngerclass" "Slm_masngerclassmasngerclass"
	,"callregister"."visittype" "visittype"
	,"visittype"."visittypeNm" "visittypevisittypeNm"
	,"callregister"."cstumername" "cstumername"
	,"callregister"."activitycomments" "activitycomments"
	,"callregister"."Personalname1" "Personalname1"
	,"callregister"."Personaljob1" "Personaljob1"
	,"callregister"."Mobile" "Mobile"
	,"callregister"."Extinstion" "Extinstion"
	,"callregister"."Personalname" "Personalname"
	,"callregister"."Personaljob" "Personaljob"
	,"callregister"."companywebsite" "companywebsite"
	,"callregister"."cutomercomments" "cutomercomments"
	,"callregister"."Personalemail" "Personalemail"
	,"callregister"."callercomments" "callercomments"
	,"callregister"."mangmentcomments" "mangmentcomments"
	,"callregister"."cutomerRequest" "cutomerRequest"
	,"callregister"."startdatetime" "startdatetime"
	,"callregister"."Visitstatus" "Visitstatus"
	,"Visitstatus"."Visitstatusname" "VisitstatusVisitstatusname"
	,"callregister"."productsID" "productsID"
	,"products"."productsname" "productsproductsname"
	,"callregister"."savedatetime" "savedatetime"
	,"callregister"."ModifiedBy" "ModifiedBy"
	,"callregister"."ModifiedOn" "ModifiedOn"
	,"callregister"."CreatedBy" "CreatedBy"
	,"callregister"."CreatedOn" "CreatedOn"
from "dbo"."callregister" "callregister"
	left join "dbo"."Marketplan" "Marketplan" on "callregister"."MarketplanID" = "Marketplan"."MarketplanID"
	left join "dbo"."Stslsman" "Slm_" on "callregister"."Slm_No" = "Slm_"."Slm_No"
	left join "dbo"."masngerclass" "Slm_masngerclass" on "Slm_"."masngerclassid" = "Slm_masngerclass"."masngerclassid"
	left join "dbo"."visittypelist" "visittype" on "callregister"."visittype" = "visittype"."visittypeNo"
	left join "dbo"."Visitstatuslist" "Visitstatus" on "callregister"."Visitstatus" = "Visitstatus"."VisitstatusID"
	left join "dbo"."ProductsList" "products" on "callregister"."productsID" = "products"."productsid"
]]></text>
    </command>
