    <field name="parentid" type="Int64" label="Parentid" showInSummary="true" contextFields="Acc_Bab=Acc_Bab,sgm=sgm" calculated="true">
      <items style="Lookup" dataController="GLmfband" newDataView="createForm1" searchOnStart="true" dataView="treeview1" dataTextField="Acc_Nm" dataValueField="Acc_Bnd" copy="lvl=lvl" />
    </field>
    <field name="parentAcc_Nm" type="String" readOnly="true" label="parent Acc Nm" length="50" />
    <field name="parentaccclassaccountclass" type="String" readOnly="true" label="Parentaccclassaccountclass" length="100" />
