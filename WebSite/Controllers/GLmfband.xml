<?xml version="1.0" encoding="utf-8"?>
<dataController name="GLmfband" conflictDetection="overwriteChanges" label="G Lmfband" plugIn="eZee.Data.AnnotationPlugIn" handler="eZee.Rules.glmfbandhandeler" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"GLmfband"."Acc_Bnd" "Acc_Bnd"
	,"GLmfband"."parentid" "parentid"
	,"parent"."Acc_Nm" "parentAcc_Nm"
	,"parentaccclass"."accountclass" "parentaccclassaccountclass"
	,"parentAcc_Bab"."Acc_Nm" "parentAcc_BabAcc_Nm"
	,"GLmfband"."Acc_Bab" "Acc_Bab"
	,"TheAcc_Bab"."Acc_Nm" "TheAcc_BabAcc_Nm"
	,"GLmfband"."lvl" "lvl"
	,"GLmfband"."Acc_Nm" "Acc_Nm"
	,"GLmfband"."Acc_Nme" "Acc_Nme"
	,"GLmfband"."accclass" "accclass"
	,"accclass"."accountclass" "accclassaccountclass"
	,"GLmfband"."ModifiedBy" "ModifiedBy"
	,"GLmfband"."ModifiedOn" "ModifiedOn"
	,"GLmfband"."CreatedBy" "CreatedBy"
	,"GLmfband"."CreatedOn" "CreatedOn"
	,"GLmfband"."accountold" "accountold"	
	
from "dbo"."GLmfband" "GLmfband"
	left join "dbo"."GLmfband" "parent" on "GLmfband"."parentid" = "parent"."Acc_Bnd"
	left join "dbo"."accountclasslist" "parentaccclass" on "parent"."accclass" = "parentaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "parentAcc_Bab" on "parent"."Acc_Bab" = "parentAcc_Bab"."Acc_Bab"
	left join "dbo"."GLmfbab" "TheAcc_Bab" on "GLmfband"."Acc_Bab" = "TheAcc_Bab"."Acc_Bab"
	left join "dbo"."accountclasslist" "accclass" on "GLmfband"."accclass" = "accclass"."accountclassid"
]]></text>
    </command>
  </commands>
