    </field>
    <field name="TheAcc_BabAcc_Nm" type="String" readOnly="true" label="Acc Bab Acc Nm" length="250" />
    <field name="lvl" type="Int64" label="Lvl" showInSummary="true" default="" contextFields="Parentid" calculated="true" />
