<?xml version="1.0" encoding="utf-8"?>
<dataController name="Issue" conflictDetection="overwriteChanges" label="Issue" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Issue"."iss_id" "iss_id"
	,"Issue"."cat_id" "cat_id"
	,"cat_"."cat_name" "cat_cat_name"
	,"Issue"."priority_code" "priority_code"
	,"priority_"."ModifiedBy" "priority_ModifiedBy"
	,"Issue"."assign_id" "assign_id"
	,"Issue"."iss_creatorname" "iss_creatorname"
	,"Issue"."iss_label" "iss_label"
	,"Issue"."iss_desc" "iss_desc"
	,"Issue"."iss_datecreated" "iss_datecreated"
	,"Issue"."iss_dateresolved" "iss_dateresolved"
	,"Issue"."status_code" "status_code"
	,"status_"."status_name" "status_status_name"
	,"Issue"."iss_days" "iss_days"
	,"Issue"."iss_hours" "iss_hours"
	,"Issue"."req_id" "req_id"
	,"req_"."req_details" "req_req_details"
	,"req_Deduc_"."Deduc_Nm" "req_Deduc_Deduc_Nm"
	,"req_emp_"."Emp_NOt" "req_emp_Emp_NOt"
	,"req_emp_sgm"."sgm_Nm" "req_emp_sgmsgm_Nm"
	,"req_emp_salrycat"."grade_desc_a" "req_emp_salrycatgrade_desc_a"
	,"req_emp_Blood_Type"."ModifiedBy" "req_emp_Blood_TypeModifiedBy"
	,"req_emp_Bnk_"."Bnk_Acc" "req_emp_Bnk_Bnk_Acc"
	,"req_emp_Sub_Bnk"."Bnk_Acc" "req_emp_Sub_BnkBnk_Acc"
	,"req_emp_Cntry_"."Cntry_Nm" "req_emp_Cntry_Cntry_Nm"
	,"req_emp_Depm_"."Depm_Nm" "req_emp_Depm_Depm_Nm"
	,"req_emp_Educt_Type"."Educt_Nm" "req_emp_Educt_TypeEduct_Nm"
	,"req_emp_Job_Stu"."Emp_Stunm" "req_emp_Job_StuEmp_Stunm"
	,"req_emp_Earning_"."Earning_Nm" "req_emp_Earning_Earning_Nm"
	,"req_emp_Job_"."Job_Nm" "req_emp_Job_Job_Nm"
	,"req_emp_Ownr_"."Ownr_ID" "req_emp_Ownr_Ownr_ID"
	,"req_emp_Reljan"."Reljan_nm" "req_emp_ReljanReljan_nm"
	,"req_emp_Status_Type"."Status_Nm" "req_emp_Status_TypeStatus_Nm"
	,"req_emp_branch"."Desc1" "req_emp_branchDesc1"
	,"req_reqcat_"."reqcat_name" "req_reqcat_reqcat_name"
	,"req_reqcat_Req_type"."req_Type" "req_reqcat_Req_typereq_Type"
	,"Issue"."ModifiedBy" "ModifiedBy"
	,"Issue"."ModifiedOn" "ModifiedOn"
	,"Issue"."CreatedBy" "CreatedBy"
	,"Issue"."CreatedOn" "CreatedOn"
from "dbo"."Issue" "Issue"
	left join "dbo"."category" "cat_" on "Issue"."cat_id" = "cat_"."cat_id"
	left join "dbo"."priority" "priority_" on "Issue"."priority_code" = "priority_"."priority_code"
	left join "dbo"."status" "status_" on "Issue"."status_code" = "status_"."status_code"
	left join "dbo"."Request" "req_" on "Issue"."req_id" = "req_"."reqID"
	left join "dbo"."Pydedctyp" "req_Deduc_" on "req_"."Deduc_No" = "req_Deduc_"."Deduc_No"
	left join "dbo"."Pyempmfs" "req_emp_" on "req_"."emp_no" = "req_emp_"."Emp_No"
	left join "dbo"."glmulcmp" "req_emp_sgm" on "req_emp_"."sgm" = "req_emp_sgm"."sgmid"
	left join "dbo"."grade" "req_emp_salrycat" on "req_emp_"."salrycat" = "req_emp_salrycat"."grade_code"
	left join "dbo"."pyBlood" "req_emp_Blood_Type" on "req_emp_"."Blood_Type" = "req_emp_Blood_Type"."Blood_Type"
	left join "dbo"."PybnkAcc" "req_emp_Bnk_" on "req_emp_"."Bnk_No" = "req_emp_Bnk_"."Bnk_No"
	left join "dbo"."PybnkAcc" "req_emp_Sub_Bnk" on "req_emp_"."Sub_Bnk" = "req_emp_Sub_Bnk"."Bnk_No"
	left join "dbo"."PYCntry" "req_emp_Cntry_" on "req_emp_"."Cntry_No" = "req_emp_Cntry_"."Cntry_No"
	left join "dbo"."Pydepm" "req_emp_Depm_" on "req_emp_"."Depm_No" = "req_emp_Depm_"."Depm_No"
	left join "dbo"."PyempEdu" "req_emp_Educt_Type" on "req_emp_"."Educt_Type" = "req_emp_Educt_Type"."Educt_Type"
	left join "dbo"."Pyempstu" "req_emp_Job_Stu" on "req_emp_"."Job_Stu" = "req_emp_Job_Stu"."Emp_stu"
	left join "dbo"."Pyerntsalyp1" "req_emp_Earning_" on "req_emp_"."Earning_no" = "req_emp_Earning_"."Earning_No"
	left join "dbo"."Pyjobs" "req_emp_Job_" on "req_emp_"."Job_No" = "req_emp_Job_"."Job_No"
	left join "dbo"."Pyownrmf" "req_emp_Ownr_" on "req_emp_"."Ownr_No" = "req_emp_Ownr_"."Ownr_No"
	left join "dbo"."PyReljan" "req_emp_Reljan" on "req_emp_"."Reljan" = "req_emp_Reljan"."reljan"
	left join "dbo"."PyStatus" "req_emp_Status_Type" on "req_emp_"."Status_Type" = "req_emp_Status_Type"."Status_Type"
	left join "dbo"."schBranch" "req_emp_branch" on "req_emp_"."branch" = "req_emp_branch"."branch"
	left join "dbo"."RequestCategories" "req_reqcat_" on "req_"."reqcat_id" = "req_reqcat_"."reqcat_id"
	left join "dbo"."RequestTypes" "req_reqcat_Req_type" on "req_reqcat_"."Req_typeid" = "req_reqcat_Req_type"."req_Typeid"
]]></text>
    </command>
    <command id="iss_idIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="iss_id" />
      </output>
    </command>
  </commands>
