<?xml version="1.0" encoding="utf-8"?>
<dataController name="PyLocationdis" conflictDetection="overwriteChanges" label="Py Locationdis" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"PyLocationdis"."locNodis" "locNodis"
	,"PyLocationdis"."branch" "branch"
	,"Thebranch"."Desc1" "ThebranchDesc1"
	,"Thebranchsgm"."sgm_Nm" "Thebranchsgmsgm_Nm"
	,"ThebranchsgmOpco"."OpcoName" "ThebranchsgmOpcoOpcoName"
	,"ThebranchGender"."Gender" "ThebranchGenderGender"
	,"Thebranchstage"."ShortDesc1" "ThebranchstageShortDesc1"
	,"Thebranchschtyp"."schtypDesc" "ThebranchschtypschtypDesc"
	,"PyLocationdis"."Depm_no" "Depm_no"
	,"Depm_"."Depm_Nm" "Depm_Depm_Nm"
	,"PyLocationdis"."loc_No" "loc_No"
	,"loc_"."Loc_Nm" "loc_Loc_Nm"
	,"PyLocationdis"."locdisno" "locdisno"
	,"locdis"."locdiscription" "locdislocdiscription"
from "dbo"."PyLocationdis" "PyLocationdis"
	left join "dbo"."schBranch" "Thebranch" on "PyLocationdis"."branch" = "Thebranch"."branch"
	left join "dbo"."glmulcmp" "Thebranchsgm" on "Thebranch"."sgm" = "Thebranchsgm"."sgmid"
	left join "dbo"."OpCo" "ThebranchsgmOpco" on "Thebranchsgm"."OpcoID" = "ThebranchsgmOpco"."OpcoIDidentity"
	left join "dbo"."PyGender" "ThebranchGender" on "Thebranch"."Genderid" = "ThebranchGender"."Genderid"
	left join "dbo"."schStage" "Thebranchstage" on "Thebranch"."stagecode" = "Thebranchstage"."Code"
	left join "dbo"."schtyp" "Thebranchschtyp" on "Thebranch"."schtypid" = "Thebranchschtyp"."schtypid"
	left join "dbo"."Pydepm" "Depm_" on "PyLocationdis"."Depm_no" = "Depm_"."Depm_No"
	left join "dbo"."PyLocation" "loc_" on "PyLocationdis"."loc_No" = "loc_"."loc_No"
	left join "dbo"."pylocdis" "locdis" on "PyLocationdis"."locdisno" = "locdis"."locdisno"
]]></text>
    </command>
    <command id="locNodisIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="locNodis" />
      </output>
    </command>
  </commands>
