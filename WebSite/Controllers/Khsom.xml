            <dataField fieldName="lawyerAddress" />
            <dataField fieldName="address" />
            <dataField fieldName="mailBox" columns="50" />
            <dataField fieldName="remarks" />
            <dataField fieldName="khosmetype" aliasFieldName="Thekhosmetypekhosmetypename" />
            <dataField fieldName="ModifiedBy" columns="50" />
            <dataField fieldName="ModifiedOn" columns="10" />
            <dataField fieldName="CreatedBy" columns="50" />
            <dataField fieldName="CreatedOn" columns="10" />
          </dataFields>
        </category>
      </categories>
    </view>
