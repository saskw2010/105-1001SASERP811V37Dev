            <dataField fieldName="religion" aliasFieldName="religionReljan_nm" hyperlinkFormatString="~/Pages/PyReljan.aspx?reljan={religion}&amp;_controller=PyReljan&amp;_commandName=Select&amp;_commandArgument=editForm1" />
