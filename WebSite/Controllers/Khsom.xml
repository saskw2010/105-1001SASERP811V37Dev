<?xml version="1.0" encoding="utf-8"?>
<dataController name="Khsom" conflictDetection="overwriteChanges" label="Khsom" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Khsom"."khosmcode" "khosmcode"
	,"Khsom"."name" "name"
	,"Khsom"."nationality" "nationality"
	,"nationality"."Cntry_Nm" "nationalityCntry_Nm"
	,"Khsom"."religion" "religion"
	,"religion"."Reljan_nm" "religionReljan_nm"
	,"Khsom"."madny" "madny"
	,"Khsom"."lawyerName" "lawyerName"
	,"Khsom"."lawyerAddress" "lawyerAddress"
	,"Khsom"."address" "address"
	,"Khsom"."mailBox" "mailBox"
	,"Khsom"."remarks" "remarks"
	,"Khsom"."khosmetype" "khosmetype"
	,"Thekhosmetype"."khosmetypename" "Thekhosmetypekhosmetypename"
	,"Khsom"."ModifiedBy" "ModifiedBy"
	,"Khsom"."ModifiedOn" "ModifiedOn"
	,"Khsom"."CreatedBy" "CreatedBy"
	,"Khsom"."CreatedOn" "CreatedOn"
from "dbo"."Khsom" "Khsom"
	left join "dbo"."PYCntry" "nationality" on "Khsom"."nationality" = "nationality"."Cntry_No"
	left join "dbo"."PyReljan" "religion" on "Khsom"."religion" = "religion"."reljan"
	left join "dbo"."khosmetypetable" "Thekhosmetype" on "Khsom"."khosmetype" = "Thekhosmetype"."khosmetype"
]]></text>
    </command>
    <command id="khosmcodeIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="khosmcode" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="khosmcode" type="Int64" allowNulls="false" isPrimaryKey="true" label="Khosmcode" readOnly="true" />
    <field name="name" type="String" label="Name" length="100" showInSummary="true" />
    <field name="nationality" type="Int64" label="Nationality" showInSummary="true">
      <items style="Lookup" dataController="PYCntry" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="nationalityCntry_Nm" type="String" readOnly="true" label="nationality Cntry Nm" length="255" />
    <field name="religion" type="Int64" label="Religion" showInSummary="true">
      <items style="Lookup" dataController="PyReljan" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="religionReljan_nm" type="String" readOnly="true" label="religion Reljan nm" length="50" />
    <field name="madny" type="String" label="Madny" length="50" showInSummary="true" />
    <field name="lawyerName" type="String" label="lawyer Name" length="100" showInSummary="true" />
    <field name="lawyerAddress" type="String" label="lawyer Address" length="100" />
    <field name="address" type="String" label="Address" length="100" />
    <field name="mailBox" type="String" label="mail Box" length="50" />
    <field name="remarks" type="String" label="Remarks" length="100" />
    <field name="khosmetype" type="Int64" label="Khosmetype">
      <items style="Lookup" dataController="khosmetypetable" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="Thekhosmetypekhosmetypename" type="String" readOnly="true" label="Thekhosmetypekhosmetypename" length="50" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" />
    <field name="ModifiedOn" type="DateTime" label="Modified On" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="CreatedOn" type="DateTime" label="Created On" />
  </fields>
  <views>
