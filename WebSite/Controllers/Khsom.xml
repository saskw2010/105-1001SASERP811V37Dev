        <dataField fieldName="religion" aliasFieldName="religionReljan_nm" hyperlinkFormatString="~/Pages/PyReljan.aspx?reljan={religion}&amp;_controller=PyReljan&amp;_commandName=Select&amp;_commandArgument=editForm1" />
        <dataField fieldName="madny" columns="50" />
        <dataField fieldName="lawyerName" />
        <dataField fieldName="lawyerAddress" />
        <dataField fieldName="address" />
        <dataField fieldName="mailBox" columns="50" />
        <dataField fieldName="remarks" />
        <dataField fieldName="khosmetype" aliasFieldName="Thekhosmetypekhosmetypename" hyperlinkFormatString="~/Pages/khosmetypetable.aspx?khosmetype={khosmetype}&amp;_controller=khosmetypetable&amp;_commandName=Select&amp;_commandArgument=editForm1" />
        <dataField fieldName="ModifiedBy" columns="50" />
