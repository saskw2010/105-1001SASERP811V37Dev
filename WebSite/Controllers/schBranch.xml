<?xml version="1.0" encoding="utf-8"?>
<dataController name="schBranch" conflictDetection="overwriteChanges" label="sch Branch" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text>
    <![CDATA[
select
	"schBranch"."branch" "branch"
	,"schBranch"."Desc1" "Desc1"
	,"schBranch"."Desc2" "Desc2"
  ,"schBranch"."Desc11" "Desc11"
	,"schBranch"."schtypid" "schtypid"
	,"schtyp"."schtypDesc" "schtypschtypDesc"
	,"schBranch"."sgm" "sgm"
	,"sgm"."sgm_Nm" "sgmsgm_Nm"
	,"sgmOpco"."OpcoName" "sgmOpcoOpcoName"
	,"schBranch"."ModifiedBy" "ModifiedBy"
	,"schBranch"."ModifiedOn" "ModifiedOn"
	,"schBranch"."CreatedBy" "CreatedBy"
	,"schBranch"."CreatedOn" "CreatedOn"
	,"schBranch"."Genderid" "Genderid"
	,"Gender"."Gender" "GenderGender"
	,"schBranch"."stagecode" "stagecode"
	,"stage"."ShortDesc1" "stageShortDesc1"
from "dbo"."schBranch" "schBranch"
	left join "dbo"."schtyp" "schtyp" on "schBranch"."schtypid" = "schtyp"."schtypid"
	left join "dbo"."glmulcmp" "sgm" on "schBranch"."sgm" = "sgm"."sgmid"
	left join "dbo"."OpCo" "sgmOpco" on "sgm"."OpcoID" = "sgmOpco"."OpcoIDidentity"
	left join "dbo"."PyGender" "Gender" on "schBranch"."Genderid" = "Gender"."Genderid"
	left join "dbo"."schStage" "stage" on "schBranch"."stagecode" = "stage"."Code"
]]></text>
    </command>
    <command id="branchIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="branch" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="branch" type="Int64" allowNulls="false" isPrimaryKey="true" label="Branch" readOnly="true" />
    <field name="Desc1" type="String" label="Desc1" length="50" showInSummary="true" />
    <field name="Desc2" type="String" label="Desc2" length="50" showInSummary="true" />
    <field name="schtypid" type="Int64" label="Schtypid" showInSummary="true">
      <items style="Lookup" dataController="schtyp" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="schtypschtypDesc" type="String" readOnly="true" label="schtypschtyp Desc" length="150" />
    <field name="sgm" type="Int64" label="Sgm" showInSummary="true">
      <items style="Lookup" dataController="glmulcmp" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="sgmsgm_Nm" type="String" readOnly="true" label="sgmsgm Nm" length="255" />
    <field name="sgmOpcoOpcoName" type="String" readOnly="true" label="sgm Opco Name" length="50" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" showInSummary="true" />
    <field name="ModifiedOn" type="DateTime" label="Modified On" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="CreatedOn" type="DateTime" label="Created On" />
    <field name="Genderid" type="Int64" label="Genderid">
      <items style="Lookup" dataController="PyGender" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="GenderGender" type="String" readOnly="true" label="Gender" length="100" />
    <field name="stagecode" type="Int64" label="Stagecode">
      <items style="Lookup" dataController="schStage" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="stageShortDesc1" type="String" readOnly="true" label="stage Short Desc1" length="50" />
    <field name="Desc11" type="String" label="Desc11" length="50" showInSummary="true" />
    
  </fields>
