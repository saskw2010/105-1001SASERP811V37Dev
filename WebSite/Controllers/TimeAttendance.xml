  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"TimeAttendance"."Tattendid" "Tattendid"
	,"TimeAttendance"."Emp_No" "Emp_No"
	,"Emp_"."Emp_NOt" "Emp_Emp_NOt"
	,"Emp_sgm"."sgm_Nm" "Emp_sgmsgm_Nm"
	,"Emp_sgmOpco"."OpcoName" "Emp_sgmOpcoOpcoName"
	,"Emp_salrycat"."grade_desc_a" "Emp_salrycatgrade_desc_a"
	,"Emp_salrycatEarning_"."Earning_Nm" "Emp_salrycatEarning_Earning_Nm"
	,"Emp_Blood_Type"."ModifiedBy" "Emp_Blood_TypeModifiedBy"
	,"Emp_Bnk_"."Bnk_Acc" "Emp_Bnk_Bnk_Acc"
	,"Emp_Sub_Bnk"."Bnk_Acc" "Emp_Sub_BnkBnk_Acc"
	,"Emp_Cntry_"."Cntry_Nm" "Emp_Cntry_Cntry_Nm"
	,"Emp_Depm_"."Depm_Nm" "Emp_Depm_Depm_Nm"
	,"Emp_Educt_Type"."Educt_Nm" "Emp_Educt_TypeEduct_Nm"
	,"Emp_Job_Stu"."Emp_Stunm" "Emp_Job_StuEmp_Stunm"
	,"Emp_Earning_"."Earning_Nm" "Emp_Earning_Earning_Nm"
	,"Emp_Gender"."Gender" "Emp_GenderGender"
	,"Emp_Job_"."Job_Nm" "Emp_Job_Job_Nm"
	,"Emp_Loc_"."Loc_Nm" "Emp_Loc_Loc_Nm"
	,"Emp_Ownr_"."Ownr_ID" "Emp_Ownr_Ownr_ID"
	,"Emp_Ownr_cmp_"."Grp_Nm" "Emp_Ownr_cmp_Grp_Nm"
	,"Emp_Reljan"."Reljan_nm" "Emp_ReljanReljan_nm"
	,"Emp_Status_Type"."Status_Nm" "Emp_Status_TypeStatus_Nm"
	,"Emp_branch"."Desc1" "Emp_branchDesc1"
	,"Emp_branchsgm"."sgm_Nm" "Emp_branchsgmsgm_Nm"
	,"Emp_branchGender"."Gender" "Emp_branchGenderGender"
	,"Emp_branchstage"."ShortDesc1" "Emp_branchstageShortDesc1"
	,"Emp_branchschtyp"."schtypDesc" "Emp_branchschtypschtypDesc"
	,"TimeAttendance"."AC-No#" "ACNo"
	,"TimeAttendance"."Name" "Name"
	,"TimeAttendance"."Date" "Date"
	,"TimeAttendance"."Timetable" "Timetable"
	,"TimeAttendance"."On duty" "Onduty"
	,"TimeAttendance"."Off duty" "Offduty"
	,"TimeAttendance"."Clock In" "ClockIn"
	,"TimeAttendance"."Clock Out" "ClockOut"
	,"TimeAttendance"."Late" "Late"
	,"TimeAttendance"."Early" "Early"
	,"TimeAttendance"."Absent" "Absent"
	,"TimeAttendance"."Work Time" "WorkTime"
	,"TimeAttendance"."Department" "Department"
	,"TimeAttendance"."week" "week"
from "dbo"."TimeAttendance" "TimeAttendance"
	left join "dbo"."Pyempmfs" "Emp_" on "TimeAttendance"."Emp_No" = "Emp_"."Emp_No"
	left join "dbo"."glmulcmp" "Emp_sgm" on "Emp_"."sgm" = "Emp_sgm"."sgmid"
	left join "dbo"."OpCo" "Emp_sgmOpco" on "Emp_sgm"."OpcoID" = "Emp_sgmOpco"."OpcoIDidentity"
	left join "dbo"."grade" "Emp_salrycat" on "Emp_"."salrycat" = "Emp_salrycat"."grade_code"
	left join "dbo"."Pyerntsalyp1" "Emp_salrycatEarning_" on "Emp_salrycat"."Earning_No" = "Emp_salrycatEarning_"."Earning_No"
	left join "dbo"."pyBlood" "Emp_Blood_Type" on "Emp_"."Blood_Type" = "Emp_Blood_Type"."Blood_Type"
	left join "dbo"."PybnkAcc" "Emp_Bnk_" on "Emp_"."Bnk_No" = "Emp_Bnk_"."Bnk_No"
	left join "dbo"."PybnkAcc" "Emp_Sub_Bnk" on "Emp_"."Sub_Bnk" = "Emp_Sub_Bnk"."Bnk_No"
	left join "dbo"."PYCntry" "Emp_Cntry_" on "Emp_"."Cntry_No" = "Emp_Cntry_"."Cntry_No"
	left join "dbo"."Pydepm" "Emp_Depm_" on "Emp_"."Depm_No" = "Emp_Depm_"."Depm_No"
	left join "dbo"."PyempEdu" "Emp_Educt_Type" on "Emp_"."Educt_Type" = "Emp_Educt_Type"."Educt_Type"
	left join "dbo"."Pyempstu" "Emp_Job_Stu" on "Emp_"."Job_Stu" = "Emp_Job_Stu"."Emp_stu"
	left join "dbo"."Pyerntsalyp1" "Emp_Earning_" on "Emp_"."Earning_no" = "Emp_Earning_"."Earning_No"
	left join "dbo"."PyGender" "Emp_Gender" on "Emp_"."Genderid" = "Emp_Gender"."Genderid"
	left join "dbo"."Pyjobs" "Emp_Job_" on "Emp_"."Job_No" = "Emp_Job_"."Job_No"
	left join "dbo"."PyLocation" "Emp_Loc_" on "Emp_"."Loc_No" = "Emp_Loc_"."loc_No"
	left join "dbo"."Pyownrmf" "Emp_Ownr_" on "Emp_"."Ownr_No" = "Emp_Ownr_"."Ownr_No"
	left join "dbo"."Pycnstnt" "Emp_Ownr_cmp_" on "Emp_Ownr_"."cmp_no" = "Emp_Ownr_cmp_"."Cmp_No"
	left join "dbo"."PyReljan" "Emp_Reljan" on "Emp_"."Reljan" = "Emp_Reljan"."reljan"
	left join "dbo"."PyStatus" "Emp_Status_Type" on "Emp_"."Status_Type" = "Emp_Status_Type"."Status_Type"
	left join "dbo"."schBranch" "Emp_branch" on "Emp_"."branch" = "Emp_branch"."branch"
	left join "dbo"."glmulcmp" "Emp_branchsgm" on "Emp_branch"."sgm" = "Emp_branchsgm"."sgmid"
	left join "dbo"."PyGender" "Emp_branchGender" on "Emp_branch"."Genderid" = "Emp_branchGender"."Genderid"
	left join "dbo"."schStage" "Emp_branchstage" on "Emp_branch"."stagecode" = "Emp_branchstage"."Code"
	left join "dbo"."schtyp" "Emp_branchschtyp" on "Emp_branch"."schtypid" = "Emp_branchschtyp"."schtypid"
]]></text>
    </command>
