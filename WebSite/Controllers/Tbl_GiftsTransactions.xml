<?xml version="1.0" encoding="utf-8"?>
<dataController name="Tbl_GiftsTransactions" conflictDetection="overwriteChanges" label="Tbl Gifts Transactions" plugIn="eZee.Data.AnnotationPlugIn" handler="eZee.Rules.Tbl_GiftsTransactionsBusinessRules" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Tbl_GiftsTransactions"."Autoid" "Autoid"
	,"Tbl_GiftsTransactions"."Voucher_no" "Voucher_no"
	,"Tbl_GiftsTransactions"."Rainbow_no" "Rainbow_no"
	,"Tbl_GiftsTransactions"."Item_code" "Item_code"
	,"Item_"."arab_Desc" "Item_arab_Desc"
	,"Tbl_GiftsTransactions"."Qty" "Qty"
	,"Tbl_GiftsTransactions"."Date" "Date"
	,"Tbl_GiftsTransactions"."Delivery_Dealer_Gifts" "Delivery_Dealer_Gifts"
	,"Tbl_GiftsTransactions"."Gifts_In" "Gifts_In"
	,"Tbl_GiftsTransactions"."Gifts_Out" "Gifts_Out"
	,"Tbl_GiftsTransactions"."user_name" "user_name"
	,"Tbl_GiftsTransactions"."cause the exi" "causetheexi"
	,"Tbl_GiftsTransactions"."enterGifts" "enterGifts"
	,"Tbl_GiftsTransactions"."entertime" "entertime"
	,"Tbl_GiftsTransactions"."ModifiedBy" "ModifiedBy"
	,"Tbl_GiftsTransactions"."ModifiedOn" "ModifiedOn"
	,"Tbl_GiftsTransactions"."CreatedBy" "CreatedBy"
	,"Tbl_GiftsTransactions"."CreatedOn" "CreatedOn"
from "dbo"."Tbl_Gifts Transactions" "Tbl_GiftsTransactions"
	left join "dbo"."Tbl_Items and Codes" "Item_" on "Tbl_GiftsTransactions"."Item_code" = "Item_"."Item_Code"
]]></text>
    </command>
    <command id="AutoidIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Autoid" />
      </output>
    </command>
  </commands>
