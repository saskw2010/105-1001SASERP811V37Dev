    <field name="Cstm_Cntry_Cntry_Nm" type="String" readOnly="true" label="Cstm Cntry Nm" length="255" />
    <field name="Cstm_curncy_Curncy_Nm" type="String" readOnly="true" label="Cstm curncy Curncy Nm" length="200" />
    <field name="Cstm_TSNEFTSNEF_name" type="String" readOnly="true" label="Cstm TSNEFTSNEF name" length="150" />
    <field name="Cstm_TSNEFNO1TSNEF1_name" type="String" readOnly="true" label="Cstm TSNEFNO1TSNEF1 name" length="150" />
    <field name="Cstm_Slm_Slm_Nm" type="String" readOnly="true" label="Cstm Slm Nm" length="100" />
    <field name="Cstm_Slm_masngerclassmasngerclass" type="String" readOnly="true" allowQBE="false" allowSorting="false" label="Cstm Slm masngerclassmasngerclass" length="-1" />
    <field name="TransorderInform" type="String" label="Transorder Inform" length="160" showInSummary="true" />
    <field name="TransorderPrice" type="Decimal" label="Transorder Price" showInSummary="true" />
    <field name="transcurrentid" type="Int64" label="Transcurrentid" showInSummary="true">
      <items style="Lookup" dataController="transcurrentlocation" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="transcurrenttranscurrentdesc" type="String" readOnly="true" label="Transcurrenttranscurrentdesc" length="250" />
    <field name="transloadingDestid" type="Int64" label="transloading Destid">
      <items style="Lookup" dataController="transloadingDest" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="transloadingDesttransloadingDestdesc" type="String" readOnly="true" label="transloading Desttransloading Destdesc" length="250" />
    <field name="loadingtypeid" type="Int64" label="Loadingtypeid">
      <items style="Lookup" dataController="transloadingtype" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="loadingtypeloadingtypedesc" type="String" readOnly="true" label="Loadingtypeloadingtypedesc" length="250" />
    <field name="trnsdilvno" type="Int64" label="Trnsdilvno">
      <items style="Lookup" dataController="trnsdilvtype" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="trnsdilvtrnsdilvdesc" type="String" readOnly="true" label="Trnsdilvtrnsdilvdesc" length="250" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" />
    <field name="ModifiedOn" type="DateTime" label="Modified On" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="CreatedOn" type="DateTime" label="Created On" />
  </fields>
