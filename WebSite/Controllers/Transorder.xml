<?xml version="1.0" encoding="utf-8"?>
<dataController name="Transorder" conflictDetection="overwriteChanges" label="Transorder" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Transorder"."TransorderID" "TransorderID"
	,"Transorder"."TransorderDate" "TransorderDate"
	,"Transorder"."Cstm_no" "Cstm_no"
	,"Cstm_"."Cstm_Nm" "Cstm_Cstm_Nm"
	,"Cstm_Area"."Area_name" "Cstm_AreaArea_name"
	,"Cstm_Evaluation"."Evaluation_name" "Cstm_EvaluationEvaluation_name"
	,"Cstm_acc_"."Acc_Nm" "Cstm_acc_Acc_Nm"
	,"Cstm_acc_Clsacc_"."Acc_Nm" "Cstm_acc_Clsacc_Acc_Nm"
	,"Cstm_acc_Acc_Bnd"."Acc_Nm" "Cstm_acc_Acc_BndAcc_Nm"
	,"Cstm_Cntry_"."Cntry_Nm" "Cstm_Cntry_Cntry_Nm"
	,"Cstm_curncy_"."Curncy_Nm" "Cstm_curncy_Curncy_Nm"
	,"Cstm_TSNEF"."TSNEF_name" "Cstm_TSNEFTSNEF_name"
	,"Cstm_TSNEFNO1"."TSNEF1_name" "Cstm_TSNEFNO1TSNEF1_name"
	,"Cstm_Slm_"."Slm_Nm" "Cstm_Slm_Slm_Nm"
	,"Cstm_Slm_masngerclass"."masngerclass" "Cstm_Slm_masngerclassmasngerclass"
	,"Transorder"."TransorderInform" "TransorderInform"
	,"Transorder"."TransorderPrice" "TransorderPrice"
	,"Transorder"."transcurrentid" "transcurrentid"
	,"transcurrent"."transcurrentdesc" "transcurrenttranscurrentdesc"
	,"Transorder"."transloadingDestid" "transloadingDestid"
	,"transloadingDest"."transloadingDestdesc" "transloadingDesttransloadingDestdesc"
	,"Transorder"."loadingtypeid" "loadingtypeid"
	,"loadingtype"."loadingtypedesc" "loadingtypeloadingtypedesc"
	,"Transorder"."trnsdilvno" "trnsdilvno"
	,"trnsdilv"."trnsdilvdesc" "trnsdilvtrnsdilvdesc"
	,"Transorder"."ModifiedBy" "ModifiedBy"
	,"Transorder"."ModifiedOn" "ModifiedOn"
	,"Transorder"."CreatedBy" "CreatedBy"
	,"Transorder"."CreatedOn" "CreatedOn"
from "dbo"."Transorder" "Transorder"
	left join "dbo"."Stcustmr" "Cstm_" on "Transorder"."Cstm_no" = "Cstm_"."Cstm_No"
	left join "dbo"."AreaList" "Cstm_Area" on "Cstm_"."AreaNo" = "Cstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "Cstm_Evaluation" on "Cstm_"."EvaluationNO" = "Cstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "Cstm_acc_" on "Cstm_"."acc_no" = "Cstm_acc_"."Acc_No"
	left join "dbo"."GLmfbab" "Cstm_acc_Clsacc_" on "Cstm_acc_"."Clsacc_No" = "Cstm_acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Cstm_acc_Acc_Bnd" on "Cstm_acc_"."Acc_Bnd" = "Cstm_acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."PYCntry" "Cstm_Cntry_" on "Cstm_"."Cntry_No" = "Cstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Cstm_curncy_" on "Cstm_"."curncy_no" = "Cstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "Cstm_TSNEF" on "Cstm_"."TSNEFNO" = "Cstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "Cstm_TSNEFNO1" on "Cstm_"."TSNEFNO1" = "Cstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "Cstm_Slm_" on "Cstm_"."Slm_No" = "Cstm_Slm_"."Slm_No"
	left join "dbo"."masngerclass" "Cstm_Slm_masngerclass" on "Cstm_Slm_"."masngerclassid" = "Cstm_Slm_masngerclass"."masngerclassid"
	left join "dbo"."transcurrentlocation" "transcurrent" on "Transorder"."transcurrentid" = "transcurrent"."transcurrentid"
	left join "dbo"."transloadingDest" "transloadingDest" on "Transorder"."transloadingDestid" = "transloadingDest"."transloadingDestid"
	left join "dbo"."transloadingtype" "loadingtype" on "Transorder"."loadingtypeid" = "loadingtype"."loadingtypeid"
	left join "dbo"."trnsdilvtype" "trnsdilv" on "Transorder"."trnsdilvno" = "trnsdilv"."trnsdilvno"
]]></text>
    </command>
    <command id="TransorderIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="TransorderID" />
      </output>
    </command>
  </commands>
