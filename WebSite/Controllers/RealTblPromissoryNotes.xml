	left join "dbo"."RealTblCustomers" "Rainbow_" on "RealTblPromissoryNotes"."Rainbow_no" = "Rainbow_"."Rainbow_no"
	left join "dbo"."Ref_Kindlist" "Rainbow_Ref_Kind" on "Rainbow_"."Ref_KindID" = "Rainbow_Ref_Kind"."Ref_KindID"
	left join "dbo"."Stcustmr" "Rainbow_Customer_" on "Rainbow_"."Customer_no" = "Rainbow_Customer_"."Cstm_No"
	left join "dbo"."AreaList" "Rainbow_Customer_Area" on "Rainbow_Customer_"."AreaNo" = "Rainbow_Customer_Area"."AreaNo"
	left join "dbo"."EvaluationList" "Rainbow_Customer_Evaluation" on "Rainbow_Customer_"."EvaluationNO" = "Rainbow_Customer_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "Rainbow_Customer_acc_" on "Rainbow_Customer_"."acc_no" = "Rainbow_Customer_acc_"."Acc_No"
	left join "dbo"."PYCntry" "Rainbow_Customer_Cntry_" on "Rainbow_Customer_"."Cntry_No" = "Rainbow_Customer_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Rainbow_Customer_curncy_" on "Rainbow_Customer_"."curncy_no" = "Rainbow_Customer_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "Rainbow_Customer_TSNEF" on "Rainbow_Customer_"."TSNEFNO" = "Rainbow_Customer_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "Rainbow_Customer_TSNEFNO1" on "Rainbow_Customer_"."TSNEFNO1" = "Rainbow_Customer_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "Rainbow_Customer_Slm_" on "Rainbow_Customer_"."Slm_No" = "Rainbow_Customer_Slm_"."Slm_No"
	left join "dbo"."Tbl_Collectors" "Rainbow_collector" on "Rainbow_"."collectorid" = "Rainbow_collector"."collectorid"
	left join "dbo"."collectmdlist" "Rainbow_collectorcollectmd" on "Rainbow_collector"."collectmdid" = "Rainbow_collectorcollectmd"."collectmdid"
	left join "dbo"."Tbl_Dealers" "Rainbow_Dealer" on "Rainbow_"."DealerID" = "Rainbow_Dealer"."DealerID"
	left join "dbo"."Tbl_MD" "Rainbow_DealerMD" on "Rainbow_Dealer"."MDID" = "Rainbow_DealerMD"."MDID"
	left join "dbo"."Tbl_MD" "Rainbow_MD" on "Rainbow_"."MDID" = "Rainbow_MD"."MDID"
	left join "dbo"."Tbl_Nationality" "Rainbow_Nationality" on "Rainbow_"."NationalityID" = "Rainbow_Nationality"."NationalityID"
	left join "dbo"."Tbl_Regions" "Rainbow_Region" on "Rainbow_"."RegionID" = "Rainbow_Region"."RegionID"
	left join "dbo"."tblmodellist" "Rainbow_model" on "Rainbow_"."modelno" = "Rainbow_model"."modelno"
	left join "dbo"."tblCatidlist" "Rainbow_modelCatidlist" on "Rainbow_model"."Catidlistid" = "Rainbow_modelCatidlist"."Catidlistid"
	left join "dbo"."tblReplacelist" "Rainbow_Replace" on "Rainbow_"."Replaceid" = "Rainbow_Replace"."Replaceid"
	left join "dbo"."Tbl_Collectors" "collector" on "RealTblPromissoryNotes"."collectorid" = "collector"."collectorid"
	left join "dbo"."collectmdlist" "collectorcollectmd" on "collector"."collectmdid" = "collectorcollectmd"."collectmdid"
]]></text>
