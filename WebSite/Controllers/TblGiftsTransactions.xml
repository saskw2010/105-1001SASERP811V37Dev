	,"Rainbow_Region"."Region" "Rainbow_RegionRegion"
	,"Rainbow_IDATUTO"."Time" "Rainbow_IDATUTOTime"
	,"Rainbow_IDATUTOCallDep"."CallDepDesc" "Rainbow_IDATUTOCallDepCallDepDesc"
	,"Rainbow_IDATUTORef_Kind"."Ref_Kind" "Rainbow_IDATUTORef_KindRef_Kind"
	,"Rainbow_IDATUTObranch"."Desc1" "Rainbow_IDATUTObranchDesc1"
	,"Rainbow_IDATUTODay"."اليوم" "Rainbow_IDATUTODayاليوم"
	,"Rainbow_IDATUTODealer"."Dealer_name" "Rainbow_IDATUTODealerDealer_name"
	,"Rainbow_IDATUTOMD"."MD_Name" "Rainbow_IDATUTOMDMD_Name"
	,"Rainbow_IDATUTORegion"."Region" "Rainbow_IDATUTORegionRegion"
	,"Rainbow_IDATUTOResult"."ResultDesc" "Rainbow_IDATUTOResultResultDesc"
	,"Rainbow_IDATUTOTele_Mark_Sec"."Tele_Mark_Sec" "Rainbow_IDATUTOTele_Mark_SecTele_Mark_Sec"
	,"Rainbow_IDATUTOTime"."timedesc" "Rainbow_IDATUTOTimetimedesc"
	,"Rainbow_model"."modeldesc" "Rainbow_modelmodeldesc"
	,"Rainbow_modelCatidlist"."Catidlistdesc" "Rainbow_modelCatidlistCatidlistdesc"
	,"Rainbow_Replace"."ReplaceDesc" "Rainbow_ReplaceReplaceDesc"
	,"TblGiftsTransactions"."Item_code" "Item_code"
	,"Item_"."arab_Desc" "Item_arab_Desc"
	,"TblGiftsTransactions"."Qty" "Qty"
	,"TblGiftsTransactions"."Date" "Date"
	,"TblGiftsTransactions"."DealerID" "DealerID"
