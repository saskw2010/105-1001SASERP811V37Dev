	left join "dbo"."TblCustomers" "Rainbow_" on "TblGiftsTransactions"."Rainbow_no" = "Rainbow_"."Rainbow_no"
	left join "dbo"."Ref_Kindlist" "Rainbow_Ref_Kind" on "Rainbow_"."Ref_KindID" = "Rainbow_Ref_Kind"."Ref_KindID"
	left join "dbo"."Tbl_Collectors" "Rainbow_collector" on "Rainbow_"."collectorid" = "Rainbow_collector"."collectorid"
	left join "dbo"."collectmdlist" "Rainbow_collectorcollectmd" on "Rainbow_collector"."collectmdid" = "Rainbow_collectorcollectmd"."collectmdid"
	left join "dbo"."Tbl_Dealers" "Rainbow_Dealer" on "Rainbow_"."DealerID" = "Rainbow_Dealer"."DealerID"
	left join "dbo"."Tbl_MD" "Rainbow_DealerMD" on "Rainbow_Dealer"."MDID" = "Rainbow_DealerMD"."MDID"
	left join "dbo"."Tbl_MD" "Rainbow_MD" on "Rainbow_"."MDID" = "Rainbow_MD"."MDID"
	left join "dbo"."Tbl_Nationality" "Rainbow_Nationality" on "Rainbow_"."NationalityID" = "Rainbow_Nationality"."NationalityID"
	left join "dbo"."Tbl_Regions" "Rainbow_Region" on "Rainbow_"."RegionID" = "Rainbow_Region"."RegionID"
	left join "dbo"."TblAppointments" "Rainbow_IDATUTO" on "Rainbow_"."IDATUTO" = "Rainbow_IDATUTO"."IDATUTO"
	left join "dbo"."CallDeplist" "Rainbow_IDATUTOCallDep" on "Rainbow_IDATUTO"."CallDepid" = "Rainbow_IDATUTOCallDep"."CallDepid"
	left join "dbo"."Ref_Kindlist" "Rainbow_IDATUTORef_Kind" on "Rainbow_IDATUTO"."Ref_KindID" = "Rainbow_IDATUTORef_Kind"."Ref_KindID"
	left join "dbo"."schBranch" "Rainbow_IDATUTObranch" on "Rainbow_IDATUTO"."branch" = "Rainbow_IDATUTObranch"."branch"
	left join "dbo"."Tbl_Day" "Rainbow_IDATUTODay" on "Rainbow_IDATUTO"."DayID" = "Rainbow_IDATUTODay"."DayID"
	left join "dbo"."Tbl_Dealers" "Rainbow_IDATUTODealer" on "Rainbow_IDATUTO"."DealerID" = "Rainbow_IDATUTODealer"."DealerID"
	left join "dbo"."Tbl_MD" "Rainbow_IDATUTOMD" on "Rainbow_IDATUTO"."MDID" = "Rainbow_IDATUTOMD"."MDID"
	left join "dbo"."Tbl_Regions" "Rainbow_IDATUTORegion" on "Rainbow_IDATUTO"."RegionID" = "Rainbow_IDATUTORegion"."RegionID"
	left join "dbo"."Tbl_Results" "Rainbow_IDATUTOResult" on "Rainbow_IDATUTO"."ResultID" = "Rainbow_IDATUTOResult"."ResultID"
	left join "dbo"."Tbl_Tele_Mark_Sec" "Rainbow_IDATUTOTele_Mark_Sec" on "Rainbow_IDATUTO"."Tele_Mark_SecID" = "Rainbow_IDATUTOTele_Mark_Sec"."Tele_Mark_SecID"
	left join "dbo"."Tbl_Time" "Rainbow_IDATUTOTime" on "Rainbow_IDATUTO"."Time" = "Rainbow_IDATUTOTime"."الوقت"
	left join "dbo"."tblmodellist" "Rainbow_model" on "Rainbow_"."modelno" = "Rainbow_model"."modelno"
	left join "dbo"."tblCatidlist" "Rainbow_modelCatidlist" on "Rainbow_model"."Catidlistid" = "Rainbow_modelCatidlist"."Catidlistid"
	left join "dbo"."tblReplacelist" "Rainbow_Replace" on "Rainbow_"."Replaceid" = "Rainbow_Replace"."Replaceid"
	left join "dbo"."Tbl_Items and Codes" "Item_" on "TblGiftsTransactions"."Item_code" = "Item_"."Item_Code"
	left join "dbo"."Tbl_Dealers" "Dealer" on "TblGiftsTransactions"."DealerID" = "Dealer"."DealerID"
	left join "dbo"."Tbl_MD" "DealerMD" on "Dealer"."MDID" = "DealerMD"."MDID"
]]></text>
