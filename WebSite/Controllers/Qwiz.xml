  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Qwiz"."qwizid" "qwizid"
	,"Qwiz"."qwizquastion" "qwizquastion"
	,"Qwiz"."catid1" "catid1"
	,"Thecatid1"."cat1name" "Thecatid1cat1name"
	,"Qwiz"."catid2" "catid2"
	,"Thecatid2"."cat2name" "Thecatid2cat2name"
	,"Qwiz"."catid3" "catid3"
	,"Thecatid3"."cat3name" "Thecatid3cat3name"
	,"Qwiz"."catid4" "catid4"
	,"Thecatid4"."cat4name" "Thecatid4cat4name"
	,"Qwiz"."catid5" "catid5"
	,"Thecatid5"."cat5name" "Thecatid5cat5name"
	,"Qwiz"."catid6" "catid6"
	,"Thecatid6"."cat6name" "Thecatid6cat6name"
	,"Qwiz"."catid7" "catid7"
	,"Thecatid7"."cat7name" "Thecatid7cat7name"
	,"Qwiz"."catid8" "catid8"
	,"Thecatid8"."cat8name" "Thecatid8cat8name"
	,"Qwiz"."catid9" "catid9"
	,"Thecatid9"."cat9name" "Thecatid9cat9name"
	,"Qwiz"."ModifiedBy" "ModifiedBy"
	,"Qwiz"."ModifiedOn" "ModifiedOn"
	,"Qwiz"."CreatedBy" "CreatedBy"
	,"Qwiz"."CreatedOn" "CreatedOn"
from "dbo"."Qwiz" "Qwiz"
	left join "dbo"."Qwaizcat1" "Thecatid1" on "Qwiz"."catid1" = "Thecatid1"."catid1"
	left join "dbo"."Qwaizcat2" "Thecatid2" on "Qwiz"."catid2" = "Thecatid2"."catid2"
	left join "dbo"."Qwaizcat3" "Thecatid3" on "Qwiz"."catid3" = "Thecatid3"."catid3"
	left join "dbo"."Qwaizcat4" "Thecatid4" on "Qwiz"."catid4" = "Thecatid4"."catid4"
	left join "dbo"."Qwaizcat5" "Thecatid5" on "Qwiz"."catid5" = "Thecatid5"."catid5"
	left join "dbo"."Qwaizcat6" "Thecatid6" on "Qwiz"."catid6" = "Thecatid6"."catid6"
	left join "dbo"."Qwaizcat7" "Thecatid7" on "Qwiz"."catid7" = "Thecatid7"."catid7"
	left join "dbo"."Qwaizcat8" "Thecatid8" on "Qwiz"."catid8" = "Thecatid8"."catid8"
	left join "dbo"."Qwaizcat9" "Thecatid9" on "Qwiz"."catid9" = "Thecatid9"."catid9"
]]></text>
    </command>
    <command id="qwizidIdentityCommand" type="Text" event="Inserted">
