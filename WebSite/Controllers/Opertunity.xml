    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Opertunity"."OpertunityID" "OpertunityID"
	,"Opertunity"."callid" "callid"
	,"call"."customertel1" "callcustomertel1"
	,"callMarketplan"."MarketplanDesc" "callMarketplanMarketplanDesc"
	,"callproducts"."productsname" "callproductsproductsname"
	,"callSlm_"."Slm_Nm" "callSlm_Slm_Nm"
	,"callSlm_masngerclass"."masngerclass" "callSlm_masngerclassmasngerclass"
	,"callVisitstatus"."Visitstatusname" "callVisitstatusVisitstatusname"
	,"callvisittype"."visittypeNm" "callvisittypevisittypeNm"
	,"Opertunity"."Cstm_no" "Cstm_no"
	,"Cstm_"."Cstm_Nm" "Cstm_Cstm_Nm"
	,"Cstm_Area"."Area_name" "Cstm_AreaArea_name"
	,"Cstm_Evaluation"."Evaluation_name" "Cstm_EvaluationEvaluation_name"
	,"Cstm_acc_"."Acc_Nm" "Cstm_acc_Acc_Nm"
	,"Cstm_acc_Clsacc_"."Acc_Nm" "Cstm_acc_Clsacc_Acc_Nm"
	,"Cstm_acc_Acc_Bnd"."Acc_Nm" "Cstm_acc_Acc_BndAcc_Nm"
	,"Cstm_Cntry_"."Cntry_Nm" "Cstm_Cntry_Cntry_Nm"
	,"Cstm_curncy_"."Curncy_Nm" "Cstm_curncy_Curncy_Nm"
	,"Cstm_TSNEF"."TSNEF_name" "Cstm_TSNEFTSNEF_name"
	,"Cstm_TSNEFNO1"."TSNEF1_name" "Cstm_TSNEFNO1TSNEF1_name"
	,"Cstm_Slm_"."Slm_Nm" "Cstm_Slm_Slm_Nm"
	,"Cstm_Slm_masngerclass"."masngerclass" "Cstm_Slm_masngerclassmasngerclass"
	,"Opertunity"."Slm_No" "Slm_No"
	,"Slm_"."Slm_Nm" "Slm_Slm_Nm"
	,"Slm_masngerclass"."masngerclass" "Slm_masngerclassmasngerclass"
	,"Opertunity"."openslm_no" "openslm_no"
	,"openslm_"."Slm_Nm" "openslm_Slm_Nm"
	,"openslm_masngerclass"."masngerclass" "openslm_masngerclassmasngerclass"
	,"Opertunity"."OpertunityclassID" "OpertunityclassID"
	,"Opertunityclass"."Opertunityclass" "OpertunityclassOpertunityclass"
	,"Opertunity"."OpertunitydegreeID" "OpertunitydegreeID"
	,"Opertunitydegree"."Opertunitydegree" "OpertunitydegreeOpertunitydegree"
	,"Opertunity"."Opendate" "Opendate"
	,"Opertunity"."Estmationclosedate" "Estmationclosedate"
	,"Opertunity"."EstAmountmin" "EstAmountmin"
	,"Opertunity"."EstAmountmax" "EstAmountmax"
	,"Opertunity"."OpertunityDesc" "OpertunityDesc"
	,"Opertunity"."OpertunityComments" "OpertunityComments"
	,"Opertunity"."ModifiedBy" "ModifiedBy"
	,"Opertunity"."ModifiedOn" "ModifiedOn"
	,"Opertunity"."CreatedBy" "CreatedBy"
	,"Opertunity"."CreatedOn" "CreatedOn"
from "dbo"."Opertunity" "Opertunity"
	left join "dbo"."callregister" "call" on "Opertunity"."callid" = "call"."callid"
	left join "dbo"."Marketplan" "callMarketplan" on "call"."MarketplanID" = "callMarketplan"."MarketplanID"
	left join "dbo"."ProductsList" "callproducts" on "call"."productsID" = "callproducts"."productsid"
	left join "dbo"."Stslsman" "callSlm_" on "call"."Slm_No" = "callSlm_"."Slm_No"
	left join "dbo"."masngerclass" "callSlm_masngerclass" on "callSlm_"."masngerclassid" = "callSlm_masngerclass"."masngerclassid"
	left join "dbo"."Visitstatuslist" "callVisitstatus" on "call"."Visitstatus" = "callVisitstatus"."VisitstatusID"
	left join "dbo"."visittypelist" "callvisittype" on "call"."visittype" = "callvisittype"."visittypeNo"
	left join "dbo"."Stcustmr" "Cstm_" on "Opertunity"."Cstm_no" = "Cstm_"."Cstm_No"
	left join "dbo"."AreaList" "Cstm_Area" on "Cstm_"."AreaNo" = "Cstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "Cstm_Evaluation" on "Cstm_"."EvaluationNO" = "Cstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "Cstm_acc_" on "Cstm_"."acc_no" = "Cstm_acc_"."Acc_No"
	left join "dbo"."GLmfbab" "Cstm_acc_Clsacc_" on "Cstm_acc_"."Clsacc_No" = "Cstm_acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Cstm_acc_Acc_Bnd" on "Cstm_acc_"."Acc_Bnd" = "Cstm_acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."PYCntry" "Cstm_Cntry_" on "Cstm_"."Cntry_No" = "Cstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Cstm_curncy_" on "Cstm_"."curncy_no" = "Cstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "Cstm_TSNEF" on "Cstm_"."TSNEFNO" = "Cstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "Cstm_TSNEFNO1" on "Cstm_"."TSNEFNO1" = "Cstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "Cstm_Slm_" on "Cstm_"."Slm_No" = "Cstm_Slm_"."Slm_No"
	left join "dbo"."masngerclass" "Cstm_Slm_masngerclass" on "Cstm_Slm_"."masngerclassid" = "Cstm_Slm_masngerclass"."masngerclassid"
	left join "dbo"."Stslsman" "Slm_" on "Opertunity"."Slm_No" = "Slm_"."Slm_No"
	left join "dbo"."masngerclass" "Slm_masngerclass" on "Slm_"."masngerclassid" = "Slm_masngerclass"."masngerclassid"
	left join "dbo"."Stslsman" "openslm_" on "Opertunity"."openslm_no" = "openslm_"."Slm_No"
	left join "dbo"."masngerclass" "openslm_masngerclass" on "openslm_"."masngerclassid" = "openslm_masngerclass"."masngerclassid"
	left join "dbo"."Opertunityclasslist" "Opertunityclass" on "Opertunity"."OpertunityclassID" = "Opertunityclass"."OpertunityclassID"
	left join "dbo"."Opertunitydegreelist" "Opertunitydegree" on "Opertunity"."OpertunitydegreeID" = "Opertunitydegree"."OpertunitydegreeID"
]]></text>
    </command>
