  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"CustOrderHist_Cache"."CacheID" "CacheID"
	,"CustOrderHist_Cache"."SessionTime" "SessionTime"
	,"CustOrderHist_Cache"."recordTime1" "recordTime1"
	,"CustOrderHist_Cache"."CacheUserName" "CacheUserName"
	,"CustOrderHist_Cache"."CacheExpires" "CacheExpires"
	,"CustOrderHist_Cache"."qwizid" "qwizid"
	,"CustOrderHist_Cache"."QwizidORDER" "QwizidORDER"
	,"CustOrderHist_Cache"."catid2" "catid2"
	,"Thecatid2"."cat2name" "Thecatid2cat2name"
	,"Thecatid2Cntry_"."Cntry_Nm" "Thecatid2Cntry_Cntry_Nm"
	,"CustOrderHist_Cache"."catid3" "catid3"
	,"Thecatid3"."cat3name" "Thecatid3cat3name"
	,"Thecatid3catid2"."cat2name" "Thecatid3catid2cat2name"
	,"Thecatid3catid2Cntry_"."Cntry_Nm" "Thecatid3catid2Cntry_Cntry_Nm"
	,"CustOrderHist_Cache"."catid4" "catid4"
	,"Thecatid4"."cat4name" "Thecatid4cat4name"
	,"CustOrderHist_Cache"."catid5" "catid5"
	,"Thecatid5"."cat5name" "Thecatid5cat5name"
	,"CustOrderHist_Cache"."catid1" "catid1"
	,"CustOrderHist_Cache"."catid6" "catid6"
	,"CustOrderHist_Cache"."catid7" "catid7"
	,"CustOrderHist_Cache"."catid8" "catid8"
	,"CustOrderHist_Cache"."catid9" "catid9"
	,"CustOrderHist_Cache"."catid10" "catid10"
	,"CustOrderHist_Cache"."catid11" "catid11"
	,"CustOrderHist_Cache"."catid12" "catid12"
	,"CustOrderHist_Cache"."catid13" "catid13"
	,"CustOrderHist_Cache"."catid14" "catid14"
from "dbo"."CustOrderHist_Cache" "CustOrderHist_Cache"
	left join "dbo"."Qwaizcat2" "Thecatid2" on "CustOrderHist_Cache"."catid2" = "Thecatid2"."catid2"
	left join "dbo"."PYCntry" "Thecatid2Cntry_" on "Thecatid2"."Cntry_No" = "Thecatid2Cntry_"."Cntry_No"
	left join "dbo"."Qwaizcat3" "Thecatid3" on "CustOrderHist_Cache"."catid3" = "Thecatid3"."catid3"
	left join "dbo"."Qwaizcat2" "Thecatid3catid2" on "Thecatid3"."catid2" = "Thecatid3catid2"."catid2"
	left join "dbo"."PYCntry" "Thecatid3catid2Cntry_" on "Thecatid3catid2"."Cntry_No" = "Thecatid3catid2Cntry_"."Cntry_No"
	left join "dbo"."Qwaizcat4" "Thecatid4" on "CustOrderHist_Cache"."catid4" = "Thecatid4"."catid4"
	left join "dbo"."Qwaizcat5" "Thecatid5" on "CustOrderHist_Cache"."catid5" = "Thecatid5"."catid5"
]]></text>
    </command>
