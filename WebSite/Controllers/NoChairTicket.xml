	,"NoChairTicket"."Price" "Price"
	,"NoChairTicket"."HallID" "HallID"
	,"NoChairTicket"."ClassID" "ClassID"
	,"NoChairTicket"."ModifiedBy" "ModifiedBy"
	,"NoChairTicket"."ModifiedByUserId" "ModifiedByUserId"
	,"NoChairTicket"."ModifiedOn" "ModifiedOn"
	,"NoChairTicket"."CreatedBy" "CreatedBy"
	,"NoChairTicket"."CreatedById" "CreatedById"
	,"NoChairTicket"."CreatedOn" "CreatedOn"
from "dbo"."NoChairTicket" "NoChairTicket"
	left join "dbo"."AdminData" "Admin" on "NoChairTicket"."AdminID" = "Admin"."AdminID"
]]></text>
    </command>
    <command id="TicketNumberIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
