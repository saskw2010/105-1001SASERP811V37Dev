    <command id="command1" type="Text">
      <text><![CDATA[
select
	"NoChairTicket"."AdminID" "AdminID"
	,"Admin"."LoginName" "AdminLoginName"
	,"NoChairTicket"."TicketNumber" "TicketNumber"
	,"NoChairTicket"."ChairNo" "ChairNo"
