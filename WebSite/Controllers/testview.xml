<?xml version="1.0" encoding="utf-8"?>
<dataController name="testview" conflictDetection="overwriteChanges" label="Testview" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"testview"."reqcat_id" "reqcat_id"
	,"testview"."reqcat_name" "reqcat_name"
	,"testview"."req_Type" "req_Type"
	,"testview"."req_url" "req_url"
	,"testview"."userarea_id" "userarea_id"
	,"testview"."userarea_Desc" "userarea_Desc"
	,"testview"."company_name" "company_name"
	,"testview"."company_id" "company_id"
from "dbo"."testview" "testview"
]]></text>
    </command>
    <command id="company_idIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="company_id" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="reqcat_id" type="Int64" allowNulls="false" label="Reqcat#" showInSummary="true" />
    <field name="reqcat_name" type="String" label="Reqcat Name" length="100" showInSummary="true" />
    <field name="req_Type" type="String" allowNulls="false" label="req Type" length="100" showInSummary="true" />
    <field name="req_url" type="String" allowQBE="false" allowSorting="false" label="Req Url" length="-1" showInSummary="true" />
    <field name="userarea_id" type="Int64" label="Userarea#" showInSummary="true" />
    <field name="userarea_Desc" type="String" label="userarea Desc" length="100" />
    <field name="company_name" type="String" label="Company Name" length="150" />
    <field name="company_id" type="Int64" label="Company#" readOnly="true" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Testview">
      <headerText>$DefaultGridViewDescription</headerText>
