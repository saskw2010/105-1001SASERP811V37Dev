          <dataFields>
            <dataField fieldName="reqcat_id" columns="15" />
            <dataField fieldName="reqcat_name" />
            <dataField fieldName="req_Type" />
            <dataField fieldName="req_url" rows="5" />
            <dataField fieldName="userarea_id" columns="15" />
            <dataField fieldName="userarea_Desc" />
            <dataField fieldName="company_name" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
  <actions>
    <actionGroup id="ag1" scope="Grid">
