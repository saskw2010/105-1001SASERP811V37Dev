  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"prostermcars"."prostermcarsid" "prostermcarsid"
	,"prostermcars"."prostermcode" "prostermcode"
	,"prostermcars"."Car_type" "Car_type"
	,"Car_type"."Styl_Nm" "Car_typeStyl_Nm"
	,"prostermcars"."carkindid" "carkindid"
	,"carkind"."carkinddesc" "carkindcarkinddesc"
	,"carkindStyl_"."Styl_Nm" "carkindStyl_Styl_Nm"
	,"prostermcars"."Fbrct_Yer" "Fbrct_Yer"
	,"prostermcars"."Daily_Rate" "Daily_Rate"
	,"prostermcars"."weekly_Rate" "weekly_Rate"
	,"prostermcars"."Monthly_Rate" "Monthly_Rate"
	,"prostermcars"."Freekm" "Freekm"
	,"prostermcars"."KM_Penalty" "KM_Penalty"
	,"prostermcars"."ExtraDayprice" "ExtraDayprice"
	,"prostermcars"."Extrahourprice" "Extrahourprice"
	,"prostermcars"."ModifiedBy" "ModifiedBy"
	,"prostermcars"."ModifiedByUserId" "ModifiedByUserId"
	,"prostermcars"."ModifiedOn" "ModifiedOn"
	,"prostermcars"."CreatedBy" "CreatedBy"
	,"prostermcars"."CreatedById" "CreatedById"
	,"prostermcars"."CreatedOn" "CreatedOn"
from "dbo"."prostermcars" "prostermcars"
	left join "dbo"."CaCrstyl" "Car_type" on "prostermcars"."Car_type" = "Car_type"."Styl_No"
	left join "dbo"."carkindlist" "carkind" on "prostermcars"."carkindid" = "carkind"."carkindid"
	left join "dbo"."CaCrstyl" "carkindStyl_" on "carkind"."Styl_No" = "carkindStyl_"."Styl_No"
]]></text>
    </command>
    <command id="prostermcarsidIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="prostermcarsid" />
      </output>
    </command>
  </commands>
