	,"GROUPStk"."ModifiedOn" "ModifiedOn"
	,"GROUPStk"."CreatedBy" "CreatedBy"
	,"GROUPStk"."CreatedOn" "CreatedOn"
	,"GROUPStk"."id" "id"
from "dbo"."GROUPStk" "GROUPStk"
	left join "dbo"."GROUPStk" "MASTERNOD" on "GROUPStk"."MASTERNOD" = "MASTERNOD"."IDGROUP"
]]></text>
