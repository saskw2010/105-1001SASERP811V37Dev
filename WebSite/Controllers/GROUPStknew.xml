select
	"GROUPStk"."IDGROUP" "IDGROUP"
	,"GROUPStk"."MASTERNOD" "MASTERNOD"
	,"MASTERNOD"."IDGROUP" "MASTERNODID_USER"
	,"GROUPStk"."ID_USER" "ID_USER"
