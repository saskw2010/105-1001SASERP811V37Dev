  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"CntCert"."Cstm_No" "Cstm_No"
	,"Cstm_"."Cstm_Nm" "Cstm_Cstm_Nm"
	,"Cstm_Area"."Area_name" "Cstm_AreaArea_name"
	,"Cstm_Evaluation"."Evaluation_name" "Cstm_EvaluationEvaluation_name"
	,"Cstm_acc_"."Acc_Nm" "Cstm_acc_Acc_Nm"
	,"Cstm_acc_Clsacc_"."Acc_Nm" "Cstm_acc_Clsacc_Acc_Nm"
	,"Cstm_acc_Acc_Bnd"."Acc_Nm" "Cstm_acc_Acc_BndAcc_Nm"
	,"Cstm_Cntry_"."Cntry_Nm" "Cstm_Cntry_Cntry_Nm"
	,"Cstm_curncy_"."Curncy_Nm" "Cstm_curncy_Curncy_Nm"
	,"Cstm_TSNEF"."TSNEF_name" "Cstm_TSNEFTSNEF_name"
	,"Cstm_TSNEFNO1"."TSNEF1_name" "Cstm_TSNEFNO1TSNEF1_name"
	,"Cstm_Slm_"."Slm_Nm" "Cstm_Slm_Slm_Nm"
	,"Cstm_Slm_masngerclass"."masngerclass" "Cstm_Slm_masngerclassmasngerclass"
	,"CntCert"."ItmCnNo" "ItmCnNo"
	,"CntCert"."Prj_No" "Prj_No"
	,"Prj_"."Prj_Nm" "Prj_Prj_Nm"
	,"Prj_Cstm_"."Cstm_Nm" "Prj_Cstm_Cstm_Nm"
	,"Prj_Cstm_Area"."Area_name" "Prj_Cstm_AreaArea_name"
	,"Prj_Cstm_Evaluation"."Evaluation_name" "Prj_Cstm_EvaluationEvaluation_name"
	,"Prj_Cstm_acc_"."Acc_Nm" "Prj_Cstm_acc_Acc_Nm"
	,"Prj_Cstm_Cntry_"."Cntry_Nm" "Prj_Cstm_Cntry_Cntry_Nm"
	,"Prj_Cstm_curncy_"."Curncy_Nm" "Prj_Cstm_curncy_Curncy_Nm"
	,"Prj_Cstm_TSNEF"."TSNEF_name" "Prj_Cstm_TSNEFTSNEF_name"
	,"Prj_Cstm_TSNEFNO1"."TSNEF1_name" "Prj_Cstm_TSNEFNO1TSNEF1_name"
	,"Prj_Cstm_Slm_"."Slm_Nm" "Prj_Cstm_Slm_Slm_Nm"
	,"CntCert"."No" "No"
	,"CntCert"."itmCode" "itmCode"
	,"itm"."Ac_Nm1" "itmAc_Nm1"
	,"CntCert"."Exec_Priv" "Exec_Priv"
	,"CntCert"."Exec_Prs" "Exec_Prs"
	,"CntCert"."OutPut" "OutPut"
	,"CntCert"."Discou" "Discou"
	,"CntCert"."Pure" "Pure"
	,"CntCert"."Exec_Date" "Exec_Date"
	,"CntCert"."Doc_no" "Doc_no"
	,"CntCert"."ModifiedBy" "ModifiedBy"
	,"CntCert"."ModifiedOn" "ModifiedOn"
	,"CntCert"."CreatedBy" "CreatedBy"
	,"CntCert"."CreatedOn" "CreatedOn"
from "dbo"."CntCert" "CntCert"
	left join "dbo"."Stcustmr" "Cstm_" on "CntCert"."Cstm_No" = "Cstm_"."Cstm_No"
	left join "dbo"."AreaList" "Cstm_Area" on "Cstm_"."AreaNo" = "Cstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "Cstm_Evaluation" on "Cstm_"."EvaluationNO" = "Cstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "Cstm_acc_" on "Cstm_"."acc_no" = "Cstm_acc_"."Acc_No"
	left join "dbo"."GLmfbab" "Cstm_acc_Clsacc_" on "Cstm_acc_"."Clsacc_No" = "Cstm_acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Cstm_acc_Acc_Bnd" on "Cstm_acc_"."Acc_Bnd" = "Cstm_acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."PYCntry" "Cstm_Cntry_" on "Cstm_"."Cntry_No" = "Cstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Cstm_curncy_" on "Cstm_"."curncy_no" = "Cstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "Cstm_TSNEF" on "Cstm_"."TSNEFNO" = "Cstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "Cstm_TSNEFNO1" on "Cstm_"."TSNEFNO1" = "Cstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "Cstm_Slm_" on "Cstm_"."Slm_No" = "Cstm_Slm_"."Slm_No"
	left join "dbo"."masngerclass" "Cstm_Slm_masngerclass" on "Cstm_Slm_"."masngerclassid" = "Cstm_Slm_masngerclass"."masngerclassid"
	left join "dbo"."PJcntmf" "Prj_" on "CntCert"."Prj_No" = "Prj_"."Prj_No"
	left join "dbo"."Stcustmr" "Prj_Cstm_" on "Prj_"."Cstm_No" = "Prj_Cstm_"."Cstm_No"
	left join "dbo"."AreaList" "Prj_Cstm_Area" on "Prj_Cstm_"."AreaNo" = "Prj_Cstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "Prj_Cstm_Evaluation" on "Prj_Cstm_"."EvaluationNO" = "Prj_Cstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "Prj_Cstm_acc_" on "Prj_Cstm_"."acc_no" = "Prj_Cstm_acc_"."Acc_No"
	left join "dbo"."PYCntry" "Prj_Cstm_Cntry_" on "Prj_Cstm_"."Cntry_No" = "Prj_Cstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Prj_Cstm_curncy_" on "Prj_Cstm_"."curncy_no" = "Prj_Cstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "Prj_Cstm_TSNEF" on "Prj_Cstm_"."TSNEFNO" = "Prj_Cstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "Prj_Cstm_TSNEFNO1" on "Prj_Cstm_"."TSNEFNO1" = "Prj_Cstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "Prj_Cstm_Slm_" on "Prj_Cstm_"."Slm_No" = "Prj_Cstm_Slm_"."Slm_No"
	left join "dbo"."Prjbudgac1" "itm" on "CntCert"."itmCode" = "itm"."Ac_No1"
]]></text>
    </command>
