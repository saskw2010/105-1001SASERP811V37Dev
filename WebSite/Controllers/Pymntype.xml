<?xml version="1.0" encoding="utf-8"?>
<dataController name="Pymntype" conflictDetection="overwriteChanges" label="Pymntype" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Pymntype"."Pymnt_No" "Pymnt_No"
	,"Pymntype"."Pymnt_Nm" "Pymnt_Nm"
	,"Pymntype"."Pymnt_Nme" "Pymnt_Nme"
	,"Pymntype"."sgm" "sgm"
	,"sgm"."sgm_Nm" "sgmsgm_Nm"
	,"sgmOpco"."OpcoName" "sgmOpcoOpcoName"
	,"Pymntype"."pymnt_acc" "pymnt_acc"
	,"pymnt_acc"."Acc_Nm" "pymnt_accAcc_Nm"
	,"pymnt_accClsacc_"."Acc_Nm" "pymnt_accClsacc_Acc_Nm"
	,"pymnt_accAcc_Bnd"."Acc_Nm" "pymnt_accAcc_BndAcc_Nm"
	,"pymnt_accAcc_Bndaccclass"."accountclass" "pymnt_accAcc_Bndaccclassaccountclass"
	,"pymnt_accAcc_BndAcc_Bab"."Acc_Nm" "pymnt_accAcc_BndAcc_BabAcc_Nm"
	,"Pymntype"."Nof_Emp" "Nof_Emp"
	,"Pymntype"."ModifiedBy" "ModifiedBy"
	,"Pymntype"."ModifiedOn" "ModifiedOn"
	,"Pymntype"."CreatedBy" "CreatedBy"
	,"Pymntype"."CreatedOn" "CreatedOn"
from "dbo"."Pymntype" "Pymntype"
	left join "dbo"."glmulcmp" "sgm" on "Pymntype"."sgm" = "sgm"."sgmid"
	left join "dbo"."OpCo" "sgmOpco" on "sgm"."OpcoID" = "sgmOpco"."OpcoIDidentity"
	left join "dbo"."GLmstrfl" "pymnt_acc" on "Pymntype"."pymnt_acc" = "pymnt_acc"."Acc_No"
	left join "dbo"."GLmfbab" "pymnt_accClsacc_" on "pymnt_acc"."Clsacc_No" = "pymnt_accClsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "pymnt_accAcc_Bnd" on "pymnt_acc"."Acc_Bnd" = "pymnt_accAcc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "pymnt_accAcc_Bndaccclass" on "pymnt_accAcc_Bnd"."accclass" = "pymnt_accAcc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "pymnt_accAcc_BndAcc_Bab" on "pymnt_accAcc_Bnd"."Acc_Bab" = "pymnt_accAcc_BndAcc_Bab"."Acc_Bab"
]]></text>
    </command>
    <command id="Pymnt_NoIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Pymnt_No" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="Pymnt_No" type="Int64" allowNulls="false" isPrimaryKey="true" label="Pymnt No" readOnly="true" />
    <field name="Pymnt_Nm" type="String" allowNulls="false" label="Pymnt Nm" length="255" showInSummary="true" />
    <field name="Pymnt_Nme" type="String" label="Pymnt Nme" length="120" showInSummary="true" />
    <field name="sgm" type="Int64" label="Sgm" showInSummary="true">
      <items style="Lookup" dataController="glmulcmp" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="sgmsgm_Nm" type="String" readOnly="true" label="sgmsgm Nm" length="255" />
    <field name="sgmOpcoOpcoName" type="String" readOnly="true" label="sgm Opco Name" length="50" />
    <field name="pymnt_acc" type="Int64" allowNulls="false" label="Pymnt Acc" showInSummary="true">
      <items style="Lookup" dataController="GLmstrfl" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="pymnt_accAcc_Nm" type="String" readOnly="true" label="pymnt acc Acc Nm" length="255" />
    <field name="pymnt_accClsacc_Acc_Nm" type="String" readOnly="true" label="pymnt acc Clsacc Acc Nm" length="250" />
    <field name="pymnt_accAcc_BndAcc_Nm" type="String" readOnly="true" label="pymnt acc Acc Bnd Acc Nm" length="50" />
    <field name="pymnt_accAcc_Bndaccclassaccountclass" type="String" readOnly="true" label="pymnt acc Acc Bndaccclassaccountclass" length="100" />
    <field name="pymnt_accAcc_BndAcc_BabAcc_Nm" type="String" readOnly="true" label="pymnt acc Acc Bnd Acc Bab Acc Nm" length="250" />
    <field name="Nof_Emp" type="Double" default="((0))" label="Nof Emp" showInSummary="true" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" />
    <field name="ModifiedOn" type="DateTime" label="Modified On" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="CreatedOn" type="DateTime" label="Created On" />
  </fields>
