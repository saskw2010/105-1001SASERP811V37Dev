	,"tbl_Reservation"."FilesCount" "FilesCount"
	,"tbl_Reservation"."Approve" "Approve"
	,"tbl_Reservation"."Rating" "Rating"
from "dbo"."tbl_Reservation" "tbl_Reservation"
	left join "dbo"."tbl_Users" "User" on "tbl_Reservation"."UserId" = "User"."id"
]]></text>
    </command>
    <command id="idIdentityCommand" type="Text" event="Inserted">
