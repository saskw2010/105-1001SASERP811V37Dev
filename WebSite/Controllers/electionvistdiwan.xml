    <command id="command1" type="Text">
      <text><![CDATA[
select
	"electionvistdiwan"."vistid" "vistid"
	,"electionvistdiwan"."vistdate" "vistdate"
	,"electionvistdiwan"."vistname" "vistname"
	,"electionvistdiwan"."areaid" "areaid"
	,"area"."gadwalname" "areagadwalname"
	,"areadwaer"."dwaername" "areadwaerdwaername"
	,"electionvistdiwan"."fromtaraf" "fromtaraf"
	,"electionvistdiwan"."telphone" "telphone"
	,"electionvistdiwan"."notes" "notes"
	,"electionvistdiwan"."notes1" "notes1"
	,"electionvistdiwan"."ModifiedBy" "ModifiedBy"
	,"electionvistdiwan"."ModifiedOn" "ModifiedOn"
	,"electionvistdiwan"."CreatedBy" "CreatedBy"
	,"electionvistdiwan"."CreatedOn" "CreatedOn"
from "dbo"."electionvistdiwan" "electionvistdiwan"
	left join "dbo"."ELectionGadwal" "area" on "electionvistdiwan"."areaid" = "area"."Gadwalcode"
	left join "dbo"."ELectionDAWAER" "areadwaer" on "area"."dwaercode" = "areadwaer"."dwaercode"
]]></text>
    </command>
    <command id="vistidIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="vistid" />
      </output>
    </command>
  </commands>
