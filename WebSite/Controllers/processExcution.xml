<?xml version="1.0" encoding="utf-8"?>
<dataController name="processExcution" conflictDetection="overwriteChanges" label="process Excution" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"processExcution"."processExecutionCode" "processExecutionCode"
	,"processExcution"."ExecutionCode" "ExecutionCode"
	,"Execution"."ExecutionProfileCode" "ExecutionExecutionProfileCode"
	,"ExecutionDwaer"."Dwaername" "ExecutionDwaerDwaername"
	,"Executionprofile"."internetCode" "ExecutionprofileinternetCode"
	,"ExecutionprofileCort"."CortName" "ExecutionprofileCortCortName"
	,"ExecutionprofileIdary"."Dwaername" "ExecutionprofileIdaryDwaername"
	,"Executionprofilekhesm"."name" "Executionprofilekhesmname"
	,"Executionprofilelaywer"."laywername" "Executionprofilelaywerlaywername"
	,"ExecutionprofileMokel"."name" "ExecutionprofileMokelname"
	,"ExecutionprofileMokp"."name" "ExecutionprofileMokpname"
	,"ExecutionprofilepaymentsType"."Name" "ExecutionprofilepaymentsTypeName"
	,"ExecutionprofileEmpolyee"."name" "ExecutionprofileEmpolyeename"
	,"Executionprofileprofiltype"."name" "Executionprofileprofiltypename"
	,"processExcution"."procdate" "procdate"
	,"processExcution"."process" "process"
	,"processExcution"."UID" "UID"
	,"processExcution"."ModifiedBy" "ModifiedBy"
	,"processExcution"."ModifiedOn" "ModifiedOn"
	,"processExcution"."CreatedBy" "CreatedBy"
	,"processExcution"."CreatedOn" "CreatedOn"
from "dbo"."processExcution" "processExcution"
	left join "dbo"."Excution" "Execution" on "processExcution"."ExecutionCode" = "Execution"."ExecutionCode"
	left join "dbo"."Dwaerex" "ExecutionDwaer" on "Execution"."Dwaercode" = "ExecutionDwaer"."Dwaercode"
	left join "dbo"."ProfileMain" "Executionprofile" on "Execution"."profilecode" = "Executionprofile"."profilecode"
	left join "dbo"."Corts" "ExecutionprofileCort" on "Executionprofile"."CortID" = "ExecutionprofileCort"."CortID"
	left join "dbo"."Dwaer" "ExecutionprofileIdary" on "Executionprofile"."IdaryID" = "ExecutionprofileIdary"."Dwaercode"
	left join "dbo"."Khsom" "Executionprofilekhesm" on "Executionprofile"."khesmcode" = "Executionprofilekhesm"."khosmcode"
	left join "dbo"."Lawyers" "Executionprofilelaywer" on "Executionprofile"."laywercode" = "Executionprofilelaywer"."laywercode"
	left join "dbo"."Mowakel" "ExecutionprofileMokel" on "Executionprofile"."Mokelcode" = "ExecutionprofileMokel"."Mokelcode"
	left join "dbo"."MowakelProperties" "ExecutionprofileMokp" on "Executionprofile"."Mokpcode" = "ExecutionprofileMokp"."Mokpcode"
	left join "dbo"."PaymentTypes" "ExecutionprofilepaymentsType" on "Executionprofile"."paymentsType" = "ExecutionprofilepaymentsType"."Code"
	left join "dbo"."profileEmployees" "ExecutionprofileEmpolyee" on "Executionprofile"."Empolyeecode" = "ExecutionprofileEmpolyee"."Empolyeecode"
	left join "dbo"."ProfileType" "Executionprofileprofiltype" on "Executionprofile"."profiltype" = "Executionprofileprofiltype"."code"
]]></text>
    </command>
    <command id="processExecutionCodeIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="processExecutionCode" />
      </output>
    </command>
  </commands>
