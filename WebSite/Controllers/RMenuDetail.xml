<?xml version="1.0" encoding="utf-8"?>
<dataController name="RMenuDetail" conflictDetection="overwriteChanges" label="R Menu Detail" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"RMenuDetail"."RMenudtlId" "RMenudtlId"
	,"RMenuDetail"."MenuID" "MenuID"
	,"Menu"."Text" "MenuText"
	,"RMenuDetail"."RMenuDetail" "RMenuDetail"
	,"RMenuDetail"."Imageurl" "Imageurl"
	,"RMenuDetail"."ModifiedBy" "ModifiedBy"
	,"RMenuDetail"."ModifiedOn" "ModifiedOn"
	,"RMenuDetail"."CreatedBy" "CreatedBy"
	,"RMenuDetail"."CreatedOn" "CreatedOn"
from "dbo"."RMenuDetail" "RMenuDetail"
	left join "dbo"."RMenu" "Menu" on "RMenuDetail"."MenuID" = "Menu"."MenuID"
]]></text>
    </command>
  </commands>
  <fields>
    <field name="RMenudtlId" type="Int64" allowNulls="false" isPrimaryKey="true" label="R Menudtl#" />
    <field name="MenuID" type="Int64" label="Menu#" showInSummary="true">
      <items style="Lookup" dataController="RMenu" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="MenuText" type="String" readOnly="true" label="Menu Text" length="50" />
    <field name="RMenuDetail" type="String" allowQBE="false" allowSorting="false" label="R Menu Detail" length="-1" showInSummary="true" />
    <field name="Imageurl" type="String" label="Imageurl" length="255" showInSummary="true" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" showInSummary="true" />
    <field name="ModifiedOn" type="DateTime" label="Modified On" showInSummary="true" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="CreatedOn" type="DateTime" label="Created On" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="R Menu Detail">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
