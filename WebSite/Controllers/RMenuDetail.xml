        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New R Menu Detail">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New R Menu Detail" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="RMenudtlId" columns="15" />
            <dataField fieldName="MenuID" aliasFieldName="MenuText" />
            <dataField fieldName="RMenuDetail" rows="5" />
            <dataField fieldName="Imageurl" />
