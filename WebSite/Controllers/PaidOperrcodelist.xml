            <dataField fieldName="withdrawalTax" columns="15" />
            <dataField fieldName="appliedflag" />
            <dataField fieldName="VASRegioncode" aliasFieldName="VASRegionVasRegiondescription" />
            <dataField fieldName="VASCurrenceyCode" aliasFieldName="VASCurrenceyVasCurrenceyDescription" />
            <dataField fieldName="PaidopCategory" aliasFieldName="ThePaidopCategoryPaidopCategoryDescription" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
