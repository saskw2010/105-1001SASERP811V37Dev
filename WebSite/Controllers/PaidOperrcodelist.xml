<?xml version="1.0" encoding="utf-8"?>
<dataController name="PaidOperrcodelist" conflictDetection="overwriteChanges" label="Paid Operrcodelist" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"PaidOperrcodelist"."Paidoperatorcode" "Paidoperatorcode"
	,"PaidOperrcodelist"."PaidoperatorcodeDescription" "PaidoperatorcodeDescription"
	,"PaidOperrcodelist"."Taxs" "Taxs"
	,"PaidOperrcodelist"."withdrawalTax" "withdrawalTax"
	,"PaidOperrcodelist"."appliedflag" "appliedflag"
	,"PaidOperrcodelist"."VASRegioncode" "VASRegioncode"
	,"VASRegion"."VasRegiondescription" "VASRegionVasRegiondescription"
	,"PaidOperrcodelist"."VASCurrenceyCode" "VASCurrenceyCode"
	,"VASCurrencey"."VasCurrenceyDescription" "VASCurrenceyVasCurrenceyDescription"
	,"PaidOperrcodelist"."PaidopCategory" "PaidopCategory"
	,"ThePaidopCategory"."PaidopCategoryDescription" "ThePaidopCategoryPaidopCategoryDescription"
from "dbo"."PaidOperrcodelist" "PaidOperrcodelist"
	left join "dbo"."VasRegionlist" "VASRegion" on "PaidOperrcodelist"."VASRegioncode" = "VASRegion"."VasRegioncode"
	left join "dbo"."VasCurrenceylist" "VASCurrencey" on "PaidOperrcodelist"."VASCurrenceyCode" = "VASCurrencey"."VasCurrenceyCode"
	left join "dbo"."PaidOperatorCategory" "ThePaidopCategory" on "PaidOperrcodelist"."PaidopCategory" = "ThePaidopCategory"."PaidopCategory"
]]></text>
    </command>
    <command id="PaidoperatorcodeIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Paidoperatorcode" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="Paidoperatorcode" type="Int64" allowNulls="false" isPrimaryKey="true" label="Paidoperatorcode" readOnly="true" />
    <field name="PaidoperatorcodeDescription" type="String" label="Paidoperatorcode Description" length="150" showInSummary="true" />
    <field name="Taxs" type="Decimal" default="((0))" label="Taxs" showInSummary="true" />
    <field name="withdrawalTax" type="Decimal" label="withdrawal Tax" showInSummary="true" />
    <field name="appliedflag" type="Boolean" label="Appliedflag" showInSummary="true" />
    <field name="VASRegioncode" type="Int64" label="VAS Regioncode" showInSummary="true">
      <items style="Lookup" dataController="VasRegionlist" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="VASRegionVasRegiondescription" type="String" readOnly="true" label="VAS Region Vas Regiondescription" length="150" />
    <field name="VASCurrenceyCode" type="Int64" label="VAS Currencey Code">
      <items style="Lookup" dataController="VasCurrenceylist" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="VASCurrenceyVasCurrenceyDescription" type="String" readOnly="true" label="VAS Currencey Vas Currencey Description" length="150" />
    <field name="PaidopCategory" type="Int64" label="Paidop Category">
      <items style="Lookup" dataController="PaidOperatorCategory" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="ThePaidopCategoryPaidopCategoryDescription" type="String" readOnly="true" label="Paidop Category Description" length="150" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Paid Operrcodelist">
