            <dataField fieldName="VASCurrenceyCode" aliasFieldName="VASCurrenceyVasCurrenceyDescription" hyperlinkFormatString="~/Pages/VasCurrenceylist.aspx?VasCurrenceyCode={VASCurrenceyCode}&amp;_controller=VasCurrenceylist&amp;_commandName=Select&amp;_commandArgument=editForm1" />
            <dataField fieldName="PaidopCategory" aliasFieldName="ThePaidopCategoryPaidopCategoryDescription" hyperlinkFormatString="~/Pages/PaidOperatorCategory.aspx?PaidopCategory={PaidopCategory}&amp;_controller=PaidOperatorCategory&amp;_commandName=Select&amp;_commandArgument=editForm1" />
