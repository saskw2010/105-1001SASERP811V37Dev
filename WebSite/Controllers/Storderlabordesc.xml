<?xml version="1.0" encoding="utf-8"?>
<dataController name="Storderlabordesc" conflictDetection="overwriteChanges" label="Storderlabordesc" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Storderlabordesc"."ln_no" "ln_no"
	,"Storderlabordesc"."Work_order_no" "Work_order_no"
	,"Work_order_"."Notes" "Work_order_Notes"
	,"Work_order_Request_"."Notes" "Work_order_Request_Notes"
	,"Work_order_Request_Div_"."Emp_Nmdriver" "Work_order_Request_Div_Emp_Nmdriver"
	,"Work_order_Request_reqtype"."reqDesc" "Work_order_Request_reqtypereqDesc"
	,"Work_order_Request_Cstm_"."Cstm_Nm" "Work_order_Request_Cstm_Cstm_Nm"
	,"Work_order_Inspc_"."inspction_name" "Work_order_Inspc_inspction_name"
	,"Work_order_status_"."storderdesc" "Work_order_status_storderdesc"
	,"Storderlabordesc"."Work_desc" "Work_desc"
	,"Storderlabordesc"."Labor_cost" "Labor_cost"
	,"Storderlabordesc"."labor_no" "labor_no"
	,"labor_"."Labor_nm" "labor_Labor_nm"
	,"Storderlabordesc"."labor_timehors" "labor_timehors"
	,"Storderlabordesc"."labor_timemints1" "labor_timemints1"
	,"Storderlabordesc"."ModifiedBy" "ModifiedBy"
	,"Storderlabordesc"."ModifiedOn" "ModifiedOn"
	,"Storderlabordesc"."CreatedBy" "CreatedBy"
	,"Storderlabordesc"."CreatedOn" "CreatedOn"
from "dbo"."Storderlabordesc" "Storderlabordesc"
	left join "dbo"."Storderhdr" "Work_order_" on "Storderlabordesc"."Work_order_no" = "Work_order_"."Work_order_no"
	left join "dbo"."maintncrequest" "Work_order_Request_" on "Work_order_"."Request_no" = "Work_order_Request_"."Doc_No"
	left join "dbo"."Cadriver" "Work_order_Request_Div_" on "Work_order_Request_"."Div_No" = "Work_order_Request_Div_"."Emp_No"
	left join "dbo"."maintainReqtype" "Work_order_Request_reqtype" on "Work_order_Request_"."reqtype" = "Work_order_Request_reqtype"."reqtype"
	left join "dbo"."Stcustmr" "Work_order_Request_Cstm_" on "Work_order_Request_"."Cstm_No" = "Work_order_Request_Cstm_"."Cstm_No"
	left join "dbo"."StorderInspctions" "Work_order_Inspc_" on "Work_order_"."Inspc_No" = "Work_order_Inspc_"."Inspction_no"
	left join "dbo"."Storderstatus" "Work_order_status_" on "Work_order_"."status_code" = "Work_order_status_"."storderstuid"
	left join "dbo"."MainLabors" "labor_" on "Storderlabordesc"."labor_no" = "labor_"."Labor_no"
]]></text>
    </command>
    <command id="ln_noIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="ln_no" />
      </output>
    </command>
  </commands>
