<?xml version="1.0" encoding="utf-8"?>
<dataController name="ServerLog" conflictDetection="overwriteChanges" label="Server Log" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"ServerLog"."ID" "ID"
	,"ServerLog"."EVENT" "EVENT"
	,"ServerLog"."USERID" "USERID"
	,"ServerLog"."EnrollNumber" "EnrollNumber"
	,"ServerLog"."parameter" "parameter"
	,"ServerLog"."EVENTTIME" "EVENTTIME"
	,"ServerLog"."SENSORID" "SENSORID"
	,"ServerLog"."OPERATOR" "OPERATOR"
from "dbo"."ServerLog" "ServerLog"
]]></text>
    </command>
    <command id="IDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="ID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="ID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Id" readOnly="true" />
    <field name="EVENT" type="String" allowNulls="false" label="Event" length="30" showInSummary="true" />
    <field name="USERID" type="Int32" allowNulls="false" label="Userid" showInSummary="true" />
    <field name="EnrollNumber" type="String" label="Enroll Number" length="30" showInSummary="true" />
    <field name="parameter" type="Int16" label="Parameter" showInSummary="true" />
    <field name="EVENTTIME" type="DateTime" allowNulls="false" label="Eventtime" showInSummary="true" />
    <field name="SENSORID" type="String" label="Sensorid" length="5" />
    <field name="OPERATOR" type="String" label="Operator" length="20" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Server Log">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="EVENT" columns="30" />
        <dataField fieldName="USERID" columns="15" />
        <dataField fieldName="EnrollNumber" columns="30" />
        <dataField fieldName="parameter" columns="15" />
        <dataField fieldName="EVENTTIME" dataFormatString="t" columns="10" />
        <dataField fieldName="SENSORID" columns="5" />
        <dataField fieldName="OPERATOR" columns="20" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Server Log">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Server Log" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="EVENT" columns="30" />
            <dataField fieldName="USERID" columns="15" />
            <dataField fieldName="EnrollNumber" columns="30" />
            <dataField fieldName="parameter" columns="15" />
            <dataField fieldName="EVENTTIME" dataFormatString="t" columns="10" />
            <dataField fieldName="SENSORID" columns="5" />
            <dataField fieldName="OPERATOR" columns="20" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Server Log">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New Server Log" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="EVENT" columns="30" />
            <dataField fieldName="USERID" columns="15" />
            <dataField fieldName="EnrollNumber" columns="30" />
            <dataField fieldName="parameter" columns="15" />
            <dataField fieldName="EVENTTIME" dataFormatString="t" columns="10" />
            <dataField fieldName="SENSORID" columns="5" />
            <dataField fieldName="OPERATOR" columns="20" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
  <actions>
    <actionGroup id="ag1" scope="Grid">
      <action id="a1" commandName="Select" commandArgument="editForm1" />
      <action id="a2" commandName="Edit" />
      <action id="a3" commandName="Delete" />
      <action id="a6" />
