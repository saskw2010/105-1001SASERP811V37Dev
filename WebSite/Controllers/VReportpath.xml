<?xml version="1.0" encoding="utf-8"?>
<dataController name="VReportpath" conflictDetection="overwriteChanges" label="V Reportpath" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"VReportpath"."idauto" "idauto"
	,"VReportpath"."Form_path" "Form_path"
	,"VReportpath"."reportfullattch" "reportfullattch"
	,"VReportpath"."reportfullname" "reportfullname"
	,"VReportpath"."Form_Description" "Form_Description"
	,"VReportpath"."reqcat_id" "reqcat_id"
from "dbo"."VReportpath" "VReportpath"
]]></text>
    </command>
    <command id="idautoIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="idauto" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="idauto" type="Int64" allowNulls="false" label="Idauto" readOnly="true" showInSummary="true" />
