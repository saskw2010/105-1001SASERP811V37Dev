        <dataField fieldName="reportfullname" rows="5" />
        <dataField fieldName="Form_Description" rows="5" />
        <dataField fieldName="reqcat_id" columns="15" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review V Reportpath">
