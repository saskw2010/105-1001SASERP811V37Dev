        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New V Reportpath">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New V Reportpath" flow="NewColumn"  newColumn="true">
