    <field name="reportfullattch" type="String" allowQBE="false" allowSorting="false" label="Reportfullattch" length="-1" showInSummary="true" />
    <field name="reportfullname" type="String" allowQBE="false" allowSorting="false" label="Reportfullname" length="-1" showInSummary="true" />
    <field name="Form_Description" type="String" allowQBE="false" allowSorting="false" label="Form Description" length="-1" showInSummary="true" />
    <field name="reqcat_id" type="Int64" label="Reqcat#" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="V Reportpath">
      <headerText>$DefaultGridViewDescription</headerText>
