select
	"GlAstTyp"."Aset_Typ" "Aset_Typ"
	,"GlAstTyp"."Aset_TypDesc" "Aset_TypDesc"
	,"GlAstTyp"."Aset_TypDesce" "Aset_TypDesce"
	,"GlAstTyp"."Cat_No" "Cat_No"
	,"Cat_"."Cat_Nm" "Cat_Cat_Nm"
	,"GlAstTyp"."acc_no" "acc_no"
	,"acc_"."Acc_Nm" "acc_Acc_Nm"
	,"acc_Clsacc_"."Acc_Nm" "acc_Clsacc_Acc_Nm"
	,"acc_Acc_Bnd"."Acc_Nm" "acc_Acc_BndAcc_Nm"
	,"acc_Acc_Bndaccclass"."accountclass" "acc_Acc_Bndaccclassaccountclass"
	,"acc_Acc_BndAcc_Bab"."Acc_Nm" "acc_Acc_BndAcc_BabAcc_Nm"
	,"GlAstTyp"."acc_no1" "acc_no1"
	,"acc_no1"."Acc_Nm" "acc_no1Acc_Nm"
	,"acc_no1Clsacc_"."Acc_Nm" "acc_no1Clsacc_Acc_Nm"
	,"acc_no1Acc_Bnd"."Acc_Nm" "acc_no1Acc_BndAcc_Nm"
	,"acc_no1Acc_Bndaccclass"."accountclass" "acc_no1Acc_Bndaccclassaccountclass"
	,"acc_no1Acc_BndAcc_Bab"."Acc_Nm" "acc_no1Acc_BndAcc_BabAcc_Nm"
	,"GlAstTyp"."acc_no2" "acc_no2"
	,"acc_no2"."Acc_Nm" "acc_no2Acc_Nm"
	,"acc_no2Clsacc_"."Acc_Nm" "acc_no2Clsacc_Acc_Nm"
	,"acc_no2Acc_Bnd"."Acc_Nm" "acc_no2Acc_BndAcc_Nm"
	,"acc_no2Acc_Bndaccclass"."accountclass" "acc_no2Acc_Bndaccclassaccountclass"
	,"acc_no2Acc_BndAcc_Bab"."Acc_Nm" "acc_no2Acc_BndAcc_BabAcc_Nm"
	,"GlAstTyp"."costcenterno" "costcenterno"
	,"costcenter"."Costcntr_Nm" "costcenterCostcntr_Nm"
	,"costcentercost_typ"."Costcntr_Nm" "costcentercost_typCostcntr_Nm"
	,"GlAstTyp"."costcenterno1" "costcenterno1"
	,"costcenterno1"."Costcntr_Nm" "costcenterno1Costcntr_Nm"
	,"costcenterno1cost_typ"."Costcntr_Nm" "costcenterno1cost_typCostcntr_Nm"
from "dbo"."GlAstTyp" "GlAstTyp"
	left join "dbo"."GLAsetsCat" "Cat_" on "GlAstTyp"."Cat_No" = "Cat_"."Cat_No"
	left join "dbo"."GLmstrfl" "acc_" on "GlAstTyp"."acc_no" = "acc_"."Acc_No"
	left join "dbo"."GLmfbab" "acc_Clsacc_" on "acc_"."Clsacc_No" = "acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "acc_Acc_Bnd" on "acc_"."Acc_Bnd" = "acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "acc_Acc_Bndaccclass" on "acc_Acc_Bnd"."accclass" = "acc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "acc_Acc_BndAcc_Bab" on "acc_Acc_Bnd"."Acc_Bab" = "acc_Acc_BndAcc_Bab"."Acc_Bab"
	left join "dbo"."GLmstrfl" "acc_no1" on "GlAstTyp"."acc_no1" = "acc_no1"."Acc_No"
	left join "dbo"."GLmfbab" "acc_no1Clsacc_" on "acc_no1"."Clsacc_No" = "acc_no1Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "acc_no1Acc_Bnd" on "acc_no1"."Acc_Bnd" = "acc_no1Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "acc_no1Acc_Bndaccclass" on "acc_no1Acc_Bnd"."accclass" = "acc_no1Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "acc_no1Acc_BndAcc_Bab" on "acc_no1Acc_Bnd"."Acc_Bab" = "acc_no1Acc_BndAcc_Bab"."Acc_Bab"
	left join "dbo"."GLmstrfl" "acc_no2" on "GlAstTyp"."acc_no2" = "acc_no2"."Acc_No"
	left join "dbo"."GLmfbab" "acc_no2Clsacc_" on "acc_no2"."Clsacc_No" = "acc_no2Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "acc_no2Acc_Bnd" on "acc_no2"."Acc_Bnd" = "acc_no2Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "acc_no2Acc_Bndaccclass" on "acc_no2Acc_Bnd"."accclass" = "acc_no2Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "acc_no2Acc_BndAcc_Bab" on "acc_no2Acc_Bnd"."Acc_Bab" = "acc_no2Acc_BndAcc_Bab"."Acc_Bab"
	left join "dbo"."GLcostcntr" "costcenter" on "GlAstTyp"."costcenterno" = "costcenter"."Costcntr_No"
	left join "dbo"."Glcostcentgruop" "costcentercost_typ" on "costcenter"."cost_typ" = "costcentercost_typ"."Costcntr_No"
	left join "dbo"."GLcostcntr1" "costcenterno1" on "GlAstTyp"."costcenterno1" = "costcenterno1"."Costcntr_No"
	left join "dbo"."Glcostcentgruop1" "costcenterno1cost_typ" on "costcenterno1"."cost_typ" = "costcenterno1cost_typ"."Costcntr_No"
]]></text>
    </command>
