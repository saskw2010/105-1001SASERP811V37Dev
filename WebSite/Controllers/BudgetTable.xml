  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"BudgetTable"."BudgetiD" "BudgetiD"
	,"BudgetTable"."acdcode" "acdcode"
	,"acd"."AcademicYear" "acdAcademicYear"
	,"acdGlFinperiod"."Fin_period_info" "acdGlFinperiodFin_period_info"
	,"acdGlFinperiodaccountnumber"."Acc_Nm" "acdGlFinperiodaccountnumberAcc_Nm"
	,"BudgetTable"."branch" "branch"
	,"Thebranch"."Desc1" "ThebranchDesc1"
	,"Thebranchsgm"."sgm_Nm" "Thebranchsgmsgm_Nm"
	,"ThebranchsgmOpco"."OpcoName" "ThebranchsgmOpcoOpcoName"
	,"ThebranchGender"."Gender" "ThebranchGenderGender"
	,"Thebranchstage"."ShortDesc1" "ThebranchstageShortDesc1"
	,"Thebranchschtyp"."schtypDesc" "ThebranchschtypschtypDesc"
	,"BudgetTable"."GradeCode" "GradeCode"
	,"Grade"."ShortDesc1" "GradeShortDesc1"
	,"Gradeacd"."AcademicYear" "GradeacdAcademicYear"
	,"GradeacdGlFinperiod"."Fin_period_info" "GradeacdGlFinperiodFin_period_info"
	,"GradeStage"."ShortDesc1" "GradeStageShortDesc1"
	,"BudgetTable"."Classcount" "Classcount"
	,"BudgetTable"."StCount" "StCount"
	,"BudgetTable"."RealClasscount" "RealClasscount"
	,"BudgetTable"."RealStCount" "RealStCount"
	,"BudgetTable"."PassClasscount" "PassClasscount"
	,"BudgetTable"."PassStCount" "PassStCount"
	,"BudgetTable"."Stvaluerosom" "Stvaluerosom"
	,"BudgetTable"."StvalueBook" "StvalueBook"
	,"BudgetTable"."Stvaluetransportation" "Stvaluetransportation"
	,"BudgetTable"."RealStvaluerosom" "RealStvaluerosom"
	,"BudgetTable"."RealStvalueBook" "RealStvalueBook"
	,"BudgetTable"."RealStvaluetransportation" "RealStvaluetransportation"
	,"BudgetTable"."PassStvaluerosom" "PassStvaluerosom"
	,"BudgetTable"."PassStvalueBook" "PassStvalueBook"
	,"BudgetTable"."PassStvaluetransportation" "PassStvaluetransportation"
from "dbo"."BudgetTable" "BudgetTable"
	left join "dbo"."schAcademicyear" "acd" on "BudgetTable"."acdcode" = "acd"."Code"
	left join "dbo"."GlFinanceperiod" "acdGlFinperiod" on "acd"."GlFinperiodID" = "acdGlFinperiod"."GlFinperiodID"
	left join "dbo"."GLmstrfl" "acdGlFinperiodaccountnumber" on "acdGlFinperiod"."accountnumber" = "acdGlFinperiodaccountnumber"."Acc_No"
	left join "dbo"."schBranch" "Thebranch" on "BudgetTable"."branch" = "Thebranch"."branch"
	left join "dbo"."glmulcmp" "Thebranchsgm" on "Thebranch"."sgm" = "Thebranchsgm"."sgmid"
	left join "dbo"."OpCo" "ThebranchsgmOpco" on "Thebranchsgm"."OpcoID" = "ThebranchsgmOpco"."OpcoIDidentity"
	left join "dbo"."PyGender" "ThebranchGender" on "Thebranch"."Genderid" = "ThebranchGender"."Genderid"
	left join "dbo"."schStage" "Thebranchstage" on "Thebranch"."stagecode" = "Thebranchstage"."Code"
	left join "dbo"."schtyp" "Thebranchschtyp" on "Thebranch"."schtypid" = "Thebranchschtyp"."schtypid"
	left join "dbo"."schGrade" "Grade" on "BudgetTable"."GradeCode" = "Grade"."GradeCode"
	left join "dbo"."schAcademicyear" "Gradeacd" on "Grade"."acdcode" = "Gradeacd"."Code"
	left join "dbo"."GlFinanceperiod" "GradeacdGlFinperiod" on "Gradeacd"."GlFinperiodID" = "GradeacdGlFinperiod"."GlFinperiodID"
	left join "dbo"."schStage" "GradeStage" on "Grade"."StageCode" = "GradeStage"."Code"
]]></text>
    </command>
    <command id="BudgetiDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="BudgetiD" />
      </output>
    </command>
  </commands>
