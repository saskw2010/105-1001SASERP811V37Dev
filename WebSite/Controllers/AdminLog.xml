<?xml version="1.0" encoding="utf-8"?>
<dataController name="AdminLog" conflictDetection="overwriteChanges" label="Admin Log" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"AdminLog"."ID" "ID"
	,"AdminLog"."AdminID" "AdminID"
	,"Admin"."LoginName" "AdminLoginName"
	,"AdminLog"."Status" "Status"
	,"AdminLog"."Des" "Des"
	,"AdminLog"."TStamp" "TStamp"
	,"AdminLog"."ModifiedBy" "ModifiedBy"
	,"AdminLog"."ModifiedByUserId" "ModifiedByUserId"
	,"AdminLog"."ModifiedOn" "ModifiedOn"
	,"AdminLog"."CreatedBy" "CreatedBy"
	,"AdminLog"."CreatedById" "CreatedById"
	,"AdminLog"."CreatedOn" "CreatedOn"
from "dbo"."AdminLog" "AdminLog"
	left join "dbo"."AdminData" "Admin" on "AdminLog"."AdminID" = "Admin"."AdminID"
]]></text>
    </command>
    <command id="IDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="ID" />
      </output>
    </command>
  </commands>
  <fields>
