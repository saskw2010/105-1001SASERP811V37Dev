<?xml version="1.0" encoding="utf-8"?>
<dataController name="BatchDaileypaymentitems" conflictDetection="overwriteChanges" label="Batch Daileypaymentitems" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"BatchDaileypaymentitems"."itmiD" "itmiD"
	,"BatchDaileypaymentitems"."itmDescription" "itmDescription"
	,"BatchDaileypaymentitems"."itmDescription1" "itmDescription1"
	,"BatchDaileypaymentitems"."itmacc_no" "itmacc_no"
	,"itmacc_"."Acc_Nm" "itmacc_Acc_Nm"
	,"itmacc_Clsacc_"."Acc_Nm" "itmacc_Clsacc_Acc_Nm"
	,"itmacc_Acc_Bnd"."Acc_Nm" "itmacc_Acc_BndAcc_Nm"
	,"itmacc_Acc_Bndaccclass"."accountclass" "itmacc_Acc_Bndaccclassaccountclass"
	,"itmacc_Acc_BndAcc_Bab"."Acc_Nm" "itmacc_Acc_BndAcc_BabAcc_Nm"
from "dbo"."BatchDaileypaymentitems" "BatchDaileypaymentitems"
	left join "dbo"."GLmstrfl" "itmacc_" on "BatchDaileypaymentitems"."itmacc_no" = "itmacc_"."Acc_No"
	left join "dbo"."GLmfbab" "itmacc_Clsacc_" on "itmacc_"."Clsacc_No" = "itmacc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "itmacc_Acc_Bnd" on "itmacc_"."Acc_Bnd" = "itmacc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "itmacc_Acc_Bndaccclass" on "itmacc_Acc_Bnd"."accclass" = "itmacc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "itmacc_Acc_BndAcc_Bab" on "itmacc_Acc_Bnd"."Acc_Bab" = "itmacc_Acc_BndAcc_Bab"."Acc_Bab"
]]></text>
    </command>
    <command id="itmiDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="itmiD" />
      </output>
    </command>
  </commands>
