    <field name="itmDescription" type="String" label="itm Description" length="50" showInSummary="true" />
    <field name="itmDescription1" type="String" label="itm Description1" length="50" showInSummary="true" />
    <field name="itmacc_no" type="Int64" label="Itmacc No" showInSummary="true">
