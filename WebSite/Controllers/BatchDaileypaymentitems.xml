    <field name="itmacc_Acc_BndAcc_Nm" type="String" readOnly="true" label="itmacc Acc Bnd Acc Nm" length="50" />
    <field name="itmacc_Acc_Bndaccclassaccountclass" type="String" readOnly="true" label="itmacc Acc Bndaccclassaccountclass" length="100" />
