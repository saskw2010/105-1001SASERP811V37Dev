<?xml version="1.0" encoding="utf-8"?>
<dataController name="user_details" conflictDetection="overwriteChanges" label="User Details" plugIn="eZee.Data.AnnotationPlugIn" handler="eZee.Rules.user_detailsBusinessRules" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"user_details"."UserName" "UserName"
	,"user_details"."UserEmail" "UserEmail"
	,"user_details"."UserAboutme" "UserAboutme"
	,"user_details"."FirstName" "FirstName"
	,"user_details"."LastName" "LastName"
	,"user_details"."Myphone" "Myphone"
	,"user_details"."MyphoneTel" "MyphoneTel"
	,"user_details"."Myadress" "Myadress"
	,"user_details"."catid1" "catid1"
	,"Thecatid1"."cat1name" "Thecatid1cat1name"
	,"user_details"."catid2" "catid2"
	,"Thecatid2"."cat2name" "Thecatid2cat2name"
	,"user_details"."catid3" "catid3"
	,"Thecatid3"."cat3name" "Thecatid3cat3name"
	,"user_details"."catid4" "catid4"
	,"Thecatid4"."cat4name" "Thecatid4cat4name"
	,"user_details"."catid5" "catid5"
	,"Thecatid5"."cat5name" "Thecatid5cat5name"
	,"user_details"."catid6" "catid6"
	,"Thecatid6"."cat6name" "Thecatid6cat6name"
	,"user_details"."catid7" "catid7"
	,"Thecatid7"."cat7name" "Thecatid7cat7name"
	,"user_details"."catid8" "catid8"
	,"Thecatid8"."cat8name" "Thecatid8cat8name"
	,"user_details"."catid9" "catid9"
	,"Thecatid9"."cat9name" "Thecatid9cat9name"
	,"user_details"."stage" "stage"
	,"stage"."ShortDesc1" "stageShortDesc1"
	,"user_details"."Isactive" "Isactive"
	,"user_details"."isdeleted" "isdeleted"
	,"user_details"."ModifiedBy" "ModifiedBy"
	,"user_details"."ModifiedOn" "ModifiedOn"
	,"user_details"."CreatedBy" "CreatedBy"
	,"user_details"."CreatedOn" "CreatedOn"
from "dbo"."user_details" "user_details"
	left join "dbo"."Qwaizcat1" "Thecatid1" on "user_details"."catid1" = "Thecatid1"."catid1"
	left join "dbo"."Qwaizcat2" "Thecatid2" on "user_details"."catid2" = "Thecatid2"."catid2"
	left join "dbo"."Qwaizcat3" "Thecatid3" on "user_details"."catid3" = "Thecatid3"."catid3"
	left join "dbo"."Qwaizcat4" "Thecatid4" on "user_details"."catid4" = "Thecatid4"."catid4"
	left join "dbo"."Qwaizcat5" "Thecatid5" on "user_details"."catid5" = "Thecatid5"."catid5"
	left join "dbo"."Qwaizcat6" "Thecatid6" on "user_details"."catid6" = "Thecatid6"."catid6"
	left join "dbo"."Qwaizcat7" "Thecatid7" on "user_details"."catid7" = "Thecatid7"."catid7"
	left join "dbo"."Qwaizcat8" "Thecatid8" on "user_details"."catid8" = "Thecatid8"."catid8"
	left join "dbo"."Qwaizcat9" "Thecatid9" on "user_details"."catid9" = "Thecatid9"."catid9"
	left join "dbo"."schStage" "stage" on "user_details"."stage" = "stage"."Code"
]]></text>
    </command>
  </commands>
  <fields>
    <field name="UserName" type="String" allowNulls="false" isPrimaryKey="true" label="User Name" length="50" showInSummary="true">
      <formula>@BusinessRules_UserName</formula>
    </field>
    <field name="UserEmail" type="String" label="User Email" length="250" showInSummary="true" allowNulls="false" calculated="true">
      <formula>@BusinessRules_UserEmail</formula>
    </field>
    <field name="UserAboutme" type="String" allowQBE="false" allowSorting="false" label="User Aboutme" showInSummary="true" />
    <field name="FirstName" type="String" label="First Name" length="50" showInSummary="true" allowNulls="false" />
    <field name="LastName" type="String" label="Last Name" length="50" showInSummary="true" />
    <field name="Myphone" type="String" label="Myphone" length="50" allowNulls="false" />
    <field name="MyphoneTel" type="String" label="Myphone Tel" length="50" />
    <field name="Myadress" type="String" allowQBE="false" allowSorting="false" label="Myadress" length="-1" />
    <field name="catid1" type="Int64" label="Catid1">
      <items style="Lookup" dataController="Qwaizcat1" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="Thecatid1cat1name" type="String" readOnly="true" label="Thecatid1cat1name" length="255" />
    <field name="catid2" type="Int64" label="Catid2">
      <items style="Lookup" dataController="Qwaizcat2" newDataView="createForm1" searchOnStart="true" />
