    </field>
    <field name="Thecatid5cat5name" type="String" readOnly="true" label="Thecatid5cat5name" length="255" />
    <field name="catid6" type="Int64" label="Catid6">
      <items style="Lookup" dataController="Qwaizcat6" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="Thecatid6cat6name" type="String" readOnly="true" label="Thecatid6cat6name" length="255" />
    <field name="catid7" type="Int64" label="Catid7">
      <items style="Lookup" dataController="Qwaizcat7" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="Thecatid7cat7name" type="String" readOnly="true" label="Thecatid7cat7name" length="255" />
    <field name="catid8" type="Int64" label="Catid8">
      <items style="Lookup" dataController="Qwaizcat8" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="Thecatid8cat8name" type="String" readOnly="true" label="Thecatid8cat8name" length="255" />
    <field name="catid9" type="Int64" label="Catid9">
      <items style="Lookup" dataController="Qwaizcat9" newDataView="createForm1" searchOnStart="true" />
