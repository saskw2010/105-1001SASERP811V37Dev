  <businessRules>
    <rule id="r100" type="JavaScript" commandName="Insert|Update" phase="Before" name="UserEmail_Validator"><![CDATA[var fieldValue = [UserEmail];
if (fieldValue == null) {
    // prevent the default action processing
    this.preventDefault();
    // set the focus on the field and display an error
    this.result.focus('UserEmail', 'Required field.');
}]]></rule>
