  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Storderdtl"."Work_order_no" "Work_order_no"
	,"Work_order_"."Notes" "Work_order_Notes"
	,"Work_order_Request_"."Notes" "Work_order_Request_Notes"
	,"Work_order_Request_Div_"."Emp_Nmdriver" "Work_order_Request_Div_Emp_Nmdriver"
	,"Work_order_Request_Costcnter_"."Costcntr_Nm" "Work_order_Request_Costcnter_Costcntr_Nm"
	,"Work_order_Request_reqtype"."reqDesc" "Work_order_Request_reqtypereqDesc"
	,"Work_order_Request_Cstm_"."Cstm_Nm" "Work_order_Request_Cstm_Cstm_Nm"
	,"Work_order_Inspc_"."inspction_name" "Work_order_Inspc_inspction_name"
	,"Work_order_status_"."storderdesc" "Work_order_status_storderdesc"
	,"Storderdtl"."Ln_No" "Ln_No"
	,"Storderdtl"."notesdtl" "notesdtl"
	,"Storderdtl"."carno" "carno"
	,"car"."PLAT_No" "carPLAT_No"
	,"carCar_Type"."Styl_Nm" "carCar_TypeStyl_Nm"
	,"carcarclass"."carclassname" "carcarclasscarclassname"
	,"carcarstutus_"."carstutus_name" "carcarstutus_carstutus_name"
	,"carCar_Colr"."Color_name" "carCar_ColrColor_name"
	,"carsgm"."sgm_Nm" "carsgmsgm_Nm"
	,"carsgmOpco"."OpcoName" "carsgmOpcoOpcoName"
	,"carInsurance_type"."Insurance_name" "carInsurance_typeInsurance_name"
	,"carCar_Lik"."Like_name" "carCar_LikLike_name"
	,"carFbrct_Cntry"."Cntry_Nm" "carFbrct_CntryCntry_Nm"
	,"carOwnr_"."Ownr_ID" "carOwnr_Ownr_ID"
	,"carOwnr_cmp_"."Grp_Nm" "carOwnr_cmp_Grp_Nm"
	,"carTrfc_Dpm"."State_Nm" "carTrfc_DpmState_Nm"
	,"carSup_"."Sup_Nm" "carSup_Sup_Nm"
	,"carSup_Acc_"."Acc_Nm" "carSup_Acc_Acc_Nm"
	,"carSup_Cntry_"."Cntry_Nm" "carSup_Cntry_Cntry_Nm"
	,"carSup_Curncy_"."Curncy_Nm" "carSup_Curncy_Curncy_Nm"
	,"Storderdtl"."section_no" "section_no"
	,"section_"."Brn_Nm" "section_Brn_Nm"
	,"Storderdtl"."complnsid" "complnsid"
	,"complns"."complnsdesc" "complnscomplnsdesc"
	,"complnssection_typ"."Brn_Nm" "complnssection_typBrn_Nm"
	,"Storderdtl"."ModifiedBy" "ModifiedBy"
	,"Storderdtl"."ModifiedOn" "ModifiedOn"
	,"Storderdtl"."CreatedBy" "CreatedBy"
	,"Storderdtl"."CreatedOn" "CreatedOn"
from "dbo"."Storderdtl" "Storderdtl"
	left join "dbo"."Storderhdr" "Work_order_" on "Storderdtl"."Work_order_no" = "Work_order_"."Work_order_no"
	left join "dbo"."maintncrequest" "Work_order_Request_" on "Work_order_"."Request_no" = "Work_order_Request_"."Doc_No"
	left join "dbo"."Cadriver" "Work_order_Request_Div_" on "Work_order_Request_"."Div_No" = "Work_order_Request_Div_"."Emp_No"
	left join "dbo"."GLcostcntr" "Work_order_Request_Costcnter_" on "Work_order_Request_"."Costcnter_no" = "Work_order_Request_Costcnter_"."Costcntr_No"
	left join "dbo"."maintainReqtype" "Work_order_Request_reqtype" on "Work_order_Request_"."reqtype" = "Work_order_Request_reqtype"."reqtype"
	left join "dbo"."Stcustmr" "Work_order_Request_Cstm_" on "Work_order_Request_"."Cstm_No" = "Work_order_Request_Cstm_"."Cstm_No"
	left join "dbo"."StorderInspctions" "Work_order_Inspc_" on "Work_order_"."Inspc_No" = "Work_order_Inspc_"."Inspction_no"
	left join "dbo"."Storderstatus" "Work_order_status_" on "Work_order_"."status_code" = "Work_order_status_"."storderstuid"
	left join "dbo"."CarInfom" "car" on "Storderdtl"."carno" = "car"."Car_No"
	left join "dbo"."CaCrstyl" "carCar_Type" on "car"."Car_Type" = "carCar_Type"."Styl_No"
	left join "dbo"."carclass" "carcarclass" on "car"."carclass" = "carcarclass"."carclass"
	left join "dbo"."carstutuslist" "carcarstutus_" on "car"."carstutus_no" = "carcarstutus_"."carstutus_no"
	left join "dbo"."Colorlist" "carCar_Colr" on "car"."Car_Colr" = "carCar_Colr"."ColorNo"
	left join "dbo"."glmulcmp" "carsgm" on "car"."sgm" = "carsgm"."sgmid"
	left join "dbo"."OpCo" "carsgmOpco" on "carsgm"."OpcoID" = "carsgmOpco"."OpcoIDidentity"
	left join "dbo"."Insurancetype" "carInsurance_type" on "car"."Insurance_type" = "carInsurance_type"."Insurance_type"
	left join "dbo"."Likelist" "carCar_Lik" on "car"."Car_Lik" = "carCar_Lik"."LikeNo"
	left join "dbo"."PYCntry" "carFbrct_Cntry" on "car"."Fbrct_Cntry" = "carFbrct_Cntry"."Cntry_No"
	left join "dbo"."Pyownrmf" "carOwnr_" on "car"."Ownr_No" = "carOwnr_"."Ownr_No"
	left join "dbo"."Pycnstnt" "carOwnr_cmp_" on "carOwnr_"."cmp_no" = "carOwnr_cmp_"."Cmp_No"
	left join "dbo"."PyState" "carTrfc_Dpm" on "car"."Trfc_Dpm" = "carTrfc_Dpm"."State"
	left join "dbo"."Stsuplir" "carSup_" on "car"."Sup_No" = "carSup_"."Sup_No"
	left join "dbo"."GLmstrfl" "carSup_Acc_" on "carSup_"."Acc_No" = "carSup_Acc_"."Acc_No"
	left join "dbo"."PYCntry" "carSup_Cntry_" on "carSup_"."Cntry_No" = "carSup_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "carSup_Curncy_" on "carSup_"."Curncy_No" = "carSup_Curncy_"."Curncy_No"
	left join "dbo"."Stwhrhouseorder" "section_" on "Storderdtl"."section_no" = "section_"."Brn_No"
	left join "dbo"."Stordercopmlains" "complns" on "Storderdtl"."complnsid" = "complns"."complnsid"
	left join "dbo"."Stwhrhouseorder" "complnssection_typ" on "complns"."section_typ" = "complnssection_typ"."Brn_No"
]]></text>
    </command>
