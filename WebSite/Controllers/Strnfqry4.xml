  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
SELECT  [Ln_No]
      ,[Doc_No]
      ,[Rainbow_no]
      ,[ModifiedBy]
      ,[ModifiedOn]
      ,[CreatedBy]
      ,[CreatedOn]
	  ,[flag]
  FROM [dbo].[StLcDtlserial]
  
]]></text>
    </command>
    <command id="Doc_NoIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Doc_No" />
      </output>
    </command>
  </commands>
