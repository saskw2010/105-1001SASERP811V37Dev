            <dataField fieldName="SQLDICDESC" rows="5" />
            <dataField fieldName="SQLSTATMENT" rows="5" />
            <dataField fieldName="MODID" aliasFieldName="MODMODAr" hyperlinkFormatString="~/Pages/MODMaster.aspx?MODID={MODID}&amp;_controller=MODMaster&amp;_commandName=Select&amp;_commandArgument=editForm1" />
            <dataField fieldName="ModifiedBy" columns="50" />
            <dataField fieldName="ModifiedOn" columns="10" />
            <dataField fieldName="CreatedBy" columns="50" />
            <dataField fieldName="CreatedOn" columns="10" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New SQ Ldictionary">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New SQ Ldictionary" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="SQLCATID" aliasFieldName="SQLCATSQLCATDESC" />
            <dataField fieldName="SQLDICDESC" rows="5" />
            <dataField fieldName="SQLSTATMENT" rows="5" />
            <dataField fieldName="MODID" aliasFieldName="MODMODAr" />
            <dataField fieldName="ModifiedBy" columns="50" />
            <dataField fieldName="ModifiedOn" columns="10" />
            <dataField fieldName="CreatedBy" columns="50" />
            <dataField fieldName="CreatedOn" columns="10" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
