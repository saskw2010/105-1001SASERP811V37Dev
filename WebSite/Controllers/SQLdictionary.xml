<?xml version="1.0" encoding="utf-8"?>
<dataController name="SQLdictionary" conflictDetection="overwriteChanges" label="SQ Ldictionary" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"SQLdictionary"."SQLDICID" "SQLDICID"
	,"SQLdictionary"."SQLCATID" "SQLCATID"
	,"SQLCAT"."SQLCATDESC" "SQLCATSQLCATDESC"
	,"SQLdictionary"."SQLDICDESC" "SQLDICDESC"
	,"SQLdictionary"."SQLSTATMENT" "SQLSTATMENT"
	,"SQLdictionary"."MODID" "MODID"
	,"MOD"."MODAr" "MODMODAr"
	,"SQLdictionary"."ModifiedBy" "ModifiedBy"
	,"SQLdictionary"."ModifiedOn" "ModifiedOn"
	,"SQLdictionary"."CreatedBy" "CreatedBy"
	,"SQLdictionary"."CreatedOn" "CreatedOn"
from "dbo"."SQLdictionary" "SQLdictionary"
	left join "dbo"."SQLCATLIST" "SQLCAT" on "SQLdictionary"."SQLCATID" = "SQLCAT"."SQLCATID"
	left join "dbo"."MODMaster" "MOD" on "SQLdictionary"."MODID" = "MOD"."MODID"
]]></text>
    </command>
    <command id="SQLDICIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="SQLDICID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="SQLDICID" type="Int64" allowNulls="false" isPrimaryKey="true" label="Sqldicid" readOnly="true" />
    <field name="SQLCATID" type="Int64" label="Sqlcatid" showInSummary="true">
      <items style="Lookup" dataController="SQLCATLIST" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="SQLCATSQLCATDESC" type="String" readOnly="true" label="Sqlcatsqlcatdesc" length="100" />
    <field name="SQLDICDESC" type="String" allowQBE="false" allowSorting="false" label="Sqldicdesc" length="-1" showInSummary="true" />
    <field name="SQLSTATMENT" type="String" allowQBE="false" allowSorting="false" label="Sqlstatment" length="-1" showInSummary="true" />
    <field name="MODID" type="Int64" label="Modid" showInSummary="true">
      <items style="Lookup" dataController="MODMaster" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="MODMODAr" type="String" readOnly="true" label="MODMOD Ar" length="50" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" showInSummary="true" />
    <field name="ModifiedOn" type="DateTime" label="Modified On" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="CreatedOn" type="DateTime" label="Created On" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="SQ Ldictionary">
      <headerText>$DefaultGridViewDescription</headerText>
