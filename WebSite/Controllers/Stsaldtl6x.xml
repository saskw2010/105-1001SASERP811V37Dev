	,"Doc_Brn_Cmp_"."Grp_Nm" "Doc_Brn_Cmp_Grp_Nm"
	,"Doc_typcmsup"."SubledgerDescrption" "Doc_typcmsupSubledgerDescrption"
	,"Doc_Pym_"."Pymnt_Nm" "Doc_Pym_Pymnt_Nm"
	,"Doc_Pym_pymnt_acc"."Acc_Nm" "Doc_Pym_pymnt_accAcc_Nm"
	,"Doc_Pym_sgm"."sgm_Nm" "Doc_Pym_sgmsgm_Nm"
	,"Doc_cur"."Curncy_Nm" "Doc_curCurncy_Nm"
	,"Doc_Cstm_"."Cstm_Nm" "Doc_Cstm_Cstm_Nm"
	,"Doc_Cstm_Area"."Area_name" "Doc_Cstm_AreaArea_name"
	,"Doc_Cstm_Evaluation"."Evaluation_name" "Doc_Cstm_EvaluationEvaluation_name"
	,"Doc_Cstm_acc_"."Acc_Nm" "Doc_Cstm_acc_Acc_Nm"
	,"Doc_Cstm_Cntry_"."Cntry_Nm" "Doc_Cstm_Cntry_Cntry_Nm"
	,"Doc_Cstm_curncy_"."Curncy_Nm" "Doc_Cstm_curncy_Curncy_Nm"
	,"Doc_Cstm_TSNEF"."TSNEF_name" "Doc_Cstm_TSNEFTSNEF_name"
	,"Doc_Cstm_TSNEFNO1"."TSNEF1_name" "Doc_Cstm_TSNEFNO1TSNEF1_name"
	,"Doc_Cstm_Slm_"."Slm_Nm" "Doc_Cstm_Slm_Slm_Nm"
	,"Doc_Slm_"."Slm_Nm" "Doc_Slm_Slm_Nm"
	,"Doc_Slm_masngerclass"."masngerclass" "Doc_Slm_masngerclassmasngerclass"
	,"Doc_"."Brn_No" "Brn_No"
	,"Brn_"."Brn_Nm" "Brn_Brn_Nm"
	,"Brn_Cmp_"."Grp_Nm" "Brn_Cmp_Grp_Nm"
	,"Brn_Cmp_Grp_"."Grp_Nm" "Brn_Cmp_Grp_Grp_Nm"
	,"Stsaldtl"."Itm_No" "Itm_No"
	,"Itm_"."ID_USER" "Itm_ID_USER"
	,"Itm_"."ITEM" "Itm_ITEM"
        ,"Itm_"."Barcod1" "extrabons"
	,"Itm_IDGROUP"."ID_USER" "Itm_IDGROUPID_USER"
        ,"Itm_IDGROUP"."GROUPA" "GROUPA"
	,"Itm_Sup_"."Item_name" "Itm_Sup_Item_name"
	,"Itm_id1"."DESCRP1" "Itm_id1DESCRP1"
	,"Itm_id2"."DESCRP1" "Itm_id2DESCRP1"
	,"Itm_id3"."DESCRP1" "Itm_id3DESCRP1"
	,"Itm_id4"."DESCRP1" "Itm_id4DESCRP1"
	,"Itm_id5"."DESCRP1" "Itm_id5DESCRP1"
	,"Itm_id6"."DESCRP1" "Itm_id6DESCRP1"
	,"Itm_id7"."DESCRP1" "Itm_id7DESCRP1"
	,"Stsaldtl"."Unit_No" "Unit_No"
	,"Unit_"."Unit_Nm" "Unit_Unit_Nm"
	,"Stsaldtl"."UnitPrice" "UnitPrice"
	,"Stsaldtl"."Quantity" "Quantity"
	,"Stsaldtl"."Discount" "Discount"
	,"Stsaldtl"."Titm_Sal" "Titm_Sal"
	,"Stsaldtl"."Titm_Cost" "Titm_Cost"
	,"Stsaldtl"."Itm_Cost" "Itm_Cost"
	,"Stsaldtl"."Catg_No" "Catg_No"
	,"Stsaldtl"."Sup_No" "Sup_No"
	,"Stsaldtl"."notesdtl" "notesdtl"
	,"Stsaldtl"."User_Id" "User_Id"
	,"Stsaldtl"."sgm" "sgm"
	,"Stsaldtl"."darb" "darb"
	,"Stsaldtl"."paplcprc" "paplcprc"
	,"Stsaldtl"."nakdy" "nakdy"
	,"Stsaldtl"."shad" "shad"
	,"Stsaldtl"."carno" "carno"
	,"Stsaldtl"."locno" "locno"
	,"Stsaldtl"."Refcode" "Refcode"
	,"Stsaldtl"."Reftxt" "Reftxt"
	,"Stsaldtl"."whhus" "whhus"
	,"Stsaldtl"."Reftxt1" "Reftxt1"
	,"Stsaldtl"."idate" "idate"
	,"Stsaldtl"."ModifiedBy" "ModifiedBy"
	,"Stsaldtl"."ModifiedOn" "ModifiedOn"
,"Stsaldtl"."ModifiedOn" "MTime"
	,"Stsaldtl"."CreatedBy" "CreatedBy"
	,"Stsaldtl"."CreatedOn" "CreatedOn"
from "dbo"."Stsaldtl" "Stsaldtl"
	left join "dbo"."STsalhdr" "Doc_" on "Stsaldtl"."Doc_No" = "Doc_"."Doc_No"
	left join "dbo"."Branchcnst" "Doc_Brn_" on "Doc_"."Brn_No" = "Doc_Brn_"."Brn_No"
	left join "dbo"."Pycnstnt" "Doc_Brn_Cmp_" on "Doc_Brn_"."Cmp_code" = "Doc_Brn_Cmp_"."Cmp_No"
	left join "dbo"."GLSubledgertyplist" "Doc_typcmsup" on "Doc_"."typcmsup" = "Doc_typcmsup"."Subledgerid"
	left join "dbo"."Pymntype" "Doc_Pym_" on "Doc_"."Pym_No" = "Doc_Pym_"."Pymnt_No"
	left join "dbo"."GLmstrfl" "Doc_Pym_pymnt_acc" on "Doc_Pym_"."pymnt_acc" = "Doc_Pym_pymnt_acc"."Acc_No"
	left join "dbo"."glmulcmp" "Doc_Pym_sgm" on "Doc_Pym_"."sgm" = "Doc_Pym_sgm"."sgmid"
	left join "dbo"."Stcurncy" "Doc_cur" on "Doc_"."curcode" = "Doc_cur"."Curncy_No"
	left join "dbo"."Stcustmr" "Doc_Cstm_" on "Doc_"."Cstm_No" = "Doc_Cstm_"."Cstm_No"
	left join "dbo"."AreaList" "Doc_Cstm_Area" on "Doc_Cstm_"."AreaNo" = "Doc_Cstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "Doc_Cstm_Evaluation" on "Doc_Cstm_"."EvaluationNO" = "Doc_Cstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "Doc_Cstm_acc_" on "Doc_Cstm_"."acc_no" = "Doc_Cstm_acc_"."Acc_No"
	left join "dbo"."PYCntry" "Doc_Cstm_Cntry_" on "Doc_Cstm_"."Cntry_No" = "Doc_Cstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Doc_Cstm_curncy_" on "Doc_Cstm_"."curncy_no" = "Doc_Cstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "Doc_Cstm_TSNEF" on "Doc_Cstm_"."TSNEFNO" = "Doc_Cstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "Doc_Cstm_TSNEFNO1" on "Doc_Cstm_"."TSNEFNO1" = "Doc_Cstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "Doc_Cstm_Slm_" on "Doc_Cstm_"."Slm_No" = "Doc_Cstm_Slm_"."Slm_No"
	left join "dbo"."Stslsman" "Doc_Slm_" on "Doc_"."Slm_No" = "Doc_Slm_"."Slm_No"
	left join "dbo"."masngerclass" "Doc_Slm_masngerclass" on "Doc_Slm_"."masngerclassid" = "Doc_Slm_masngerclass"."masngerclassid"
	left join "dbo"."Branchcnst" "Brn_" on "Doc_"."Brn_No" = "Brn_"."Brn_No"
	left join "dbo"."Pycnstnt" "Brn_Cmp_" on "Brn_"."Cmp_code" = "Brn_Cmp_"."Cmp_No"
	left join "dbo"."Groupcnst" "Brn_Cmp_Grp_" on "Brn_Cmp_"."Grp_No" = "Brn_Cmp_Grp_"."Grp_No"
	left join "dbo"."ITEMS" "Itm_" on "Stsaldtl"."Itm_No" = "Itm_"."ID_comp"
	left join "dbo"."GROUPStk" "Itm_IDGROUP" on "Itm_"."IDGROUP" = "Itm_IDGROUP"."IDGROUP"
	left join "dbo"."ItemList" "Itm_Sup_" on "Itm_"."Sup_No" = "Itm_Sup_"."ItemNo"
	left join "dbo"."PYHldtypsv1" "Itm_id1" on "Itm_"."id1" = "Itm_id1"."ID1"
	left join "dbo"."PYHldtypsv2" "Itm_id2" on "Itm_"."id2" = "Itm_id2"."ID2"
	left join "dbo"."PYHldtypsv3" "Itm_id3" on "Itm_"."id3" = "Itm_id3"."id3"
	left join "dbo"."PYHldtypsv4" "Itm_id4" on "Itm_"."id4" = "Itm_id4"."id4"
	left join "dbo"."PYHldtypsv5" "Itm_id5" on "Itm_"."id5" = "Itm_id5"."id5"
	left join "dbo"."PYHldtypsv6" "Itm_id6" on "Itm_"."id6" = "Itm_id6"."id6"
	left join "dbo"."PYHldtypsv7" "Itm_id7" on "Itm_"."id7" = "Itm_id7"."id7"
	left join "dbo"."Stunit" "Unit_" on "Stsaldtl"."Unit_No" = "Unit_"."Unit_No"
]]></text>
    </command>
    <command id="ln_noIdentityCommand" type="Text" event="Inserted">
