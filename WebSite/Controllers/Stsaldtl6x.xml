<rule id="r108" type="JavaScript" commandName="Insert|Update" phase="Before" name="darb_Validator"><![CDATA[var fieldValue = [darb];
if (fieldValue == null) {
    // prevent the default action processing
    this.preventDefault();
    // set the focus on the field and display an error
    this.result.focus('darb', 'Required field.');
}
if (fieldValue >100) {
    // prevent the default action processing
    this.preventDefault();
    // set the focus on the field and display an error
    this.result.focus('darb', 'Required less than or equal  100 files amount');
}]]></rule>
    <rule id="r109" type="JavaScript" commandName="Insert|Update" phase="Before" name="Discount_Validator"><![CDATA[var fieldValue = [Discount];
if (fieldValue == null) {
    // prevent the default action processing
    this.preventDefault();
    // set the focus on the field and display an error
    this.result.focus('Discount', 'Required field.');
}
if (fieldValue >10) {
    // prevent the default action processing
