    <command id="command1" type="Text">
      <text><![CDATA[
select
	"ReorderListDetailes"."ReorderIDDetailes" "ReorderIDDetailes"
	,"ReorderListDetailes"."ReorderID" "ReorderID"
	,"Reorder"."ModifiedBy" "ReorderModifiedBy"
	,"ReorderListDetailes"."ReorderDate" "ReorderDate"
	,"ReorderListDetailes"."ID_Comp" "ID_Comp"
	,"ID_Comp"."ID_USER" "ID_CompID_USER"
        ,"ID_Comp"."ITEM" "ITEM"
	,"ID_CompIDGROUP"."GROUPA" "ID_CompIDGROUPID_USER"
	,"ID_CompSup_"."Item_name" "ID_CompSup_Item_name"
	,"ID_Compid1"."DESCRP1" "ID_Compid1DESCRP1"
	,"ID_Compid2"."DESCRP1" "ID_Compid2DESCRP1"
	,"ID_Compid3"."DESCRP1" "ID_Compid3DESCRP1"
	,"ID_Compid4"."DESCRP1" "ID_Compid4DESCRP1"
	,"ID_Compid5"."DESCRP1" "ID_Compid5DESCRP1"
	,"ID_Compid6"."DESCRP1" "ID_Compid6DESCRP1"
	,"ID_Compid7"."DESCRP1" "ID_Compid7DESCRP1"
	,"ReorderListDetailes"."ModifiedBy" "ModifiedBy"
	,"ReorderListDetailes"."ModifiedOn" "ModifiedOn"
	,"ReorderListDetailes"."CreatedBy" "CreatedBy"
	,"ReorderListDetailes"."CreatedOn" "CreatedOn"
	,"ReorderListDetailes"."Salefor90" "Salefor90"
	,"ReorderListDetailes"."Salefor901" "Salefor901"
	,"ReorderListDetailes"."Salefor902" "Salefor902"
	,"ReorderListDetailes"."Salefor903" "Salefor903"
	,"ReorderListDetailes"."Salefor904" "Salefor904"
	,"ReorderListDetailes"."Salefor21" "Salefor21"
	,"ReorderListDetailes"."Salefor211" "Salefor211"
	,"ReorderListDetailes"."Salefor212" "Salefor212"
	,"ReorderListDetailes"."Salefor213" "Salefor213"
	,"ReorderListDetailes"."Salefor214" "Salefor214"
	,"ReorderListDetailes"."finalCalc" "finalCalc"
from "dbo"."ReorderListDetailes" "ReorderListDetailes"
	left join "dbo"."ReorderList" "Reorder" on "ReorderListDetailes"."ReorderID" = "Reorder"."ReorderID"
	left join "dbo"."ITEMS" "ID_Comp" on "ReorderListDetailes"."ID_Comp" = "ID_Comp"."ID_comp"
	left join "dbo"."GROUPStk" "ID_CompIDGROUP" on "ID_Comp"."IDGROUP" = "ID_CompIDGROUP"."IDGROUP"
	left join "dbo"."ItemList" "ID_CompSup_" on "ID_Comp"."Sup_No" = "ID_CompSup_"."ItemNo"
	left join "dbo"."PYHldtypsv1" "ID_Compid1" on "ID_Comp"."id1" = "ID_Compid1"."ID1"
	left join "dbo"."PYHldtypsv2" "ID_Compid2" on "ID_Comp"."id2" = "ID_Compid2"."ID2"
	left join "dbo"."PYHldtypsv3" "ID_Compid3" on "ID_Comp"."id3" = "ID_Compid3"."id3"
	left join "dbo"."PYHldtypsv4" "ID_Compid4" on "ID_Comp"."id4" = "ID_Compid4"."id4"
	left join "dbo"."PYHldtypsv5" "ID_Compid5" on "ID_Comp"."id5" = "ID_Compid5"."id5"
	left join "dbo"."PYHldtypsv6" "ID_Compid6" on "ID_Comp"."id6" = "ID_Compid6"."id6"
	left join "dbo"."PYHldtypsv7" "ID_Compid7" on "ID_Comp"."id7" = "ID_Compid7"."id7"
]]></text>
    </command>
