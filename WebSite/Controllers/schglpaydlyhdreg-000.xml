    <rule id="schglpaydlyhdreg1001" commandName="Calculate" type="Sql" phase="Execute">
  <![CDATA[
-- duplicate the order
if @Pymnt_No is null or @Pymnt_No=0
begin
set @Pymnt_No=2
end 
if @payReferanceNo is null or @payReferanceNo=0
begin
if @branch is not null and @acdcode is not null 
begin

    set @sgm=(select sgm from [schbranch] where branch=@branch)
    set @payReferanceNo = (select max(payReferanceNo) from [schglpaydlyhdreg] where approved=1 and  sgm=@sgm and Pymnt_No=@Pymnt_No)
    set @payReferanceNo=@payReferanceNo+1
end 
end


]]></rule>
    
    <rule id="r101" commandName="Custom" commandArgument="FullDuplicatewithDetails" type="Sql" phase="Execute"><![CDATA[-- duplicate the order
INSERT INTO [dbo].[schglpaydlyhdreg]
           ([sgm]
           ,[branch]
           ,[Trs_Dt]
           ,[payBalance]
           ,[payReferanceNo]
           ,[payIDregorg]
           ,[payDelevername]
           ,[payDelevercivil]
           ,[payDeleverTel]
           ,[payNotes]
           ,[approved]
           ,[posted]
           ,[deleted]
           ,[havdetails]
           ,[ModifiedBy]
           ,[ModifiedOn]
           ,[CreatedBy]
           ,[CreatedOn]
           ,[acdcode]
           ,[Pymnt_No])
select 	   [sgm]
           ,[branch]
           ,[Trs_Dt]
