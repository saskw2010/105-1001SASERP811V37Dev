<?xml version="1.0" encoding="utf-8"?>
<dataController name="assignments" conflictDetection="overwriteChanges" label="Assignments" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"assignments"."id" "id"
	,"assignments"."quiz_id" "quiz_id"
	,"assignments"."question_seq" "question_seq"
	,"assignments"."answer_seq" "answer_seq"
	,"assignments"."results_mode" "results_mode"
	,"assignments"."quiz_start_time" "quiz_start_time"
	,"assignments"."quiz_end_time" "quiz_end_time"
	,"assignments"."added_date" "added_date"
	,"assignments"."quiz_time" "quiz_time"
	,"assignments"."show_results" "show_results"
	,"assignments"."pass_score" "pass_score"
	,"assignments"."quiz_type" "quiz_type"
	,"assignments"."status" "status"
from "dbo"."assignments" "assignments"
]]></text>
    </command>
    <command id="idIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="id" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="id" type="Int32" allowNulls="false" isPrimaryKey="true" label="Id" readOnly="true" />
    <field name="quiz_id" type="Int32" allowNulls="false" label="Quiz#" showInSummary="true" />
    <field name="question_seq" type="Int32" allowNulls="false" label="Question Seq" showInSummary="true" />
    <field name="answer_seq" type="Int32" allowNulls="false" label="Answer Seq" showInSummary="true" />
    <field name="results_mode" type="Int32" allowNulls="false" label="Results Mode" showInSummary="true" />
    <field name="quiz_start_time" type="DateTime" allowNulls="false" default="(getdate())" label="Quiz Start Time" showInSummary="true" />
    <field name="quiz_end_time" type="DateTime" allowNulls="false" default="(getdate())" label="Quiz End Time" />
    <field name="added_date" type="DateTime" allowNulls="false" default="(getdate())" label="Added Date" />
    <field name="quiz_time" type="Int32" allowNulls="false" default="((0))" label="Quiz Time" />
    <field name="show_results" type="Int32" label="Show Results" />
    <field name="pass_score" type="Decimal" label="Pass Score" />
    <field name="quiz_type" type="Int32" allowNulls="false" default="((1))" label="Quiz Type" />
    <field name="status" type="Int32" allowNulls="false" default="((0))" label="Status" />
