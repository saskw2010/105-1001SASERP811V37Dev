  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"RequestCategories"."reqcat_id" "reqcat_id"
	,"RequestCategories"."reqcat_name" "reqcat_name"
	,"RequestCategories"."reqcat_namee" "reqcat_namee"
	,"RequestCategories"."Req_typeid" "Req_typeid"
	,"Req_type"."req_Type" "Req_typereq_Type"
	,"Req_typeuserarea_"."userarea_Desc" "Req_typeuserarea_userarea_Desc"
	,"RequestCategories"."onrequest" "onrequest"
	,"RequestCategories"."formrequired" "formrequired"
	,"RequestCategories"."softcopy_availble" "softcopy_availble"
	,"RequestCategories"."reqcat_parentid" "reqcat_parentid"
	,"reqcat_parent"."reqcat_name" "reqcat_parentreqcat_name"
	,"reqcat_parentReq_type"."req_Type" "reqcat_parentReq_typereq_Type"
	,"reqcat_parentReq_typeuserarea_"."userarea_Desc" "reqcat_parentReq_typeuserarea_userarea_Desc"
	,"RequestCategories"."reqcat_active" "reqcat_active"
	,"RequestCategories"."reqcat_createDate" "reqcat_createDate"
	,"RequestCategories"."reqcat_modifyDate" "reqcat_modifyDate"
	,"RequestCategories"."reqcat_creatoruser" "reqcat_creatoruser"
	,"RequestCategories"."reqcat_modifyuser" "reqcat_modifyuser"
	,"RequestCategories"."req_url" "req_url"
	,"RequestCategories"."ModifiedBy" "ModifiedBy"
	,"RequestCategories"."ModifiedOn" "ModifiedOn"
	,"RequestCategories"."CreatedBy" "CreatedBy"
	,"RequestCategories"."CreatedOn" "CreatedOn"
from "dbo"."RequestCategories" "RequestCategories"
	left join "dbo"."RequestTypes" "Req_type" on "RequestCategories"."Req_typeid" = "Req_type"."req_Typeid"
	left join "dbo"."Userarea" "Req_typeuserarea_" on "Req_type"."userarea_id" = "Req_typeuserarea_"."userarea_id"
	left join "dbo"."RequestCategories" "reqcat_parent" on "RequestCategories"."reqcat_parentid" = "reqcat_parent"."reqcat_id"
	left join "dbo"."RequestTypes" "reqcat_parentReq_type" on "reqcat_parent"."Req_typeid" = "reqcat_parentReq_type"."req_Typeid"
	left join "dbo"."Userarea" "reqcat_parentReq_typeuserarea_" on "reqcat_parentReq_type"."userarea_id" = "reqcat_parentReq_typeuserarea_"."userarea_id"
]]></text>
    </command>
  </commands>
