	,"ardmast"."note" "note"
	,"ardmast"."naslat" "naslat"
	,"ardmast"."wazefet" "wazefet"
	,"ardmast"."Esmsaheb" "Esmsaheb"
	,"ardmast"."arddate" "arddate"
	,"ardmast"."typcmsup" "typcmsup"
	,"typcmsup"."SubledgerDescrption" "typcmsupSubledgerDescrption"
	,"ardmast"."Cstm_No" "Cstm_No"
	,"Cstm_"."Cstm_Nm" "Cstm_Cstm_Nm"
	,"Cstm_Area"."Area_name" "Cstm_AreaArea_name"
	,"Cstm_Evaluation"."Evaluation_name" "Cstm_EvaluationEvaluation_name"
	,"Cstm_acc_"."Acc_Nm" "Cstm_acc_Acc_Nm"
	,"Cstm_acc_Clsacc_"."Acc_Nm" "Cstm_acc_Clsacc_Acc_Nm"
	,"Cstm_acc_Acc_Bnd"."Acc_Nm" "Cstm_acc_Acc_BndAcc_Nm"
	,"Cstm_Cntry_"."Cntry_Nm" "Cstm_Cntry_Cntry_Nm"
	,"Cstm_curncy_"."Curncy_Nm" "Cstm_curncy_Curncy_Nm"
	,"Cstm_TSNEF"."TSNEF_name" "Cstm_TSNEFTSNEF_name"
	,"Cstm_TSNEFNO1"."TSNEF1_name" "Cstm_TSNEFNO1TSNEF1_name"
	,"Cstm_Slm_"."Slm_Nm" "Cstm_Slm_Slm_Nm"
	,"Cstm_Slm_masngerclass"."masngerclass" "Cstm_Slm_masngerclassmasngerclass"
	,"ardmast"."Cstm_Nm" "Cstm_Nm"
	,"ardmast"."Slm_Nm" "Slm_Nm"
	,"ardmast"."Tarhel" "Tarhel"
	,"ardmast"."sgm" "sgm"
	,"ardmast"."currncy" "currncy"
	,"ardmast"."posted" "posted"
	,"ardmast"."closed" "closed"
	,"ardmast"."arvdate" "arvdate"
	,"ardmast"."ModifiedBy" "ModifiedBy"
	,"ardmast"."ModifiedOn" "ModifiedOn"
	,"ardmast"."CreatedBy" "CreatedBy"
	,"ardmast"."CreatedOn" "CreatedOn"
	,"ardmast"."Pym_No" "Pym_No"
	,"Pym_"."Pymnt_Nm" "Pym_Pymnt_Nm"
	,"Pym_pymnt_acc"."Acc_Nm" "Pym_pymnt_accAcc_Nm"
	,"Pym_pymnt_accClsacc_"."Acc_Nm" "Pym_pymnt_accClsacc_Acc_Nm"
	,"Pym_pymnt_accAcc_Bnd"."Acc_Nm" "Pym_pymnt_accAcc_BndAcc_Nm"
	,"Pym_sgm"."sgm_Nm" "Pym_sgmsgm_Nm"
	,"Pym_sgmOpco"."OpcoName" "Pym_sgmOpcoOpcoName"
	,"ardmast"."curcode" "curcode"
	,"cur"."Curncy_Nm" "curCurncy_Nm"
	,"ardmast"."Slm_No" "Slm_No"
	,"Slm_"."Slm_Nm" "Slm_Slm_Nm"
	,"Slm_masngerclass"."masngerclass" "Slm_masngerclassmasngerclass"
from "dbo"."ardmast" "ardmast"
	left join "dbo"."Branchcnst" "Brn_" on "ardmast"."Brn_No" = "Brn_"."Brn_No"
	left join "dbo"."docard" "Thedoc_typ" on "ardmast"."doc_typ" = "Thedoc_typ"."doc_typ"
	left join "dbo"."GLSubledgertyplist" "typcmsup" on "ardmast"."typcmsup" = "typcmsup"."Subledgerid"
	left join "dbo"."Stcustmr" "Cstm_" on "ardmast"."Cstm_No" = "Cstm_"."Cstm_No"
	left join "dbo"."AreaList" "Cstm_Area" on "Cstm_"."AreaNo" = "Cstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "Cstm_Evaluation" on "Cstm_"."EvaluationNO" = "Cstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "Cstm_acc_" on "Cstm_"."acc_no" = "Cstm_acc_"."Acc_No"
	left join "dbo"."GLmfbab" "Cstm_acc_Clsacc_" on "Cstm_acc_"."Clsacc_No" = "Cstm_acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Cstm_acc_Acc_Bnd" on "Cstm_acc_"."Acc_Bnd" = "Cstm_acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."PYCntry" "Cstm_Cntry_" on "Cstm_"."Cntry_No" = "Cstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Cstm_curncy_" on "Cstm_"."curncy_no" = "Cstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "Cstm_TSNEF" on "Cstm_"."TSNEFNO" = "Cstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "Cstm_TSNEFNO1" on "Cstm_"."TSNEFNO1" = "Cstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "Cstm_Slm_" on "Cstm_"."Slm_No" = "Cstm_Slm_"."Slm_No"
	left join "dbo"."masngerclass" "Cstm_Slm_masngerclass" on "Cstm_Slm_"."masngerclassid" = "Cstm_Slm_masngerclass"."masngerclassid"
	left join "dbo"."Pymntype" "Pym_" on "ardmast"."Pym_No" = "Pym_"."Pymnt_No"
	left join "dbo"."GLmstrfl" "Pym_pymnt_acc" on "Pym_"."pymnt_acc" = "Pym_pymnt_acc"."Acc_No"
	left join "dbo"."GLmfbab" "Pym_pymnt_accClsacc_" on "Pym_pymnt_acc"."Clsacc_No" = "Pym_pymnt_accClsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Pym_pymnt_accAcc_Bnd" on "Pym_pymnt_acc"."Acc_Bnd" = "Pym_pymnt_accAcc_Bnd"."Acc_Bnd"
	left join "dbo"."glmulcmp" "Pym_sgm" on "Pym_"."sgm" = "Pym_sgm"."sgmid"
	left join "dbo"."OpCo" "Pym_sgmOpco" on "Pym_sgm"."OpcoID" = "Pym_sgmOpco"."OpcoIDidentity"
	left join "dbo"."Stcurncy" "cur" on "ardmast"."curcode" = "cur"."Curncy_No"
	left join "dbo"."Stslsman" "Slm_" on "ardmast"."Slm_No" = "Slm_"."Slm_No"
	left join "dbo"."masngerclass" "Slm_masngerclass" on "Slm_"."masngerclassid" = "Slm_masngerclass"."masngerclassid"
]]></text>
