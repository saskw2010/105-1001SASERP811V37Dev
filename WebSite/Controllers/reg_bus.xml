<?xml version="1.0" encoding="utf-8"?>
<dataController name="reg_bus" conflictDetection="overwriteChanges" label="Reg Bus" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"reg_bus"."id" "id"
	,"reg_bus"."name_1" "name_1"
	,"reg_bus"."name_2" "name_2"
	,"reg_bus"."active" "active"
	,"reg_bus"."drive_name" "drive_name"
	,"reg_bus"."drive_phone" "drive_phone"
	,"reg_bus"."bus_no" "bus_no"
	,"reg_bus"."notes" "notes"
	,"reg_bus"."notes_2" "notes_2"
	,"reg_bus"."ModifiedBy" "ModifiedBy"
	,"reg_bus"."ModifiedOn" "ModifiedOn"
	,"reg_bus"."CreatedBy" "CreatedBy"
	,"reg_bus"."CreatedOn" "CreatedOn"
	,"reg_bus"."branch" "branch"
	,"Thebranch"."Desc1" "ThebranchDesc1"
	,"Thebranchsgm"."sgm_Nm" "Thebranchsgmsgm_Nm"
	,"ThebranchsgmOpco"."OpcoName" "ThebranchsgmOpcoOpcoName"
	,"ThebranchGender"."Gender" "ThebranchGenderGender"
	,"Thebranchstage"."ShortDesc1" "ThebranchstageShortDesc1"
	,"Thebranchschtyp"."schtypDesc" "ThebranchschtypschtypDesc"
from "dbo"."reg_bus" "reg_bus"
	left join "dbo"."schBranch" "Thebranch" on "reg_bus"."branch" = "Thebranch"."branch"
	left join "dbo"."glmulcmp" "Thebranchsgm" on "Thebranch"."sgm" = "Thebranchsgm"."sgmid"
	left join "dbo"."OpCo" "ThebranchsgmOpco" on "Thebranchsgm"."OpcoID" = "ThebranchsgmOpco"."OpcoIDidentity"
	left join "dbo"."PyGender" "ThebranchGender" on "Thebranch"."Genderid" = "ThebranchGender"."Genderid"
	left join "dbo"."schStage" "Thebranchstage" on "Thebranch"."stagecode" = "Thebranchstage"."Code"
	left join "dbo"."schtyp" "Thebranchschtyp" on "Thebranch"."schtypid" = "Thebranchschtyp"."schtypid"
]]></text>
    </command>
    <command id="idIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="id" />
      </output>
    </command>
  </commands>
