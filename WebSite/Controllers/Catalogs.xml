<?xml version="1.0" encoding="utf-8"?>
<dataController name="Catalogs" conflictDetection="overwriteChanges" label="Catalogs" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Catalogs"."catalog_id" "catalog_id"
	,"Catalogs"."catalog_name" "catalog_name"
	,"Catalogs"."catalog_publisher" "catalog_publisher"
	,"Catalogs"."catalog_description" "catalog_description"
	,"Catalogs"."date_of_publication" "date_of_publication"
	,"Catalogs"."date_of_latest_revision" "date_of_latest_revision"
from "dbo"."Catalogs" "Catalogs"
]]></text>
    </command>
    <command id="catalog_idIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="catalog_id" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="catalog_id" type="Int32" allowNulls="false" isPrimaryKey="true" label="Catalog#" readOnly="true" />
    <field name="catalog_name" type="String" label="Catalog Name" length="50" showInSummary="true" />
    <field name="catalog_publisher" type="String" label="Catalog Publisher" length="80" showInSummary="true" />
    <field name="catalog_description" type="String" label="Catalog Description" length="255" showInSummary="true" />
    <field name="date_of_publication" type="DateTime" label="Date Of Publication" showInSummary="true" />
    <field name="date_of_latest_revision" type="DateTime" label="Date Of Latest Revision" showInSummary="true" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Catalogs">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="catalog_name" columns="50" />
        <dataField fieldName="catalog_publisher" />
        <dataField fieldName="catalog_description" />
        <dataField fieldName="date_of_publication" columns="10" />
        <dataField fieldName="date_of_latest_revision" columns="10" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Catalogs">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Catalogs" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="catalog_name" columns="50" />
            <dataField fieldName="catalog_publisher" />
            <dataField fieldName="catalog_description" />
            <dataField fieldName="date_of_publication" columns="10" />
            <dataField fieldName="date_of_latest_revision" columns="10" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Catalogs">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New Catalogs" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="catalog_name" columns="50" />
            <dataField fieldName="catalog_publisher" />
            <dataField fieldName="catalog_description" />
            <dataField fieldName="date_of_publication" columns="10" />
            <dataField fieldName="date_of_latest_revision" columns="10" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
  <actions>
    <actionGroup id="ag1" scope="Grid">
      <action id="a1" commandName="Select" commandArgument="editForm1" />
      <action id="a2" commandName="Edit" />
      <action id="a3" commandName="Delete" />
      <action id="a6" />
      <action id="a7" commandName="New" commandArgument="grid1" />
      <action id="a8" commandName="Duplicate" commandArgument="createForm1" />
      <action id="a9" />
