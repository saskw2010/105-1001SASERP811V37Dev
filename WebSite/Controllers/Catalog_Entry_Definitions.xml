	left join "dbo"."Catalog_Structure" "Thecatalog_level_number" on "Catalog_Entry_Definitions"."catalog_level_number" = "Thecatalog_level_number"."catalog_level_number"
	left join "dbo"."Catalogs" "Thecatalog_level_numbercatalog_" on "Thecatalog_level_number"."catalog_id" = "Thecatalog_level_numbercatalog_"."catalog_id"
	left join "dbo"."Attribute_Definitions" "attribute_" on "Catalog_Entry_Definitions"."attribute_id" = "attribute_"."attribute_id"
]]></text>
    </command>
  </commands>
