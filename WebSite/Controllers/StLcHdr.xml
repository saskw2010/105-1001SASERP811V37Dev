from "dbo"."StLcHdr" "StLcHdr"
	left join "dbo"."Branchcnst" "brn_" on "StLcHdr"."brn_no" = "brn_"."Brn_No"
	left join "dbo"."schBranch" "brn_Branch" on "brn_"."Branch" = "brn_Branch"."branch"
	left join "dbo"."glmulcmp" "brn_Branchsgm" on "brn_Branch"."sgm" = "brn_Branchsgm"."sgmid"
	left join "dbo"."PyGender" "brn_BranchGender" on "brn_Branch"."Genderid" = "brn_BranchGender"."Genderid"
	left join "dbo"."schStage" "brn_Branchstage" on "brn_Branch"."stagecode" = "brn_Branchstage"."Code"
	left join "dbo"."schtyp" "brn_Branchschtyp" on "brn_Branch"."schtypid" = "brn_Branchschtyp"."schtypid"
	left join "dbo"."PaymentTypes" "Pym_" on "StLcHdr"."Pym_No" = "Pym_"."PaymentTypeID"
	left join "dbo"."Stcurncy" "Curncy_" on "StLcHdr"."Curncy_No" = "Curncy_"."Curncy_No"
	left join "dbo"."Stsuplir" "Sup_" on "StLcHdr"."Sup_No" = "Sup_"."Sup_No"
	left join "dbo"."GLmstrfl" "Sup_Acc_" on "Sup_"."Acc_No" = "Sup_Acc_"."Acc_No"
	left join "dbo"."GLmfbab" "Sup_Acc_Clsacc_" on "Sup_Acc_"."Clsacc_No" = "Sup_Acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Sup_Acc_Acc_Bnd" on "Sup_Acc_"."Acc_Bnd" = "Sup_Acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."glmulcmp" "Sup_sgm" on "Sup_"."sgm" = "Sup_sgm"."sgmid"
	left join "dbo"."OpCo" "Sup_sgmOpco" on "Sup_sgm"."OpcoID" = "Sup_sgmOpco"."OpcoIDidentity"
	left join "dbo"."PYCntry" "Sup_Cntry_" on "Sup_"."Cntry_No" = "Sup_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Sup_Curncy_" on "Sup_"."Curncy_No" = "Sup_Curncy_"."Curncy_No"
]]></text>
