<?xml version="1.0" encoding="utf-8"?>
<dataController name="Emp_Duties" conflictDetection="overwriteChanges" label="Emp Duties" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Emp_Duties"."SID" "SID"
	,"Emp_Duties"."Duty_SID" "Duty_SID"
	,"Duty_S"."Duty_Code" "Duty_SDuty_Code"
	,"Duty_SDuty_Type_S"."Duty_Code" "Duty_SDuty_Type_SDuty_Code"
	,"Emp_Duties"."Duty_Shift_SID" "Duty_Shift_SID"
	,"Emp_Duties"."Emp_SID" "Emp_SID"
	,"Emp_Duties"."Start_Date" "Start_Date"
	,"Emp_Duties"."End_Date" "End_Date"
	,"Emp_Duties"."Last_Modified" "Last_Modified"
	,"Emp_Duties"."IsActive" "IsActive"
	,"Emp_Duties"."IsNew" "IsNew"
	,"Emp_Duties"."Emp_Duty_Masters_SID" "Emp_Duty_Masters_SID"
	,"Emp_Duty_Masters_S"."Duty_SID" "Emp_Duty_Masters_SDuty_SID"
from "dbo"."Emp_Duties" "Emp_Duties"
	left join "dbo"."Duties" "Duty_S" on "Emp_Duties"."Duty_SID" = "Duty_S"."SID"
	left join "dbo"."Duty_Types" "Duty_SDuty_Type_S" on "Duty_S"."Duty_Type_SID" = "Duty_SDuty_Type_S"."SID"
	left join "dbo"."Emp_Duty_Masters" "Emp_Duty_Masters_S" on "Emp_Duties"."Emp_Duty_Masters_SID" = "Emp_Duty_Masters_S"."SID"
]]></text>
    </command>
    <command id="SIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="SID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="SID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Sid" readOnly="true" />
    <field name="Duty_SID" type="Int32" allowNulls="false" label="Duty SID" showInSummary="true">
      <items style="Lookup" dataController="Duties" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="Duty_SDuty_Code" type="String" readOnly="true" label="Duty S Duty Code" length="50" />
    <field name="Duty_SDuty_Type_SDuty_Code" type="String" readOnly="true" label="Duty S Duty Type S Duty Code" length="50" />
    <field name="Duty_Shift_SID" type="Int32" label="Duty Shift SID" showInSummary="true" />
    <field name="Emp_SID" type="String" allowNulls="false" label="Emp SID" length="255" showInSummary="true" />
    <field name="Start_Date" type="DateTime" label="Start Date" showInSummary="true" />
    <field name="End_Date" type="DateTime" label="End Date" showInSummary="true" />
    <field name="Last_Modified" type="DateTime" label="Last Modified" />
    <field name="IsActive" type="Boolean" label="Is Active" />
    <field name="IsNew" type="Boolean" default="((0))" label="Is New" />
    <field name="Emp_Duty_Masters_SID" type="Int32" label="Emp Duty Masters SID">
      <items style="Lookup" dataController="Emp_Duty_Masters" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="Emp_Duty_Masters_SDuty_SID" type="Int32" readOnly="true" label="Emp Duty Masters S Duty SID" />
  </fields>
