<?xml version="1.0" encoding="utf-8"?>
<dataController name="AlertLevel" conflictDetection="overwriteChanges" label="Alert Level" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"AlertLevel"."AlertID" "AlertID"
	,"AlertLevel"."AlertLevelDesc" "AlertLevelDesc"
	,"AlertLevel"."Curncy_No" "Curncy_No"
	,"Curncy_"."Curncy_Nm" "Curncy_Curncy_Nm"
	,"AlertLevel"."OpcoID" "OpcoID"
	,"Opco"."OpcoName" "OpcoOpcoName"
	,"AlertLevel"."AlertAmountmax" "AlertAmountmax"
	,"AlertLevel"."AlertAmount" "AlertAmount"
from "dbo"."AlertLevel" "AlertLevel"
	left join "dbo"."Stcurncy" "Curncy_" on "AlertLevel"."Curncy_No" = "Curncy_"."Curncy_No"
	left join "dbo"."OpCo" "Opco" on "AlertLevel"."OpcoID" = "Opco"."OpcoIDidentity"
]]></text>
    </command>
    <command id="AlertIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="AlertID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="AlertID" type="Int64" allowNulls="false" isPrimaryKey="true" label="Alert#" readOnly="true" />
    <field name="AlertLevelDesc" type="String" label="Alert Level Desc" length="50" showInSummary="true" />
    <field name="Curncy_No" type="Int64" label="Curncy No" showInSummary="true">
      <items style="Lookup" dataController="Stcurncy" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="Curncy_Curncy_Nm" type="String" readOnly="true" label="Curncy Nm" length="200" />
    <field name="OpcoID" type="Int64" label="Opco#" showInSummary="true">
      <items style="Lookup" dataController="OpCo" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="OpcoOpcoName" type="String" readOnly="true" label="Opco Name" length="50" />
    <field name="AlertAmountmax" type="Decimal" label="Alert Amountmax" showInSummary="true" />
    <field name="AlertAmount" type="Decimal" label="Alert Amount" showInSummary="true" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Alert Level">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="AlertLevelDesc" columns="50" />
        <dataField fieldName="Curncy_No" aliasFieldName="Curncy_Curncy_Nm" hyperlinkFormatString="~/Pages/Stcurncy.aspx?Curncy_No={Curncy_No}&amp;_controller=Stcurncy&amp;_commandName=Select&amp;_commandArgument=editForm1" />
        <dataField fieldName="OpcoID" aliasFieldName="OpcoOpcoName" hyperlinkFormatString="~/Pages/OpCo.aspx?OpcoIDidentity={OpcoID}&amp;_controller=OpCo&amp;_commandName=Select&amp;_commandArgument=editForm1" />
        <dataField fieldName="AlertAmountmax" columns="15" />
        <dataField fieldName="AlertAmount" columns="15" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Alert Level">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Alert Level" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="AlertLevelDesc" columns="50" />
            <dataField fieldName="Curncy_No" aliasFieldName="Curncy_Curncy_Nm" hyperlinkFormatString="~/Pages/Stcurncy.aspx?Curncy_No={Curncy_No}&amp;_controller=Stcurncy&amp;_commandName=Select&amp;_commandArgument=editForm1" />
            <dataField fieldName="OpcoID" aliasFieldName="OpcoOpcoName" hyperlinkFormatString="~/Pages/OpCo.aspx?OpcoIDidentity={OpcoID}&amp;_controller=OpCo&amp;_commandName=Select&amp;_commandArgument=editForm1" />
            <dataField fieldName="AlertAmountmax" columns="15" />
            <dataField fieldName="AlertAmount" columns="15" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Alert Level">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New Alert Level" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="AlertLevelDesc" columns="50" />
            <dataField fieldName="Curncy_No" aliasFieldName="Curncy_Curncy_Nm" />
            <dataField fieldName="OpcoID" aliasFieldName="OpcoOpcoName" />
            <dataField fieldName="AlertAmountmax" columns="15" />
            <dataField fieldName="AlertAmount" columns="15" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
  <actions>
    <actionGroup id="ag1" scope="Grid">
      <action id="a1" commandName="Select" commandArgument="editForm1" />
      <action id="a2" commandName="Edit" />
      <action id="a3" commandName="Delete" />
      <action id="a6" />
      <action id="a7" commandName="New" commandArgument="grid1" />
      <action id="a8" commandName="Duplicate" commandArgument="createForm1" />
      <action id="a9" />
      <action id="a10" commandName="BatchEdit" />
      <action id="a11" commandName="BatchEdit" commandArgument="editForm1" />
    </actionGroup>
    <actionGroup id="ag2" scope="Form">
      <action id="a1" commandName="Edit" />
      <action id="a2" commandName="Delete" />
      <action id="a3" commandName="Cancel" />
      <action id="a4" whenLastCommandName="Edit" commandName="Update" />
      <action id="a5" whenLastCommandName="Edit" commandName="Delete" />
      <action id="a6" whenLastCommandName="Edit" commandName="Cancel" />
      <action id="a7" whenLastCommandName="New" commandName="Insert" />
      <action id="a8" whenLastCommandName="New" commandName="Cancel" />
      <action id="a9" whenLastCommandName="Duplicate" commandName="Insert" />
      <action id="a10" whenLastCommandName="Duplicate" commandName="Cancel" />
      <action id="a11" whenLastCommandName="BatchEdit" commandName="Update" />
      <action id="a12" whenLastCommandName="BatchEdit" commandName="Cancel" />
      <action id="a13" whenLastCommandName="Insert" whenView="createForm1" commandName="Select" commandArgument="editForm1" whenKeySelected="true" whenClientScript="this.get_hasDetails()" />
    </actionGroup>
    <actionGroup id="ag3" scope="ActionBar" headerText="New" flat="true">
      <action id="a1" commandName="New" commandArgument="createForm1" cssClass="NewIcon" />
    </actionGroup>
    <actionGroup id="ag4" scope="ActionBar" headerText="Edit/Delete" flat="true">
      <action id="a1" whenKeySelected="true" commandName="Edit" commandArgument="editForm1" cssClass="EditIcon" whenView="grid1" />
      <action id="a2" whenKeySelected="true" commandName="Delete" cssClass="DeleteIcon" whenView="grid1" />
    </actionGroup>
    <actionGroup id="ag5" scope="ActionBar" headerText="Actions">
      <action id="a1" commandName="ExportCsv" />
      <action id="a2" />
      <action id="a3" commandName="ExportRowset" />
      <action id="a4" commandName="ExportRss" />
