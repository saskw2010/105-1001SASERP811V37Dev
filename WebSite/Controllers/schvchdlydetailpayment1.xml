  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[SELECT     schvchdlydetailpayment.schvchdlydetailpaymentid, schvchdlydetailpayment.schvchdlyhdregid, schvchdlyhdreg.ReferanceNo AS schvchdlyhdregReferanceNo, 
                      schvchdlyhdregsgm.sgm_Nm AS schvchdlyhdregsgmsgm_Nm, schvchdlyhdregsgmOpco.OpcoName AS schvchdlyhdregsgmOpcoOpcoName, 
                      schvchdlyhdregIDregorg.NAME_1 AS schvchdlyhdregIDregorgNAME_1, schvchdlydetailpayment.Pymnt_No, Pymnt_.Pymnt_Nm AS Pymnt_Pymnt_Nm, 
                      Pymnt_pymnt_acc.Acc_Nm AS Pymnt_pymnt_accAcc_Nm, Pymnt_pymnt_accClsacc_.Acc_Nm AS Pymnt_pymnt_accClsacc_Acc_Nm, 
                      Pymnt_pymnt_accAcc_Bnd.Acc_Nm AS Pymnt_pymnt_accAcc_BndAcc_Nm, Pymnt_sgm.sgm_Nm AS Pymnt_sgmsgm_Nm, 
                      Pymnt_sgmOpco.OpcoName AS Pymnt_sgmOpcoOpcoName, schvchdlydetailpayment.paymentwaynumber, schvchdlydetailpayment.schBnk_No, 
                      schBnk_.schBnk_Nm AS schBnk_schBnk_Nm, schvchdlydetailpayment.checkAccount, schvchdlydetailpayment.checkdate, schvchdlydetailpayment.moneypaidto, 
                      schvchdlydetailpayment.paymentnotes, schvchdlyhdreg.Trs_Dt, schvchdlyhdreg.Balance
FROM         dbo.schvchdlydetailpayment AS schvchdlydetailpayment LEFT OUTER JOIN
                      dbo.schvchdlyhdreg AS schvchdlyhdreg ON schvchdlydetailpayment.schvchdlyhdregid = schvchdlyhdreg.schvchdlyhdregid LEFT OUTER JOIN
                      dbo.glmulcmp AS schvchdlyhdregsgm ON schvchdlyhdreg.sgm = schvchdlyhdregsgm.sgm LEFT OUTER JOIN
                      dbo.OpCo AS schvchdlyhdregsgmOpco ON schvchdlyhdregsgm.OpcoID = schvchdlyhdregsgmOpco.OpcoID LEFT OUTER JOIN
                      dbo.REG_ORGANIZATIONS AS schvchdlyhdregIDregorg ON schvchdlyhdreg.IDregorg = schvchdlyhdregIDregorg.IDIDregorg LEFT OUTER JOIN
                      dbo.Pymntype AS Pymnt_ ON schvchdlydetailpayment.Pymnt_No = Pymnt_.Pymnt_No LEFT OUTER JOIN
                      dbo.GLmstrfl AS Pymnt_pymnt_acc ON Pymnt_.pymnt_acc = Pymnt_pymnt_acc.Acc_No LEFT OUTER JOIN
                      dbo.GLmfbab AS Pymnt_pymnt_accClsacc_ ON Pymnt_pymnt_acc.Clsacc_No = Pymnt_pymnt_accClsacc_.Acc_Bab LEFT OUTER JOIN
                      dbo.GLmfband AS Pymnt_pymnt_accAcc_Bnd ON Pymnt_pymnt_acc.Acc_Bnd = Pymnt_pymnt_accAcc_Bnd.Acc_Bnd LEFT OUTER JOIN
                      dbo.glmulcmp AS Pymnt_sgm ON Pymnt_.sgm = Pymnt_sgm.sgm LEFT OUTER JOIN
                      dbo.OpCo AS Pymnt_sgmOpco ON Pymnt_sgm.OpcoID = Pymnt_sgmOpco.OpcoID LEFT OUTER JOIN
                      dbo.schGLacnBnk AS schBnk_ ON schvchdlydetailpayment.schBnk_No = schBnk_.schBnk_No]]></text>
    </command>
