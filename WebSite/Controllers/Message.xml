<?xml version="1.0" encoding="utf-8"?>
<dataController name="Message" conflictDetection="overwriteChanges" label="Message" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Message"."MessageID" "MessageID"
	,"Message"."RoomID" "RoomID"
	,"Room"."Name" "RoomName"
	,"Message"."UserID" "UserID"
	,"User"."UserName" "UserUserName"
	,"Usersgm"."sgm_Nm" "Usersgmsgm_Nm"
	,"UsersgmOpco"."OpcoName" "UsersgmOpcoOpcoName"
	,"UserUserTyp"."UsrTypNm" "UserUserTypUsrTypNm"
	,"Message"."ToUserID" "ToUserID"
	,"ToUser"."UserName" "ToUserUserName"
	,"ToUsersgm"."sgm_Nm" "ToUsersgmsgm_Nm"
	,"ToUsersgmOpco"."OpcoName" "ToUsersgmOpcoOpcoName"
	,"ToUserUserTyp"."UsrTypNm" "ToUserUserTypUsrTypNm"
	,"Message"."Text" "Text"
	,"Message"."TimeStamp" "TimeStamp"
	,"Message"."Color" "Color"
	,"Message"."ModifiedBy" "ModifiedBy"
	,"Message"."ModifiedByUserId" "ModifiedByUserId"
	,"Message"."ModifiedOn" "ModifiedOn"
	,"Message"."CreatedBy" "CreatedBy"
	,"Message"."CreatedById" "CreatedById"
	,"Message"."CreatedOn" "CreatedOn"
from "dbo"."Message" "Message"
	left join "dbo"."Room" "Room" on "Message"."RoomID" = "Room"."RoomID"
	left join "dbo"."Users" "User" on "Message"."UserID" = "User"."UserID"
	left join "dbo"."glmulcmp" "Usersgm" on "User"."sgm" = "Usersgm"."sgmid"
	left join "dbo"."OpCo" "UsersgmOpco" on "Usersgm"."OpcoID" = "UsersgmOpco"."OpcoIDidentity"
	left join "dbo"."UsersTypes" "UserUserTyp" on "User"."UserTyp" = "UserUserTyp"."UsrTypID"
	left join "dbo"."Users" "ToUser" on "Message"."ToUserID" = "ToUser"."UserID"
	left join "dbo"."glmulcmp" "ToUsersgm" on "ToUser"."sgm" = "ToUsersgm"."sgmid"
	left join "dbo"."OpCo" "ToUsersgmOpco" on "ToUsersgm"."OpcoID" = "ToUsersgmOpco"."OpcoIDidentity"
	left join "dbo"."UsersTypes" "ToUserUserTyp" on "ToUser"."UserTyp" = "ToUserUserTyp"."UsrTypID"
]]></text>
    </command>
    <command id="MessageIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="MessageID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="MessageID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Message#" readOnly="true" />
    <field name="RoomID" type="Int32" label="Room#" showInSummary="true">
      <items style="Lookup" dataController="Room" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="RoomName" type="String" readOnly="true" label="Room Name" length="50" />
    <field name="UserID" type="Int64" allowNulls="false" label="User#" showInSummary="true">
      <items searchOnStart="true" />
    </field>
    <field name="UserUserName" type="String" readOnly="true" label="User Name" length="50" />
    <field name="Usersgmsgm_Nm" type="String" readOnly="true" label="Usersgmsgm Nm" length="255" />
    <field name="UsersgmOpcoOpcoName" type="String" readOnly="true" label="Usersgm Opco Name" length="50" />
    <field name="UserUserTypUsrTypNm" type="String" readOnly="true" label="User Typ Usr Typ Nm" length="255" />
    <field name="ToUserID" type="Int64" label="To User#" showInSummary="true">
      <items searchOnStart="true" />
    </field>
    <field name="ToUserUserName" type="String" readOnly="true" label="To User Name" length="50" />
    <field name="ToUsersgmsgm_Nm" type="String" readOnly="true" label="To Usersgmsgm Nm" length="255" />
    <field name="ToUsersgmOpcoOpcoName" type="String" readOnly="true" label="To Usersgm Opco Name" length="50" />
    <field name="ToUserUserTypUsrTypNm" type="String" readOnly="true" label="To User Typ Usr Typ Nm" length="255" />
    <field name="Text" type="String" allowNulls="false" label="Text" length="100" showInSummary="true" />
    <field name="TimeStamp" type="DateTime" allowNulls="false" default="(getdate())" label="Time Stamp" showInSummary="true" />
    <field name="Color" type="String" label="Color" length="50" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" />
    <field name="ModifiedByUserId" type="Guid" label="Modified By User#" />
    <field name="ModifiedOn" type="DateTime" label="Modified On" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="CreatedById" type="Guid" label="Created By#" />
    <field name="CreatedOn" type="DateTime" label="Created On" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Message">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="UserID" aliasFieldName="UserUserName" />
        <dataField fieldName="RoomID" aliasFieldName="RoomName" hyperlinkFormatString="~/Pages/Room.aspx?RoomID={RoomID}&amp;_controller=Room&amp;_commandName=Select&amp;_commandArgument=editForm1" />
        <dataField fieldName="ToUserID" aliasFieldName="ToUserUserName" hyperlinkFormatString="~/Pages/Users.aspx?UserID={ToUserID}&amp;_controller=Users&amp;_commandName=Select&amp;_commandArgument=editForm1" />
        <dataField fieldName="Text" />
        <dataField fieldName="TimeStamp" dataFormatString="t" columns="10" />
        <dataField fieldName="Color" columns="50" />
        <dataField fieldName="ModifiedBy" columns="50" />
        <dataField fieldName="ModifiedByUserId" columns="15" />
        <dataField fieldName="ModifiedOn" columns="10" />
        <dataField fieldName="CreatedBy" columns="50" />
        <dataField fieldName="CreatedById" columns="15" />
        <dataField fieldName="CreatedOn" columns="10" />
        <dataField fieldName="Usersgmsgm_Nm" />
        <dataField fieldName="UsersgmOpcoOpcoName" columns="50" />
        <dataField fieldName="UserUserTypUsrTypNm" />
        <dataField fieldName="ToUsersgmsgm_Nm" />
        <dataField fieldName="ToUsersgmOpcoOpcoName" columns="50" />
        <dataField fieldName="ToUserUserTypUsrTypNm" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Message">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Message" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="RoomID" aliasFieldName="RoomName" hyperlinkFormatString="~/Pages/Room.aspx?RoomID={RoomID}&amp;_controller=Room&amp;_commandName=Select&amp;_commandArgument=editForm1" />
            <dataField fieldName="UserID" aliasFieldName="UserUserName" hyperlinkFormatString="~/Pages/Users.aspx?UserID={UserID}&amp;_controller=Users&amp;_commandName=Select&amp;_commandArgument=editForm1" />
            <dataField fieldName="ToUserID" aliasFieldName="ToUserUserName" hyperlinkFormatString="~/Pages/Users.aspx?UserID={ToUserID}&amp;_controller=Users&amp;_commandName=Select&amp;_commandArgument=editForm1" />
            <dataField fieldName="Text" />
            <dataField fieldName="TimeStamp" dataFormatString="t" columns="10" />
            <dataField fieldName="Color" columns="50" />
            <dataField fieldName="ModifiedBy" columns="50" />
            <dataField fieldName="ModifiedByUserId" columns="15" />
            <dataField fieldName="ModifiedOn" columns="10" />
            <dataField fieldName="CreatedBy" columns="50" />
            <dataField fieldName="CreatedById" columns="15" />
            <dataField fieldName="CreatedOn" columns="10" />
          </dataFields>
        </category>
        <category id="c2" headerText="Reference Information">
          <description><![CDATA[$DefaultReferenceDescription]]></description>
          <dataFields>
            <dataField fieldName="Usersgmsgm_Nm" />
            <dataField fieldName="UsersgmOpcoOpcoName" columns="50" />
            <dataField fieldName="UserUserTypUsrTypNm" />
            <dataField fieldName="ToUsersgmsgm_Nm" />
            <dataField fieldName="ToUsersgmOpcoOpcoName" columns="50" />
            <dataField fieldName="ToUserUserTypUsrTypNm" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Message">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New Message" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="RoomID" aliasFieldName="RoomName" />
            <dataField fieldName="UserID" aliasFieldName="UserUserName" />
            <dataField fieldName="ToUserID" aliasFieldName="ToUserUserName" />
            <dataField fieldName="Text" />
            <dataField fieldName="TimeStamp" dataFormatString="t" columns="10" />
            <dataField fieldName="Color" columns="50" />
            <dataField fieldName="ModifiedBy" columns="50" />
            <dataField fieldName="ModifiedByUserId" columns="15" />
            <dataField fieldName="ModifiedOn" columns="10" />
            <dataField fieldName="CreatedBy" columns="50" />
            <dataField fieldName="CreatedById" columns="15" />
            <dataField fieldName="CreatedOn" columns="10" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
