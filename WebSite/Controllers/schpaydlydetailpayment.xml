    <command id="command1" type="Text">
      <text><![CDATA[
select
	"schpaydlydetailpayment"."schpaydlydetailpaymentid" "schpaydlydetailpaymentid"
	,"schpaydlydetailpayment"."schpaydlyhdregid" "schpaydlyhdregid"
	,"schpaydlyhdreg"."ReferanceNo" "schpaydlyhdregReferanceNo"
	,"schpaydlyhdregsgm"."sgm_Nm" "schpaydlyhdregsgmsgm_Nm"
	,"schpaydlyhdregsgmOpco"."OpcoName" "schpaydlyhdregsgmOpcoOpcoName"
	,"schpaydlyhdregIDregorg"."NAME_1" "schpaydlyhdregIDregorgNAME_1"
	,"schpaydlydetailpayment"."Pymnt_No" "Pymnt_No"
	,"Pymnt_"."Pymnt_Nm" "Pymnt_Pymnt_Nm"
	,"Pymnt_pymnt_acc"."Acc_Nm" "Pymnt_pymnt_accAcc_Nm"
	,"Pymnt_pymnt_accClsacc_"."Acc_Nm" "Pymnt_pymnt_accClsacc_Acc_Nm"
	,"Pymnt_pymnt_accAcc_Bnd"."Acc_Nm" "Pymnt_pymnt_accAcc_BndAcc_Nm"
	,"Pymnt_sgm"."sgm_Nm" "Pymnt_sgmsgm_Nm"
	,"Pymnt_sgmOpco"."OpcoName" "Pymnt_sgmOpcoOpcoName"
	,"schpaydlydetailpayment"."paymentwaynumber" "paymentwaynumber"
	,"schpaydlydetailpayment"."schBnk_No" "schBnk_No"
	,"schpaydlydetailpayment"."checkAccount" "checkAccount"
	,"schpaydlydetailpayment"."checkdate" "checkdate"
	,"schpaydlydetailpayment"."moneypaidto" "moneypaidto"
	,"schpaydlydetailpayment"."paymentnotes" "paymentnotes"
	,"schpaydlydetailpayment"."ModifiedBy" "ModifiedBy"
	,"schpaydlydetailpayment"."ModifiedOn" "ModifiedOn"
	,"schpaydlydetailpayment"."CreatedBy" "CreatedBy"
	,"schpaydlydetailpayment"."CreatedOn" "CreatedOn"
from "dbo"."schpaydlydetailpayment" "schpaydlydetailpayment"
	left join "dbo"."schpaydlyhdreg" "schpaydlyhdreg" on "schpaydlydetailpayment"."schpaydlyhdregid" = "schpaydlyhdreg"."schpaydlyhdregid"
	left join "dbo"."glmulcmp" "schpaydlyhdregsgm" on "schpaydlyhdreg"."sgm" = "schpaydlyhdregsgm"."sgmid"
	left join "dbo"."OpCo" "schpaydlyhdregsgmOpco" on "schpaydlyhdregsgm"."OpcoID" = "schpaydlyhdregsgmOpco"."OpcoIDidentity"
	left join "dbo"."REG_ORGANIZATIONS" "schpaydlyhdregIDregorg" on "schpaydlyhdreg"."IDregorg" = "schpaydlyhdregIDregorg"."IDIDregorg"
	left join "dbo"."Pymntype" "Pymnt_" on "schpaydlydetailpayment"."Pymnt_No" = "Pymnt_"."Pymnt_No"
	left join "dbo"."GLmstrfl" "Pymnt_pymnt_acc" on "Pymnt_"."pymnt_acc" = "Pymnt_pymnt_acc"."Acc_No"
	left join "dbo"."GLmfbab" "Pymnt_pymnt_accClsacc_" on "Pymnt_pymnt_acc"."Clsacc_No" = "Pymnt_pymnt_accClsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Pymnt_pymnt_accAcc_Bnd" on "Pymnt_pymnt_acc"."Acc_Bnd" = "Pymnt_pymnt_accAcc_Bnd"."Acc_Bnd"
	left join "dbo"."glmulcmp" "Pymnt_sgm" on "Pymnt_"."sgm" = "Pymnt_sgm"."sgmid"
	left join "dbo"."OpCo" "Pymnt_sgmOpco" on "Pymnt_sgm"."OpcoID" = "Pymnt_sgmOpco"."OpcoIDidentity"
]]></text>
    </command>
