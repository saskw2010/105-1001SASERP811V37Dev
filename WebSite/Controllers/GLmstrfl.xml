  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"GLmstrfl"."Acc_No" "Acc_No"
	,"GLmstrfl"."Acc_Bnd" "Acc_Bnd"
	,"TheAcc_Bnd"."Acc_Nm" "TheAcc_BndAcc_Nm"
	,"TheAcc_Bndaccclass"."accountclass" "TheAcc_Bndaccclassaccountclass"
	,"TheAcc_BndAcc_Bab"."Acc_Nm" "TheAcc_BndAcc_BabAcc_Nm"
	,"GLmstrfl"."Acc_Nm" "Acc_Nm"
	,"GLmstrfl"."Acc_Nme" "Acc_Nme"
	,"GLmstrfl"."Clsacc_No" "Clsacc_No"
	,"Clsacc_"."Acc_Nm" "Clsacc_Acc_Nm"
	,"GLmstrfl"."Acc_Ntr" "Acc_Ntr"
	,"GLmstrfl"."CostCntr_No" "CostCntr_No"
	,"glcross"."cross_Nm" "cross_Nm"
	,"GLmstrfl"."ModifiedBy" "ModifiedBy"
	,"GLmstrfl"."ModifiedOn" "ModifiedOn"
	,"GLmstrfl"."CreatedBy" "CreatedBy"
	,"GLmstrfl"."CreatedOn" "CreatedOn"
	,"GLmstrfl"."accountold" "accountold"
	
from "dbo"."GLmstrfl" "GLmstrfl"
	left join "dbo"."GLmfband" "TheAcc_Bnd" on "GLmstrfl"."Acc_Bnd" = "TheAcc_Bnd"."Acc_Bnd"
	left join "dbo"."glcross" "glcross" on "GLmstrfl"."CostCntr_No" = "glcross"."cross_NO"
	left join "dbo"."accountclasslist" "TheAcc_Bndaccclass" on "TheAcc_Bnd"."accclass" = "TheAcc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "TheAcc_BndAcc_Bab" on "TheAcc_Bnd"."Acc_Bab" = "TheAcc_BndAcc_Bab"."Acc_Bab"
	left join "dbo"."GLmfbab" "Clsacc_" on "GLmstrfl"."Clsacc_No" = "Clsacc_"."Acc_Bab"
	
]]></text>
    </command>
