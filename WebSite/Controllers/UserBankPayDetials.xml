<?xml version="1.0" encoding="utf-8"?>
<dataController name="UserBankPayDetials" conflictDetection="overwriteChanges" label="User Bank Pay Detials" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"UserBankPayDetials"."BankPayID" "BankPayID"
	,"BankPay"."ModifiedBy" "BankPayModifiedBy"
	,"BankPayBank"."BankName" "BankPayBankBankName"
	,"BankPayUser"."LoginName" "BankPayUserLoginName"
	,"UserBankPayDetials"."UserID" "UserID"
	,"UserBankPayDetials"."ChairID" "ChairID"
	,"Chair"."Location" "ChairLocation"
	,"ChairHall"."LayoutName" "ChairHallLayoutName"
	,"ChairShow"."DateName" "ChairShowDateName"
	,"ChairTime"."Time" "ChairTimeTime"
	,"ChairTimeShow"."DateName" "ChairTimeShowDateName"
	,"UserBankPayDetials"."ModifiedBy" "ModifiedBy"
	,"UserBankPayDetials"."ModifiedByUserId" "ModifiedByUserId"
	,"UserBankPayDetials"."ModifiedOn" "ModifiedOn"
	,"UserBankPayDetials"."CreatedBy" "CreatedBy"
	,"UserBankPayDetials"."CreatedById" "CreatedById"
	,"UserBankPayDetials"."CreatedOn" "CreatedOn"
from "dbo"."UserBankPayDetials" "UserBankPayDetials"
	left join "dbo"."UserBankPay" "BankPay" on "UserBankPayDetials"."BankPayID" = "BankPay"."BankPayID"
	left join "dbo"."BankData" "BankPayBank" on "BankPay"."BankID" = "BankPayBank"."BankID"
	left join "dbo"."UserData" "BankPayUser" on "BankPay"."UserID" = "BankPayUser"."UserID"
	left join "dbo"."ChairData" "Chair" on "UserBankPayDetials"."ChairID" = "Chair"."ChairID"
	left join "dbo"."HallLayout" "ChairHall" on "Chair"."HallID" = "ChairHall"."HallID"
	left join "dbo"."ShowDate" "ChairShow" on "Chair"."ShowID" = "ChairShow"."ShowID"
	left join "dbo"."ShowTime" "ChairTime" on "Chair"."TimeID" = "ChairTime"."TimeID"
	left join "dbo"."ShowDate" "ChairTimeShow" on "ChairTime"."ShowID" = "ChairTimeShow"."ShowID"
]]></text>
    </command>
  </commands>
