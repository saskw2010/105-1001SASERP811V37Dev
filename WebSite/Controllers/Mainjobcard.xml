<?xml version="1.0" encoding="utf-8"?>
<dataController name="Mainjobcard" conflictDetection="overwriteChanges" label="Mainjobcard" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Mainjobcard"."jobcardid" "jobcardid"
	,"Mainjobcard"."Work_order_no" "Work_order_no"
	,"Mainjobcard"."Jobcard_datein" "Jobcard_datein"
	,"Mainjobcard"."Jobcard_timein" "Jobcard_timein"
	,"Mainjobcard"."Jobcard_dateout" "Jobcard_dateout"
	,"Mainjobcard"."labor_no" "labor_no"
	,"labor_"."Labor_nm" "labor_Labor_nm"
	,"Mainjobcard"."Major_complains" "Major_complains"
	,"Mainjobcard"."car_no" "car_no"
	,"Mainjobcard"."tril_no" "tril_no"
	,"Mainjobcard"."ModifiedBy" "ModifiedBy"
	,"Mainjobcard"."ModifiedOn" "ModifiedOn"
	,"Mainjobcard"."CreatedBy" "CreatedBy"
	,"Mainjobcard"."CreatedOn" "CreatedOn"
from "dbo"."Mainjobcard" "Mainjobcard"
	left join "dbo"."MainLabors" "labor_" on "Mainjobcard"."labor_no" = "labor_"."Labor_no"
]]></text>
    </command>
    <command id="jobcardidIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="jobcardid" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="jobcardid" type="Int64" allowNulls="false" isPrimaryKey="true" label="Jobcardid" readOnly="true" />
    <field name="Work_order_no" type="Int64" label="Work order no" showInSummary="true" />
    <field name="Jobcard_datein" type="DateTime" label="Jobcard datein" showInSummary="true" />
    <field name="Jobcard_timein" type="Byte[]" allowQBE="false" label="Jobcard timein" readOnly="true" showInSummary="true" />
    <field name="Jobcard_dateout" type="DateTime" label="Jobcard dateout" showInSummary="true" />
    <field name="labor_no" type="Int64" label="Labor No" showInSummary="true">
      <items style="Lookup" dataController="MainLabors" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="labor_Labor_nm" type="String" readOnly="true" label="labor Labor nm" length="150" />
    <field name="Major_complains" type="String" allowQBE="false" allowSorting="false" label="Major complains" length="-1" />
    <field name="car_no" type="Int64" label="Car No" />
    <field name="tril_no" type="Int64" label="Tril No" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" />
    <field name="ModifiedOn" type="DateTime" label="Modified On" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="CreatedOn" type="DateTime" label="Created On" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Mainjobcard">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="Work_order_no" columns="15" />
        <dataField fieldName="Jobcard_datein" columns="10" />
        <dataField fieldName="Jobcard_timein" columns="15" />
        <dataField fieldName="Jobcard_dateout" columns="10" />
        <dataField fieldName="labor_no" aliasFieldName="labor_Labor_nm" hyperlinkFormatString="~/Pages/MainLabors.aspx?Labor_no={labor_no}&amp;_controller=MainLabors&amp;_commandName=Select&amp;_commandArgument=editForm1" />
        <dataField fieldName="Major_complains" rows="5" />
        <dataField fieldName="car_no" columns="15" />
        <dataField fieldName="tril_no" columns="15" />
        <dataField fieldName="ModifiedBy" columns="50" />
        <dataField fieldName="ModifiedOn" columns="10" />
        <dataField fieldName="CreatedBy" columns="50" />
        <dataField fieldName="CreatedOn" columns="10" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Mainjobcard">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Mainjobcard" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="Work_order_no" columns="15" />
            <dataField fieldName="Jobcard_datein" columns="10" />
            <dataField fieldName="Jobcard_timein" columns="15" />
            <dataField fieldName="Jobcard_dateout" columns="10" />
            <dataField fieldName="labor_no" aliasFieldName="labor_Labor_nm" hyperlinkFormatString="~/Pages/MainLabors.aspx?Labor_no={labor_no}&amp;_controller=MainLabors&amp;_commandName=Select&amp;_commandArgument=editForm1" />
            <dataField fieldName="Major_complains" rows="5" />
            <dataField fieldName="car_no" columns="15" />
            <dataField fieldName="tril_no" columns="15" />
            <dataField fieldName="ModifiedBy" columns="50" />
            <dataField fieldName="ModifiedOn" columns="10" />
            <dataField fieldName="CreatedBy" columns="50" />
            <dataField fieldName="CreatedOn" columns="10" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Mainjobcard">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New Mainjobcard" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="Work_order_no" columns="15" />
            <dataField fieldName="Jobcard_datein" columns="10" />
            <dataField fieldName="Jobcard_dateout" columns="10" />
            <dataField fieldName="labor_no" aliasFieldName="labor_Labor_nm" />
            <dataField fieldName="Major_complains" rows="5" />
            <dataField fieldName="car_no" columns="15" />
            <dataField fieldName="tril_no" columns="15" />
            <dataField fieldName="ModifiedBy" columns="50" />
            <dataField fieldName="ModifiedOn" columns="10" />
            <dataField fieldName="CreatedBy" columns="50" />
            <dataField fieldName="CreatedOn" columns="10" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
