  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"VASHitsFOP"."VASHitscode" "VASHitscode"
	,"VASHitsFOP"."Recivingdate" "Recivingdate"
	,"VASHitsFOP"."shortcode" "shortcode"
	,"short"."shortcodeDescription" "shortshortcodeDescription"
	,"shortVasCurrencey"."VasCurrenceyDescription" "shortVasCurrenceyVasCurrenceyDescription"
	,"shortVASoperator"."VASoperatorcodeDescription" "shortVASoperatorVASoperatorcodeDescription"
	,"shortVASoperatorAcc_"."Acc_Nm" "shortVASoperatorAcc_Acc_Nm"
	,"shortVASoperatorVasCurrencey"."VasCurrenceyDescription" "shortVASoperatorVasCurrenceyVasCurrenceyDescription"
	,"shortVASoperatorVASopCategory"."VASopCategoryDescription" "shortVASoperatorVASopCategoryVASopCategoryDescription"
	,"shortVASoperatorVasRegion"."VasRegiondescription" "shortVASoperatorVasRegionVasRegiondescription"
	,"shortVasRegion"."VasRegiondescription" "shortVasRegionVasRegiondescription"
	,"shorttypeofService"."typeofServiceDescription" "shorttypeofServicetypeofServiceDescription"
	,"shorttypeofServicetypeofServiceCategory"."typeofServiceCategoryDescription" "shorttypeofServicetypeofServiceCategorytypeofServiceCategoryDescription"
	,"shortSHORTCODEOWNER"."SHORTCODEOWNERName" "shortSHORTCODEOWNERSHORTCODEOWNERName"
	,"shortSHORTCODEOWNERownercategory"."SHORTCADEOWNERMain" "shortSHORTCODEOWNERownercategorySHORTCADEOWNERMain"
	,"shorttypeofshort"."typeofshortDescription" "shorttypeofshorttypeofshortDescription"
	,"shorttypeofshortIncomeAcc_"."Acc_Nm" "shorttypeofshortIncomeAcc_Acc_Nm"
	,"shorttypeofshortCostAcc_"."Acc_Nm" "shorttypeofshortCostAcc_Acc_Nm"
	,"VASHitsFOP"."OPHits" "OPHits"
	,"VASHitsFOP"."price" "price"
	,"VASHitsFOP"."AMOUNT" "AMOUNT"
	,"VASHitsFOP"."Sharepercentage" "Sharepercentage"
	,"VASHitsFOP"."Status" "Status"
	,"VASHitsFOP"."notes10011" "notes10011"
	,"VASHitsFOP"."notes10012" "notes10012"
	,"VASHitsFOP"."notes10013" "notes10013"
	,"VASHitsFOP"."VASoperatorcode" "VASoperatorcode"
	,"VASHitsFOP"."YearMonth" "YearMonth"
	,"VASHitsFOP"."branch" "branch"
	,"VASHitsFOP"."acdcode" "acdcode"
	,"VASHitsFOP"."batchapproved" "batchapproved"
from "dbo"."VASHitsFOP" "VASHitsFOP"
	left join "dbo"."VASShortcodelist" "short" on "VASHitsFOP"."shortcode" = "short"."shortcode"
	left join "dbo"."VasCurrenceylist" "shortVasCurrencey" on "short"."VasCurrenceyCode" = "shortVasCurrencey"."VasCurrenceyCode"
	left join "dbo"."VASOperrcodelist" "shortVASoperator" on "short"."VASoperatorcode" = "shortVASoperator"."VASoperatorcode"
	left join "dbo"."GLmstrfl" "shortVASoperatorAcc_" on "shortVASoperator"."Acc_no" = "shortVASoperatorAcc_"."Acc_No"
	left join "dbo"."VasCurrenceylist" "shortVASoperatorVasCurrencey" on "shortVASoperator"."VasCurrenceyCode" = "shortVASoperatorVasCurrencey"."VasCurrenceyCode"
	left join "dbo"."VASOperatorCategory" "shortVASoperatorVASopCategory" on "shortVASoperator"."VASopCategory" = "shortVASoperatorVASopCategory"."VASopCategory"
	left join "dbo"."VasRegionlist" "shortVASoperatorVasRegion" on "shortVASoperator"."VasRegioncode" = "shortVASoperatorVasRegion"."VasRegioncode"
	left join "dbo"."VasRegionlist" "shortVasRegion" on "short"."VasRegioncode" = "shortVasRegion"."VasRegioncode"
	left join "dbo"."VASServiceType" "shorttypeofService" on "short"."typeofService" = "shorttypeofService"."typeofService"
	left join "dbo"."VAStypeofServiceCategory" "shorttypeofServicetypeofServiceCategory" on "shorttypeofService"."typeofServiceCategory" = "shorttypeofServicetypeofServiceCategory"."typeofServiceCategory"
	left join "dbo"."VASSHORTCODEOWNERS" "shortSHORTCODEOWNER" on "short"."SHORTCODEOWNER" = "shortSHORTCODEOWNER"."SHORTCODEOWNER"
	left join "dbo"."VASOWNERCATLIST" "shortSHORTCODEOWNERownercategory" on "shortSHORTCODEOWNER"."ownercategory" = "shortSHORTCODEOWNERownercategory"."SHORTCADEOWNER"
	left join "dbo"."VAStypeofshortcodelist" "shorttypeofshort" on "short"."typeofshortcode" = "shorttypeofshort"."typeofshortcode"
	left join "dbo"."GLmstrfl" "shorttypeofshortIncomeAcc_" on "shorttypeofshort"."IncomeAcc_no" = "shorttypeofshortIncomeAcc_"."Acc_No"
	left join "dbo"."GLmstrfl" "shorttypeofshortCostAcc_" on "shorttypeofshort"."CostAcc_no" = "shorttypeofshortCostAcc_"."Acc_No"
]]></text>
    </command>
