<?xml version="1.0" encoding="utf-8"?>
<dataController name="Attachment" conflictDetection="overwriteChanges" label="Attachment" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Attachment"."FileId" "FileId"
	,"Attachment"."ReqId" "ReqId"
	,"Attachment"."FileName" "FileName"
	,"Attachment"."FileSize" "FileSize"
	,"Attachment"."FileType" "FileType"
	,"Attachment"."UploadedBy" "UploadedBy"
	,"Attachment"."UploadedDate" "UploadedDate"
	,"Attachment"."ModifiedBy" "ModifiedBy"
	,"Attachment"."ModifiedOn" "ModifiedOn"
	,"Attachment"."CreatedBy" "CreatedBy"
	,"Attachment"."CreatedOn" "CreatedOn"
from "dbo"."Attachment" "Attachment"
]]></text>
    </command>
  </commands>
  <fields>
    <field name="FileId" type="String" allowNulls="false" isPrimaryKey="true" label="File#" length="50" showInSummary="true" />
    <field name="ReqId" type="Int32" allowNulls="false" label="Req#" showInSummary="true" />
    <field name="FileName" type="String" allowNulls="false" label="File Name" length="510" showInSummary="true" />
    <field name="FileSize" type="Int32" allowNulls="false" label="File Size" showInSummary="true" />
    <field name="FileType" type="String" allowNulls="false" label="File Type" length="100" showInSummary="true" />
    <field name="UploadedBy" type="String" allowNulls="false" label="Uploaded By" length="200" />
    <field name="UploadedDate" type="DateTime" allowNulls="false" default="(getdate())" label="Uploaded Date" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" />
    <field name="ModifiedOn" type="DateTime" label="Modified On" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="CreatedOn" type="DateTime" label="Created On" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Attachment">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="FileId" columns="50" />
        <dataField fieldName="ReqId" columns="15" />
        <dataField fieldName="FileName" />
        <dataField fieldName="FileSize" columns="15" />
        <dataField fieldName="FileType" />
        <dataField fieldName="UploadedBy" />
        <dataField fieldName="UploadedDate" columns="10" />
        <dataField fieldName="ModifiedBy" columns="50" />
        <dataField fieldName="ModifiedOn" columns="10" />
        <dataField fieldName="CreatedBy" columns="50" />
        <dataField fieldName="CreatedOn" columns="10" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Attachment">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Attachment" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="FileId" columns="50" />
            <dataField fieldName="ReqId" columns="15" />
            <dataField fieldName="FileName" />
            <dataField fieldName="FileSize" columns="15" />
            <dataField fieldName="FileType" />
            <dataField fieldName="UploadedBy" />
            <dataField fieldName="UploadedDate" columns="10" />
            <dataField fieldName="ModifiedBy" columns="50" />
            <dataField fieldName="ModifiedOn" columns="10" />
            <dataField fieldName="CreatedBy" columns="50" />
            <dataField fieldName="CreatedOn" columns="10" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Attachment">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New Attachment" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="FileId" columns="50" />
            <dataField fieldName="ReqId" columns="15" />
            <dataField fieldName="FileName" />
            <dataField fieldName="FileSize" columns="15" />
            <dataField fieldName="FileType" />
            <dataField fieldName="UploadedBy" />
            <dataField fieldName="UploadedDate" columns="10" />
            <dataField fieldName="ModifiedBy" columns="50" />
            <dataField fieldName="ModifiedOn" columns="10" />
            <dataField fieldName="CreatedBy" columns="50" />
            <dataField fieldName="CreatedOn" columns="10" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
  <actions>
    <actionGroup id="ag1" scope="Grid">
