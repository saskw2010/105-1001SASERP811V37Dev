<?xml version="1.0" encoding="utf-8"?>
<dataController name="Tbl_Collectors" conflictDetection="overwriteChanges" label="Tbl Collectors" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Tbl_Collectors"."Collector_name" "Collector_name"
	,"Tbl_Collectors"."collectmdid" "collectmdid"
	,"collectmd"."collectmdname" "collectmdcollectmdname"
	,"Tbl_Collectors"."collectorid" "collectorid"
	,"Tbl_Collectors"."Collector_name1" "Collector_name1"
	,"Tbl_Collectors"."joindate" "joindate"
	,"Tbl_Collectors"."usercodename" "usercodename"
from "dbo"."Tbl_Collectors" "Tbl_Collectors"
	left join "dbo"."collectmdlist" "collectmd" on "Tbl_Collectors"."collectmdid" = "collectmd"."collectmdid"
]]></text>
    </command>
    <command id="collectoridIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="collectorid" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="Collector_name" type="String" allowNulls="false" label="Collector name" length="50" showInSummary="true" />
    <field name="collectmdid" type="Int64" allowNulls="false" label="Collectmdid" showInSummary="true">
      <items style="Lookup" dataController="collectmdlist" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="collectmdcollectmdname" type="String" readOnly="true" label="Collectmdcollectmdname" length="100" />
    <field name="collectorid" type="Int64" allowNulls="false" isPrimaryKey="true" label="Collectorid" readOnly="true" />
    <field name="Collector_name1" type="String" label="Collector name1" length="50" showInSummary="true" />
    <field name="joindate" type="DateTime" label="Joindate" showInSummary="true" />
    <field name="usercodename" type="String" label="Usercodename" length="50" showInSummary="true" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Tbl Collectors">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="Collector_name" columns="50" />
        <dataField fieldName="collectmdid" aliasFieldName="collectmdcollectmdname" hyperlinkFormatString="~/Pages/collectmdlist.aspx?collectmdid={collectmdid}&amp;_controller=collectmdlist&amp;_commandName=Select&amp;_commandArgument=editForm1" />
        <dataField fieldName="Collector_name1" columns="50" />
        <dataField fieldName="joindate" columns="10" />
        <dataField fieldName="usercodename" columns="50" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Tbl Collectors">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Tbl Collectors" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="Collector_name" columns="50" />
            <dataField fieldName="collectmdid" aliasFieldName="collectmdcollectmdname" hyperlinkFormatString="~/Pages/collectmdlist.aspx?collectmdid={collectmdid}&amp;_controller=collectmdlist&amp;_commandName=Select&amp;_commandArgument=editForm1" />
            <dataField fieldName="Collector_name1" columns="50" />
            <dataField fieldName="joindate" columns="10" />
            <dataField fieldName="usercodename" columns="50" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Tbl Collectors">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New Tbl Collectors" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="Collector_name" columns="50" />
            <dataField fieldName="collectmdid" aliasFieldName="collectmdcollectmdname" />
            <dataField fieldName="Collector_name1" columns="50" />
            <dataField fieldName="joindate" columns="10" />
            <dataField fieldName="usercodename" columns="50" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
  <actions>
    <actionGroup id="ag1" scope="Grid">
      <action id="a1" commandName="Select" commandArgument="editForm1" />
      <action id="a2" commandName="Edit" />
      <action id="a3" commandName="Delete" />
