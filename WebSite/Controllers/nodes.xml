select
	"nodes"."nodekey" "nodekey"
	,"nodes"."caption" "caption"
	,"nodes"."parent" "parent"
	,"nodes"."NormalIcon" "NormalIcon"
	,"nodes"."SelectedIcon" "SelectedIcon"
	,"nodes"."form_name" "form_name"
