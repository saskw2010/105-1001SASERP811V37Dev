  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"prjorderhdr"."OrderID" "OrderID"
	,"prjorderhdr"."cstm_no" "cstm_no"
	,"prjorderhdr"."issuorder_Dt" "issuorder_Dt"
	,"prjorderhdr"."project_Desc" "project_Desc"
	,"prjorderhdr"."Expcted_outdate" "Expcted_outdate"
	,"prjorderhdr"."Real_outdate" "Real_outdate"
	,"prjorderhdr"."status_code" "status_code"
	,"status_"."status_name" "status_status_name"
	,"prjorderhdr"."ModifiedBy" "ModifiedBy"
	,"prjorderhdr"."ModifiedOn" "ModifiedOn"
	,"prjorderhdr"."CreatedBy" "CreatedBy"
	,"prjorderhdr"."CreatedOn" "CreatedOn"
from "dbo"."prjorderhdr" "prjorderhdr"
	left join "dbo"."status" "status_" on "prjorderhdr"."status_code" = "status_"."status_code"
]]></text>
    </command>
    <command id="OrderIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="OrderID" />
      </output>
    </command>
  </commands>
