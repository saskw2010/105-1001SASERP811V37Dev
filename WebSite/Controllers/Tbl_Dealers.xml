<?xml version="1.0" encoding="utf-8"?>
<dataController name="Tbl_Dealers" conflictDetection="overwriteChanges" label="Tbl Dealers" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Tbl_Dealers"."DealerID" "DealerID"
	,"Tbl_Dealers"."Dealer_name" "Dealer_name"
	,"Tbl_Dealers"."MDID" "MDID"
	,"MD"."MD_Name" "MDMD_Name"
	,"Tbl_Dealers"."activeflageD" "activeflageD"
	,"Tbl_Dealers"."Dealer_name1" "Dealer_name1"
	,"Tbl_Dealers"."joindate" "joindate"
	,"Tbl_Dealers"."usercodename" "usercodename"
    ,"Tbl_Dealers"."branch" "branch"	
from "dbo"."Tbl_Dealers" "Tbl_Dealers"
	left join "dbo"."Tbl_MD" "MD" on "Tbl_Dealers"."MDID" = "MD"."MDID"
]]></text>
    </command>
    <command id="DealerIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="DealerID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="DealerID" type="Int64" allowNulls="false" isPrimaryKey="true" label="Dealer#" readOnly="true" />
    <field name="Dealer_name" type="String" allowNulls="false" label="Dealer name" length="50" showInSummary="true" />
    <field name="MDID" type="Int64" label="Mdid" showInSummary="true">
      <items style="Lookup" dataController="Tbl_MD" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="MDMD_Name" type="String" readOnly="true" label="MDMD Name" length="50" />
    <field name="activeflageD" type="Boolean" allowNulls="false" default="((1))" label="activeflage D" showInSummary="true" />
    <field name="Dealer_name1" type="String" label="Dealer name1" length="50" showInSummary="true" />
    <field name="joindate" type="DateTime" label="Joindate" showInSummary="true" />
    <field name="usercodename" type="String" label="Usercodename" length="50" />
    <field name="branch" type="Int64" label="Branch" showInSummary="true">
      <items style="DropDownList" dataController="schBranch" newDataView="createForm1" searchOnStart="true" />
    </field>
	
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Tbl Dealers">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
	  <dataField fieldName="DealerID"	  columns="50" />
        <dataField fieldName="Dealer_name" columns="50" />
        <dataField fieldName="MDID" aliasFieldName="MDMD_Name" hyperlinkFormatString="~/Pages/Tbl_MD.aspx?MDID={MDID}&amp;_controller=Tbl_MD&amp;_commandName=Select&amp;_commandArgument=editForm1" />
        <dataField fieldName="activeflageD" />
        <dataField fieldName="Dealer_name1" columns="50" />
        <dataField fieldName="joindate" columns="10" />
        <dataField fieldName="usercodename" columns="50" />
	    <dataField fieldName="branch" aliasFieldName="ThebranchDesc1"  />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Tbl Dealers">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Tbl Dealers" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
		   <dataField fieldName="DealerID"	  columns="50" />
            <dataField fieldName="Dealer_name" columns="50" />
            <dataField fieldName="MDID" aliasFieldName="MDMD_Name" hyperlinkFormatString="~/Pages/Tbl_MD.aspx?MDID={MDID}&amp;_controller=Tbl_MD&amp;_commandName=Select&amp;_commandArgument=editForm1" />
            <dataField fieldName="activeflageD" />
            <dataField fieldName="Dealer_name1" columns="50" />
            <dataField fieldName="joindate" columns="10" />
            <dataField fieldName="usercodename" columns="50" />
		  <dataField fieldName="branch" aliasFieldName="ThebranchDesc1"  />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Tbl Dealers">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New Tbl Dealers" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="Dealer_name" columns="50" />
            <dataField fieldName="MDID" aliasFieldName="MDMD_Name" />
            <dataField fieldName="activeflageD" />
			 <dataField fieldName="branch" aliasFieldName="ThebranchDesc1"  />
            <dataField fieldName="Dealer_name1" columns="50" />
            <dataField fieldName="joindate" columns="10" />
            <dataField fieldName="usercodename" columns="50" />
			
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
