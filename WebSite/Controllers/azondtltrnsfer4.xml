<rule id="r108j" type="JavaScript" commandName="Insert|Update" phase="Before" name="ITEMOUT_Validator"><![CDATA[
var fieldValue = [ITEMOUT];
if (fieldValue == null) {
  
       fieldValue =0 ;
   
  
}
var fieldValue1 = [balance];
if (fieldValue1 == null) {
  
       fieldValue1 =0 ;
}
if (fieldValue > fieldValue1 ) {
    // prevent the default action processing
    this.preventDefault();
    // set the focus on the field and display an error
    this.result.focus('ITEMOUT', 'Required less than or equal  balance');
}]]></rule>


