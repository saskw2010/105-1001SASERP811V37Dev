	left join "dbo"."softwareprojecttype" "Tasksprsprtype" on "Taskspr"."sprtype" = "Tasksprsprtype"."sprtypeid"
	left join "dbo"."softwareprojectsteam" "Tasksoftwareprojectt" on "Task"."softwareprojecttid" = "Tasksoftwareprojectt"."softwareprojecttid"
	left join "dbo"."sprjobs" "Tasksoftwareprojecttsprjob" on "Tasksoftwareprojectt"."sprjob" = "Tasksoftwareprojecttsprjob"."sprjobid"
	left join "dbo"."softwareprojectsteam" "TaskTakleef" on "Task"."TakleefID" = "TaskTakleef"."softwareprojecttid"
