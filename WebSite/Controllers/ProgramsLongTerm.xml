	left join "dbo"."Val_Per" "MaintenanceEmergency_VAL" on "ProgramsLongTerm"."MaintenanceEmergency_VAL" = "MaintenanceEmergency_VAL"."Code"
	left join "dbo"."Val_Per" "MaintenanceExpenses_VAL" on "ProgramsLongTerm"."MaintenanceExpenses_VAL" = "MaintenanceExpenses_VAL"."Code"
	left join "dbo"."Val_Per" "TrafficCharges_VAL" on "ProgramsLongTerm"."TrafficCharges_VAL" = "TrafficCharges_VAL"."Code"
	left join "dbo"."Val_Per" "Commissions_VAL" on "ProgramsLongTerm"."Commissions_VAL" = "Commissions_VAL"."Code"
