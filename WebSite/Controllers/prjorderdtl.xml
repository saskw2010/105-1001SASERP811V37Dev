  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"prjorderdtl"."dtlOrderID" "dtlOrderID"
	,"prjorderdtl"."OrderID" "OrderID"
	,"Order"."project_Desc" "Orderproject_Desc"
	,"Orderstatus_"."status_name" "Orderstatus_status_name"
	,"prjorderdtl"."modID" "modID"
	,"mod"."MODAr" "modMODAr"
	,"prjorderdtl"."GroupID" "GroupID"
	,"Group"."GroupAr" "GroupGroupAr"
	,"GroupMOD"."MODAr" "GroupMODMODAr"
	,"prjorderdtl"."ModifiedBy" "ModifiedBy"
	,"prjorderdtl"."ModifiedOn" "ModifiedOn"
	,"prjorderdtl"."CreatedBy" "CreatedBy"
	,"prjorderdtl"."CreatedOn" "CreatedOn"
from "dbo"."prjorderdtl" "prjorderdtl"
	left join "dbo"."prjorderhdr" "Order" on "prjorderdtl"."OrderID" = "Order"."OrderID"
	left join "dbo"."status" "Orderstatus_" on "Order"."status_code" = "Orderstatus_"."status_code"
	left join "dbo"."MODMaster" "mod" on "prjorderdtl"."modID" = "mod"."MODID"
	left join "dbo"."GroupMaster" "Group" on "prjorderdtl"."GroupID" = "Group"."GroupID"
	left join "dbo"."MODMaster" "GroupMOD" on "Group"."MODID" = "GroupMOD"."MODID"
]]></text>
    </command>
    <command id="dtlOrderIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="dtlOrderID" />
      </output>
    </command>
  </commands>
