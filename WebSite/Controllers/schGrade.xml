            <dataField fieldName="Desc2" columns="50" />
            <dataField fieldName="StageCode" aliasFieldName="StageShortDesc1" hyperlinkFormatString="~/Pages/schStage.aspx?Code={StageCode}&amp;_controller=schStage&amp;_commandName=Select&amp;_commandArgument=editForm1" />
            <dataField fieldName="acdcode" aliasFieldName="acdAcademicYear" />
            <dataField fieldName="agecalculatedate" columns="10" />
