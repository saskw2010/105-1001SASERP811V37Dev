            <dataField fieldName="CreatedBy" columns="50" />
            <dataField fieldName="CreatedOn" columns="10" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New sch Grade">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New sch Grade" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="ShortDesc1" columns="50" />
            <dataField fieldName="ShortDesc2" columns="50" />
            <dataField fieldName="Desc1" columns="50" />
            <dataField fieldName="Desc2" columns="50" />
            <dataField fieldName="StageCode" aliasFieldName="StageShortDesc1" />
            <dataField fieldName="acdcode" aliasFieldName="acdAcademicYear" />
            <dataField fieldName="agecalculatedate" columns="10" />
            <dataField fieldName="ageexpectedbyyear" columns="15" />
            <dataField fieldName="ageexpectedbymonth" columns="15" />
            <dataField fieldName="ageexpectedbyyear1" columns="15" />
            <dataField fieldName="ageexpectedbymonth1" columns="15" />
            <dataField fieldName="ModifiedBy" columns="50" />
            <dataField fieldName="ModifiedOn" columns="10" />
            <dataField fieldName="CreatedBy" columns="50" />
            <dataField fieldName="CreatedOn" columns="10" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
