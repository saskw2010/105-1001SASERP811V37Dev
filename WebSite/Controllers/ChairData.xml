<?xml version="1.0" encoding="utf-8"?>
<dataController name="ChairData" conflictDetection="overwriteChanges" label="Chair Data" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"ChairData"."ChairID" "ChairID"
	,"ChairData"."HallID" "HallID"
	,"Hall"."LayoutName" "HallLayoutName"
	,"ChairData"."MovieID" "MovieID"
	,"ChairData"."ShowID" "ShowID"
	,"Show"."DateName" "ShowDateName"
	,"ChairData"."TimeID" "TimeID"
	,"Time"."Time" "TimeTime"
	,"TimeShow"."DateName" "TimeShowDateName"
	,"ChairData"."UserID" "UserID"
	,"ChairData"."AdminID" "AdminID"
	,"ChairData"."BookID" "BookID"
	,"ChairData"."Location" "Location"
	,"ChairData"."Book" "Book"
	,"ChairData"."Status" "Status"
	,"ChairData"."Rows" "Rows"
	,"ChairData"."Cols" "Cols"
	,"ChairData"."ModifiedBy" "ModifiedBy"
	,"ChairData"."ModifiedByUserId" "ModifiedByUserId"
	,"ChairData"."ModifiedOn" "ModifiedOn"
	,"ChairData"."CreatedBy" "CreatedBy"
	,"ChairData"."CreatedById" "CreatedById"
	,"ChairData"."CreatedOn" "CreatedOn"
from "dbo"."ChairData" "ChairData"
	left join "dbo"."HallLayout" "Hall" on "ChairData"."HallID" = "Hall"."HallID"
	left join "dbo"."ShowDate" "Show" on "ChairData"."ShowID" = "Show"."ShowID"
	left join "dbo"."ShowTime" "Time" on "ChairData"."TimeID" = "Time"."TimeID"
	left join "dbo"."ShowDate" "TimeShow" on "Time"."ShowID" = "TimeShow"."ShowID"
]]></text>
    </command>
    <command id="ChairIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="ChairID" />
      </output>
    </command>
  </commands>
  <fields>
