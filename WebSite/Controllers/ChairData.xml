    <field name="HallID" type="Int32" allowNulls="false" label="Hall#" showInSummary="true">
      <items style="Lookup" dataController="HallLayout" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="HallLayoutName" type="String" readOnly="true" label="Hall Layout Name" length="50" />
