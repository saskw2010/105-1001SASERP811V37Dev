  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Tbl_28"."Autoid" "Autoid"
	,"Tbl_28"."Rainbow_no" "Rainbow_no"
	,"Tbl_28"."Item_Code" "Item_Code"
	,"Item_"."Arab_Desc" "Item_Arab_Desc"
	,"Tbl_28"."QTY" "QTY"
	,"Tbl_28"."Sale_Price" "Sale_Price"
	,"Tbl_28"."Invoice" "Invoice"
	,"Tbl_28"."Delivery_Dealer" "Delivery_Dealer"
	,"Tbl_28"."Kind_mashin" "Kind_mashin"
	,"Tbl_28"."Kind_Serves" "Kind_Serves"
	,"Tbl_28"."In_date" "In_date"
	,"Tbl_28"."Exit_date" "Exit_date"
	,"Tbl_28"."ModifiedBy" "ModifiedBy"
	,"Tbl_28"."ModifiedOn" "ModifiedOn"
	,"Tbl_28"."CreatedBy" "CreatedBy"
	,"Tbl_28"."CreatedOn" "CreatedOn"
,"Tbl_28"."Notes" "Notes"
from "dbo"."Tbl_28" "Tbl_28"
	left join "dbo"."Tbl_29" "Item_" on "Tbl_28"."Item_Code" = "Item_"."Item_Code"
]]></text>
    </command>
    <command id="AutoidIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Autoid" />
      </output>
    </command>
  </commands>
