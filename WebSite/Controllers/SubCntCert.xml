	,"SubCntCert"."Exec_Prs" "Exec_Prs"
	,"SubCntCert"."OutPut" "OutPut"
	,"SubCntCert"."Discou" "Discou"
	,"SubCntCert"."Pure" "Pure"
	,"SubCntCert"."Exec_Date" "Exec_Date"
	,"SubCntCert"."Doc_no" "Doc_no"
	,"SubCntCert"."ModifiedBy" "ModifiedBy"
	,"SubCntCert"."ModifiedOn" "ModifiedOn"
	,"SubCntCert"."CreatedBy" "CreatedBy"
	,"SubCntCert"."CreatedOn" "CreatedOn"
from "dbo"."SubCntCert" "SubCntCert"
	left join "dbo"."SubContrctr" "subContor_" on "SubCntCert"."subContor_No" = "subContor_"."Sub_No"
]]></text>
    </command>
