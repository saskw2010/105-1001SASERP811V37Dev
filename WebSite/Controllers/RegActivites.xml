  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"RegActivites"."Activitesid" "Activitesid"
	,"RegActivites"."Activitesparentid" "Activitesparentid"
	,"Activitesparent"."ActivitesDescrption" "ActivitesparentActivitesDescrption"
	,"Activitesparentschpaymenttype"."schpaymenttypenm" "Activitesparentschpaymenttypeschpaymenttypenm"
	,"Activitesparentschpaymenttypedebitaccount"."Acc_Nm" "ActivitesparentschpaymenttypedebitaccountAcc_Nm"
	,"Activitesparentschpaymenttypecreditaccount"."Acc_Nm" "ActivitesparentschpaymenttypecreditaccountAcc_Nm"
	,"RegActivites"."ActivitesDescrption" "ActivitesDescrption"
	,"RegActivites"."schpaymenttypeid" "schpaymenttypeid"
	,"schpaymenttype"."schpaymenttypenm" "schpaymenttypeschpaymenttypenm"
	,"schpaymenttypedebitaccount"."Acc_Nm" "schpaymenttypedebitaccountAcc_Nm"
	,"schpaymenttypedebitaccountClsacc_"."Acc_Nm" "schpaymenttypedebitaccountClsacc_Acc_Nm"
	,"schpaymenttypedebitaccountAcc_Bnd"."Acc_Nm" "schpaymenttypedebitaccountAcc_BndAcc_Nm"
	,"schpaymenttypecreditaccount"."Acc_Nm" "schpaymenttypecreditaccountAcc_Nm"
	,"schpaymenttypecreditaccountClsacc_"."Acc_Nm" "schpaymenttypecreditaccountClsacc_Acc_Nm"
	,"schpaymenttypecreditaccountAcc_Bnd"."Acc_Nm" "schpaymenttypecreditaccountAcc_BndAcc_Nm"
from "dbo"."RegActivites" "RegActivites"
	left join "dbo"."RegActivites" "Activitesparent" on "RegActivites"."Activitesparentid" = "Activitesparent"."Activitesid"
	left join "dbo"."schpaymenttype" "Activitesparentschpaymenttype" on "Activitesparent"."schpaymenttypeid" = "Activitesparentschpaymenttype"."schpaymenttypeid"
	left join "dbo"."GLmstrfl" "Activitesparentschpaymenttypedebitaccount" on "Activitesparentschpaymenttype"."debitaccount" = "Activitesparentschpaymenttypedebitaccount"."Acc_No"
	left join "dbo"."GLmstrfl" "Activitesparentschpaymenttypecreditaccount" on "Activitesparentschpaymenttype"."creditaccount" = "Activitesparentschpaymenttypecreditaccount"."Acc_No"
	left join "dbo"."schpaymenttype" "schpaymenttype" on "RegActivites"."schpaymenttypeid" = "schpaymenttype"."schpaymenttypeid"
	left join "dbo"."GLmstrfl" "schpaymenttypedebitaccount" on "schpaymenttype"."debitaccount" = "schpaymenttypedebitaccount"."Acc_No"
	left join "dbo"."GLmfbab" "schpaymenttypedebitaccountClsacc_" on "schpaymenttypedebitaccount"."Clsacc_No" = "schpaymenttypedebitaccountClsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "schpaymenttypedebitaccountAcc_Bnd" on "schpaymenttypedebitaccount"."Acc_Bnd" = "schpaymenttypedebitaccountAcc_Bnd"."Acc_Bnd"
	left join "dbo"."GLmstrfl" "schpaymenttypecreditaccount" on "schpaymenttype"."creditaccount" = "schpaymenttypecreditaccount"."Acc_No"
	left join "dbo"."GLmfbab" "schpaymenttypecreditaccountClsacc_" on "schpaymenttypecreditaccount"."Clsacc_No" = "schpaymenttypecreditaccountClsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "schpaymenttypecreditaccountAcc_Bnd" on "schpaymenttypecreditaccount"."Acc_Bnd" = "schpaymenttypecreditaccountAcc_Bnd"."Acc_Bnd"
]]></text>
    </command>
