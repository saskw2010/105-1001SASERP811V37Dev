  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"VASServiceType"."typeofService" "typeofService"
	,"VASServiceType"."typeofServiceDescription" "typeofServiceDescription"
	,"VASServiceType"."typeofServiceDescriptionen" "typeofServiceDescriptionen"
	,"VASServiceType"."typeofServiceCategory" "typeofServiceCategory"
	,"ThetypeofServiceCategory"."typeofServiceCategoryDescription" "ThetypeofServiceCategorytypeofServiceCategoryDescription"
from "dbo"."VASServiceType" "VASServiceType"
	left join "dbo"."VAStypeofServiceCategory" "ThetypeofServiceCategory" on "VASServiceType"."typeofServiceCategory" = "ThetypeofServiceCategory"."typeofServiceCategory"
]]></text>
    </command>
    <command id="typeofServiceIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="typeofService" />
      </output>
    </command>
  </commands>
