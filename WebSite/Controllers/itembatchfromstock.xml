  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"itembatchfromstock"."Batchidstock" "Batchidstock"
	,"itembatchfromstock"."Barcodubatch" "Barcodubatch"
	,"itembatchfromstock"."descitem" "descitem"
	,"itembatchfromstock"."unit_no" "unit_no"
	,"itembatchfromstock"."Brn_No" "Brn_No"
	,itembatchfromstock.ModifiedBy
	,itembatchfromstock.ModifiedOn
	,itembatchfromstock.CreatedBy
	,itembatchfromstock.CreatedOn
	,itembatchfromstock.expirdate
	,"Brn_"."Brn_Nm" "Brn_Brn_Nm"
	,"Brn_branch"."Desc1" "Brn_branchDesc1"
	,"Brn_branchsgm"."sgm_Nm" "Brn_branchsgmsgm_Nm"
	,"Brn_branchGender"."Gender" "Brn_branchGenderGender"
	,"Brn_branchstage"."ShortDesc1" "Brn_branchstageShortDesc1"
	,"Brn_branchschtyp"."schtypDesc" "Brn_branchschtypschtypDesc"
	,"itembatchfromstock"."countrdus" "countrdus"
	,"Thecountrdus"."ardtypnm" "Thecountrdusardtypnm"
	,"ThecountrdusBrn_"."Brn_Nm" "ThecountrdusBrn_Brn_Nm"
	,"ThecountrdusBrn_branch"."Desc1" "ThecountrdusBrn_branchDesc1"
	,"ThecountrdusTRFBrn_"."Brn_Nm" "ThecountrdusTRFBrn_Brn_Nm"
	,"ThecountrdusTRFBrn_branch"."Desc1" "ThecountrdusTRFBrn_branchDesc1"
	,"Thecountrdusdoc_typ"."doc_name" "Thecountrdusdoc_typdoc_name"
	,"Thecountrdustypcmsup"."SubledgerDescrption" "ThecountrdustypcmsupSubledgerDescrption"
	,"ThecountrdusCstm_"."Cstm_Nm" "ThecountrdusCstm_Cstm_Nm"
	,"ThecountrdusCstm_Area"."Area_name" "ThecountrdusCstm_AreaArea_name"
	,"ThecountrdusCstm_Evaluation"."Evaluation_name" "ThecountrdusCstm_EvaluationEvaluation_name"
	,"ThecountrdusCstm_acc_"."Acc_Nm" "ThecountrdusCstm_acc_Acc_Nm"
	,"ThecountrdusCstm_Cntry_"."Cntry_Nm" "ThecountrdusCstm_Cntry_Cntry_Nm"
	,"ThecountrdusCstm_curncy_"."Curncy_Nm" "ThecountrdusCstm_curncy_Curncy_Nm"
	,"ThecountrdusCstm_TSNEF"."TSNEF_name" "ThecountrdusCstm_TSNEFTSNEF_name"
	,"ThecountrdusCstm_TSNEFNO1"."TSNEF1_name" "ThecountrdusCstm_TSNEFNO1TSNEF1_name"
	,"ThecountrdusCstm_Slm_"."Slm_Nm" "ThecountrdusCstm_Slm_Slm_Nm"
	,"ThecountrdusSup_"."Sup_Nm" "ThecountrdusSup_Sup_Nm"
	,"ThecountrdusSup_Acc_"."Acc_Nm" "ThecountrdusSup_Acc_Acc_Nm"
	,"ThecountrdusSup_sgm"."sgm_Nm" "ThecountrdusSup_sgmsgm_Nm"
	,"ThecountrdusSup_Cntry_"."Cntry_Nm" "ThecountrdusSup_Cntry_Cntry_Nm"
	,"ThecountrdusSup_Curncy_"."Curncy_Nm" "ThecountrdusSup_Curncy_Curncy_Nm"
	,"itembatchfromstock"."QTY" "QTY"
from "dbo"."itembatchfromstock" "itembatchfromstock"
	left join "dbo"."Branchcnst" "Brn_" on "itembatchfromstock"."Brn_No" = "Brn_"."Brn_No"
	left join "dbo"."schBranch" "Brn_branch" on "Brn_"."branch" = "Brn_branch"."branch"
	left join "dbo"."glmulcmp" "Brn_branchsgm" on "Brn_branch"."sgm" = "Brn_branchsgm"."sgmid"
	left join "dbo"."PyGender" "Brn_branchGender" on "Brn_branch"."Genderid" = "Brn_branchGender"."Genderid"
	left join "dbo"."schStage" "Brn_branchstage" on "Brn_branch"."stagecode" = "Brn_branchstage"."Code"
	left join "dbo"."schtyp" "Brn_branchschtyp" on "Brn_branch"."schtypid" = "Brn_branchschtyp"."schtypid"
	left join "dbo"."azonhdr1" "Thecountrdus" on "itembatchfromstock"."countrdus" = "Thecountrdus"."countrdus"
	left join "dbo"."Branchcnst" "ThecountrdusBrn_" on "Thecountrdus"."Brn_No" = "ThecountrdusBrn_"."Brn_No"
	left join "dbo"."schBranch" "ThecountrdusBrn_branch" on "ThecountrdusBrn_"."branch" = "ThecountrdusBrn_branch"."branch"
	left join "dbo"."Branchcnst" "ThecountrdusTRFBrn_" on "Thecountrdus"."TRFBrn_NO" = "ThecountrdusTRFBrn_"."Brn_No"
	left join "dbo"."schBranch" "ThecountrdusTRFBrn_branch" on "ThecountrdusTRFBrn_"."branch" = "ThecountrdusTRFBrn_branch"."branch"
	left join "dbo"."docsand" "Thecountrdusdoc_typ" on "Thecountrdus"."doc_typ" = "Thecountrdusdoc_typ"."doc_typ"
	left join "dbo"."GLSubledgertyplist" "Thecountrdustypcmsup" on "Thecountrdus"."typcmsup" = "Thecountrdustypcmsup"."Subledgerid"
	left join "dbo"."Stcustmr" "ThecountrdusCstm_" on "Thecountrdus"."Cstm_No" = "ThecountrdusCstm_"."Cstm_No"
	left join "dbo"."AreaList" "ThecountrdusCstm_Area" on "ThecountrdusCstm_"."AreaNo" = "ThecountrdusCstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "ThecountrdusCstm_Evaluation" on "ThecountrdusCstm_"."EvaluationNO" = "ThecountrdusCstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "ThecountrdusCstm_acc_" on "ThecountrdusCstm_"."acc_no" = "ThecountrdusCstm_acc_"."Acc_No"
	left join "dbo"."PYCntry" "ThecountrdusCstm_Cntry_" on "ThecountrdusCstm_"."Cntry_No" = "ThecountrdusCstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "ThecountrdusCstm_curncy_" on "ThecountrdusCstm_"."curncy_no" = "ThecountrdusCstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "ThecountrdusCstm_TSNEF" on "ThecountrdusCstm_"."TSNEFNO" = "ThecountrdusCstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "ThecountrdusCstm_TSNEFNO1" on "ThecountrdusCstm_"."TSNEFNO1" = "ThecountrdusCstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "ThecountrdusCstm_Slm_" on "ThecountrdusCstm_"."Slm_No" = "ThecountrdusCstm_Slm_"."Slm_No"
	left join "dbo"."Stsuplir" "ThecountrdusSup_" on "Thecountrdus"."Sup_No" = "ThecountrdusSup_"."Sup_No"
	left join "dbo"."GLmstrfl" "ThecountrdusSup_Acc_" on "ThecountrdusSup_"."Acc_No" = "ThecountrdusSup_Acc_"."Acc_No"
	left join "dbo"."glmulcmp" "ThecountrdusSup_sgm" on "ThecountrdusSup_"."sgm" = "ThecountrdusSup_sgm"."sgmid"
	left join "dbo"."PYCntry" "ThecountrdusSup_Cntry_" on "ThecountrdusSup_"."Cntry_No" = "ThecountrdusSup_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "ThecountrdusSup_Curncy_" on "ThecountrdusSup_"."Curncy_No" = "ThecountrdusSup_Curncy_"."Curncy_No"
]]></text>
    </command>
