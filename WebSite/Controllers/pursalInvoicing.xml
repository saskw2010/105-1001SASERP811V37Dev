  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"pursalInvoicing"."pursalInvoicingid" "pursalInvoicingid"
	,"pursalInvoicing"."pursalInvoicingdate" "pursalInvoicingdate"
	,"pursalInvoicing"."sgm" "sgm"
	,"sgm"."sgm_Nm" "sgmsgm_Nm"
	,"sgmOpco"."OpcoName" "sgmOpcoOpcoName"
	,"pursalInvoicing"."branch" "branch"
	,"Thebranch"."Desc1" "ThebranchDesc1"
	,"Thebranchsgm"."sgm_Nm" "Thebranchsgmsgm_Nm"
	,"ThebranchsgmOpco"."OpcoName" "ThebranchsgmOpcoOpcoName"
	,"ThebranchGender"."Gender" "ThebranchGenderGender"
	,"Thebranchstage"."ShortDesc1" "ThebranchstageShortDesc1"
	,"Thebranchschtyp"."schtypDesc" "ThebranchschtypschtypDesc"
	,"pursalInvoicing"."pursalInvoicingacc_no" "pursalInvoicingacc_no"
	,"pursalInvoicingacc_"."Acc_Nm" "pursalInvoicingacc_Acc_Nm"
	,"pursalInvoicingacc_Clsacc_"."Acc_Nm" "pursalInvoicingacc_Clsacc_Acc_Nm"
	,"pursalInvoicingacc_Acc_Bnd"."Acc_Nm" "pursalInvoicingacc_Acc_BndAcc_Nm"
	,"pursalInvoicingacc_Acc_Bndaccclass"."accountclass" "pursalInvoicingacc_Acc_Bndaccclassaccountclass"
	,"pursalInvoicingacc_Acc_BndAcc_Bab"."Acc_Nm" "pursalInvoicingacc_Acc_BndAcc_BabAcc_Nm"
	,"pursalInvoicing"."pursalInvoicingTypNO" "pursalInvoicingTypNO"
	,"pursalInvoicing"."pursalInvoicingCSUPNO" "pursalInvoicingCSUPNO"
	,"pursalInvoicing"."pursalInvoicingNotes" "pursalInvoicingNotes"
	,"pursalInvoicing"."pursalInvoicingacc_no1" "pursalInvoicingacc_no1"
	,"pursalInvoicingacc_no1"."Acc_Nm" "pursalInvoicingacc_no1Acc_Nm"
	,"pursalInvoicingacc_no1Clsacc_"."Acc_Nm" "pursalInvoicingacc_no1Clsacc_Acc_Nm"
	,"pursalInvoicingacc_no1Acc_Bnd"."Acc_Nm" "pursalInvoicingacc_no1Acc_BndAcc_Nm"
	,"pursalInvoicingacc_no1Acc_Bndaccclass"."accountclass" "pursalInvoicingacc_no1Acc_Bndaccclassaccountclass"
	,"pursalInvoicingacc_no1Acc_BndAcc_Bab"."Acc_Nm" "pursalInvoicingacc_no1Acc_BndAcc_BabAcc_Nm"
	,"pursalInvoicing"."pursalInvoicingTypNO1" "pursalInvoicingTypNO1"
	,"pursalInvoicing"."pursalInvoicingCSUPNO1" "pursalInvoicingCSUPNO1"
from "dbo"."pursalInvoicing" "pursalInvoicing"
	left join "dbo"."glmulcmp" "sgm" on "pursalInvoicing"."sgm" = "sgm"."sgmid"
	left join "dbo"."OpCo" "sgmOpco" on "sgm"."OpcoID" = "sgmOpco"."OpcoIDidentity"
	left join "dbo"."schBranch" "Thebranch" on "pursalInvoicing"."branch" = "Thebranch"."branch"
	left join "dbo"."glmulcmp" "Thebranchsgm" on "Thebranch"."sgm" = "Thebranchsgm"."sgmid"
	left join "dbo"."OpCo" "ThebranchsgmOpco" on "Thebranchsgm"."OpcoID" = "ThebranchsgmOpco"."OpcoIDidentity"
	left join "dbo"."PyGender" "ThebranchGender" on "Thebranch"."Genderid" = "ThebranchGender"."Genderid"
	left join "dbo"."schStage" "Thebranchstage" on "Thebranch"."stagecode" = "Thebranchstage"."Code"
	left join "dbo"."schtyp" "Thebranchschtyp" on "Thebranch"."schtypid" = "Thebranchschtyp"."schtypid"
	left join "dbo"."GLmstrfl" "pursalInvoicingacc_" on "pursalInvoicing"."pursalInvoicingacc_no" = "pursalInvoicingacc_"."Acc_No"
	left join "dbo"."GLmfbab" "pursalInvoicingacc_Clsacc_" on "pursalInvoicingacc_"."Clsacc_No" = "pursalInvoicingacc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "pursalInvoicingacc_Acc_Bnd" on "pursalInvoicingacc_"."Acc_Bnd" = "pursalInvoicingacc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "pursalInvoicingacc_Acc_Bndaccclass" on "pursalInvoicingacc_Acc_Bnd"."accclass" = "pursalInvoicingacc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "pursalInvoicingacc_Acc_BndAcc_Bab" on "pursalInvoicingacc_Acc_Bnd"."Acc_Bab" = "pursalInvoicingacc_Acc_BndAcc_Bab"."Acc_Bab"
	left join "dbo"."GLmstrfl" "pursalInvoicingacc_no1" on "pursalInvoicing"."pursalInvoicingacc_no1" = "pursalInvoicingacc_no1"."Acc_No"
	left join "dbo"."GLmfbab" "pursalInvoicingacc_no1Clsacc_" on "pursalInvoicingacc_no1"."Clsacc_No" = "pursalInvoicingacc_no1Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "pursalInvoicingacc_no1Acc_Bnd" on "pursalInvoicingacc_no1"."Acc_Bnd" = "pursalInvoicingacc_no1Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "pursalInvoicingacc_no1Acc_Bndaccclass" on "pursalInvoicingacc_no1Acc_Bnd"."accclass" = "pursalInvoicingacc_no1Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "pursalInvoicingacc_no1Acc_BndAcc_Bab" on "pursalInvoicingacc_no1Acc_Bnd"."Acc_Bab" = "pursalInvoicingacc_no1Acc_BndAcc_Bab"."Acc_Bab"
]]></text>
    </command>
