    <command id="command1" type="Text">
      <text><![CDATA[
select
	"usertypRolsjoin"."UserID" "UserID"
	,"User"."UserName" "UserUserName"
	,"Usersgm"."sgm_Nm" "Usersgmsgm_Nm"
	,"UsersgmOpco"."OpcoName" "UsersgmOpcoOpcoName"
	,"UserUserTyp"."UsrTypNm" "UserUserTypUsrTypNm"
	,"usertypRolsjoin"."UsrTypID" "UsrTypID"
	,"UsrTyp"."UsrTypNm" "UsrTypUsrTypNm"
	,"usertypRolsjoin"."ModifiedBy" "ModifiedBy"
	,"usertypRolsjoin"."ModifiedOn" "ModifiedOn"
	,"usertypRolsjoin"."CreatedBy" "CreatedBy"
	,"usertypRolsjoin"."CreatedOn" "CreatedOn"
from "dbo"."usertypRolsjoin" "usertypRolsjoin"
	left join "dbo"."Users" "User" on "usertypRolsjoin"."UserID" = "User"."UserID"
	left join "dbo"."glmulcmp" "Usersgm" on "User"."sgm" = "Usersgm"."sgmid"
	left join "dbo"."OpCo" "UsersgmOpco" on "Usersgm"."OpcoID" = "UsersgmOpco"."OpcoIDidentity"
	left join "dbo"."UsersTypes" "UserUserTyp" on "User"."UserTyp" = "UserUserTyp"."UsrTypID"
	left join "dbo"."UsersTypes" "UsrTyp" on "usertypRolsjoin"."UsrTypID" = "UsrTyp"."UsrTypID"
]]></text>
