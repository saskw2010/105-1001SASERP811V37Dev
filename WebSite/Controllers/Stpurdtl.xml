	left join "dbo"."PyGender" "Doc_branchGender" on "Doc_branch"."Genderid" = "Doc_branchGender"."Genderid"
	left join "dbo"."schStage" "Doc_branchstage" on "Doc_branch"."stagecode" = "Doc_branchstage"."Code"
	left join "dbo"."schtyp" "Doc_branchschtyp" on "Doc_branch"."schtypid" = "Doc_branchschtyp"."schtypid"
	left join "dbo"."Stcurncy" "Doc_cur" on "Doc_"."curcode" = "Doc_cur"."Curncy_No"
	left join "dbo"."Stsuplir" "Doc_Cstm_" on "Doc_"."Cstm_No" = "Doc_Cstm_"."Sup_No"
	left join "dbo"."GLmstrfl" "Doc_Cstm_Acc_" on "Doc_Cstm_"."Acc_No" = "Doc_Cstm_Acc_"."Acc_No"
	left join "dbo"."PYCntry" "Doc_Cstm_Cntry_" on "Doc_Cstm_"."Cntry_No" = "Doc_Cstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Doc_Cstm_Curncy_" on "Doc_Cstm_"."Curncy_No" = "Doc_Cstm_Curncy_"."Curncy_No"
	left join "dbo"."Stslsman" "Doc_Slm_" on "Doc_"."Slm_No" = "Doc_Slm_"."Slm_No"
	left join "dbo"."masngerclass" "Doc_Slm_masngerclass" on "Doc_Slm_"."masngerclassid" = "Doc_Slm_masngerclass"."masngerclassid"
	left join "dbo"."Branchcnst" "Brn_" on "Stpurdtl"."Brn_No" = "Brn_"."Brn_No"
	left join "dbo"."Pycnstnt" "Brn_Cmp_" on "Brn_"."Cmp_code" = "Brn_Cmp_"."Cmp_No"
	left join "dbo"."Groupcnst" "Brn_Cmp_Grp_" on "Brn_Cmp_"."Grp_No" = "Brn_Cmp_Grp_"."Grp_No"
	left join "dbo"."ITEMS" "Itm_" on "Stpurdtl"."Itm_No" = "Itm_"."ID_comp"
	left join "dbo"."GROUPStk" "Itm_IDGROUP" on "Itm_"."IDGROUP" = "Itm_IDGROUP"."IDGROUP"
	left join "dbo"."ItemList" "Itm_Sup_" on "Itm_"."Sup_No" = "Itm_Sup_"."ItemNo"
	left join "dbo"."PYHldtypsv1" "Itm_id1" on "Itm_"."id1" = "Itm_id1"."ID1"
	left join "dbo"."PYHldtypsv2" "Itm_id2" on "Itm_"."id2" = "Itm_id2"."ID2"
	left join "dbo"."PYHldtypsv3" "Itm_id3" on "Itm_"."id3" = "Itm_id3"."id3"
	left join "dbo"."PYHldtypsv4" "Itm_id4" on "Itm_"."id4" = "Itm_id4"."id4"
	left join "dbo"."PYHldtypsv5" "Itm_id5" on "Itm_"."id5" = "Itm_id5"."id5"
	left join "dbo"."PYHldtypsv6" "Itm_id6" on "Itm_"."id6" = "Itm_id6"."id6"
	left join "dbo"."PYHldtypsv7" "Itm_id7" on "Itm_"."id7" = "Itm_id7"."id7"
	left join "dbo"."Stunit" "Unit_" on "Stpurdtl"."Unit_No" = "Unit_"."Unit_No"
