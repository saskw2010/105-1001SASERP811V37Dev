      ,"Branchcnst"."Acc_costofSales" "Acc_costofSales"
from "dbo"."Branchcnst" "Branchcnst"
	left join "dbo"."Pycnstnt" "Cmp_" on "Branchcnst"."Cmp_code" = "Cmp_"."Cmp_No"
	left join "dbo"."Groupcnst" "Cmp_Grp_" on "Cmp_"."Grp_No" = "Cmp_Grp_"."Grp_No"
