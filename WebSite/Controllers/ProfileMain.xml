  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"ProfileMain"."profilecode" "profilecode"
	,"ProfileMain"."Profileyear" "Profileyear"
	,"ProfileMain"."Mokelcode" "Mokelcode"
	,"Mokel"."name" "Mokelname"
	,"ProfileMain"."Mokpcode" "Mokpcode"
	,"Mokp"."name" "Mokpname"
	,"ProfileMain"."khesmcode" "khesmcode"
	,"khesm"."name" "khesmname"
	,"khesmkhosmetype"."khosmetypename" "khesmkhosmetypekhosmetypename"
	,"khesmnationality"."Cntry_Nm" "khesmnationalityCntry_Nm"
	,"khesmreligion"."Reljan_nm" "khesmreligionReljan_nm"
	,"ProfileMain"."number1" "number1"
	,"ProfileMain"."internetCode" "internetCode"
	,"ProfileMain"."summary" "summary"
	,"ProfileMain"."localMony" "localMony"
	,"ProfileMain"."profiltype" "profiltype"
	,"profiltype"."name" "profiltypename"
	,"ProfileMain"."Enterdate" "Enterdate"
	,"ProfileMain"."laywercode" "laywercode"
	,"laywer"."laywername" "laywerlaywername"
	,"ProfileMain"."Empolyeecode" "Empolyeecode"
	,"Empolyee"."name" "Empolyeename"
	,"ProfileMain"."CortID" "CortID"
	,"Cort"."CortName" "CortCortName"
	,"ProfileMain"."CortDawaNo" "CortDawaNo"
	,"ProfileMain"."CortDawaState" "CortDawaState"
	,"ProfileMain"."paymentsType" "paymentsType"
	,"paymentsType"."Name" "paymentsTypeName"
	,"ProfileMain"."IdaryID" "IdaryID"
	,"Idary"."Dwaername" "IdaryDwaername"
	,"ProfileMain"."UID" "UID"
	,"ProfileMain"."ModifiedBy" "ModifiedBy"
	,"ProfileMain"."ModifiedOn" "ModifiedOn"
	,"ProfileMain"."CreatedBy" "CreatedBy"
	,"ProfileMain"."CreatedOn" "CreatedOn"
from "dbo"."ProfileMain" "ProfileMain"
	left join "dbo"."Mowakel" "Mokel" on "ProfileMain"."Mokelcode" = "Mokel"."Mokelcode"
	left join "dbo"."MowakelProperties" "Mokp" on "ProfileMain"."Mokpcode" = "Mokp"."Mokpcode"
	left join "dbo"."Khsom" "khesm" on "ProfileMain"."khesmcode" = "khesm"."khosmcode"
	left join "dbo"."khosmetypetable" "khesmkhosmetype" on "khesm"."khosmetype" = "khesmkhosmetype"."khosmetype"
	left join "dbo"."PYCntry" "khesmnationality" on "khesm"."nationality" = "khesmnationality"."Cntry_No"
	left join "dbo"."PyReljan" "khesmreligion" on "khesm"."religion" = "khesmreligion"."reljan"
	left join "dbo"."ProfileType" "profiltype" on "ProfileMain"."profiltype" = "profiltype"."code"
	left join "dbo"."Lawyers" "laywer" on "ProfileMain"."laywercode" = "laywer"."laywercode"
	left join "dbo"."profileEmployees" "Empolyee" on "ProfileMain"."Empolyeecode" = "Empolyee"."Empolyeecode"
	left join "dbo"."Corts" "Cort" on "ProfileMain"."CortID" = "Cort"."CortID"
	left join "dbo"."PaymentTypes" "paymentsType" on "ProfileMain"."paymentsType" = "paymentsType"."Code"
	left join "dbo"."Dwaer" "Idary" on "ProfileMain"."IdaryID" = "Idary"."Dwaercode"
]]></text>
    </command>
    <command id="profilecodeIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
