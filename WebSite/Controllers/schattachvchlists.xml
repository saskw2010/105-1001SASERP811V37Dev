    <command id="command1" type="Text">
      <text><![CDATA[
select
	"schattachvchlists"."schattachvchid" "schattachvchid"
	,"schattachvchlists"."schvchdlyhdregid" "schvchdlyhdregid"
	,"schvchdlyhdreg"."Delevername" "schvchdlyhdregDelevername"
	,"schvchdlyhdregsgm"."sgm_Nm" "schvchdlyhdregsgmsgm_Nm"
	,"schvchdlyhdregsgmOpco"."OpcoName" "schvchdlyhdregsgmOpcoOpcoName"
	,"schvchdlyhdregIDregorg"."NAME_1" "schvchdlyhdregIDregorgNAME_1"
	,"schvchdlyhdregacd"."AcademicYear" "schvchdlyhdregacdAcademicYear"
	,"schvchdlyhdregacdGlFinperiod"."Fin_period_info" "schvchdlyhdregacdGlFinperiodFin_period_info"
	,"schvchdlyhdregbranch"."Desc1" "schvchdlyhdregbranchDesc1"
	,"schvchdlyhdregbranchsgm"."sgm_Nm" "schvchdlyhdregbranchsgmsgm_Nm"
	,"schvchdlyhdregbranchGender"."Gender" "schvchdlyhdregbranchGenderGender"
	,"schvchdlyhdregbranchstage"."ShortDesc1" "schvchdlyhdregbranchstageShortDesc1"
	,"schvchdlyhdregbranchschtyp"."schtypDesc" "schvchdlyhdregbranchschtypschtypDesc"
	,"schattachvchlists"."schattachvchtype" "schattachvchtype"
	,"schattachvchlists"."schattachvchrefno" "schattachvchrefno"
	,"schattachvchlists"."schattachvchrefdate" "schattachvchrefdate"
	,"schattachvchlists"."schattachvchrefpagesno" "schattachvchrefpagesno"
	,"schattachvchlists"."notes" "notes"
	,"schattachvchlists"."ExternalDocFileName" "ExternalDocFileName"
	,"schattachvchlists"."ExternalDocContentType" "ExternalDocContentType"
	,"schattachvchlists"."ExternalDocLength" "ExternalDocLength"
from "dbo"."schattachvchlists" "schattachvchlists"
	left join "dbo"."schvchdlyhdreg" "schvchdlyhdreg" on "schattachvchlists"."schvchdlyhdregid" = "schvchdlyhdreg"."schvchdlyhdregid"
	left join "dbo"."glmulcmp" "schvchdlyhdregsgm" on "schvchdlyhdreg"."sgm" = "schvchdlyhdregsgm"."sgmid"
	left join "dbo"."OpCo" "schvchdlyhdregsgmOpco" on "schvchdlyhdregsgm"."OpcoID" = "schvchdlyhdregsgmOpco"."OpcoIDidentity"
	left join "dbo"."REG_ORGANIZATIONS" "schvchdlyhdregIDregorg" on "schvchdlyhdreg"."IDregorg" = "schvchdlyhdregIDregorg"."IDIDregorg"
	left join "dbo"."schAcademicyear" "schvchdlyhdregacd" on "schvchdlyhdreg"."acdcode" = "schvchdlyhdregacd"."Code"
	left join "dbo"."GlFinanceperiod" "schvchdlyhdregacdGlFinperiod" on "schvchdlyhdregacd"."GlFinperiodID" = "schvchdlyhdregacdGlFinperiod"."GlFinperiodID"
	left join "dbo"."schBranch" "schvchdlyhdregbranch" on "schvchdlyhdreg"."branch" = "schvchdlyhdregbranch"."branch"
	left join "dbo"."glmulcmp" "schvchdlyhdregbranchsgm" on "schvchdlyhdregbranch"."sgm" = "schvchdlyhdregbranchsgm"."sgmid"
	left join "dbo"."PyGender" "schvchdlyhdregbranchGender" on "schvchdlyhdregbranch"."Genderid" = "schvchdlyhdregbranchGender"."Genderid"
	left join "dbo"."schStage" "schvchdlyhdregbranchstage" on "schvchdlyhdregbranch"."stagecode" = "schvchdlyhdregbranchstage"."Code"
	left join "dbo"."schtyp" "schvchdlyhdregbranchschtyp" on "schvchdlyhdregbranch"."schtypid" = "schvchdlyhdregbranchschtyp"."schtypid"
]]></text>
