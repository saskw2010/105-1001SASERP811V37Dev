<?xml version="1.0" encoding="utf-8"?>
<dataController name="EVOLMemo" conflictDetection="overwriteChanges" label="EVOL Memo" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"EVOLMemo"."ID" "ID"
	,"TheID"."title" "TheIDtitle"
	,"TheIDCategory"."Categoryname" "TheIDCategoryCategoryname"
	,"EVOLMemo"."UserName" "UserName"
	,"TheUserName"."UserEmail" "TheUserNameUserEmail"
	,"TheUserNamecatid1"."cat1name" "TheUserNamecatid1cat1name"
	,"TheUserNamecatid2"."cat2name" "TheUserNamecatid2cat2name"
	,"TheUserNamecatid3"."cat3name" "TheUserNamecatid3cat3name"
	,"TheUserNamecatid4"."cat4name" "TheUserNamecatid4cat4name"
	,"TheUserNamecatid5"."cat5name" "TheUserNamecatid5cat5name"
	,"TheUserNamecatid6"."cat6name" "TheUserNamecatid6cat6name"
	,"TheUserNamecatid7"."cat7name" "TheUserNamecatid7cat7name"
	,"TheUserNamecatid8"."cat8name" "TheUserNamecatid8cat8name"
	,"TheUserNamecatid9"."cat9name" "TheUserNamecatid9cat9name"
	,"TheUserNamestage"."ShortDesc1" "TheUserNamestageShortDesc1"
from "dbo"."EVOLMemo" "EVOLMemo"
	left join "dbo"."EVOL_Memo" "TheID" on "EVOLMemo"."ID" = "TheID"."ID"
	left join "dbo"."EVOL_MemoCategory" "TheIDCategory" on "TheID"."CategoryID" = "TheIDCategory"."CategoryID"
	left join "dbo"."user_details" "TheUserName" on "EVOLMemo"."UserName" = "TheUserName"."UserName"
	left join "dbo"."Qwaizcat1" "TheUserNamecatid1" on "TheUserName"."catid1" = "TheUserNamecatid1"."catid1"
	left join "dbo"."Qwaizcat2" "TheUserNamecatid2" on "TheUserName"."catid2" = "TheUserNamecatid2"."catid2"
	left join "dbo"."Qwaizcat3" "TheUserNamecatid3" on "TheUserName"."catid3" = "TheUserNamecatid3"."catid3"
	left join "dbo"."Qwaizcat4" "TheUserNamecatid4" on "TheUserName"."catid4" = "TheUserNamecatid4"."catid4"
	left join "dbo"."Qwaizcat5" "TheUserNamecatid5" on "TheUserName"."catid5" = "TheUserNamecatid5"."catid5"
	left join "dbo"."Qwaizcat6" "TheUserNamecatid6" on "TheUserName"."catid6" = "TheUserNamecatid6"."catid6"
	left join "dbo"."Qwaizcat7" "TheUserNamecatid7" on "TheUserName"."catid7" = "TheUserNamecatid7"."catid7"
	left join "dbo"."Qwaizcat8" "TheUserNamecatid8" on "TheUserName"."catid8" = "TheUserNamecatid8"."catid8"
	left join "dbo"."Qwaizcat9" "TheUserNamecatid9" on "TheUserName"."catid9" = "TheUserNamecatid9"."catid9"
	left join "dbo"."schStage" "TheUserNamestage" on "TheUserName"."stage" = "TheUserNamestage"."Code"
]]></text>
    </command>
  </commands>
  <fields>
    <field name="ID" type="Int64" allowNulls="false" isPrimaryKey="true" label="Id" showInSummary="true">
      <items style="Lookup" dataController="EVOL_Memo" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="TheIDtitle" type="String" readOnly="true" label="I Dtitle" length="400" />
    <field name="TheIDCategoryCategoryname" type="String" readOnly="true" label="ID Category Categoryname" length="100" />
    <field name="UserName" type="String" allowNulls="false" isPrimaryKey="true" label="User Name" length="50" showInSummary="true">
      <items style="Lookup" dataController="user_details" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="TheUserNameUserEmail" type="String" readOnly="true" label="User Name User Email" length="250" />
    <field name="TheUserNamecatid1cat1name" type="String" readOnly="true" label="User Namecatid1cat1name" length="255" />
    <field name="TheUserNamecatid2cat2name" type="String" readOnly="true" label="User Namecatid2cat2name" length="255" />
    <field name="TheUserNamecatid3cat3name" type="String" readOnly="true" label="User Namecatid3cat3name" length="255" />
