    <field name="TheUserNamecatid7cat7name" type="String" readOnly="true" label="User Namecatid7cat7name" length="255" />
    <field name="TheUserNamecatid8cat8name" type="String" readOnly="true" label="User Namecatid8cat8name" length="255" />
    <field name="TheUserNamecatid9cat9name" type="String" readOnly="true" label="User Namecatid9cat9name" length="255" />
    <field name="TheUserNamestageShortDesc1" type="String" readOnly="true" label="User Namestage Short Desc1" length="50" />
  </fields>
  <views>
