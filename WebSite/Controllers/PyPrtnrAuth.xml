	,"PyPrtnrAuth"."CreatedOn" "CreatedOn"
from "dbo"."PyPrtnrAuth" "PyPrtnrAuth"
	left join "dbo"."Pyownrmf" "Cmp_" on "PyPrtnrAuth"."Cmp_No" = "Cmp_"."Ownr_No"
	left join "dbo"."Pycnstnt" "Cmp_cmp_" on "Cmp_"."cmp_no" = "Cmp_cmp_"."Cmp_No"
	left join "dbo"."Groupcnst" "Cmp_cmp_Grp_" on "Cmp_cmp_"."Grp_No" = "Cmp_cmp_Grp_"."Grp_No"
	left join "dbo"."PyPrtnrMf" "Prtnr_" on "PyPrtnrAuth"."Prtnr_No" = "Prtnr_"."Emp_No"
]]></text>
