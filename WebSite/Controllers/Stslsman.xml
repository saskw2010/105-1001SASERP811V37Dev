  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Stslsman"."Slm_No" "Slm_No"
	,"Stslsman"."Slm_Nm" "Slm_Nm"
	,"Stslsman"."Slm_Nme" "Slm_Nme"
	,"Stslsman"."slmprcnt" "slmprcnt"
	,"Stslsman"."slmaccount" "slmaccount"
	,"Stslsman"."masngerclassid" "masngerclassid"
	,"masngerclass"."masngerclass" "masngerclassmasngerclass"
	,"Stslsman"."ModifiedBy" "ModifiedBy"
	,"Stslsman"."ModifiedOn" "ModifiedOn"
	,"Stslsman"."CreatedBy" "CreatedBy"
	,"Stslsman"."CreatedOn" "CreatedOn"
from "dbo"."Stslsman" "Stslsman"
	left join "dbo"."masngerclass" "masngerclass" on "Stslsman"."masngerclassid" = "masngerclass"."masngerclassid"
]]></text>
    </command>
    <command id="Slm_NoIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Slm_No" />
      </output>
    </command>
  </commands>
