<?xml version="1.0" encoding="utf-8"?>
<dataController name="QwizAnswer" conflictDetection="overwriteChanges" label="Qwiz Answer" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"QwizAnswer"."AnswerID" "AnswerID"
	,"QwizAnswer"."qwizid" "qwizid"
	,"qwiz"."qwizquastion" "qwizqwizquastion"
	,"qwizcatid1"."cat1name" "qwizcatid1cat1name"
	,"qwizcatid2"."cat2name" "qwizcatid2cat2name"
	,"qwizcatid3"."cat3name" "qwizcatid3cat3name"
	,"qwizcatid4"."cat4name" "qwizcatid4cat4name"
	,"qwizcatid5"."cat5name" "qwizcatid5cat5name"
	,"qwizcatid6"."cat6name" "qwizcatid6cat6name"
	,"qwizcatid7"."cat7name" "qwizcatid7cat7name"
	,"qwizcatid8"."cat8name" "qwizcatid8cat8name"
	,"qwizcatid9"."cat9name" "qwizcatid9cat9name"
	,"QwizAnswer"."Answerline" "Answerline"
	,"QwizAnswer"."Answernotes" "Answernotes"
	,"QwizAnswer"."Answeraudiolink" "Answeraudiolink"
	,"QwizAnswer"."Answervediolink" "Answervediolink"
	,"QwizAnswer"."Answerstatus" "Answerstatus"
	,"QwizAnswer"."ModifiedBy" "ModifiedBy"
	,"QwizAnswer"."ModifiedOn" "ModifiedOn"
	,"QwizAnswer"."CreatedBy" "CreatedBy"
	,"QwizAnswer"."CreatedOn" "CreatedOn"
from "dbo"."QwizAnswer" "QwizAnswer"
	left join "dbo"."Qwiz" "qwiz" on "QwizAnswer"."qwizid" = "qwiz"."qwizid"
	left join "dbo"."Qwaizcat1" "qwizcatid1" on "qwiz"."catid1" = "qwizcatid1"."catid1"
	left join "dbo"."Qwaizcat2" "qwizcatid2" on "qwiz"."catid2" = "qwizcatid2"."catid2"
	left join "dbo"."Qwaizcat3" "qwizcatid3" on "qwiz"."catid3" = "qwizcatid3"."catid3"
	left join "dbo"."Qwaizcat4" "qwizcatid4" on "qwiz"."catid4" = "qwizcatid4"."catid4"
	left join "dbo"."Qwaizcat5" "qwizcatid5" on "qwiz"."catid5" = "qwizcatid5"."catid5"
	left join "dbo"."Qwaizcat6" "qwizcatid6" on "qwiz"."catid6" = "qwizcatid6"."catid6"
	left join "dbo"."Qwaizcat7" "qwizcatid7" on "qwiz"."catid7" = "qwizcatid7"."catid7"
	left join "dbo"."Qwaizcat8" "qwizcatid8" on "qwiz"."catid8" = "qwizcatid8"."catid8"
	left join "dbo"."Qwaizcat9" "qwizcatid9" on "qwiz"."catid9" = "qwizcatid9"."catid9"
]]></text>
    </command>
    <command id="AnswerIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="AnswerID" />
      </output>
    </command>
  </commands>
