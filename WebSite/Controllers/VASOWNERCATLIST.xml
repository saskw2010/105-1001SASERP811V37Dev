	,"SHORTCADEOWNERMainAcc_"."Acc_Nm" "SHORTCADEOWNERMainAcc_Acc_Nm"
	,"SHORTCADEOWNERMainAcc_Clsacc_"."Acc_Nm" "SHORTCADEOWNERMainAcc_Clsacc_Acc_Nm"
	,"SHORTCADEOWNERMainAcc_Acc_Bnd"."Acc_Nm" "SHORTCADEOWNERMainAcc_Acc_BndAcc_Nm"
	,"VASOWNERCATLIST"."Acc_no" "Acc_no"
	,"Acc_"."Acc_Nm" "Acc_Acc_Nm"
	,"Acc_Clsacc_"."Acc_Nm" "Acc_Clsacc_Acc_Nm"
	,"Acc_Acc_Bnd"."Acc_Nm" "Acc_Acc_BndAcc_Nm"
	,"Acc_Acc_Bndaccclass"."accountclass" "Acc_Acc_Bndaccclassaccountclass"
	,"Acc_Acc_BndAcc_Bab"."Acc_Nm" "Acc_Acc_BndAcc_BabAcc_Nm"
from "dbo"."VASOWNERCATLIST" "VASOWNERCATLIST"
	left join "dbo"."VASOWNERCATLIST" "SHORTCADEOWNERMain" on "VASOWNERCATLIST"."SHORTCADEOWNERMain" = "SHORTCADEOWNERMain"."SHORTCADEOWNER"
	left join "dbo"."GLmstrfl" "SHORTCADEOWNERMainAcc_" on "SHORTCADEOWNERMain"."Acc_no" = "SHORTCADEOWNERMainAcc_"."Acc_No"
	left join "dbo"."GLmfbab" "SHORTCADEOWNERMainAcc_Clsacc_" on "SHORTCADEOWNERMainAcc_"."Clsacc_No" = "SHORTCADEOWNERMainAcc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "SHORTCADEOWNERMainAcc_Acc_Bnd" on "SHORTCADEOWNERMainAcc_"."Acc_Bnd" = "SHORTCADEOWNERMainAcc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."GLmstrfl" "Acc_" on "VASOWNERCATLIST"."Acc_no" = "Acc_"."Acc_No"
	left join "dbo"."GLmfbab" "Acc_Clsacc_" on "Acc_"."Clsacc_No" = "Acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Acc_Acc_Bnd" on "Acc_"."Acc_Bnd" = "Acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "Acc_Acc_Bndaccclass" on "Acc_Acc_Bnd"."accclass" = "Acc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "Acc_Acc_BndAcc_Bab" on "Acc_Acc_Bnd"."Acc_Bab" = "Acc_Acc_BndAcc_Bab"."Acc_Bab"
]]></text>
    </command>
