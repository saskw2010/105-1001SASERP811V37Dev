    <command id="command1" type="Text">
      <text><![CDATA[
select
	"VASOWNERCATLIST"."SHORTCADEOWNER" "SHORTCADEOWNER"
	,"VASOWNERCATLIST"."CADEOWNERDescription" "CADEOWNERDescription"
	,"VASOWNERCATLIST"."SHORTCADEOWNERMain" "SHORTCADEOWNERMain"
