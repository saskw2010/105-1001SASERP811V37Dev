  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"CntItmd_1"."Prj_No" "Prj_No"
	,"Prj_"."Prj_Nm" "Prj_Prj_Nm"
	,"Prj_Cstm_"."Cstm_Nm" "Prj_Cstm_Cstm_Nm"
	,"Prj_Cstm_Area"."Area_name" "Prj_Cstm_AreaArea_name"
	,"Prj_Cstm_Evaluation"."Evaluation_name" "Prj_Cstm_EvaluationEvaluation_name"
	,"Prj_Cstm_acc_"."Acc_Nm" "Prj_Cstm_acc_Acc_Nm"
	,"Prj_Cstm_Cntry_"."Cntry_Nm" "Prj_Cstm_Cntry_Cntry_Nm"
	,"Prj_Cstm_curncy_"."Curncy_Nm" "Prj_Cstm_curncy_Curncy_Nm"
	,"Prj_Cstm_TSNEF"."TSNEF_name" "Prj_Cstm_TSNEFTSNEF_name"
	,"Prj_Cstm_TSNEFNO1"."TSNEF1_name" "Prj_Cstm_TSNEFNO1TSNEF1_name"
	,"Prj_Cstm_Slm_"."Slm_Nm" "Prj_Cstm_Slm_Slm_Nm"
	,"CntItmd_1"."ItmNo" "ItmNo"
	,"CntItmd_1"."ItmCode" "ItmCode"
	,"Itm"."Ac_Nm1" "ItmAc_Nm1"
	,"CntItmd_1"."Pr_Mo" "Pr_Mo"
	,"CntItmd_1"."Qu" "Qu"
	,"CntItmd_1"."Itmval" "Itmval"
	,"CntItmd_1"."Itm_stdate" "Itm_stdate"
	,"CntItmd_1"."Itm_Endate" "Itm_Endate"
	,"CntItmd_1"."PymDt" "PymDt"
	,"CntItmd_1"."sgm" "sgm"
from "dbo"."CntItmd_1" "CntItmd_1"
	left join "dbo"."PJcntmf" "Prj_" on "CntItmd_1"."Prj_No" = "Prj_"."Prj_No"
	left join "dbo"."Stcustmr" "Prj_Cstm_" on "Prj_"."Cstm_No" = "Prj_Cstm_"."Cstm_No"
	left join "dbo"."AreaList" "Prj_Cstm_Area" on "Prj_Cstm_"."AreaNo" = "Prj_Cstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "Prj_Cstm_Evaluation" on "Prj_Cstm_"."EvaluationNO" = "Prj_Cstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "Prj_Cstm_acc_" on "Prj_Cstm_"."acc_no" = "Prj_Cstm_acc_"."Acc_No"
	left join "dbo"."PYCntry" "Prj_Cstm_Cntry_" on "Prj_Cstm_"."Cntry_No" = "Prj_Cstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Prj_Cstm_curncy_" on "Prj_Cstm_"."curncy_no" = "Prj_Cstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "Prj_Cstm_TSNEF" on "Prj_Cstm_"."TSNEFNO" = "Prj_Cstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "Prj_Cstm_TSNEFNO1" on "Prj_Cstm_"."TSNEFNO1" = "Prj_Cstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "Prj_Cstm_Slm_" on "Prj_Cstm_"."Slm_No" = "Prj_Cstm_Slm_"."Slm_No"
	left join "dbo"."Prjbudgac1" "Itm" on "CntItmd_1"."ItmCode" = "Itm"."Ac_No1"
]]></text>
    </command>
  </commands>
