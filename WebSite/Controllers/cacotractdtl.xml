select
	"cacotractdtl"."Trs_No" "Trs_No"
	,"cacotractdtl"."contractid" "contractid"
	,"contract"."GehatNo" "contractGehatNo"
	,"contractcontractstatus"."status_name" "contractcontractstatusstatus_name"
	,"cacotractdtl"."car_No" "car_No"
	,"car_"."PLAT_No" "car_PLAT_No"
	,"car_Car_Type"."Styl_Nm" "car_Car_TypeStyl_Nm"
	,"car_carclass"."carclassname" "car_carclasscarclassname"
	,"car_carstutus_"."carstutus_name" "car_carstutus_carstutus_name"
	,"car_Car_Colr"."Color_name" "car_Car_ColrColor_name"
	,"car_sgm"."sgm_Nm" "car_sgmsgm_Nm"
	,"car_sgmOpco"."OpcoName" "car_sgmOpcoOpcoName"
	,"car_Insurance_type"."Insurance_name" "car_Insurance_typeInsurance_name"
	,"car_Car_Lik"."Like_name" "car_Car_LikLike_name"
	,"car_Fbrct_Cntry"."Cntry_Nm" "car_Fbrct_CntryCntry_Nm"
	,"car_Ownr_"."Ownr_ID" "car_Ownr_Ownr_ID"
	,"car_Ownr_cmp_"."Grp_Nm" "car_Ownr_cmp_Grp_Nm"
	,"car_Trfc_Dpm"."State_Nm" "car_Trfc_DpmState_Nm"
	,"car_Sup_"."Sup_Nm" "car_Sup_Sup_Nm"
	,"car_Sup_Acc_"."Acc_Nm" "car_Sup_Acc_Acc_Nm"
	,"car_Sup_Cntry_"."Cntry_Nm" "car_Sup_Cntry_Cntry_Nm"
	,"car_Sup_Curncy_"."Curncy_Nm" "car_Sup_Curncy_Curncy_Nm"
	,"cacotractdtl"."Trs_Dt" "Trs_Dt"
	,"cacotractdtl"."Notes1" "Notes1"
	,"cacotractdtl"."cstm_no" "cstm_no"
	,"cstm_"."Cstm_Nm" "cstm_Cstm_Nm"
	,"cstm_Area"."Area_name" "cstm_AreaArea_name"
	,"cstm_Evaluation"."Evaluation_name" "cstm_EvaluationEvaluation_name"
	,"cstm_acc_"."Acc_Nm" "cstm_acc_Acc_Nm"
	,"cstm_acc_Clsacc_"."Acc_Nm" "cstm_acc_Clsacc_Acc_Nm"
	,"cstm_acc_Acc_Bnd"."Acc_Nm" "cstm_acc_Acc_BndAcc_Nm"
	,"cstm_Cntry_"."Cntry_Nm" "cstm_Cntry_Cntry_Nm"
	,"cstm_curncy_"."Curncy_Nm" "cstm_curncy_Curncy_Nm"
	,"cstm_TSNEF"."TSNEF_name" "cstm_TSNEFTSNEF_name"
	,"cstm_TSNEFNO1"."TSNEF1_name" "cstm_TSNEFNO1TSNEF1_name"
	,"cstm_Slm_"."Slm_Nm" "cstm_Slm_Slm_Nm"
	,"cstm_Slm_masngerclass"."masngerclass" "cstm_Slm_masngerclassmasngerclass"
	,"cacotractdtl"."ModifiedBy" "ModifiedBy"
	,"cacotractdtl"."ModifiedOn" "ModifiedOn"
	,"cacotractdtl"."CreatedBy" "CreatedBy"
	,"cacotractdtl"."CreatedOn" "CreatedOn"
from "dbo"."cacotractdtl" "cacotractdtl"
	left join "dbo"."cacotractmain" "contract" on "cacotractdtl"."contractid" = "contract"."contractid"
	left join "dbo"."contractstatus" "contractcontractstatus" on "contract"."contractstatus" = "contractcontractstatus"."cntstcode"
	left join "dbo"."CarInfom" "car_" on "cacotractdtl"."car_No" = "car_"."Car_No"
	left join "dbo"."CaCrstyl" "car_Car_Type" on "car_"."Car_Type" = "car_Car_Type"."Styl_No"
	left join "dbo"."carclass" "car_carclass" on "car_"."carclass" = "car_carclass"."carclass"
	left join "dbo"."carstutuslist" "car_carstutus_" on "car_"."carstutus_no" = "car_carstutus_"."carstutus_no"
	left join "dbo"."Colorlist" "car_Car_Colr" on "car_"."Car_Colr" = "car_Car_Colr"."ColorNo"
	left join "dbo"."glmulcmp" "car_sgm" on "car_"."sgm" = "car_sgm"."sgmid"
	left join "dbo"."OpCo" "car_sgmOpco" on "car_sgm"."OpcoID" = "car_sgmOpco"."OpcoIDidentity"
	left join "dbo"."Insurancetype" "car_Insurance_type" on "car_"."Insurance_type" = "car_Insurance_type"."Insurance_type"
	left join "dbo"."Likelist" "car_Car_Lik" on "car_"."Car_Lik" = "car_Car_Lik"."LikeNo"
	left join "dbo"."PYCntry" "car_Fbrct_Cntry" on "car_"."Fbrct_Cntry" = "car_Fbrct_Cntry"."Cntry_No"
	left join "dbo"."Pyownrmf" "car_Ownr_" on "car_"."Ownr_No" = "car_Ownr_"."Ownr_No"
	left join "dbo"."Pycnstnt" "car_Ownr_cmp_" on "car_Ownr_"."cmp_no" = "car_Ownr_cmp_"."Cmp_No"
	left join "dbo"."PyState" "car_Trfc_Dpm" on "car_"."Trfc_Dpm" = "car_Trfc_Dpm"."State"
	left join "dbo"."Stsuplir" "car_Sup_" on "car_"."Sup_No" = "car_Sup_"."Sup_No"
	left join "dbo"."GLmstrfl" "car_Sup_Acc_" on "car_Sup_"."Acc_No" = "car_Sup_Acc_"."Acc_No"
	left join "dbo"."PYCntry" "car_Sup_Cntry_" on "car_Sup_"."Cntry_No" = "car_Sup_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "car_Sup_Curncy_" on "car_Sup_"."Curncy_No" = "car_Sup_Curncy_"."Curncy_No"
	left join "dbo"."Stcustmr" "cstm_" on "cacotractdtl"."cstm_no" = "cstm_"."Cstm_No"
	left join "dbo"."AreaList" "cstm_Area" on "cstm_"."AreaNo" = "cstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "cstm_Evaluation" on "cstm_"."EvaluationNO" = "cstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "cstm_acc_" on "cstm_"."acc_no" = "cstm_acc_"."Acc_No"
	left join "dbo"."GLmfbab" "cstm_acc_Clsacc_" on "cstm_acc_"."Clsacc_No" = "cstm_acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "cstm_acc_Acc_Bnd" on "cstm_acc_"."Acc_Bnd" = "cstm_acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."PYCntry" "cstm_Cntry_" on "cstm_"."Cntry_No" = "cstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "cstm_curncy_" on "cstm_"."curncy_no" = "cstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "cstm_TSNEF" on "cstm_"."TSNEFNO" = "cstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "cstm_TSNEFNO1" on "cstm_"."TSNEFNO1" = "cstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "cstm_Slm_" on "cstm_"."Slm_No" = "cstm_Slm_"."Slm_No"
	left join "dbo"."masngerclass" "cstm_Slm_masngerclass" on "cstm_Slm_"."masngerclassid" = "cstm_Slm_masngerclass"."masngerclassid"
]]></text>
    </command>
