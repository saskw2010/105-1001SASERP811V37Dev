<?xml version="1.0" encoding="utf-8"?>
<dataController name="RealstateOwners" conflictDetection="overwriteChanges" label="Realstate Owners" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"RealstateOwners"."Ownerid" "Ownerid"
    ,"RealstateOwners"."Ownerid" "unitowner"
	,"RealstateOwners"."RealstateOwners" "RealstateOwners"
	,"RealstateOwners"."RealstateOwnerse" "RealstateOwnerse"
	,"RealstateOwners"."ownertelphone" "ownertelphone"
	,"RealstateOwners"."ownermobile" "ownermobile"
	,"RealstateOwners"."owneradress" "owneradress"
	,"RealstateOwners"."ownercivilid" "ownercivilid"
	,"RealstateOwners"."ownerpassport" "ownerpassport"
	,"RealstateOwners"."note" "note"
	,"RealstateOwners"."cntry_no" "cntry_no"
	,"cntry_"."Cntry_Nm" "cntry_Cntry_Nm"
	,"RealstateOwners"."ownertypeid" "ownertypeid"
	,"RealstateOwners"."Job_No" "Job_No"
	,"Job_"."Job_Nm" "Job_Job_Nm"
	,"RealstateOwners"."bankacc" "bankacc"
	,"RealstateOwners"."ownerphone2" "ownerphone2"
	,"RealstateOwners"."bank_no" "bank_no"
	,"bank_"."AccountName" "AccountName"
	

from "dbo"."RealstateOwners" "RealstateOwners"

	left join "dbo"."PYCntry" "cntry_" on "RealstateOwners"."cntry_no" = "cntry_"."Cntry_No"
	left join "dbo"."Pyjobs" "Job_" on "RealstateOwners"."Job_No" = "Job_"."Job_No"
	left join "dbo"."BankData" "bank_" on "RealstateOwners"."bank_no" = "bank_"."bank_no"
	
]]></text>
    </command>
    <command id="OwneridIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Ownerid" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="Ownerid" type="Int64" allowNulls="false" isPrimaryKey="true" label="Ownerid" readOnly="true" />
	<field name="Ownerid" type="Int64" readOnly="true" />
	
    <field name="RealstateOwners" type="String" label="Realstate Owners" length="150" showInSummary="true" />
    <field name="RealstateOwnerse" type="String" label="Realstate Ownerse" length="150" showInSummary="true" />
    <field name="ownertelphone" type="String" label="Ownertelphone" length="10" showInSummary="true" />
    <field name="ownermobile" type="String" label="Ownermobile" length="10" showInSummary="true" />
    <field name="owneradress" type="String" label="Owneradress" length="150" showInSummary="true" />
    <field name="ownercivilid" type="String" label="Ownercivilid" length="50" />
    <field name="ownerpassport" type="String" label="Ownerpassport" length="50" />
    <field name="note" type="String" label="Note" length="10" />
    <field name="cntry_no" type="Int64" label="Cntry No">
      <items style="Lookup" dataController="PYCntry" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="cntry_Cntry_Nm" type="String" readOnly="true" label="cntry Cntry Nm" length="255" />
    <field name="ownertypeid" type="Int64" label="Ownertypeid" />
    <field name="Job_No" type="Int64" label="Job No">
      <items style="Lookup" dataController="Pyjobs" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="Job_Job_Nm" type="String" readOnly="true" label="Job Nm" length="255" />

    <field name="ownerphone2" type="String" label="ownerphone2" length="10" showInSummary="true" />
    <field name="bankacc" type="String" label="bankacc" length="50" />
    <field name="bank_no" type="Int64" label="bank_no" >
    <items style="Lookup" dataController="BankData" newDataView="createForm1" searchOnStart="true" />
    <field name="AccountName" type="String" readOnly="true" label="AccountName" length="255" />

</field>
  </fields>

