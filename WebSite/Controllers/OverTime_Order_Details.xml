<?xml version="1.0" encoding="utf-8"?>
<dataController name="OverTime_Order_Details" conflictDetection="overwriteChanges" label="Over Time Order Details" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"OverTime_Order_Details"."SID" "SID"
	,"OverTime_Order_Details"."TDate" "TDate"
	,"OverTime_Order_Details"."Day_SID" "Day_SID"
	,"OverTime_Order_Details"."OverTime_Orders_SID" "OverTime_Orders_SID"
	,"OverTime_Orders_S"."Decision_Number" "OverTime_Orders_SDecision_Number"
from "dbo"."OverTime_Order_Details" "OverTime_Order_Details"
	left join "dbo"."OverTime_Orders" "OverTime_Orders_S" on "OverTime_Order_Details"."OverTime_Orders_SID" = "OverTime_Orders_S"."SID"
]]></text>
    </command>
    <command id="SIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="SID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="SID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Sid" readOnly="true" />
    <field name="TDate" type="DateTime" label="T Date" showInSummary="true" />
    <field name="Day_SID" type="Int32" label="Day SID" showInSummary="true" />
    <field name="OverTime_Orders_SID" type="Int32" label="Over Time Orders SID" showInSummary="true">
      <items style="Lookup" dataController="OverTime_Orders" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="OverTime_Orders_SDecision_Number" type="String" readOnly="true" label="Over Time Orders S Decision Number" length="50" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Over Time Order Details">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="TDate" columns="10" />
        <dataField fieldName="Day_SID" columns="15" />
        <dataField fieldName="OverTime_Orders_SID" aliasFieldName="OverTime_Orders_SDecision_Number" hyperlinkFormatString="~/Pages/OverTime_Orders.aspx?SID={OverTime_Orders_SID}&amp;_controller=OverTime_Orders&amp;_commandName=Select&amp;_commandArgument=editForm1" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Over Time Order Details">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Over Time Order Details" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="TDate" columns="10" />
            <dataField fieldName="Day_SID" columns="15" />
            <dataField fieldName="OverTime_Orders_SID" aliasFieldName="OverTime_Orders_SDecision_Number" hyperlinkFormatString="~/Pages/OverTime_Orders.aspx?SID={OverTime_Orders_SID}&amp;_controller=OverTime_Orders&amp;_commandName=Select&amp;_commandArgument=editForm1" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Over Time Order Details">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New Over Time Order Details" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="TDate" columns="10" />
            <dataField fieldName="Day_SID" columns="15" />
            <dataField fieldName="OverTime_Orders_SID" aliasFieldName="OverTime_Orders_SDecision_Number" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
  <actions>
    <actionGroup id="ag1" scope="Grid">
      <action id="a1" commandName="Select" commandArgument="editForm1" />
      <action id="a2" commandName="Edit" />
      <action id="a3" commandName="Delete" />
      <action id="a6" />
