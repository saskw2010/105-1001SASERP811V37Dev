  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Attachmentstcustmr"."AttachmentNo" "AttachmentNo"
	,"Attachmentstcustmr"."cstm_no" "cstm_no"
	,"cstm_"."Cstm_Nm" "cstm_Cstm_Nm"
	,"cstm_Area"."Area_name" "cstm_AreaArea_name"
	,"cstm_Evaluation"."Evaluation_name" "cstm_EvaluationEvaluation_name"
	,"cstm_acc_"."Acc_Nm" "cstm_acc_Acc_Nm"
	,"cstm_acc_Clsacc_"."Acc_Nm" "cstm_acc_Clsacc_Acc_Nm"
	,"cstm_acc_Acc_Bnd"."Acc_Nm" "cstm_acc_Acc_BndAcc_Nm"
	,"cstm_Cntry_"."Cntry_Nm" "cstm_Cntry_Cntry_Nm"
	,"cstm_curncy_"."Curncy_Nm" "cstm_curncy_Curncy_Nm"
	,"cstm_TSNEF"."TSNEF_name" "cstm_TSNEFTSNEF_name"
	,"cstm_TSNEFNO1"."TSNEF1_name" "cstm_TSNEFNO1TSNEF1_name"
	,"cstm_Slm_"."Slm_Nm" "cstm_Slm_Slm_Nm"
	,"cstm_Slm_masngerclass"."masngerclass" "cstm_Slm_masngerclassmasngerclass"
	,"Attachmentstcustmr"."Attachmentpath" "Attachmentpath"
	,"Attachmentstcustmr"."AttachmentDescription" "AttachmentDescription"
	,"Attachmentstcustmr"."attachmentNotes" "attachmentNotes"
	,"Attachmentstcustmr"."ModifiedBy" "ModifiedBy"
	,"Attachmentstcustmr"."ModifiedOn" "ModifiedOn"
	,"Attachmentstcustmr"."CreatedBy" "CreatedBy"
	,"Attachmentstcustmr"."CreatedOn" "CreatedOn"
from "dbo"."Attachmentstcustmr" "Attachmentstcustmr"
	left join "dbo"."Stcustmr" "cstm_" on "Attachmentstcustmr"."cstm_no" = "cstm_"."Cstm_No"
	left join "dbo"."AreaList" "cstm_Area" on "cstm_"."AreaNo" = "cstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "cstm_Evaluation" on "cstm_"."EvaluationNO" = "cstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "cstm_acc_" on "cstm_"."acc_no" = "cstm_acc_"."Acc_No"
	left join "dbo"."GLmfbab" "cstm_acc_Clsacc_" on "cstm_acc_"."Clsacc_No" = "cstm_acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "cstm_acc_Acc_Bnd" on "cstm_acc_"."Acc_Bnd" = "cstm_acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."PYCntry" "cstm_Cntry_" on "cstm_"."Cntry_No" = "cstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "cstm_curncy_" on "cstm_"."curncy_no" = "cstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "cstm_TSNEF" on "cstm_"."TSNEFNO" = "cstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "cstm_TSNEFNO1" on "cstm_"."TSNEFNO1" = "cstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "cstm_Slm_" on "cstm_"."Slm_No" = "cstm_Slm_"."Slm_No"
	left join "dbo"."masngerclass" "cstm_Slm_masngerclass" on "cstm_Slm_"."masngerclassid" = "cstm_Slm_masngerclass"."masngerclassid"
]]></text>
    </command>
    <command id="AttachmentNoIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="AttachmentNo" />
      </output>
    </command>
  </commands>
