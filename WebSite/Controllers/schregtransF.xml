    <command id="command1" type="Text">
      <text><![CDATA[SELECT        schregtransF.trnsfid, schregtransF.schtrnsid, schtrns.schtrnsDesc1 AS schtrnsschtrnsDesc, schregtransF.transdate, schregtransF.StudentCode, 
                         Student.SchstCivilidnumber AS StudentSchstCivilidnumber, Student.FirstName1, Studentarea.Desc1 AS StudentareaDesc1, 
                         Studentbranch.Desc1 AS StudentbranchDesc1, Studentbranchsgm.sgm_Nm AS Studentbranchsgmsgm_Nm, 
                         StudentbranchGender.Gender AS StudentbranchGenderGender, Studentbranchschtyp.schtypDesc AS StudentbranchschtypschtypDesc, 
                         StudentCntry_.Cntry_Nm AS StudentCntry_Cntry_Nm, StudentGender.Gender AS StudentGenderGender, StudentReligion.Reljan_nm AS StudentReligionReljan_nm, 
                         Studentstate.State_Nm AS StudentstateState_Nm, Studentid.name_1 AS Studentidname_1, Studentacd.AcademicYear AS StudentacdAcademicYear, 
                         StudentacdGlFinperiod.Fin_period_info AS StudentacdGlFinperiodFin_period_info, StudentClass.ClassDesc1 AS StudentClassClassDesc1, 
                         StudentSchclasskind.Schclasskinddesc AS StudentSchclasskindSchclasskinddesc, StudentFather.fathfirstname1 AS StudentFatherfathfirstname1, 
                         StudentFatherMaritalStatus.Status_Nm AS StudentFatherMaritalStatusStatus_Nm, StudentFatherarea.Desc1 AS StudentFatherareaDesc1, 
                         StudentFatherCntry_.Cntry_Nm AS StudentFatherCntry_Cntry_Nm, StudentFatherreligion.Reljan_nm AS StudentFatherreligionReljan_nm, 
                         StudentFatherState.State_Nm AS StudentFatherStateState_Nm, StudentFatherjob_.job_desc AS StudentFatherjob_job_desc, 
                         StudentGrade.ShortDesc1 AS StudentGradeShortDesc1, StudentGradeacd.AcademicYear AS StudentGradeacdAcademicYear, 
                         StudentGradeStage.ShortDesc1 AS StudentGradeStageShortDesc1, StudentMother.firstname1 AS StudentMotherfirstname1, 
                         StudentMotherarea.Desc1 AS StudentMotherareaDesc1, StudentMotherCntry_.Cntry_Nm AS StudentMotherCntry_Cntry_Nm, 
                         StudentMotherreligion.Reljan_nm AS StudentMotherreligionReljan_nm, StudentMotherState.State_Nm AS StudentMotherStateState_Nm, 
                         StudentMotherjob_.job_desc AS StudentMotherjob_job_desc, StudentStage.ShortDesc1 AS StudentStageShortDesc1, 
                         Studentschstatus_.schstatus_name AS Studentschstatus_schstatus_name, 
                         Studentschstclasskinddmg.schstclasskinddmgDesc AS StudentschstclasskinddmgschstclasskinddmgDesc, 
                         Studentschstspl.schstsplDsc AS StudentschstsplschstsplDsc, Studentschsthlthcase.schsthlthcaseDesc AS StudentschsthlthcaseschsthlthcaseDesc, 
                         Studentstudentsefa.studentsefaar AS Studentstudentsefastudentsefaar, Studentstudentsefadebitaccount.Acc_Nm AS StudentstudentsefadebitaccountAcc_Nm, 
                         Studentschstudenttyp.schstudenttyp AS Studentschstudenttypschstudenttyp, StudentSchtransfer.SchtransferDesc AS StudentSchtransferSchtransferDesc, 
                         schregtransF.branch, Thebranch.Desc1 AS ThebranchDesc1, Thebranchsgm.sgm_Nm AS Thebranchsgmsgm_Nm, 
                         ThebranchsgmOpco.OpcoName AS ThebranchsgmOpcoOpcoName, ThebranchGender.Gender AS ThebranchGenderGender, 
                         Thebranchschtyp.schtypDesc AS ThebranchschtypschtypDesc, schregtransF.Stage, Stage.ShortDesc1 AS StageShortDesc1, schregtransF.GradeCode, 
                         Grade.ShortDesc1 AS GradeShortDesc1, Gradeacd.AcademicYear AS GradeacdAcademicYear, 
                         GradeacdGlFinperiod.Fin_period_info AS GradeacdGlFinperiodFin_period_info, GradeStage.ShortDesc1 AS GradeStageShortDesc1, schregtransF.Class, 
                         Class.ClassDesc1 AS ClassClassDesc1, schregtransF.schstatus_code, schstatus_.schstatus_name AS schstatus_schstatus_name, schregtransF.schstudenttypid, 
                         schstudenttyp.schstudenttyp AS schstudenttypschstudenttyp, schregtransF.acdcode, acd.AcademicYear AS acdAcademicYear, 
                         acdGlFinperiod.Fin_period_info AS acdGlFinperiodFin_period_info, acdGlFinperiodaccountnumber.Acc_Nm AS acdGlFinperiodaccountnumberAcc_Nm, 
                         schregtransF.resons, schregtransF.resigndate, schregtransF.startdate1, schregtransF.enddate2, schregtransF.notes, schregtransF.ModifiedBy, 
                         schregtransF.ModifiedOn, schregtransF.CreatedBy, schregtransF.CreatedOn, schregtransF.insehabdate, schregtransF.Periodbymonth, schregtransF.Amount, 
                         schregtransF.restdays, schregtransF.totaldays, schregtransF.Amountreturn, schregtransF.approved, schregtransF.Schooltransfeer, StudentFather.fathCivilidNumber, 
                         schregtransF.Amount12, schregtransF.Amount1, schregtransF.Amountreturn3, schregtransF.Amountreturn1, schregtransF.Amountreturn2, 
                         schregtransF.Classtransfeer1,
						 schregtransF.approveamount,
						 schregtransF.acdcodeapplay,
						 schregtransF.branch1
FROM            dbo.schregtransF AS schregtransF LEFT OUTER JOIN
                         dbo.schtrnstype1 AS schtrns ON schregtransF.schtrnsid = schtrns.schtrnsid1 LEFT OUTER JOIN
                         dbo.SchStudent AS Student ON schregtransF.StudentCode = Student.StudentCode LEFT OUTER JOIN
                         dbo.schArea AS Studentarea ON Student.area = Studentarea.Code LEFT OUTER JOIN
                         dbo.schBranch AS Studentbranch ON Student.branch = Studentbranch.branch LEFT OUTER JOIN
                         dbo.glmulcmp AS Studentbranchsgm ON Studentbranch.sgm = Studentbranchsgm.sgmid LEFT OUTER JOIN
                         dbo.PyGender AS StudentbranchGender ON Studentbranch.Genderid = StudentbranchGender.Genderid LEFT OUTER JOIN
                         dbo.schtyp AS Studentbranchschtyp ON Studentbranch.schtypid = Studentbranchschtyp.schtypid LEFT OUTER JOIN
                         dbo.PYCntry AS StudentCntry_ ON Student.Cntry_No = StudentCntry_.Cntry_No LEFT OUTER JOIN
                         dbo.PyGender AS StudentGender ON Student.Genderid = StudentGender.Genderid LEFT OUTER JOIN
                         dbo.PyReljan AS StudentReligion ON Student.Religion = StudentReligion.reljan LEFT OUTER JOIN
                         dbo.PyState AS Studentstate ON Student.state = Studentstate.State LEFT OUTER JOIN
                         dbo.reg_other_schools AS Studentid ON Student.id = Studentid.id LEFT OUTER JOIN
                         dbo.schAcademicyear AS Studentacd ON Student.acdcode = Studentacd.Code LEFT OUTER JOIN
                         dbo.GlFinanceperiod AS StudentacdGlFinperiod ON Studentacd.GlFinperiodID = StudentacdGlFinperiod.GlFinperiodID LEFT OUTER JOIN
                         dbo.schClassConfig AS StudentClass ON Student.Class = StudentClass.classcode LEFT OUTER JOIN
                         dbo.Schclasskindlist AS StudentSchclasskind ON Student.Schclasskindid = StudentSchclasskind.Schclasskindid LEFT OUTER JOIN
                         dbo.schFather AS StudentFather ON Student.FatherCode = StudentFather.FatherCode LEFT OUTER JOIN
                         dbo.PyStatus AS StudentFatherMaritalStatus ON StudentFather.MaritalStatus = StudentFatherMaritalStatus.Status_Type LEFT OUTER JOIN
                         dbo.schArea AS StudentFatherarea ON StudentFather.area = StudentFatherarea.Code LEFT OUTER JOIN
                         dbo.PYCntry AS StudentFatherCntry_ ON StudentFather.Cntry_No = StudentFatherCntry_.Cntry_No LEFT OUTER JOIN
                         dbo.PyReljan AS StudentFatherreligion ON StudentFather.religion = StudentFatherreligion.reljan LEFT OUTER JOIN
                         dbo.PyState AS StudentFatherState ON StudentFather.State = StudentFatherState.State LEFT OUTER JOIN
                         dbo.SchFatherjob AS StudentFatherjob_ ON StudentFather.job_no = StudentFatherjob_.job_no LEFT OUTER JOIN
                         dbo.schGrade AS StudentGrade ON Student.GradeCode = StudentGrade.GradeCode LEFT OUTER JOIN
                         dbo.schAcademicyear AS StudentGradeacd ON StudentGrade.acdcode = StudentGradeacd.Code LEFT OUTER JOIN
                         dbo.schStage AS StudentGradeStage ON StudentGrade.StageCode = StudentGradeStage.Code LEFT OUTER JOIN
                         dbo.schMother AS StudentMother ON Student.MotherCode = StudentMother.MotherCode LEFT OUTER JOIN
                         dbo.schArea AS StudentMotherarea ON StudentMother.area = StudentMotherarea.Code LEFT OUTER JOIN
                         dbo.PYCntry AS StudentMotherCntry_ ON StudentMother.Cntry_No = StudentMotherCntry_.Cntry_No LEFT OUTER JOIN
                         dbo.PyReljan AS StudentMotherreligion ON StudentMother.religion = StudentMotherreligion.reljan LEFT OUTER JOIN
                         dbo.PyState AS StudentMotherState ON StudentMother.State = StudentMotherState.State LEFT OUTER JOIN
                         dbo.SchMotherjob AS StudentMotherjob_ ON StudentMother.job_no = StudentMotherjob_.job_no LEFT OUTER JOIN
                         dbo.schStage AS StudentStage ON Student.Stage = StudentStage.Code LEFT OUTER JOIN
                         dbo.schstatus AS Studentschstatus_ ON Student.schstatus_code = Studentschstatus_.schstatus_code LEFT OUTER JOIN
                         dbo.schstclasskinddmg AS Studentschstclasskinddmg ON Student.schstclasskinddmgid = Studentschstclasskinddmg.schstclasskinddmgid LEFT OUTER JOIN
                         dbo.schstespetialest AS Studentschstspl ON Student.schstsplid = Studentschstspl.schstsplid LEFT OUTER JOIN
                         dbo.schsthealthcase AS Studentschsthlthcase ON Student.schsthlthcaseid = Studentschsthlthcase.schsthlthcaseid LEFT OUTER JOIN
                         dbo.Schstudentsefa AS Studentstudentsefa ON Student.studentsefa = Studentstudentsefa.studentsefaid LEFT OUTER JOIN
                         dbo.GLmstrfl AS Studentstudentsefadebitaccount ON Studentstudentsefa.debitaccount = Studentstudentsefadebitaccount.Acc_No LEFT OUTER JOIN
                         dbo.schstudenttyp AS Studentschstudenttyp ON Student.schstudenttypid = Studentschstudenttyp.schstudenttypid LEFT OUTER JOIN
                         dbo.Schtransferlist AS StudentSchtransfer ON Student.Schtransferid = StudentSchtransfer.Schtransferid LEFT OUTER JOIN
                         dbo.schBranch AS Thebranch ON schregtransF.branch = Thebranch.branch LEFT OUTER JOIN
						 dbo.schBranch AS Thebranch1 ON schregtransF.branch1 = Thebranch1.branch LEFT OUTER JOIN
                         dbo.glmulcmp AS Thebranchsgm ON Thebranch.sgm = Thebranchsgm.sgmid LEFT OUTER JOIN
                         dbo.OpCo AS ThebranchsgmOpco ON Thebranchsgm.OpcoID = ThebranchsgmOpco.OpcoIDidentity LEFT OUTER JOIN
                         dbo.PyGender AS ThebranchGender ON Thebranch.Genderid = ThebranchGender.Genderid LEFT OUTER JOIN
                         dbo.schtyp AS Thebranchschtyp ON Thebranch.schtypid = Thebranchschtyp.schtypid LEFT OUTER JOIN
                         dbo.schStage AS Stage ON schregtransF.Stage = Stage.Code LEFT OUTER JOIN
                         dbo.schGrade AS Grade ON schregtransF.GradeCode = Grade.GradeCode LEFT OUTER JOIN
                         dbo.schAcademicyear AS Gradeacd ON Grade.acdcode = Gradeacd.Code LEFT OUTER JOIN
                         dbo.GlFinanceperiod AS GradeacdGlFinperiod ON Gradeacd.GlFinperiodID = GradeacdGlFinperiod.GlFinperiodID LEFT OUTER JOIN
                         dbo.schStage AS GradeStage ON Grade.StageCode = GradeStage.Code LEFT OUTER JOIN
                         dbo.schClassConfig AS Class ON schregtransF.Class = Class.classcode LEFT OUTER JOIN
                         dbo.schstatus AS schstatus_ ON schregtransF.schstatus_code = schstatus_.schstatus_code LEFT OUTER JOIN
                         dbo.schstudenttyp AS schstudenttyp ON schregtransF.schstudenttypid = schstudenttyp.schstudenttypid LEFT OUTER JOIN
                         dbo.schAcademicyear AS acd ON schregtransF.acdcode = acd.Code LEFT OUTER JOIN
                         dbo.GlFinanceperiod AS acdGlFinperiod ON acd.GlFinperiodID = acdGlFinperiod.GlFinperiodID LEFT OUTER JOIN
                         dbo.GLmstrfl AS acdGlFinperiodaccountnumber ON acdGlFinperiod.accountnumber = acdGlFinperiodaccountnumber.Acc_No]]></text>
