<?xml version="1.0" encoding="utf-8"?>
<dataController name="BatchInvoicing" conflictDetection="overwriteChanges" label="Batch Invoicing" plugIn="eZee.Data.AnnotationPlugIn" handler="eZee.Rules.BatchInvoicingBusinessRules" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"BatchInvoicing"."BatchInvoicingid" "BatchInvoicingid"
	,"BatchInvoicing"."BatchInvoicingdate" "BatchInvoicingdate"
	,"BatchInvoicing"."sgm" "sgm"
	,"sgm"."sgm_Nm" "sgmsgm_Nm"
	,"sgmOpco"."OpcoName" "sgmOpcoOpcoName"
	,"BatchInvoicing"."branch" "branch"
	,"Thebranch"."Desc1" "ThebranchDesc1"
	,"Thebranchsgm"."sgm_Nm" "Thebranchsgmsgm_Nm"
	,"ThebranchsgmOpco"."OpcoName" "ThebranchsgmOpcoOpcoName"
	,"ThebranchGender"."Gender" "ThebranchGenderGender"
	,"Thebranchstage"."ShortDesc1" "ThebranchstageShortDesc1"
	,"Thebranchschtyp"."schtypDesc" "ThebranchschtypschtypDesc"
	,"BatchInvoicing"."BatchInvoicingacc_no" "BatchInvoicingacc_no"
	,"BatchInvoicingacc_"."Acc_Nm" "BatchInvoicingacc_Acc_Nm"
	,"BatchInvoicingacc_Clsacc_"."Acc_Nm" "BatchInvoicingacc_Clsacc_Acc_Nm"
	,"BatchInvoicingacc_Acc_Bnd"."Acc_Nm" "BatchInvoicingacc_Acc_BndAcc_Nm"
	,"BatchInvoicingacc_Acc_Bndaccclass"."accountclass" "BatchInvoicingacc_Acc_Bndaccclassaccountclass"
	,"BatchInvoicingacc_Acc_BndAcc_Bab"."Acc_Nm" "BatchInvoicingacc_Acc_BndAcc_BabAcc_Nm"
	,"BatchInvoicing"."BatchInvoicingTypNO" "BatchInvoicingTypNO"
	,"BatchInvoicing"."BatchInvoicingCSUPNO" "BatchInvoicingCSUPNO"
	,"BatchInvoicing"."BatchInvoicingNotes" "BatchInvoicingNotes"
from "dbo"."BatchInvoicing" "BatchInvoicing"
	left join "dbo"."glmulcmp" "sgm" on "BatchInvoicing"."sgm" = "sgm"."sgmid"
	left join "dbo"."OpCo" "sgmOpco" on "sgm"."OpcoID" = "sgmOpco"."OpcoIDidentity"
	left join "dbo"."schBranch" "Thebranch" on "BatchInvoicing"."branch" = "Thebranch"."branch"
	left join "dbo"."glmulcmp" "Thebranchsgm" on "Thebranch"."sgm" = "Thebranchsgm"."sgmid"
	left join "dbo"."OpCo" "ThebranchsgmOpco" on "Thebranchsgm"."OpcoID" = "ThebranchsgmOpco"."OpcoIDidentity"
	left join "dbo"."PyGender" "ThebranchGender" on "Thebranch"."Genderid" = "ThebranchGender"."Genderid"
	left join "dbo"."schStage" "Thebranchstage" on "Thebranch"."stagecode" = "Thebranchstage"."Code"
	left join "dbo"."schtyp" "Thebranchschtyp" on "Thebranch"."schtypid" = "Thebranchschtyp"."schtypid"
	left join "dbo"."GLmstrfl" "BatchInvoicingacc_" on "BatchInvoicing"."BatchInvoicingacc_no" = "BatchInvoicingacc_"."Acc_No"
	left join "dbo"."GLmfbab" "BatchInvoicingacc_Clsacc_" on "BatchInvoicingacc_"."Clsacc_No" = "BatchInvoicingacc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "BatchInvoicingacc_Acc_Bnd" on "BatchInvoicingacc_"."Acc_Bnd" = "BatchInvoicingacc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "BatchInvoicingacc_Acc_Bndaccclass" on "BatchInvoicingacc_Acc_Bnd"."accclass" = "BatchInvoicingacc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "BatchInvoicingacc_Acc_BndAcc_Bab" on "BatchInvoicingacc_Acc_Bnd"."Acc_Bab" = "BatchInvoicingacc_Acc_BndAcc_Bab"."Acc_Bab"
]]></text>
    </command>
    <command id="BatchInvoicingidIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="BatchInvoicingid" />
      </output>
    </command>
  </commands>
