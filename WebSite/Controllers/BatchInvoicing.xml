    <field name="BatchInvoicingacc_Acc_BndAcc_BabAcc_Nm" type="String" readOnly="true" label="Batch Invoicingacc Acc Bnd Acc Bab Acc Nm" length="250" />
    <field name="BatchInvoicingTypNO" type="Int64" label="Batch Invoicing Typ NO" showInSummary="true" />
    <field name="BatchInvoicingCSUPNO" type="Int64" label="Batch Invoicing CSUPNO" />
    <field name="BatchInvoicingNotes" type="String" label="Batch Invoicing Notes" length="50" />
