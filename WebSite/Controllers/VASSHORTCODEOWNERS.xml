  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"VASSHORTCODEOWNERS"."SHORTCODEOWNER" "SHORTCODEOWNER"
	,"VASSHORTCODEOWNERS"."SHORTCODEOWNERName" "SHORTCODEOWNERName"
	,"VASSHORTCODEOWNERS"."SHORTCODEOWNERName1" "SHORTCODEOWNERName1"
	,"VASSHORTCODEOWNERS"."ownercategory" "ownercategory"
	,"ownercategory"."SHORTCADEOWNERMain" "ownercategorySHORTCADEOWNERMain"
	,"ownercategoryAcc_"."Acc_Nm" "ownercategoryAcc_Acc_Nm"
	,"ownercategoryAcc_Clsacc_"."Acc_Nm" "ownercategoryAcc_Clsacc_Acc_Nm"
	,"ownercategoryAcc_Acc_Bnd"."Acc_Nm" "ownercategoryAcc_Acc_BndAcc_Nm"
from "dbo"."VASSHORTCODEOWNERS" "VASSHORTCODEOWNERS"
	left join "dbo"."VASOWNERCATLIST" "ownercategory" on "VASSHORTCODEOWNERS"."ownercategory" = "ownercategory"."SHORTCADEOWNER"
	left join "dbo"."GLmstrfl" "ownercategoryAcc_" on "ownercategory"."Acc_no" = "ownercategoryAcc_"."Acc_No"
	left join "dbo"."GLmfbab" "ownercategoryAcc_Clsacc_" on "ownercategoryAcc_"."Clsacc_No" = "ownercategoryAcc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "ownercategoryAcc_Acc_Bnd" on "ownercategoryAcc_"."Acc_Bnd" = "ownercategoryAcc_Acc_Bnd"."Acc_Bnd"
]]></text>
    </command>
    <command id="SHORTCODEOWNERIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="SHORTCODEOWNER" />
      </output>
    </command>
  </commands>
