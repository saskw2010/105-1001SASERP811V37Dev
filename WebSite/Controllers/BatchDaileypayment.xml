  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"BatchDaileypayment"."BatchDaileypayment" "BatchDaileypayment"
	,"BatchDaileypayment"."Batchstartdate" "Batchstartdate"
	,"BatchDaileypayment"."sgm" "sgm"
	,"sgm"."sgm_Nm" "sgmsgm_Nm"
	,"sgmOpco"."OpcoName" "sgmOpcoOpcoName"
	,"BatchDaileypayment"."branch" "branch"
	,"Thebranch"."Desc1" "ThebranchDesc1"
	,"Thebranchsgm"."sgm_Nm" "Thebranchsgmsgm_Nm"
	,"ThebranchsgmOpco"."OpcoName" "ThebranchsgmOpcoOpcoName"
	,"ThebranchGender"."Gender" "ThebranchGenderGender"
	,"Thebranchstage"."ShortDesc1" "ThebranchstageShortDesc1"
	,"Thebranchschtyp"."schtypDesc" "ThebranchschtypschtypDesc"
	,"BatchDaileypayment"."acc_no" "acc_no"
	,"acc_"."Acc_Nm" "acc_Acc_Nm"
	,"acc_Clsacc_"."Acc_Nm" "acc_Clsacc_Acc_Nm"
	,"acc_Acc_Bnd"."Acc_Nm" "acc_Acc_BndAcc_Nm"
	,"acc_Acc_Bndaccclass"."accountclass" "acc_Acc_Bndaccclassaccountclass"
	,"acc_Acc_BndAcc_Bab"."Acc_Nm" "acc_Acc_BndAcc_BabAcc_Nm"
	,"BatchDaileypayment"."Balancesuppose" "Balancesuppose"
	,"BatchDaileypayment"."Notes" "Notes"
	,"BatchDaileypayment"."acdcode" "acdcode"
	,"acd"."AcademicYear" "acdAcademicYear"
	,"acdGlFinperiod"."Fin_period_info" "acdGlFinperiodFin_period_info"
	,"acdGlFinperiodaccountnumber"."Acc_Nm" "acdGlFinperiodaccountnumberAcc_Nm"
	,"BatchDaileypayment"."RecivedMoney" "RecivedMoney"
	,"BatchDaileypayment"."RecievedDate" "RecievedDate"
	,"BatchDaileypayment"."Pymnt_No" "Pymnt_No"
	,"Pymnt_"."Pymnt_Nm" "Pymnt_Pymnt_Nm"
	,"Pymnt_pymnt_acc"."Acc_Nm" "Pymnt_pymnt_accAcc_Nm"
	,"Pymnt_pymnt_accClsacc_"."Acc_Nm" "Pymnt_pymnt_accClsacc_Acc_Nm"
	,"Pymnt_pymnt_accAcc_Bnd"."Acc_Nm" "Pymnt_pymnt_accAcc_BndAcc_Nm"
	,"Pymnt_sgm"."sgm_Nm" "Pymnt_sgmsgm_Nm"
	,"Pymnt_sgmOpco"."OpcoName" "Pymnt_sgmOpcoOpcoName"
	,"BatchDaileypayment"."paymentwaynumber" "paymentwaynumber"
	,"BatchDaileypayment"."schBnk_No" "schBnk_No"
	,"BatchDaileypayment"."checkAccount" "checkAccount"
	,"BatchDaileypayment"."checkdate" "checkdate"
	,"BatchDaileypayment"."paymentnotes" "paymentnotes"
	,"BatchDaileypayment"."ModifiedBy" "ModifiedBy"
	,"BatchDaileypayment"."ModifiedOn" "ModifiedOn"
	,"BatchDaileypayment"."CreatedBy" "CreatedBy"
	,"BatchDaileypayment"."CreatedOn" "CreatedOn"
	,"BatchDaileypayment"."batchapproved" "batchapproved"
	,"BatchDaileypayment"."batchposted" "batchposted"
	,"BatchDaileypayment"."batchdeleted" "batchdeleted"
	,"BatchDaileypayment"."batchhavdetails" "batchhavdetails"
	,"BatchDaileypayment"."cstyp_NO" "cstyp_NO"
	,"cstyp_"."SubledgerDescrption" "cstyp_SubledgerDescrption"
	,"BatchDaileypayment"."CSMSUP_NO" "CSMSUP_NO"
from "dbo"."BatchDaileypayment" "BatchDaileypayment"
	left join "dbo"."glmulcmp" "sgm" on "BatchDaileypayment"."sgm" = "sgm"."sgmid"
	left join "dbo"."OpCo" "sgmOpco" on "sgm"."OpcoID" = "sgmOpco"."OpcoIDidentity"
	left join "dbo"."schBranch" "Thebranch" on "BatchDaileypayment"."branch" = "Thebranch"."branch"
	left join "dbo"."glmulcmp" "Thebranchsgm" on "Thebranch"."sgm" = "Thebranchsgm"."sgmid"
	left join "dbo"."OpCo" "ThebranchsgmOpco" on "Thebranchsgm"."OpcoID" = "ThebranchsgmOpco"."OpcoIDidentity"
	left join "dbo"."PyGender" "ThebranchGender" on "Thebranch"."Genderid" = "ThebranchGender"."Genderid"
	left join "dbo"."schStage" "Thebranchstage" on "Thebranch"."stagecode" = "Thebranchstage"."Code"
	left join "dbo"."schtyp" "Thebranchschtyp" on "Thebranch"."schtypid" = "Thebranchschtyp"."schtypid"
	left join "dbo"."GLmstrfl" "acc_" on "BatchDaileypayment"."acc_no" = "acc_"."Acc_No"
	left join "dbo"."GLmfbab" "acc_Clsacc_" on "acc_"."Clsacc_No" = "acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "acc_Acc_Bnd" on "acc_"."Acc_Bnd" = "acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "acc_Acc_Bndaccclass" on "acc_Acc_Bnd"."accclass" = "acc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "acc_Acc_BndAcc_Bab" on "acc_Acc_Bnd"."Acc_Bab" = "acc_Acc_BndAcc_Bab"."Acc_Bab"
	left join "dbo"."schAcademicyear" "acd" on "BatchDaileypayment"."acdcode" = "acd"."Code"
	left join "dbo"."GlFinanceperiod" "acdGlFinperiod" on "acd"."GlFinperiodID" = "acdGlFinperiod"."GlFinperiodID"
	left join "dbo"."GLmstrfl" "acdGlFinperiodaccountnumber" on "acdGlFinperiod"."accountnumber" = "acdGlFinperiodaccountnumber"."Acc_No"
	left join "dbo"."Pymntype" "Pymnt_" on "BatchDaileypayment"."Pymnt_No" = "Pymnt_"."Pymnt_No"
	left join "dbo"."GLmstrfl" "Pymnt_pymnt_acc" on "Pymnt_"."pymnt_acc" = "Pymnt_pymnt_acc"."Acc_No"
	left join "dbo"."GLmfbab" "Pymnt_pymnt_accClsacc_" on "Pymnt_pymnt_acc"."Clsacc_No" = "Pymnt_pymnt_accClsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Pymnt_pymnt_accAcc_Bnd" on "Pymnt_pymnt_acc"."Acc_Bnd" = "Pymnt_pymnt_accAcc_Bnd"."Acc_Bnd"
	left join "dbo"."glmulcmp" "Pymnt_sgm" on "Pymnt_"."sgm" = "Pymnt_sgm"."sgmid"
	left join "dbo"."OpCo" "Pymnt_sgmOpco" on "Pymnt_sgm"."OpcoID" = "Pymnt_sgmOpco"."OpcoIDidentity"
	left join "dbo"."GLSubledgertyplist" "cstyp_" on "BatchDaileypayment"."cstyp_NO" = "cstyp_"."Subledgerid"
]]></text>
    </command>
