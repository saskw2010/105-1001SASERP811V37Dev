	,"Thebranchstage"."ShortDesc1" "ThebranchstageShortDesc1"
	,"Thebranchschtyp"."schtypDesc" "ThebranchschtypschtypDesc"
	,"Gljrnvchhdr"."sgm" "sgm"
	,"sgm"."sgm_Nm" "sgmsgm_Nm"
	,"sgmOpco"."OpcoName" "sgmOpcoOpcoName"
	,"Gljrnvchhdr"."Jr_Ty" "Jr_Ty"
	,"TheJr_Ty"."Jrty_Nme" "TheJr_TyJrty_Nme"
	,"Gljrnvchhdr"."GlFinperiodID" "GlFinperiodID"
	,"GlFinperiod"."Fin_period_info" "GlFinperiodFin_period_info"
	,"GlFinperiodaccountnumber"."Acc_Nm" "GlFinperiodaccountnumberAcc_Nm"
	,"GlFinperiodaccountnumberClsacc_"."Acc_Nm" "GlFinperiodaccountnumberClsacc_Acc_Nm"
	,"GlFinperiodaccountnumberAcc_Bnd"."Acc_Nm" "GlFinperiodaccountnumberAcc_BndAcc_Nm"
	,"Gljrnvchhdr"."Trs_Dt" "Trs_Dt"
	,"Gljrnvchhdr"."Manualcode" "Manualcode"
	,"Gljrnvchhdr"."Tr_Ds" "Tr_Ds"
	,"Gljrnvchhdr"."Balance" "Balance"
	,"Gljrnvchhdr"."Notes" "Notes"
	,"Gljrnvchhdr"."approved" "approved"
	,"Gljrnvchhdr"."posted" "posted"
	,"Gljrnvchhdr"."deleted" "deleted"
	,"Gljrnvchhdr"."havdetails" "havdetails"
	,"Gljrnvchhdr"."ModifiedBy" "ModifiedBy"
	,"Gljrnvchhdr"."ModifiedOn" "ModifiedOn"
	,"Gljrnvchhdr"."CreatedBy" "CreatedBy"
	,"Gljrnvchhdr"."CreatedOn" "CreatedOn"
	,"Gljrnvchhdr"."auditing" "auditing"
	,"Gljrnvchhdr"."auditnotes" "auditnotes"
                , dbo.Qbalancegljrnvch.Expr1, dbo.Qbalancegljrnvch.Expr2
from "dbo"."Gljrnvchhdr" "Gljrnvchhdr"
LEFT OUTER JOIN  dbo.Qbalancegljrnvch ON Gljrnvchhdr.Tr_No = dbo.Qbalancegljrnvch.Tr_No
	left join "dbo"."schBranch" "Thebranch" on "Gljrnvchhdr"."branch" = "Thebranch"."branch"
	left join "dbo"."glmulcmp" "Thebranchsgm" on "Thebranch"."sgm" = "Thebranchsgm"."sgmid"
	left join "dbo"."OpCo" "ThebranchsgmOpco" on "Thebranchsgm"."OpcoID" = "ThebranchsgmOpco"."OpcoIDidentity"
	left join "dbo"."PyGender" "ThebranchGender" on "Thebranch"."Genderid" = "ThebranchGender"."Genderid"
	left join "dbo"."schStage" "Thebranchstage" on "Thebranch"."stagecode" = "Thebranchstage"."Code"
	left join "dbo"."schtyp" "Thebranchschtyp" on "Thebranch"."schtypid" = "Thebranchschtyp"."schtypid"
	left join "dbo"."glmulcmp" "sgm" on "Gljrnvchhdr"."sgm" = "sgm"."sgmid"
	left join "dbo"."OpCo" "sgmOpco" on "sgm"."OpcoID" = "sgmOpco"."OpcoIDidentity"
	left join "dbo"."GLjrntyp" "TheJr_Ty" on "Gljrnvchhdr"."Jr_Ty" = "TheJr_Ty"."Jr_Ty"
	left join "dbo"."GlFinanceperiod" "GlFinperiod" on "Gljrnvchhdr"."GlFinperiodID" = "GlFinperiod"."GlFinperiodID"
	left join "dbo"."GLmstrfl" "GlFinperiodaccountnumber" on "GlFinperiod"."accountnumber" = "GlFinperiodaccountnumber"."Acc_No"
	left join "dbo"."GLmfbab" "GlFinperiodaccountnumberClsacc_" on "GlFinperiodaccountnumber"."Clsacc_No" = "GlFinperiodaccountnumberClsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "GlFinperiodaccountnumberAcc_Bnd" on "GlFinperiodaccountnumber"."Acc_Bnd" = "GlFinperiodaccountnumberAcc_Bnd"."Acc_Bnd"
]]></text>
    </command>
