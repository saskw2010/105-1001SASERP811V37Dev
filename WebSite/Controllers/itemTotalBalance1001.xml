    <command id="command1" type="Text" tableName="ITEMS" custom="true">
      <text><![CDATA[select
	"ITEMS"."ID_comp" "ID_comp"
	,"ITEMS"."IDGROUP" "IDGROUP"
	,"TheIDGROUP"."ID_USER" "TheIDGROUPID_USER"
	,"ITEMS"."ID_USER" "ID_USER"
	,"ITEMS"."Description" "Description"
	,"ITEMS"."ITEM" "ITEM"
	,"ITEMS"."ITEME" "ITEME"
	,"ITEMS"."Sup_No" "Sup_No"
	,"Sup_"."Item_name" "Sup_Item_name"
	,"ITEMS"."Unit_No" "Unit_No"
	,"ITEMS"."Itm_Pur" "Itm_Pur"
	,"ITEMS"."Itm_COst" "Itm_COst"
	,"ITEMS"."Itm_Sal1" "Itm_Sal1"
	,"ITEMS"."Itm_Sal2" "Itm_Sal2"
	,"ITEMS"."Itm_Othr" "Itm_Othr"
	,"ITEMS"."Itm_size" "Itm_size"
	,"ITEMS"."Itm_colr" "Itm_colr"
	,"ITEMS"."Barcod1" "Barcod1"
	,"ITEMS"."Barcod2" "Barcod2"
	,"ITEMS"."combin" "combin"
	,"ITEMS"."openblnc" "openblnc"
	,"ITEMS"."talbhad" "talbhad"
	,"ITEMS"."sarfhad" "sarfhad"
	,"ITEMS"."notes" "notes"
	,"ITEMS"."id1" "id1"
	,"id1"."DESCRP1" "id1DESCRP1"
	,"ITEMS"."id2" "id2"
	,"id2"."DESCRP1" "id2DESCRP1"
	,"ITEMS"."id3" "id3"
	,"Theid3"."DESCRP1" "Theid3DESCRP1"
	,"ITEMS"."id4" "id4"
	,"Theid4"."DESCRP1" "Theid4DESCRP1"
	,"ITEMS"."id5" "id5"
	,"Theid5"."DESCRP1" "Theid5DESCRP1"
	,"ITEMS"."id6" "id6"
	,"Theid6"."DESCRP1" "Theid6DESCRP1"
	,"ITEMS"."id7" "id7"
	,"Theid7"."DESCRP1" "Theid7DESCRP1"
	,"ITEMS"."ModifiedBy" "ModifiedBy"
	,"ITEMS"."ModifiedOn" "ModifiedOn"
	,"ITEMS"."CreatedBy" "CreatedBy"
	,"ITEMS"."CreatedOn" "CreatedOn"
, TheIDGROUP.GROUPA, dbo.itemTotalBalance.balance AS Qbalance
,
                      dbo.itemTotalBalance.lastprice
from "dbo"."ITEMS" "ITEMS"
LEFT OUTER JOIN
                      dbo.itemTotalBalance ON ITEMS.ID_comp = dbo.itemTotalBalance.ID_comp
	left join "dbo"."GROUPStk" "TheIDGROUP" on "ITEMS"."IDGROUP" = "TheIDGROUP"."IDGROUP"
	left join "dbo"."ItemList" "Sup_" on "ITEMS"."Sup_No" = "Sup_"."ItemNo"
	left join "dbo"."PYHldtypsv1" "id1" on "ITEMS"."id1" = "id1"."ID1"
	left join "dbo"."PYHldtypsv2" "id2" on "ITEMS"."id2" = "id2"."ID2"
	left join "dbo"."PYHldtypsv3" "Theid3" on "ITEMS"."id3" = "Theid3"."id3"
	left join "dbo"."PYHldtypsv4" "Theid4" on "ITEMS"."id4" = "Theid4"."id4"
	left join "dbo"."PYHldtypsv5" "Theid5" on "ITEMS"."id5" = "Theid5"."id5"
	left join "dbo"."PYHldtypsv6" "Theid6" on "ITEMS"."id6" = "Theid6"."id6"
	left join "dbo"."PYHldtypsv7" "Theid7" on "ITEMS"."id7" = "Theid7"."id7"
]]></text>
    </command>
