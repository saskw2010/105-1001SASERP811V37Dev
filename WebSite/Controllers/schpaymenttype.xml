  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"schpaymenttype"."schpaymenttypeid" "schpaymenttypeid"
	,"schpaymenttype"."schpaymenttypenm" "schpaymenttypenm"
	,"schpaymenttype"."schpaymenttypenme" "schpaymenttypenme"
	,"schpaymenttype"."schpaymenttypaccount" "schpaymenttypaccount"
	,"schpaymenttype"."debitaccount" "debitaccount"
	,"debitaccount"."Acc_Nm" "debitaccountAcc_Nm"
	,"debitaccountClsacc_"."Acc_Nm" "debitaccountClsacc_Acc_Nm"
	,"debitaccountAcc_Bnd"."Acc_Nm" "debitaccountAcc_BndAcc_Nm"
	,"debitaccountAcc_Bndaccclass"."accountclass" "debitaccountAcc_Bndaccclassaccountclass"
	,"debitaccountAcc_BndAcc_Bab"."Acc_Nm" "debitaccountAcc_BndAcc_BabAcc_Nm"
	,"schpaymenttype"."creditaccount" "creditaccount"
	,"creditaccount"."Acc_Nm" "creditaccountAcc_Nm"
	,"creditaccountClsacc_"."Acc_Nm" "creditaccountClsacc_Acc_Nm"
	,"creditaccountAcc_Bnd"."Acc_Nm" "creditaccountAcc_BndAcc_Nm"
	,"creditaccountAcc_Bndaccclass"."accountclass" "creditaccountAcc_Bndaccclassaccountclass"
	,"creditaccountAcc_BndAcc_Bab"."Acc_Nm" "creditaccountAcc_BndAcc_BabAcc_Nm"
	,"schpaymenttype"."ModifiedBy" "ModifiedBy"
	,"schpaymenttype"."ModifiedOn" "ModifiedOn"
	,"schpaymenttype"."CreatedBy" "CreatedBy"
	,"schpaymenttype"."CreatedOn" "CreatedOn"
from "dbo"."schpaymenttype" "schpaymenttype"
	left join "dbo"."GLmstrfl" "debitaccount" on "schpaymenttype"."debitaccount" = "debitaccount"."Acc_No"
	left join "dbo"."GLmfbab" "debitaccountClsacc_" on "debitaccount"."Clsacc_No" = "debitaccountClsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "debitaccountAcc_Bnd" on "debitaccount"."Acc_Bnd" = "debitaccountAcc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "debitaccountAcc_Bndaccclass" on "debitaccountAcc_Bnd"."accclass" = "debitaccountAcc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "debitaccountAcc_BndAcc_Bab" on "debitaccountAcc_Bnd"."Acc_Bab" = "debitaccountAcc_BndAcc_Bab"."Acc_Bab"
	left join "dbo"."GLmstrfl" "creditaccount" on "schpaymenttype"."creditaccount" = "creditaccount"."Acc_No"
	left join "dbo"."GLmfbab" "creditaccountClsacc_" on "creditaccount"."Clsacc_No" = "creditaccountClsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "creditaccountAcc_Bnd" on "creditaccount"."Acc_Bnd" = "creditaccountAcc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "creditaccountAcc_Bndaccclass" on "creditaccountAcc_Bnd"."accclass" = "creditaccountAcc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "creditaccountAcc_BndAcc_Bab" on "creditaccountAcc_Bnd"."Acc_Bab" = "creditaccountAcc_BndAcc_Bab"."Acc_Bab"
]]></text>
    </command>
    <command id="schpaymenttypeidIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="schpaymenttypeid" />
      </output>
    </command>
  </commands>
