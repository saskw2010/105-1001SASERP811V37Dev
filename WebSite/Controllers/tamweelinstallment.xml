    <command id="command1" type="Text">
      <text><![CDATA[
select
	"tamweelinstallment"."installmentid" "installmentid"
	,"tamweelinstallment"."Tamweelreqid" "Tamweelreqid"
	,"Tamweelreq"."Tamweeldeatails" "TamweelreqTamweeldeatails"
	,"TamweelreqCurncy_"."Curncy_Nm" "TamweelreqCurncy_Curncy_Nm"
	,"TamweelreqCstm_"."Cstm_Nm" "TamweelreqCstm_Cstm_Nm"
	,"TamweelreqCstm_Area"."Area_name" "TamweelreqCstm_AreaArea_name"
	,"TamweelreqCstm_Evaluation"."Evaluation_name" "TamweelreqCstm_EvaluationEvaluation_name"
	,"TamweelreqCstm_acc_"."Acc_Nm" "TamweelreqCstm_acc_Acc_Nm"
	,"TamweelreqCstm_Cntry_"."Cntry_Nm" "TamweelreqCstm_Cntry_Cntry_Nm"
	,"TamweelreqCstm_curncy_"."Curncy_Nm" "TamweelreqCstm_curncy_Curncy_Nm"
	,"TamweelreqCstm_TSNEF"."TSNEF_name" "TamweelreqCstm_TSNEFTSNEF_name"
	,"TamweelreqCstm_TSNEFNO1"."TSNEF1_name" "TamweelreqCstm_TSNEFNO1TSNEF1_name"
	,"TamweelreqCstm_Slm_"."Slm_Nm" "TamweelreqCstm_Slm_Slm_Nm"
	,"Tamweelreqtasedway"."tasedwayname" "Tamweelreqtasedwaytasedwayname"
	,"tamweelinstallment"."installmentdate" "installmentdate"
	,"tamweelinstallment"."installmentamount" "installmentamount"
	,"tamweelinstallment"."Banksname" "Banksname"
	,"tamweelinstallment"."Checkno" "Checkno"
	,"tamweelinstallment"."Notes" "Notes"
	,"tamweelinstallment"."ModifiedBy" "ModifiedBy"
	,"tamweelinstallment"."ModifiedByUserId" "ModifiedByUserId"
	,"tamweelinstallment"."ModifiedOn" "ModifiedOn"
	,"tamweelinstallment"."CreatedBy" "CreatedBy"
	,"tamweelinstallment"."CreatedById" "CreatedById"
	,"tamweelinstallment"."CreatedOn" "CreatedOn"
from "dbo"."tamweelinstallment" "tamweelinstallment"
	left join "dbo"."Tamweelrequest" "Tamweelreq" on "tamweelinstallment"."Tamweelreqid" = "Tamweelreq"."Tamweelreqid"
	left join "dbo"."Stcurncy" "TamweelreqCurncy_" on "Tamweelreq"."Curncy_No" = "TamweelreqCurncy_"."Curncy_No"
	left join "dbo"."Stcustmr" "TamweelreqCstm_" on "Tamweelreq"."Cstm_No" = "TamweelreqCstm_"."Cstm_No"
	left join "dbo"."AreaList" "TamweelreqCstm_Area" on "TamweelreqCstm_"."AreaNo" = "TamweelreqCstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "TamweelreqCstm_Evaluation" on "TamweelreqCstm_"."EvaluationNO" = "TamweelreqCstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "TamweelreqCstm_acc_" on "TamweelreqCstm_"."acc_no" = "TamweelreqCstm_acc_"."Acc_No"
	left join "dbo"."PYCntry" "TamweelreqCstm_Cntry_" on "TamweelreqCstm_"."Cntry_No" = "TamweelreqCstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "TamweelreqCstm_curncy_" on "TamweelreqCstm_"."curncy_no" = "TamweelreqCstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "TamweelreqCstm_TSNEF" on "TamweelreqCstm_"."TSNEFNO" = "TamweelreqCstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "TamweelreqCstm_TSNEFNO1" on "TamweelreqCstm_"."TSNEFNO1" = "TamweelreqCstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "TamweelreqCstm_Slm_" on "TamweelreqCstm_"."Slm_No" = "TamweelreqCstm_Slm_"."Slm_No"
	left join "dbo"."tasedwaylist" "Tamweelreqtasedway" on "Tamweelreq"."tasedwayid" = "Tamweelreqtasedway"."tasedwayid"
]]></text>
    </command>
