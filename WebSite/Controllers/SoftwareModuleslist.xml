    <command id="command1" type="Text">
      <text><![CDATA[
select
	"SoftwareModuleslist"."sprModuleid" "sprModuleid"
	,"SoftwareModuleslist"."sprModuleDesc" "sprModuleDesc"
	,"SoftwareModuleslist"."sprModuleDesc1" "sprModuleDesc1"
	,"SoftwareModuleslist"."FutureDesc" "FutureDesc"
