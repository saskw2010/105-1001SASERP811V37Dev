	,"SoftwareModuleslist"."imagedesc" "imagedesc"
	,"SoftwareModuleslist"."InstallModule" "InstallModule"
	,"SoftwareModuleslist"."sprModulecatid" "sprModulecatid"
	,"sprModulecat"."sprModulecatDesc" "sprModulecatsprModulecatDesc"
from "dbo"."SoftwareModuleslist" "SoftwareModuleslist"
	left join "dbo"."SoftwareModulescatlist" "sprModulecat" on "SoftwareModuleslist"."sprModulecatid" = "sprModulecat"."sprModulecatid"
]]></text>
    </command>
    <command id="sprModuleidIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="sprModuleid" />
      </output>
    </command>
  </commands>
