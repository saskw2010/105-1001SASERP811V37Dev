    <command id="command1" type="Text">
      <text><![CDATA[
select
	"GLjrnvchslpr"."Ln_No" "Ln_No"
	,"GLjrnvchslpr"."Tr_No" "Tr_No"
	,"Tr_"."Pur_Wys" "Tr_Pur_Wys"
	,"Tr_brn_"."Brn_Nm" "Tr_brn_Brn_Nm"
	,"Tr_brn_Branch"."Desc1" "Tr_brn_BranchDesc1"
	,"Tr_Pym_"."PaymentTypeName" "Tr_Pym_PaymentTypeName"
	,"Tr_Curncy_"."Curncy_Nm" "Tr_Curncy_Curncy_Nm"
	,"Tr_Sup_"."Sup_Nm" "Tr_Sup_Sup_Nm"
	,"Tr_Sup_Acc_"."Acc_Nm" "Tr_Sup_Acc_Acc_Nm"
	,"Tr_Sup_sgm"."sgm_Nm" "Tr_Sup_sgmsgm_Nm"
	,"Tr_Sup_Cntry_"."Cntry_Nm" "Tr_Sup_Cntry_Cntry_Nm"
	,"Tr_Sup_Curncy_"."Curncy_Nm" "Tr_Sup_Curncy_Curncy_Nm"
	,"GLjrnvchslpr"."Tr_Dt" "Tr_Dt"
	,"GLjrnvchslpr"."Acc_No" "Acc_No"
	,"Acc_"."Acc_Nm" "Acc_Acc_Nm"
	,"Acc_Clsacc_"."Acc_Nm" "Acc_Clsacc_Acc_Nm"
	,"Acc_Acc_Bnd"."Acc_Nm" "Acc_Acc_BndAcc_Nm"
	,"Acc_Acc_Bndaccclass"."accountclass" "Acc_Acc_Bndaccclassaccountclass"
	,"Acc_Acc_BndAcc_Bab"."Acc_Nm" "Acc_Acc_BndAcc_BabAcc_Nm"
	,"GLjrnvchslpr"."Tr_Db" "Tr_Db"
	,"GLjrnvchslpr"."Tr_Cr" "Tr_Cr"
	,"GLjrnvchslpr"."Dc_No" "Dc_No"
	,"GLjrnvchslpr"."Clsacc_No" "Clsacc_No"
	,"GLjrnvchslpr"."Costcntr_No" "Costcntr_No"
	,"GLjrnvchslpr"."Tr_Dsc1" "Tr_Dsc1"
	,"GLjrnvchslpr"."docrelno" "docrelno"
	,"GLjrnvchslpr"."docreltyp" "docreltyp"
	,"GLjrnvchslpr"."curno" "curno"
	,"GLjrnvchslpr"."curprice" "curprice"
	,"GLjrnvchslpr"."curvalue" "curvalue"
	,"GLjrnvchslpr"."curvalue1" "curvalue1"
	,"GLjrnvchslpr"."cstyp_NO" "cstyp_NO"
	,"cstyp_"."SubledgerDescrption" "cstyp_SubledgerDescrption"
	,"GLjrnvchslpr"."CSMSUP_NO" "CSMSUP_NO"
	,"GLjrnvchslpr"."CSMSUP_NM" "CSMSUP_NM"
	,"GLjrnvchslpr"."trs_dt1" "trs_dt1"
	,"GLjrnvchslpr"."ModifiedBy" "ModifiedBy"
	,"GLjrnvchslpr"."ModifiedOn" "ModifiedOn"
	,"GLjrnvchslpr"."CreatedBy" "CreatedBy"
	,"GLjrnvchslpr"."CreatedOn" "CreatedOn"
from "dbo"."GLjrnvchslpr" "GLjrnvchslpr"
	left join "dbo"."StLcHdr" "Tr_" on "GLjrnvchslpr"."Tr_No" = "Tr_"."Doc_No"
	left join "dbo"."Branchcnst" "Tr_brn_" on "Tr_"."brn_no" = "Tr_brn_"."Brn_No"
	left join "dbo"."schBranch" "Tr_brn_Branch" on "Tr_brn_"."Branch" = "Tr_brn_Branch"."branch"
	left join "dbo"."PaymentTypes" "Tr_Pym_" on "Tr_"."Pym_No" = "Tr_Pym_"."PaymentTypeID"
	left join "dbo"."Stcurncy" "Tr_Curncy_" on "Tr_"."Curncy_No" = "Tr_Curncy_"."Curncy_No"
	left join "dbo"."Stsuplir" "Tr_Sup_" on "Tr_"."Sup_No" = "Tr_Sup_"."Sup_No"
	left join "dbo"."GLmstrfl" "Tr_Sup_Acc_" on "Tr_Sup_"."Acc_No" = "Tr_Sup_Acc_"."Acc_No"
	left join "dbo"."glmulcmp" "Tr_Sup_sgm" on "Tr_Sup_"."sgm" = "Tr_Sup_sgm"."sgmid"
	left join "dbo"."PYCntry" "Tr_Sup_Cntry_" on "Tr_Sup_"."Cntry_No" = "Tr_Sup_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Tr_Sup_Curncy_" on "Tr_Sup_"."Curncy_No" = "Tr_Sup_Curncy_"."Curncy_No"
	left join "dbo"."GLmstrfl" "Acc_" on "GLjrnvchslpr"."Acc_No" = "Acc_"."Acc_No"
	left join "dbo"."GLmfbab" "Acc_Clsacc_" on "Acc_"."Clsacc_No" = "Acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Acc_Acc_Bnd" on "Acc_"."Acc_Bnd" = "Acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "Acc_Acc_Bndaccclass" on "Acc_Acc_Bnd"."accclass" = "Acc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "Acc_Acc_BndAcc_Bab" on "Acc_Acc_Bnd"."Acc_Bab" = "Acc_Acc_BndAcc_Bab"."Acc_Bab"
	left join "dbo"."GLSubledgertyplist" "cstyp_" on "GLjrnvchslpr"."cstyp_NO" = "cstyp_"."Subledgerid"
]]></text>
    </command>
