  <businessRules>
<rule id="r107j" type="Sql" commandName="Insert|Update" phase="Before" ><![CDATA[
 if @curprice is null
 begin
 set @curprice=1
 end
 if @curvalue1 is null
 begin
 set @curvalue1=0
 end
 if @curvalue is null
 begin
 set @curvalue=0
 end
 Set @Tr_Cr=Round(@curvalue1*@curprice,3)
 Set @Tr_Db=Round(@curvalue*@curprice,3)
]]></rule>

