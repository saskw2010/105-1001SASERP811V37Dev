	,"Brn_"."Brn_Nm" "Brn_Brn_Nm"
	,"Brn_Branch"."Desc1" "Brn_BranchDesc1"
	,"Brn_Branchsgm"."sgm_Nm" "Brn_Branchsgmsgm_Nm"
	,"Brn_BranchGender"."Gender" "Brn_BranchGenderGender"
	,"Brn_Branchstage"."ShortDesc1" "Brn_BranchstageShortDesc1"
	,"Brn_Branchschtyp"."schtypDesc" "Brn_BranchschtypschtypDesc"
	,"azonhdrtrnsfer"."TRFBrn_NO" "TRFBrn_NO"
	,"TRFBrn_"."Brn_Nm" "TRFBrn_Brn_Nm"
	,"TRFBrn_Branch"."Desc1" "TRFBrn_BranchDesc1"
	,"TRFBrn_Branchsgm"."sgm_Nm" "TRFBrn_Branchsgmsgm_Nm"
	,"TRFBrn_BranchGender"."Gender" "TRFBrn_BranchGenderGender"
	,"TRFBrn_Branchstage"."ShortDesc1" "TRFBrn_BranchstageShortDesc1"
	,"TRFBrn_Branchschtyp"."schtypDesc" "TRFBrn_BranchschtypschtypDesc"
	,"azonhdrtrnsfer"."subject" "subject"
	,"azonhdrtrnsfer"."azndate" "azndate"
	,"azonhdrtrnsfer"."recivedflag" "recivedflag"
	,"azonhdrtrnsfer"."ModifiedBy" "ModifiedBy"
	,"azonhdrtrnsfer"."ModifiedOn" "ModifiedOn"
	,"azonhdrtrnsfer"."CreatedBy" "CreatedBy"
	,"azonhdrtrnsfer"."CreatedOn" "CreatedOn"
	,"azonhdrtrnsfer"."KETAA" "KETAA"
	,"azonhdrtrnsfer"."STreet" "STreet"
	,"azonhdrtrnsfer"."quantity" "quantity"
	,"azonhdrtrnsfer"."ID_USER" "ID_USER"
from "dbo"."azonhdrtrnsfer" "azonhdrtrnsfer"
	left join "dbo"."Branchcnst" "Brn_" on "azonhdrtrnsfer"."Brn_No" = "Brn_"."Brn_No"
	left join "dbo"."schBranch" "Brn_Branch" on "Brn_"."Branch" = "Brn_Branch"."branch"
	left join "dbo"."glmulcmp" "Brn_Branchsgm" on "Brn_Branch"."sgm" = "Brn_Branchsgm"."sgmid"
	left join "dbo"."PyGender" "Brn_BranchGender" on "Brn_Branch"."Genderid" = "Brn_BranchGender"."Genderid"
	left join "dbo"."schStage" "Brn_Branchstage" on "Brn_Branch"."stagecode" = "Brn_Branchstage"."Code"
	left join "dbo"."schtyp" "Brn_Branchschtyp" on "Brn_Branch"."schtypid" = "Brn_Branchschtyp"."schtypid"
	left join "dbo"."Branchcnst" "TRFBrn_" on "azonhdrtrnsfer"."TRFBrn_NO" = "TRFBrn_"."Brn_No"
	left join "dbo"."schBranch" "TRFBrn_Branch" on "TRFBrn_"."Branch" = "TRFBrn_Branch"."branch"
	left join "dbo"."glmulcmp" "TRFBrn_Branchsgm" on "TRFBrn_Branch"."sgm" = "TRFBrn_Branchsgm"."sgmid"
	left join "dbo"."PyGender" "TRFBrn_BranchGender" on "TRFBrn_Branch"."Genderid" = "TRFBrn_BranchGender"."Genderid"
	left join "dbo"."schStage" "TRFBrn_Branchstage" on "TRFBrn_Branch"."stagecode" = "TRFBrn_Branchstage"."Code"
	left join "dbo"."schtyp" "TRFBrn_Branchschtyp" on "TRFBrn_Branch"."schtypid" = "TRFBrn_Branchschtyp"."schtypid"
]]></text>
