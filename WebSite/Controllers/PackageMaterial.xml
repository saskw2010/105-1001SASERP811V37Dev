  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"PackageMaterial"."ID_comp" "ID_comp"
	,"PackageMaterial"."CatgID" "CatgID"
	,"PackageMaterial"."ID_USER" "ID_USER"
	,"PackageMaterial"."ITEM" "ITEM"
	,"PackageMaterial"."ITEME" "ITEME"
