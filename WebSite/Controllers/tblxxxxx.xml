<?xml version="1.0" encoding="utf-8"?>
<dataController name="Userdetailsegment" conflictDetection="overwriteChanges" label="Userdetailsegment" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Userdetailsegment"."autoId" "autoId"
	,"Userdetailsegment"."UserName" "UserName"
	,"Userdetailsegment"."branch" "branch"
	,"Thebranch"."Desc1" "ThebranchDesc1"
	,"Thebranchsgm"."sgm_Nm" "Thebranchsgmsgm_Nm"
	,"ThebranchsgmOpco"."OpcoName" "ThebranchsgmOpcoOpcoName"
	,"ThebranchGender"."Gender" "ThebranchGenderGender"
	,"Thebranchstage"."ShortDesc1" "ThebranchstageShortDesc1"
	,"Thebranchschtyp"."schtypDesc" "ThebranchschtypschtypDesc"
from "dbo"."Userdetailsegment" "Userdetailsegment"
	left outer join "dbo"."schBranch" "Thebranch" on "Userdetailsegment"."branch" = "Thebranch"."branch"
	left outer join "dbo"."glmulcmp" "Thebranchsgm" on "Thebranch"."sgm" = "Thebranchsgm"."sgmid"
	left outer join "dbo"."OpCo" "ThebranchsgmOpco" on "Thebranchsgm"."OpcoID" = "ThebranchsgmOpco"."OpcoIDidentity"
	left outer join "dbo"."PyGender" "ThebranchGender" on "Thebranch"."Genderid" = "ThebranchGender"."Genderid"
	left outer join "dbo"."schStage" "Thebranchstage" on "Thebranch"."stagecode" = "Thebranchstage"."Code"
	left outer join "dbo"."schtyp" "Thebranchschtyp" on "Thebranch"."schtypid" = "Thebranchschtyp"."schtypid"
]]></text>
    </command>
    <command id="autoIdIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="autoId" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="autoId" type="Int32" allowNulls="false" isPrimaryKey="true" label="auto#" readOnly="true" />
    <field name="UserName" type="String" allowNulls="false" label="User Name" length="256" showInSummary="true">
      
    </field>
    <field name="branch" type="Int64" label="Branch" showInSummary="true">
      <items style="DropDownList" dataController="schBranch" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="ThebranchDesc1" type="String" readOnly="true" label="Thebranch Desc1" length="50" />
    <field name="Thebranchsgmsgm_Nm" type="String" readOnly="true" label="Thebranchsgmsgm Nm" length="255" />
    <field name="ThebranchsgmOpcoOpcoName" type="String" readOnly="true" label="Thebranchsgm Opco Name" length="50" />
    <field name="ThebranchGenderGender" type="String" readOnly="true" label="Thebranch Gender" length="100" />
    <field name="ThebranchstageShortDesc1" type="String" readOnly="true" label="Thebranchstage Short Desc1" length="50" />
    <field name="ThebranchschtypschtypDesc" type="String" readOnly="true" label="Thebranchschtypschtyp Desc" length="150" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Userdetailsegment">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="UserName" />
        <dataField fieldName="branch" aliasFieldName="ThebranchDesc1"  />
        <dataField fieldName="Thebranchsgmsgm_Nm" />
        <dataField fieldName="ThebranchsgmOpcoOpcoName" columns="50" />
        <dataField fieldName="ThebranchGenderGender" />
        <dataField fieldName="ThebranchstageShortDesc1" columns="50" />
        <dataField fieldName="ThebranchschtypschtypDesc" />
        <dataField fieldName="Thesgmsgm_Nm" />
        <dataField fieldName="autoId" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Userdetailsegment">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Userdetailsegment" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="UserName" />
            <dataField fieldName="branch" aliasFieldName="ThebranchDesc1" hyperlinkFormatString="~/Pages/schBranch.aspx?branch={branch}&amp;_controller=schBranch&amp;_commandName=Select&amp;_commandArgument=editForm1" />
          </dataFields>
        </category>
        <category id="c2" headerText="Reference Information">
          <description><![CDATA[$DefaultReferenceDescription]]></description>
          <dataFields>
            <dataField fieldName="Thebranchsgmsgm_Nm" />
            <dataField fieldName="ThebranchsgmOpcoOpcoName" columns="50" />
            <dataField fieldName="ThebranchGenderGender" />
            <dataField fieldName="ThebranchstageShortDesc1" columns="50" />
            <dataField fieldName="ThebranchschtypschtypDesc" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Userdetailsegment">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New Userdetailsegment" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="UserName" />
            <dataField fieldName="branch" aliasFieldName="ThebranchDesc1" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
  <actions>
    <actionGroup id="ag1" scope="Grid">
      <action id="a1" commandName="Select" commandArgument="editForm1" />
      <action id="a2" commandName="Edit" />
      <action id="a3" commandName="Delete" />
      <action id="a6" />
      <action id="a7" commandName="New" commandArgument="grid1" />
      <action id="a8" commandName="Duplicate" commandArgument="createForm1" />
      <action id="a9" />
      <action id="a10" commandName="BatchEdit" />
      <action id="a11" commandName="BatchEdit" commandArgument="editForm1" />
    </actionGroup>
    <actionGroup id="ag2" scope="Form">
      <action id="a1" commandName="Edit" />
      <action id="a2" commandName="Delete" />
      <action id="a3" commandName="Cancel" />
      <action id="a4" whenLastCommandName="Edit" commandName="Update" />
      <action id="a5" whenLastCommandName="Edit" commandName="Delete" />
      <action id="a6" whenLastCommandName="Edit" commandName="Cancel" />
      <action id="a7" whenLastCommandName="New" commandName="Insert" />
      <action id="a8" whenLastCommandName="New" commandName="Cancel" />
      <action id="a9" whenLastCommandName="Duplicate" commandName="Insert" />
      <action id="a10" whenLastCommandName="Duplicate" commandName="Cancel" />
      <action id="a11" whenLastCommandName="BatchEdit" commandName="Update" />
