	,"schpaydlyhdreg"."Notes" "Notes"
	,"schpaydlyhdreg"."ModifiedBy" "ModifiedBy"
	,"schpaydlyhdreg"."ModifiedOn" "ModifiedOn"
	,"schpaydlyhdreg"."CreatedBy" "CreatedBy"
	,"schpaydlyhdreg"."CreatedOn" "CreatedOn"
	,"schpaydlyhdreg"."approved" "approved"
	,"schpaydlyhdreg"."posted" "posted"
	,"schpaydlyhdreg"."deleted" "deleted"
	,"schpaydlyhdreg"."havdetails" "havdetails"
from "dbo"."schpaydlyhdreg" "schpaydlyhdreg"
	left join "dbo"."glmulcmp" "sgm" on "schpaydlyhdreg"."sgm" = "sgm"."sgmid"
	left join "dbo"."OpCo" "sgmOpco" on "sgm"."OpcoID" = "sgmOpco"."OpcoIDidentity"
	left join "dbo"."REG_ORGANIZATIONS" "IDregorg" on "schpaydlyhdreg"."IDregorg" = "IDregorg"."IDIDregorg"
]]></text>
