	,"AttachmentGeneral"."Tasneefcode3" "Tasneefcode3"
	,"TheTasneefcode3"."TasneefDesc1" "TheTasneefcode3TasneefDesc1"
	,"AttachmentGeneral"."Tasneefcode4" "Tasneefcode4"
	,"TheTasneefcode4"."TasneefDesc1" "TheTasneefcode4TasneefDesc1"
	,"AttachmentGeneral"."ModifiedBy" "ModifiedBy"
	,"AttachmentGeneral"."ModifiedOn" "ModifiedOn"
	,"AttachmentGeneral"."CreatedBy" "CreatedBy"
	,"AttachmentGeneral"."CreatedOn" "CreatedOn"
	,"AttachmentGeneral"."Fielde1001" "Fielde1001"
