	,"AttachmentGeneral"."Fielde1011" "Fielde1011"
	,"AttachmentGeneral"."Fielde1012" "Fielde1012"
from "dbo"."AttachmentGeneral" "AttachmentGeneral"
	left join "dbo"."AttchTasneefgen1" "TheTasneefcode1" on "AttachmentGeneral"."Tasneefcode1" = "TheTasneefcode1"."Tasneefcode1"
	left join "dbo"."AttchTasneefgen2" "TheTasneefcode2" on "AttachmentGeneral"."Tasneefcode2" = "TheTasneefcode2"."Tasneefcode2"
	left join "dbo"."AttchTasneefgen3" "TheTasneefcode3" on "AttachmentGeneral"."Tasneefcode3" = "TheTasneefcode3"."Tasneefcode3"
	left join "dbo"."AttchTasneefgen4" "TheTasneefcode4" on "AttachmentGeneral"."Tasneefcode4" = "TheTasneefcode4"."Tasneefcode4"
]]></text>
