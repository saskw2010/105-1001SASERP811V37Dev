	,"Emp_no1sgm"."sgm_Nm" "Emp_no1sgmsgm_Nm"
	,"Emp_no1sgmOpco"."OpcoName" "Emp_no1sgmOpcoOpcoName"
	,"Emp_no1salrycat"."grade_desc_a" "Emp_no1salrycatgrade_desc_a"
	,"Emp_no1salrycatEarning_"."Earning_Nm" "Emp_no1salrycatEarning_Earning_Nm"
	,"Emp_no1Blood_Type"."ModifiedBy" "Emp_no1Blood_TypeModifiedBy"
	,"Emp_no1Bnk_"."Bnk_Acc" "Emp_no1Bnk_Bnk_Acc"
	,"Emp_no1Sub_Bnk"."Bnk_Acc" "Emp_no1Sub_BnkBnk_Acc"
	,"Emp_no1Cntry_"."Cntry_Nm" "Emp_no1Cntry_Cntry_Nm"
	,"Emp_no1Depm_"."Depm_Nm" "Emp_no1Depm_Depm_Nm"
	,"Emp_no1Educt_Type"."Educt_Nm" "Emp_no1Educt_TypeEduct_Nm"
	,"Emp_no1Job_Stu"."Emp_Stunm" "Emp_no1Job_StuEmp_Stunm"
	,"Emp_no1Earning_"."Earning_Nm" "Emp_no1Earning_Earning_Nm"
	,"Emp_no1Gender"."Gender" "Emp_no1GenderGender"
	,"Emp_no1Job_"."Job_Nm" "Emp_no1Job_Job_Nm"
	,"Emp_no1Loc_"."Loc_Nm" "Emp_no1Loc_Loc_Nm"
	,"Emp_no1Ownr_"."Ownr_ID" "Emp_no1Ownr_Ownr_ID"
	,"Emp_no1Ownr_cmp_"."Grp_Nm" "Emp_no1Ownr_cmp_Grp_Nm"
	,"Emp_no1Reljan"."Reljan_nm" "Emp_no1ReljanReljan_nm"
	,"Emp_no1Status_Type"."Status_Nm" "Emp_no1Status_TypeStatus_Nm"
	,"Emp_no1branch"."Desc1" "Emp_no1branchDesc1"
	,"Emp_no1branchsgm"."sgm_Nm" "Emp_no1branchsgmsgm_Nm"
	,"Emp_no1branchGender"."Gender" "Emp_no1branchGenderGender"
	,"Emp_no1branchstage"."ShortDesc1" "Emp_no1branchstageShortDesc1"
	,"Emp_no1branchschtyp"."schtypDesc" "Emp_no1branchschtypschtypDesc"
	,"Cadriver"."Emp_Nmdriver" "Emp_Nmdriver"
	,"Cadriver"."Emp_Nmdrivere" "Emp_Nmdrivere"
	,"Cadriver"."DrivercivilID" "DrivercivilID"
	,"Cadriver"."DriverlicenceID" "DriverlicenceID"
	,"Cadriver"."DriverMobile" "DriverMobile"
	,"Cadriver"."DriverMobile1" "DriverMobile1"
	,"Cadriver"."DriverTelphone1" "DriverTelphone1"
	,"Cadriver"."DriverTelphone2" "DriverTelphone2"
	,"Cadriver"."sgm" "sgm"
	,"sgm"."sgm_Nm" "sgmsgm_Nm"
	,"sgmOpco"."OpcoName" "sgmOpcoOpcoName"
	,"Cadriver"."ModifiedBy" "ModifiedBy"
	,"Cadriver"."ModifiedOn" "ModifiedOn"
	,"Cadriver"."CreatedBy" "CreatedBy"
	,"Cadriver"."CreatedOn" "CreatedOn"
from "dbo"."Cadriver" "Cadriver"
	left join "dbo"."Pyempmfs" "Emp_no1" on "Cadriver"."Emp_no1" = "Emp_no1"."Emp_No"
	left join "dbo"."glmulcmp" "Emp_no1sgm" on "Emp_no1"."sgm" = "Emp_no1sgm"."sgmid"
	left join "dbo"."OpCo" "Emp_no1sgmOpco" on "Emp_no1sgm"."OpcoID" = "Emp_no1sgmOpco"."OpcoIDidentity"
	left join "dbo"."grade" "Emp_no1salrycat" on "Emp_no1"."salrycat" = "Emp_no1salrycat"."grade_code"
	left join "dbo"."Pyerntsalyp1" "Emp_no1salrycatEarning_" on "Emp_no1salrycat"."Earning_No" = "Emp_no1salrycatEarning_"."Earning_No"
	left join "dbo"."pyBlood" "Emp_no1Blood_Type" on "Emp_no1"."Blood_Type" = "Emp_no1Blood_Type"."Blood_Type"
	left join "dbo"."PybnkAcc" "Emp_no1Bnk_" on "Emp_no1"."Bnk_No" = "Emp_no1Bnk_"."Bnk_No"
	left join "dbo"."PybnkAcc" "Emp_no1Sub_Bnk" on "Emp_no1"."Sub_Bnk" = "Emp_no1Sub_Bnk"."Bnk_No"
	left join "dbo"."PYCntry" "Emp_no1Cntry_" on "Emp_no1"."Cntry_No" = "Emp_no1Cntry_"."Cntry_No"
	left join "dbo"."Pydepm" "Emp_no1Depm_" on "Emp_no1"."Depm_No" = "Emp_no1Depm_"."Depm_No"
	left join "dbo"."PyempEdu" "Emp_no1Educt_Type" on "Emp_no1"."Educt_Type" = "Emp_no1Educt_Type"."Educt_Type"
	left join "dbo"."Pyempstu" "Emp_no1Job_Stu" on "Emp_no1"."Job_Stu" = "Emp_no1Job_Stu"."Emp_stu"
	left join "dbo"."Pyerntsalyp1" "Emp_no1Earning_" on "Emp_no1"."Earning_no" = "Emp_no1Earning_"."Earning_No"
	left join "dbo"."PyGender" "Emp_no1Gender" on "Emp_no1"."Genderid" = "Emp_no1Gender"."Genderid"
	left join "dbo"."Pyjobs" "Emp_no1Job_" on "Emp_no1"."Job_No" = "Emp_no1Job_"."Job_No"
	left join "dbo"."PyLocation" "Emp_no1Loc_" on "Emp_no1"."Loc_No" = "Emp_no1Loc_"."loc_No"
	left join "dbo"."Pyownrmf" "Emp_no1Ownr_" on "Emp_no1"."Ownr_No" = "Emp_no1Ownr_"."Ownr_No"
	left join "dbo"."Pycnstnt" "Emp_no1Ownr_cmp_" on "Emp_no1Ownr_"."cmp_no" = "Emp_no1Ownr_cmp_"."Cmp_No"
	left join "dbo"."PyReljan" "Emp_no1Reljan" on "Emp_no1"."Reljan" = "Emp_no1Reljan"."reljan"
	left join "dbo"."PyStatus" "Emp_no1Status_Type" on "Emp_no1"."Status_Type" = "Emp_no1Status_Type"."Status_Type"
	left join "dbo"."schBranch" "Emp_no1branch" on "Emp_no1"."branch" = "Emp_no1branch"."branch"
	left join "dbo"."glmulcmp" "Emp_no1branchsgm" on "Emp_no1branch"."sgm" = "Emp_no1branchsgm"."sgmid"
	left join "dbo"."PyGender" "Emp_no1branchGender" on "Emp_no1branch"."Genderid" = "Emp_no1branchGender"."Genderid"
	left join "dbo"."schStage" "Emp_no1branchstage" on "Emp_no1branch"."stagecode" = "Emp_no1branchstage"."Code"
	left join "dbo"."schtyp" "Emp_no1branchschtyp" on "Emp_no1branch"."schtypid" = "Emp_no1branchschtyp"."schtypid"
	left join "dbo"."glmulcmp" "sgm" on "Cadriver"."sgm" = "sgm"."sgmid"
	left join "dbo"."OpCo" "sgmOpco" on "sgm"."OpcoID" = "sgmOpco"."OpcoIDidentity"
]]></text>
