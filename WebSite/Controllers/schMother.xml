  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"schMother"."MotherCode" "MotherCode"
	,"schMother"."firstname1" "firstname1"
	,"schMother"."firstname2" "firstname2"
	,"schMother"."Cntry_No" "Cntry_No"
	,"Cntry_"."Cntry_Nm" "Cntry_Cntry_Nm"
	,"schMother"."job_no" "job_no"
	,"job_"."job_desc" "job_job_desc"
	,"schMother"."WorkTel1" "WorkTel1"
	,"schMother"."WorkTel2" "WorkTel2"
	,"schMother"."CivilidNumber" "CivilidNumber"
	,"schMother"."CivilidExpDate" "CivilidExpDate"
	,"schMother"."MobileNumber1" "MobileNumber1"
	,"schMother"."EmergencyTelNo1" "EmergencyTelNo1"
	,"schMother"."KuwaitnationalityNo" "KuwaitnationalityNo"
	,"schMother"."KuwaitnationalityDate" "KuwaitnationalityDate"
	,"schMother"."PassportNumber" "PassportNumber"
	,"schMother"."PassportPlace" "PassportPlace"
	,"schMother"."PassportissueDate" "PassportissueDate"
	,"schMother"."PassportExpDate" "PassportExpDate"
	,"schMother"."Residencenumber" "Residencenumber"
	,"schMother"."ResidenceExpDate" "ResidenceExpDate"
	,"schMother"."residenceaddress1" "residenceaddress1"
	,"schMother"."religion" "religion"
	,"religion"."Reljan_nm" "religionReljan_nm"
	,"schMother"."Email1" "Email1"
	,"schMother"."Email2" "Email2"
	,"schMother"."PoBox" "PoBox"
	,"schMother"."State" "State"
	,"TheState"."State_Nm" "TheStateState_Nm"
	,"schMother"."area" "area"
	,"area"."Desc1" "areaDesc1"
	,"schMother"."blockno" "blockno"
	,"schMother"."streetno" "streetno"
	,"schMother"."houseno" "houseno"
	,"schMother"."ModifiedBy" "ModifiedBy"
	,"schMother"."ModifiedOn" "ModifiedOn"
	,"schMother"."CreatedBy" "CreatedBy"
	,"schMother"."CreatedOn" "CreatedOn"
from "dbo"."schMother" "schMother"
	left join "dbo"."PYCntry" "Cntry_" on "schMother"."Cntry_No" = "Cntry_"."Cntry_No"
	left join "dbo"."SchMotherjob" "job_" on "schMother"."job_no" = "job_"."job_no"
	left join "dbo"."PyReljan" "religion" on "schMother"."religion" = "religion"."reljan"
	left join "dbo"."PyState" "TheState" on "schMother"."State" = "TheState"."State"
	left join "dbo"."schArea" "area" on "schMother"."area" = "area"."Code"
]]></text>
    </command>
    <command id="MotherCodeIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="MotherCode" />
      </output>
    </command>
