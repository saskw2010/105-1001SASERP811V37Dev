	,"schglvchdlyhdregsgmOpco"."OpcoName" "schglvchdlyhdregsgmOpcoOpcoName"
	,"schglvchdlyhdregacd"."AcademicYear" "schglvchdlyhdregacdAcademicYear"
	,"schglvchdlyhdregacdGlFinperiod"."Fin_period_info" "schglvchdlyhdregacdGlFinperiodFin_period_info"
