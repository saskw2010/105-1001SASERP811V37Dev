	,"schglvchdlyhdregbranchsgm"."sgm_Nm" "schglvchdlyhdregbranchsgmsgm_Nm"
	,"schglvchdlyhdregbranchGender"."Gender" "schglvchdlyhdregbranchGenderGender"
	,"schglvchdlyhdregbranchstage"."ShortDesc1" "schglvchdlyhdregbranchstageShortDesc1"
	,"schglvchdlyhdregbranchschtyp"."schtypDesc" "schglvchdlyhdregbranchschtypschtypDesc"
	,"schattachglvchlists"."schattachglvchtype" "schattachglvchtype"
	,"schattachglvchlists"."schattachglvchrefno" "schattachglvchrefno"
	,"schattachglvchlists"."schattachglvchrefdate" "schattachglvchrefdate"
	,"schattachglvchlists"."schattachglvchrefpagesno" "schattachglvchrefpagesno"
	,"schattachglvchlists"."notes" "notes"
	,"schattachglvchlists"."ExternalDocFileName" "ExternalDocFileName"
	,"schattachglvchlists"."ExternalDocContentType" "ExternalDocContentType"
	,"schattachglvchlists"."ExternalDocLength" "ExternalDocLength"
from "dbo"."schattachglvchlists" "schattachglvchlists"
	left join "dbo"."schglvchdlyhdreg" "schglvchdlyhdreg" on "schattachglvchlists"."schglvchdlyhdregid" = "schglvchdlyhdreg"."schglvchdlyhdregid"
	left join "dbo"."glmulcmp" "schglvchdlyhdregsgm" on "schglvchdlyhdreg"."sgm" = "schglvchdlyhdregsgm"."sgmid"
	left join "dbo"."OpCo" "schglvchdlyhdregsgmOpco" on "schglvchdlyhdregsgm"."OpcoID" = "schglvchdlyhdregsgmOpco"."OpcoIDidentity"
	left join "dbo"."schAcademicyear" "schglvchdlyhdregacd" on "schglvchdlyhdreg"."acdcode" = "schglvchdlyhdregacd"."Code"
	left join "dbo"."GlFinanceperiod" "schglvchdlyhdregacdGlFinperiod" on "schglvchdlyhdregacd"."GlFinperiodID" = "schglvchdlyhdregacdGlFinperiod"."GlFinperiodID"
	left join "dbo"."schBranch" "schglvchdlyhdregbranch" on "schglvchdlyhdreg"."branch" = "schglvchdlyhdregbranch"."branch"
	left join "dbo"."glmulcmp" "schglvchdlyhdregbranchsgm" on "schglvchdlyhdregbranch"."sgm" = "schglvchdlyhdregbranchsgm"."sgmid"
	left join "dbo"."PyGender" "schglvchdlyhdregbranchGender" on "schglvchdlyhdregbranch"."Genderid" = "schglvchdlyhdregbranchGender"."Genderid"
	left join "dbo"."schStage" "schglvchdlyhdregbranchstage" on "schglvchdlyhdregbranch"."stagecode" = "schglvchdlyhdregbranchstage"."Code"
	left join "dbo"."schtyp" "schglvchdlyhdregbranchschtyp" on "schglvchdlyhdregbranch"."schtypid" = "schglvchdlyhdregbranchschtyp"."schtypid"
]]></text>
