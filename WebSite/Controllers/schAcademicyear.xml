select
	"schAcademicyear"."Code" "Code"
	,"schAcademicyear"."AcademicYear" "AcademicYear"
	,"schAcademicyear"."startdate" "startdate"
	,"schAcademicyear"."enddate" "enddate"
	,"schAcademicyear"."GlFinperiodID" "GlFinperiodID"
	,"GlFinperiod"."Fin_period_info" "GlFinperiodFin_period_info"
	,"GlFinperiodaccountnumber"."Acc_Nm" "GlFinperiodaccountnumberAcc_Nm"
	,"GlFinperiodaccountnumberClsacc_"."Acc_Nm" "GlFinperiodaccountnumberClsacc_Acc_Nm"
	,"GlFinperiodaccountnumberAcc_Bnd"."Acc_Nm" "GlFinperiodaccountnumberAcc_BndAcc_Nm"
	,"schAcademicyear"."startdatein" "startdatein"
	,"schAcademicyear"."startdatepaystart" "startdatepaystart"
	,"schAcademicyear"."notes" "notes"
	,"schAcademicyear"."ModifiedBy" "ModifiedBy"
	,"schAcademicyear"."ModifiedOn" "ModifiedOn"
	,"schAcademicyear"."CreatedBy" "CreatedBy"
	,"schAcademicyear"."CreatedOn" "CreatedOn"
	,"schAcademicyear"."Agecalculatedate" "Agecalculatedate"
from "dbo"."schAcademicyear" "schAcademicyear"
	left join "dbo"."GlFinanceperiod" "GlFinperiod" on "schAcademicyear"."GlFinperiodID" = "GlFinperiod"."GlFinperiodID"
	left join "dbo"."GLmstrfl" "GlFinperiodaccountnumber" on "GlFinperiod"."accountnumber" = "GlFinperiodaccountnumber"."Acc_No"
	left join "dbo"."GLmfbab" "GlFinperiodaccountnumberClsacc_" on "GlFinperiodaccountnumber"."Clsacc_No" = "GlFinperiodaccountnumberClsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "GlFinperiodaccountnumberAcc_Bnd" on "GlFinperiodaccountnumber"."Acc_Bnd" = "GlFinperiodaccountnumberAcc_Bnd"."Acc_Bnd"
]]></text>
    </command>
