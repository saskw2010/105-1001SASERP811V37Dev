    <command id="command1" type="Text">
      <text><![CDATA[
select
	"emp_allow"."Srlcode" "Srlcode"
	,"emp_allow"."Emp_NO" "Emp_NO"
	,"Emp_"."Emp_No" "Emp_Emp_No"
	,"Emp_salrycat"."grade_desc_a" "Emp_salrycatgrade_desc_a"
	,"Emp_salrycatEarning_"."Earning_Nm" "Emp_salrycatEarning_Earning_Nm"
	,"Emp_Bnk_"."Bnk_Acc" "Emp_Bnk_Bnk_Acc"
	,"Emp_Sub_Bnk"."Bnk_Acc" "Emp_Sub_BnkBnk_Acc"
	,"Emp_Emp_"."Emp_NOt" "Emp_Emp_Emp_NOt"
	,"Emp_Emp_sgm"."sgm_Nm" "Emp_Emp_sgmsgm_Nm"
	,"Emp_Emp_salrycat"."grade_desc_a" "Emp_Emp_salrycatgrade_desc_a"
	,"Emp_Emp_Blood_Type"."ModifiedBy" "Emp_Emp_Blood_TypeModifiedBy"
	,"Emp_Emp_Bnk_"."Bnk_Acc" "Emp_Emp_Bnk_Bnk_Acc"
	,"Emp_Emp_Sub_Bnk"."Bnk_Acc" "Emp_Emp_Sub_BnkBnk_Acc"
	,"Emp_Emp_Cntry_"."Cntry_Nm" "Emp_Emp_Cntry_Cntry_Nm"
	,"Emp_Emp_Depm_"."Depm_Nm" "Emp_Emp_Depm_Depm_Nm"
	,"Emp_Emp_Educt_Type"."Educt_Nm" "Emp_Emp_Educt_TypeEduct_Nm"
	,"Emp_Emp_Job_Stu"."Emp_Stunm" "Emp_Emp_Job_StuEmp_Stunm"
	,"Emp_Emp_Earning_"."Earning_Nm" "Emp_Emp_Earning_Earning_Nm"
	,"Emp_Emp_Job_"."Job_Nm" "Emp_Emp_Job_Job_Nm"
	,"Emp_Emp_Ownr_"."Ownr_ID" "Emp_Emp_Ownr_Ownr_ID"
	,"Emp_Emp_Reljan"."Reljan_nm" "Emp_Emp_ReljanReljan_nm"
	,"Emp_Emp_Status_Type"."Status_Nm" "Emp_Emp_Status_TypeStatus_Nm"
	,"Emp_Emp_branch"."Desc1" "Emp_Emp_branchDesc1"
	,"Emp_Earning_"."Earning_Nm" "Emp_Earning_Earning_Nm"
	,"emp_allow"."allow_no" "allow_no"
	,"allow_"."allow_desc_a" "allow_allow_desc_a"
	,"emp_allow"."amount" "amount"
	,"emp_allow"."ModifiedBy" "ModifiedBy"
	,"emp_allow"."ModifiedOn" "ModifiedOn"
	,"emp_allow"."CreatedBy" "CreatedBy"
	,"emp_allow"."CreatedOn" "CreatedOn"
from "dbo"."emp_allow" "emp_allow"
	left join "dbo"."Pyempcnt" "Emp_" on "emp_allow"."Emp_NO" = "Emp_"."Emp_No"
	left join "dbo"."grade" "Emp_salrycat" on "Emp_"."salrycat" = "Emp_salrycat"."grade_code"
	left join "dbo"."Pyerntsalyp1" "Emp_salrycatEarning_" on "Emp_salrycat"."Earning_No" = "Emp_salrycatEarning_"."Earning_No"
	left join "dbo"."PybnkAcc" "Emp_Bnk_" on "Emp_"."Bnk_No" = "Emp_Bnk_"."Bnk_No"
	left join "dbo"."PybnkAcc" "Emp_Sub_Bnk" on "Emp_"."Sub_Bnk" = "Emp_Sub_Bnk"."Bnk_No"
	left join "dbo"."Pyempmfs" "Emp_Emp_" on "Emp_"."Emp_No" = "Emp_Emp_"."Emp_No"
	left join "dbo"."glmulcmp" "Emp_Emp_sgm" on "Emp_Emp_"."sgm" = "Emp_Emp_sgm"."sgmid"
	left join "dbo"."grade" "Emp_Emp_salrycat" on "Emp_Emp_"."salrycat" = "Emp_Emp_salrycat"."grade_code"
	left join "dbo"."pyBlood" "Emp_Emp_Blood_Type" on "Emp_Emp_"."Blood_Type" = "Emp_Emp_Blood_Type"."Blood_Type"
	left join "dbo"."PybnkAcc" "Emp_Emp_Bnk_" on "Emp_Emp_"."Bnk_No" = "Emp_Emp_Bnk_"."Bnk_No"
	left join "dbo"."PybnkAcc" "Emp_Emp_Sub_Bnk" on "Emp_Emp_"."Sub_Bnk" = "Emp_Emp_Sub_Bnk"."Bnk_No"
	left join "dbo"."PYCntry" "Emp_Emp_Cntry_" on "Emp_Emp_"."Cntry_No" = "Emp_Emp_Cntry_"."Cntry_No"
	left join "dbo"."Pydepm" "Emp_Emp_Depm_" on "Emp_Emp_"."Depm_No" = "Emp_Emp_Depm_"."Depm_No"
	left join "dbo"."PyempEdu" "Emp_Emp_Educt_Type" on "Emp_Emp_"."Educt_Type" = "Emp_Emp_Educt_Type"."Educt_Type"
	left join "dbo"."Pyempstu" "Emp_Emp_Job_Stu" on "Emp_Emp_"."Job_Stu" = "Emp_Emp_Job_Stu"."Emp_stu"
	left join "dbo"."Pyerntsalyp1" "Emp_Emp_Earning_" on "Emp_Emp_"."Earning_no" = "Emp_Emp_Earning_"."Earning_No"
	left join "dbo"."Pyjobs" "Emp_Emp_Job_" on "Emp_Emp_"."Job_No" = "Emp_Emp_Job_"."Job_No"
	left join "dbo"."Pyownrmf" "Emp_Emp_Ownr_" on "Emp_Emp_"."Ownr_No" = "Emp_Emp_Ownr_"."Ownr_No"
	left join "dbo"."PyReljan" "Emp_Emp_Reljan" on "Emp_Emp_"."Reljan" = "Emp_Emp_Reljan"."reljan"
	left join "dbo"."PyStatus" "Emp_Emp_Status_Type" on "Emp_Emp_"."Status_Type" = "Emp_Emp_Status_Type"."Status_Type"
	left join "dbo"."schBranch" "Emp_Emp_branch" on "Emp_Emp_"."branch" = "Emp_Emp_branch"."branch"
	left join "dbo"."Pyerntsalyp1" "Emp_Earning_" on "Emp_"."Earning_no" = "Emp_Earning_"."Earning_No"
	left join "dbo"."allowance" "allow_" on "emp_allow"."allow_no" = "allow_"."allow_no"
]]></text>
