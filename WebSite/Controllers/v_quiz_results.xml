    <field name="results_mode" type="Int32" label="Results Mode" showInSummary="true" />
    <field name="show_results" type="Int32" label="Show Results" showInSummary="true" />
    <field name="pass_score" type="Decimal" label="Pass Score" />
    <field name="quiz_success" type="Int32" label="Quiz Success" readOnly="true" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="V Quiz Results">
