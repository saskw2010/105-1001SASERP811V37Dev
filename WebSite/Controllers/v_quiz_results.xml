<?xml version="1.0" encoding="utf-8"?>
<dataController name="v_quiz_results" conflictDetection="overwriteChanges" label="V Quiz Results" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"v_quiz_results"."total_point" "total_point"
	,"v_quiz_results"."total_perc" "total_perc"
	,"v_quiz_results"."user_quiz_id" "user_quiz_id"
