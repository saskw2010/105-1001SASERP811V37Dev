  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"ACCODE1"."ACCODE1ID" "ACCODE1ID"
	,"ACCODE1"."code_no" "code_no"
	,"code_"."code_nm" "code_code_nm"
	,"ACCODE1"."ACC_NO" "ACC_NO"
	,"ACC_"."Acc_Nm" "ACC_Acc_Nm"
	,"ACC_Clsacc_"."Acc_Nm" "ACC_Clsacc_Acc_Nm"
	,"ACC_Acc_Bnd"."Acc_Nm" "ACC_Acc_BndAcc_Nm"
	,"ACC_Acc_Bndaccclass"."accountclass" "ACC_Acc_Bndaccclassaccountclass"
	,"ACC_Acc_BndAcc_Bab"."Acc_Nm" "ACC_Acc_BndAcc_BabAcc_Nm"
	,"ACCODE1"."ACC_Nm" "ACC_Nm"
	,"ACCODE1"."lvl" "lvl"
	,"ACCODE1"."ModifiedBy" "ModifiedBy"
	,"ACCODE1"."ModifiedOn" "ModifiedOn"
	,"ACCODE1"."CreatedBy" "CreatedBy"
	,"ACCODE1"."CreatedOn" "CreatedOn"
	,"ACCODE1"."code_nm" "code_nm"
	,"ACCODE1"."sgm" "sgm"
	,"ACCODE1"."sncode" "sncode"
	,"ACCODE1"."DCBtype" "DCBtype"
	,"ACCODE1"."groupinglist" "groupinglist"
	,"ACCODE1"."groupinglist1" "groupinglist1"
	,"ACCODE1"."groupinglist2" "groupinglist2"
	,"ACCODE1"."groupinglist3" "groupinglist3"
	,"ACCODE1"."groupinglist4" "groupinglist4"
	,"ACCODE1"."groupinglist5" "groupinglist5"
	,"ACCODE1"."groupinglist6" "groupinglist6"
	,"ACCODE1"."groupinglist7" "groupinglist7"
	,"ACCODE1"."groupinglist8" "groupinglist8"
	,"ACCODE1"."groupinglist9" "groupinglist9"
from "dbo"."ACCODE1" "ACCODE1"
	left join "dbo"."ACCODEMASTER1" "code_" on "ACCODE1"."code_no" = "code_"."code_no"
	left join "dbo"."GLmstrfl" "ACC_" on "ACCODE1"."ACC_NO" = "ACC_"."Acc_No"
	left join "dbo"."GLmfbab" "ACC_Clsacc_" on "ACC_"."Clsacc_No" = "ACC_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "ACC_Acc_Bnd" on "ACC_"."Acc_Bnd" = "ACC_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "ACC_Acc_Bndaccclass" on "ACC_Acc_Bnd"."accclass" = "ACC_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "ACC_Acc_BndAcc_Bab" on "ACC_Acc_Bnd"."Acc_Bab" = "ACC_Acc_BndAcc_Bab"."Acc_Bab"
]]></text>
    </command>
    <command id="ACCODE1IDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="ACCODE1ID" />
      </output>
    </command>
