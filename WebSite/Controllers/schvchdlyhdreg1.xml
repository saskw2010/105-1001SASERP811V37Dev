    <field name="Trcrvalue" type="Decimal" readOnly="true" label="Trcrvalue" computed="true" htmlEncode="false" hidden="false">
      <items letters="false" />
      <formula>SELECT        SUM(dbo.schvchdlydetailpayment.moneypaidto) AS Trcrvalue
FROM            dbo.schvchdlyhdreg LEFT OUTER JOIN
                         dbo.schvchdlydetailpayment ON dbo.schvchdlyhdreg.schvchdlyhdregid = dbo.schvchdlydetailpayment.schvchdlyhdregid
WHERE        (dbo.schvchdlyhdreg.schvchdlyhdregid = @schvchdlyhdregid)</formula>
