  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"HealthBudget"."Budget_id" "Budget_id"
	,"HealthBudget"."sgm" "sgm"
	,"sgm"."sgm_Nm" "sgmsgm_Nm"
	,"sgmOpco"."OpcoName" "sgmOpcoOpcoName"
	,"HealthBudget"."Branch" "Branch"
	,"Branch"."Desc1" "BranchDesc1"
	,"Branchsgm"."sgm_Nm" "Branchsgmsgm_Nm"
	,"BranchsgmOpco"."OpcoName" "BranchsgmOpcoOpcoName"
	,"BranchGender"."Gender" "BranchGenderGender"
	,"Branchstage"."ShortDesc1" "BranchstageShortDesc1"
	,"Branchschtyp"."schtypDesc" "BranchschtypschtypDesc"
	,"HealthBudget"."Demp_No" "Demp_No"
	,"Demp_"."Depm_Nm" "Demp_Depm_Nm"
	,"HealthBudget"."loc_No" "loc_No"
	,"loc_"."Loc_Nm" "loc_Loc_Nm"
	,"HealthBudget"."ReqNurse" "ReqNurse"
	,"HealthBudget"."Reqm.a.g" "Reqmag"
	,"HealthBudget"."ReqAssistant. Nurse" "ReqAssistantNurse"
	,"HealthBudget"."ReqAssistant. A nurse" "ReqAssistantAnurse"
	,"HealthBudget"."ReqA.nurse" "ReqAnurse"
	,"HealthBudget"."ReqResponsible-F" "ReqResponsibleF"
	,"HealthBudget"."ReqResponsible-M" "ReqResponsibleM"
	,"HealthBudget"."ReqGuided technical" "ReqGuidedtechnical"
	,"HealthBudget"."ReqTechnician prompt" "ReqTechnicianprompt"
	,"HealthBudget"."ReqFemale education" "ReqFemaleeducation"
	,"HealthBudget"."ReqEducation Male" "ReqEducationMale"
	,"HealthBudget"."ReqAssist President" "ReqAssistPresident"
	,"HealthBudget"."ReqAssistant President" "ReqAssistantPresident"
	,"HealthBudget"."ReqHead Nurse" "ReqHeadNurse"
	,"HealthBudget"."Reqother1" "Reqother1"
	,"HealthBudget"."Reqother2" "Reqother2"
	,"HealthBudget"."Reqother11" "Reqother11"
	,"HealthBudget"."Reqother21" "Reqother21"
	,"HealthBudget"."beds" "beds"
	,"HealthBudget"."percent" "percent"
	,"HealthBudget"."require" "require"
from "dbo"."HealthBudget" "HealthBudget"
	left join "dbo"."glmulcmp" "sgm" on "HealthBudget"."sgm" = "sgm"."sgmid"
	left join "dbo"."OpCo" "sgmOpco" on "sgm"."OpcoID" = "sgmOpco"."OpcoIDidentity"
	left join "dbo"."schBranch" "Branch" on "HealthBudget"."Branch" = "Branch"."branch"
	left join "dbo"."glmulcmp" "Branchsgm" on "Branch"."sgm" = "Branchsgm"."sgmid"
	left join "dbo"."OpCo" "BranchsgmOpco" on "Branchsgm"."OpcoID" = "BranchsgmOpco"."OpcoIDidentity"
	left join "dbo"."PyGender" "BranchGender" on "Branch"."Genderid" = "BranchGender"."Genderid"
	left join "dbo"."schStage" "Branchstage" on "Branch"."stagecode" = "Branchstage"."Code"
	left join "dbo"."schtyp" "Branchschtyp" on "Branch"."schtypid" = "Branchschtyp"."schtypid"
	left join "dbo"."Pydepm" "Demp_" on "HealthBudget"."Demp_No" = "Demp_"."Depm_No"
	left join "dbo"."PyLocation" "loc_" on "HealthBudget"."loc_No" = "loc_"."loc_No"
]]></text>
    </command>
