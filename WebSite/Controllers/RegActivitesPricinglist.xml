    <command id="command1" type="Text">
      <text><![CDATA[
select
	"RegActivitesPricinglist"."Activitesparentid" "Activitesparentid"
	,"RegActivitesPricinglist"."Activitesid" "Activitesid"
	,"Activites"."ActivitesDescrption" "ActivitesActivitesDescrption"
	,"Activitesschpaymenttype"."schpaymenttypenm" "Activitesschpaymenttypeschpaymenttypenm"
	,"Activitesschpaymenttypedebitaccount"."Acc_Nm" "ActivitesschpaymenttypedebitaccountAcc_Nm"
	,"Activitesschpaymenttypecreditaccount"."Acc_Nm" "ActivitesschpaymenttypecreditaccountAcc_Nm"
	,"RegActivitesPricinglist"."RegActivitesPricingid" "RegActivitesPricingid"
	,"RegActivitesPricing"."RegActivitesPricingUnit" "RegActivitesPricingRegActivitesPricingUnit"
from "dbo"."RegActivitesPricinglist" "RegActivitesPricinglist"
	left join "dbo"."RegActivites" "Activites" on "RegActivitesPricinglist"."Activitesid" = "Activites"."Activitesid"
	left join "dbo"."schpaymenttype" "Activitesschpaymenttype" on "Activites"."schpaymenttypeid" = "Activitesschpaymenttype"."schpaymenttypeid"
	left join "dbo"."GLmstrfl" "Activitesschpaymenttypedebitaccount" on "Activitesschpaymenttype"."debitaccount" = "Activitesschpaymenttypedebitaccount"."Acc_No"
	left join "dbo"."GLmstrfl" "Activitesschpaymenttypecreditaccount" on "Activitesschpaymenttype"."creditaccount" = "Activitesschpaymenttypecreditaccount"."Acc_No"
	left join "dbo"."RegActivitesPricing" "RegActivitesPricing" on "RegActivitesPricinglist"."RegActivitesPricingid" = "RegActivitesPricing"."RegActivitesPricingid"
]]></text>
    </command>
  </commands>
