    <field name="FormNMMenuIDGroupGroupAr" type="String" readOnly="true" label="Form NM Menu ID Group Ar" length="50" />
    <field name="Cnname" type="String" label="Cnname" length="50" showInSummary="true" />
    <field name="ControlEN" type="String" label="Control EN" length="255" showInSummary="true" />
    <field name="ControlAR" type="String" label="Control AR" length="255" showInSummary="true" />
    <field name="CtlTyp" type="String" label="Ctl Typ" length="50" showInSummary="true" />
