<?xml version="1.0" encoding="utf-8"?>
<dataController name="SecControls" conflictDetection="overwriteChanges" label="Sec Controls" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"SecControls"."SECID" "SECID"
	,"SecControls"."FormNM" "FormNM"
	,"FormNM"."FormNM" "FormNMFormNM"
	,"FormNMMenu"."MenuAr" "FormNMMenuMenuAr"
	,"FormNMMenuIDGroup"."GroupAr" "FormNMMenuIDGroupGroupAr"
	,"SecControls"."Cnname" "Cnname"
	,"SecControls"."ControlEN" "ControlEN"
	,"SecControls"."ControlAR" "ControlAR"
	,"SecControls"."CtlTyp" "CtlTyp"
	,"SecControls"."idflage" "idflage"
	,"SecControls"."TipEN" "TipEN"
	,"SecControls"."TipAR" "TipAR"
	,"SecControls"."iddd" "iddd"
	,"SecControls"."sgm" "sgm"
	,"SecControls"."ModifiedBy" "ModifiedBy"
	,"SecControls"."ModifiedOn" "ModifiedOn"
	,"SecControls"."CreatedBy" "CreatedBy"
	,"SecControls"."CreatedOn" "CreatedOn"
from "dbo"."SecControls" "SecControls"
	left join "dbo"."SecForms" "FormNM" on "SecControls"."FormNM" = "FormNM"."FormNamenet"
	left join "dbo"."MenuMaster" "FormNMMenu" on "FormNM"."MenuID" = "FormNMMenu"."MenuID"
	left join "dbo"."GroupMaster" "FormNMMenuIDGroup" on "FormNMMenu"."IDGroup" = "FormNMMenuIDGroup"."GroupID"
]]></text>
    </command>
    <command id="SECIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="SECID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="SECID" type="Int64" allowNulls="false" isPrimaryKey="true" label="Secid" readOnly="true" />
    <field name="FormNM" type="String" label="Form NM" length="50" showInSummary="true">
      <items style="Lookup" dataController="SecForms" newDataView="createForm1" searchOnStart="true" />
