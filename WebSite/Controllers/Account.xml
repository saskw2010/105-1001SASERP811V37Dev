    <field name="AccountHeadingNumber" type="String" label="Account Heading Number" readOnly="true" length="2" />
    <field name="parentid" type="Int64" label="Parentid" />
    <field name="AccountTypeID" type="Int64" label="Account Type#" />
    <field name="Acc_Bab" type="Int64" label="Acc Bab" readOnly="true" />
    <field name="AccountNameBab" type="String" label="Account Name Bab" length="250" />
    <field name="AccountNameBabe" type="String" label="Account Name Babe" length="255" />
  </fields>
  <views>
