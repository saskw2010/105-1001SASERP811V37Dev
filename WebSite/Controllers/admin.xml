<?xml version="1.0" encoding="utf-8"?>
<dataController name="admin" conflictDetection="overwriteChanges" label="Admin" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"admin"."admin_id" "admin_id"
	,"admin"."admin_log" "admin_log"
	,"admin"."admin_pass" "admin_pass"
	,"admin"."admin_name" "admin_name"
	,"admin"."admin_phone" "admin_phone"
	,"admin"."admin_email" "admin_email"
	,"admin"."admin_type" "admin_type"
from "dbo"."admin" "admin"
]]></text>
    </command>
    <command id="admin_idIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="admin_id" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="admin_id" type="Int32" allowNulls="false" isPrimaryKey="true" label="Admin#" readOnly="true" />
    <field name="admin_log" type="String" label="Admin Log" length="50" showInSummary="true" />
    <field name="admin_pass" type="String" label="Admin Pass" length="50" showInSummary="true" />
    <field name="admin_name" type="String" label="Admin Name" length="100" showInSummary="true" />
    <field name="admin_phone" type="String" label="Admin Phone" length="50" showInSummary="true" />
    <field name="admin_email" type="String" label="Admin Email" length="100" showInSummary="true" />
    <field name="admin_type" type="Byte" label="Admin Type" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Admin">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="admin_log" columns="50" />
        <dataField fieldName="admin_pass" columns="50" />
        <dataField fieldName="admin_name" />
        <dataField fieldName="admin_phone" columns="50" />
        <dataField fieldName="admin_email" />
        <dataField fieldName="admin_type" columns="15" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Admin">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Admin" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="admin_log" columns="50" />
            <dataField fieldName="admin_pass" columns="50" />
            <dataField fieldName="admin_name" />
            <dataField fieldName="admin_phone" columns="50" />
            <dataField fieldName="admin_email" />
            <dataField fieldName="admin_type" columns="15" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Admin">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New Admin" flow="NewColumn"  newColumn="true">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="admin_log" columns="50" />
            <dataField fieldName="admin_pass" columns="50" />
            <dataField fieldName="admin_name" />
            <dataField fieldName="admin_phone" columns="50" />
            <dataField fieldName="admin_email" />
            <dataField fieldName="admin_type" columns="15" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
  <actions>
    <actionGroup id="ag1" scope="Grid">
      <action id="a1" commandName="Select" commandArgument="editForm1" />
      <action id="a2" commandName="Edit" />
      <action id="a3" commandName="Delete" />
      <action id="a6" />
      <action id="a7" commandName="New" commandArgument="grid1" />
      <action id="a8" commandName="Duplicate" commandArgument="createForm1" />
      <action id="a9" />
      <action id="a10" commandName="BatchEdit" />
      <action id="a11" commandName="BatchEdit" commandArgument="editForm1" />
    </actionGroup>
    <actionGroup id="ag2" scope="Form">
      <action id="a1" commandName="Edit" />
