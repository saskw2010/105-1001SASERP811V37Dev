	left join "dbo"."PybnkAcc" "Div_Emp_no1Bnk_" on "Div_Emp_no1"."Bnk_No" = "Div_Emp_no1Bnk_"."Bnk_No"
	left join "dbo"."PybnkAcc" "Div_Emp_no1Sub_Bnk" on "Div_Emp_no1"."Sub_Bnk" = "Div_Emp_no1Sub_Bnk"."Bnk_No"
	left join "dbo"."PYCntry" "Div_Emp_no1Cntry_" on "Div_Emp_no1"."Cntry_No" = "Div_Emp_no1Cntry_"."Cntry_No"
	left join "dbo"."Pydepm" "Div_Emp_no1Depm_" on "Div_Emp_no1"."Depm_No" = "Div_Emp_no1Depm_"."Depm_No"
