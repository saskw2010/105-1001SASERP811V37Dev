	left join "dbo"."Pyempstu" "Div_Emp_no1Job_Stu" on "Div_Emp_no1"."Job_Stu" = "Div_Emp_no1Job_Stu"."Emp_stu"
	left join "dbo"."Pyerntsalyp1" "Div_Emp_no1Earning_" on "Div_Emp_no1"."Earning_no" = "Div_Emp_no1Earning_"."Earning_No"
	left join "dbo"."PyGender" "Div_Emp_no1Gender" on "Div_Emp_no1"."Genderid" = "Div_Emp_no1Gender"."Genderid"
	left join "dbo"."Pyjobs" "Div_Emp_no1Job_" on "Div_Emp_no1"."Job_No" = "Div_Emp_no1Job_"."Job_No"
	left join "dbo"."PyLocation" "Div_Emp_no1Loc_" on "Div_Emp_no1"."Loc_No" = "Div_Emp_no1Loc_"."loc_No"
	left join "dbo"."Pyownrmf" "Div_Emp_no1Ownr_" on "Div_Emp_no1"."Ownr_No" = "Div_Emp_no1Ownr_"."Ownr_No"
	left join "dbo"."PyReljan" "Div_Emp_no1Reljan" on "Div_Emp_no1"."Reljan" = "Div_Emp_no1Reljan"."reljan"
	left join "dbo"."PyStatus" "Div_Emp_no1Status_Type" on "Div_Emp_no1"."Status_Type" = "Div_Emp_no1Status_Type"."Status_Type"
	left join "dbo"."schBranch" "Div_Emp_no1branch" on "Div_Emp_no1"."branch" = "Div_Emp_no1branch"."branch"
]]></text>
