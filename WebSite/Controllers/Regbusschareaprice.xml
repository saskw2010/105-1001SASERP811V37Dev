  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Regbusschareaprice"."Regbuspriceid" "Regbuspriceid"
	,"Regbusschareaprice"."busid" "busid"
	,"bus"."name_1" "busname_1"
	,"busbranch"."Desc1" "busbranchDesc1"
	,"busbranchsgm"."sgm_Nm" "busbranchsgmsgm_Nm"
	,"busbranchGender"."Gender" "busbranchGenderGender"
	,"busbranchstage"."ShortDesc1" "busbranchstageShortDesc1"
	,"busbranchschtyp"."schtypDesc" "busbranchschtypschtypDesc"
	,"Regbusschareaprice"."schareaid" "schareaid"
	,"scharea"."Desc1" "schareaDesc1"
	,"Regbusschareaprice"."standerdprice" "standerdprice"
	,"Regbusschareaprice"."notes" "notes"
	,"Regbusschareaprice"."ModifiedBy" "ModifiedBy"
	,"Regbusschareaprice"."ModifiedOn" "ModifiedOn"
	,"Regbusschareaprice"."CreatedBy" "CreatedBy"
	,"Regbusschareaprice"."CreatedOn" "CreatedOn"
	,"Regbusschareaprice"."firstinstallmentamount" "firstinstallmentamount"
	,"Regbusschareaprice"."firstinstallmentdate" "firstinstallmentdate"
	,"Regbusschareaprice"."secondinstallmentamount1" "secondinstallmentamount1"
	,"Regbusschareaprice"."secondinstallmentdate1" "secondinstallmentdate1"
	,"Regbusschareaprice"."branch" "branch"
from "dbo"."Regbusschareaprice" "Regbusschareaprice"
	left join "dbo"."reg_bus" "bus" on "Regbusschareaprice"."busid" = "bus"."id"
	left join "dbo"."schBranch" "busbranch" on "bus"."branch" = "busbranch"."branch"
	left join "dbo"."glmulcmp" "busbranchsgm" on "busbranch"."sgm" = "busbranchsgm"."sgmid"
	left join "dbo"."PyGender" "busbranchGender" on "busbranch"."Genderid" = "busbranchGender"."Genderid"
	left join "dbo"."schStage" "busbranchstage" on "busbranch"."stagecode" = "busbranchstage"."Code"
	left join "dbo"."schtyp" "busbranchschtyp" on "busbranch"."schtypid" = "busbranchschtyp"."schtypid"
	left join "dbo"."schArea" "scharea" on "Regbusschareaprice"."schareaid" = "scharea"."Code"
]]></text>
    </command>
    <command id="RegbuspriceidIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Regbuspriceid" />
      </output>
    </command>
  </commands>
