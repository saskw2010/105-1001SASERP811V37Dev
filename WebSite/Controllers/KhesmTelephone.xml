<?xml version="1.0" encoding="utf-8"?>
<dataController name="KhesmTelephone" conflictDetection="overwriteChanges" label="Khesm Telephone" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"KhesmTelephone"."teleCode" "teleCode"
	,"KhesmTelephone"."khosmcode" "khosmcode"
	,"khosm"."name" "khosmname"
	,"khosmkhosmetype"."khosmetypename" "khosmkhosmetypekhosmetypename"
	,"khosmnationality"."Cntry_Nm" "khosmnationalityCntry_Nm"
	,"khosmreligion"."Reljan_nm" "khosmreligionReljan_nm"
	,"KhesmTelephone"."teleNumber" "teleNumber"
	,"KhesmTelephone"."teleRemarks" "teleRemarks"
	,"KhesmTelephone"."ModifiedBy" "ModifiedBy"
	,"KhesmTelephone"."ModifiedOn" "ModifiedOn"
	,"KhesmTelephone"."CreatedBy" "CreatedBy"
	,"KhesmTelephone"."CreatedOn" "CreatedOn"
from "dbo"."KhesmTelephone" "KhesmTelephone"
	left join "dbo"."Khsom" "khosm" on "KhesmTelephone"."khosmcode" = "khosm"."khosmcode"
	left join "dbo"."khosmetypetable" "khosmkhosmetype" on "khosm"."khosmetype" = "khosmkhosmetype"."khosmetype"
	left join "dbo"."PYCntry" "khosmnationality" on "khosm"."nationality" = "khosmnationality"."Cntry_No"
	left join "dbo"."PyReljan" "khosmreligion" on "khosm"."religion" = "khosmreligion"."reljan"
]]></text>
    </command>
    <command id="teleCodeIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="teleCode" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="teleCode" type="Int64" allowNulls="false" isPrimaryKey="true" label="tele Code" readOnly="true" />
    <field name="khosmcode" type="Int64" label="Khosmcode" showInSummary="true">
      <items style="Lookup" dataController="Khsom" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="khosmname" type="String" readOnly="true" label="Khosmname" length="100" />
    <field name="khosmkhosmetypekhosmetypename" type="String" readOnly="true" label="Khosmkhosmetypekhosmetypename" length="50" />
    <field name="khosmnationalityCntry_Nm" type="String" readOnly="true" label="khosmnationality Cntry Nm" length="255" />
    <field name="khosmreligionReljan_nm" type="String" readOnly="true" label="khosmreligion Reljan nm" length="50" />
    <field name="teleNumber" type="String" label="tele Number" length="50" showInSummary="true" />
    <field name="teleRemarks" type="String" label="tele Remarks" length="75" showInSummary="true" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" showInSummary="true" />
    <field name="ModifiedOn" type="DateTime" label="Modified On" showInSummary="true" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="CreatedOn" type="DateTime" label="Created On" />
  </fields>
