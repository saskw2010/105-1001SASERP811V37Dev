<?xml version="1.0" encoding="utf-8"?>
<dataController name="USER_OF_RUN" conflictDetection="overwriteChanges" label="User Of Run" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
SELECT        dbo.USER_OF_RUN.USERID, dbo.USER_OF_RUN.NUM_OF_RUN_ID, dbo.NUM_RUN.NAME, dbo.NUM_RUN.CYLE, dbo.NUM_RUN.UNITS, dbo.NUM_RUN.STARTDATE, dbo.NUM_RUN.ENDDATE, 
                         dbo.TimeUnites.UNITSNAME, dbo.Pyempmfs.Uniq_no, dbo.Pyempmfs.Emp_Nm, dbo.Pyempmfs.branch, dbo.Pyempmfs.Emp_No, dbo.Pyempmfs.Emp_NOt,
                              dbo.USER_OF_RUN.str_date_emp, dbo.USER_OF_RUN.end_date_emp,dbo.USER_OF_RUN.incrm


from dbo.USER_OF_RUN 
Left OUTER JOIN dbo.NUM_RUN  ON dbo.NUM_RUN.NUM_RUNID = dbo.USER_OF_RUN.NUM_OF_RUN_ID
Left OUTER JOIN dbo.TimeUnites  ON  dbo.TimeUnites.UNITS = dbo.NUM_RUN.UNITS
LEFT OUTER JOIN dbo.Pyempmfs ON dbo.USER_OF_RUN.USERID = dbo.Pyempmfs.Uniq_no


]]></text>
    </command>
  </commands>
  <fields>




<field name="USERID" type="Int32" allowNulls="false" label="رقم الموظف ">
      <items style="Lookup" dataController="Pyempmfs" dataView="gridsearch" searchOnStart="true" dataTextField="Emp_Nm" dataValueField="Uniq_no" />
    </field>
<field name="NUM_OF_RUN_ID" type="Int32" allowNulls="false" label="الشيف ">
      <items style="Lookup" dataController="NUM_RUN" dataView="grid1" searchOnStart="true" dataTextField="NAME" dataValueField="NUM_RUNID" />
    </field>
<field name="STARTDATE" type="DateTime" allowNulls="false"  default="('1900-1-1')" label="Startdate" showInSummary="true" />
<field name="ENDDATE" type="DateTime" allowNulls="false"  default="('2099-12-31')" label="Enddate" showInSummary="true" />   
<field name="UNITSNAME" type="String" label="UNITSNAME" length="255" />
<field name="NAME" type="String" label="الشيفت" length="255" />
<field name="str_date_emp" type="DateTime"   label="str_date_emp" showInSummary="true" />
<field name="end_date_emp" type="DateTime"   label="end_date_emp" showInSummary="true" />
<field name="Emp_No" type="Int64" allowNulls="false"  label="Emp No" readOnly="true" />
<field name="Emp_NOt" type="String" allowNulls="false" label="Emp N Ot" readOnly="true" length="50" showInSummary="true" />
<field name="Emp_Nm" type="String" label="Emp Nm" length="255" readOnly="true"/>
<field name="CYLE" type="Int16" default="((1))" label="Cyle" showInSummary="true" />
<field name="UNITS" type="Int16" default="((1))" label="Units" />
    <field name="branch" type="Int64" label="Branch" allowQBE="true" readOnly="true" >
      <items style="DropDownList" dataController="schBranch" newDataView="createForm1" searchOnStart="true" dataView="grid1" dataTextField="Desc1" dataValueField="branch"  />
    </field>
    
<field name="incrm" isPrimaryKey="true" type="Int64" label="incrm"/>

  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="User Of Run">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
       <dataField fieldName="USERID" columns="10" />
       <dataField fieldName="NUM_OF_RUN_ID" columns="10" />
        <dataField fieldName="STARTDATE" columns="10" />
        <dataField fieldName="ENDDATE" columns="10" />
        <dataField fieldName="UNITSNAME" columns="15" />
        <dataField fieldName="NAME" columns="15" />
        <dataField fieldName="branch" columns="15" />
        <dataField fieldName="str_date_emp" columns="10" />
        <dataField fieldName="end_date_emp" columns="10" />

<dataField fieldName="Emp_Nm" columns="10" />
<dataField fieldName="Emp_NOt" columns="10" />

<dataField fieldName="incrm" columns="10" />

      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review User Of Run">
      <headerText>$DefaultEditViewDescription</headerText>
