            <dataField fieldName="ENDDATE" columns="10" />
            <dataField fieldName="ISNOTOF_RUN" columns="15" />
            <dataField fieldName="ORDER_RUN" columns="15" />


        <dataField fieldName="str_date_emp" columns="10" />
        <dataField fieldName="end_date_emp" columns="10" />


          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New User Of Run">
