	,"Thebranchschtyp"."schtypDesc" "ThebranchschtypschtypDesc"
	,"schregacdmicrul"."acdcode" "acdcode"
	,"acd"."AcademicYear" "acdAcademicYear"
	,"acdGlFinperiod"."Fin_period_info" "acdGlFinperiodFin_period_info"
	,"acdGlFinperiodaccountnumber"."Acc_Nm" "acdGlFinperiodaccountnumberAcc_Nm"
	,"schregacdmicrul"."GradeCode" "GradeCode"
	,"Grade"."ShortDesc1" "GradeShortDesc1"
	,"Gradeacd"."AcademicYear" "GradeacdAcademicYear"
	,"GradeacdGlFinperiod"."Fin_period_info" "GradeacdGlFinperiodFin_period_info"
	,"GradeStage"."ShortDesc1" "GradeStageShortDesc1"
	,"schregacdmicrul"."Amount" "Amount"
	,"schregacdmicrul"."amounttype" "amounttype"
	,"amounttype"."schpaymenttypenm" "amounttypeschpaymenttypenm"
	,"amounttypedebitaccount"."Acc_Nm" "amounttypedebitaccountAcc_Nm"
	,"amounttypedebitaccountClsacc_"."Acc_Nm" "amounttypedebitaccountClsacc_Acc_Nm"
	,"amounttypedebitaccountAcc_Bnd"."Acc_Nm" "amounttypedebitaccountAcc_BndAcc_Nm"
	,"amounttypecreditaccount"."Acc_Nm" "amounttypecreditaccountAcc_Nm"
	,"amounttypecreditaccountClsacc_"."Acc_Nm" "amounttypecreditaccountClsacc_Acc_Nm"
	,"amounttypecreditaccountAcc_Bnd"."Acc_Nm" "amounttypecreditaccountAcc_BndAcc_Nm"
	,"schregacdmicrul"."instalmmentdutydate" "instalmmentdutydate"
from "dbo"."schregacdmicrul" "schregacdmicrul"
	left join "dbo"."schBranch" "Thebranch" on "schregacdmicrul"."branch" = "Thebranch"."branch"
	left join "dbo"."glmulcmp" "Thebranchsgm" on "Thebranch"."sgm" = "Thebranchsgm"."sgmid"
	left join "dbo"."OpCo" "ThebranchsgmOpco" on "Thebranchsgm"."OpcoID" = "ThebranchsgmOpco"."OpcoIDidentity"
	left join "dbo"."PyGender" "ThebranchGender" on "Thebranch"."Genderid" = "ThebranchGender"."Genderid"
	left join "dbo"."schStage" "Thebranchstage" on "Thebranch"."stagecode" = "Thebranchstage"."Code"
	left join "dbo"."schtyp" "Thebranchschtyp" on "Thebranch"."schtypid" = "Thebranchschtyp"."schtypid"
	left join "dbo"."schAcademicyear" "acd" on "schregacdmicrul"."acdcode" = "acd"."Code"
	left join "dbo"."GlFinanceperiod" "acdGlFinperiod" on "acd"."GlFinperiodID" = "acdGlFinperiod"."GlFinperiodID"
	left join "dbo"."GLmstrfl" "acdGlFinperiodaccountnumber" on "acdGlFinperiod"."accountnumber" = "acdGlFinperiodaccountnumber"."Acc_No"
	left join "dbo"."schGrade" "Grade" on "schregacdmicrul"."GradeCode" = "Grade"."GradeCode"
	left join "dbo"."schAcademicyear" "Gradeacd" on "Grade"."acdcode" = "Gradeacd"."Code"
	left join "dbo"."GlFinanceperiod" "GradeacdGlFinperiod" on "Gradeacd"."GlFinperiodID" = "GradeacdGlFinperiod"."GlFinperiodID"
	left join "dbo"."schStage" "GradeStage" on "Grade"."StageCode" = "GradeStage"."Code"
	left join "dbo"."schpaymenttype" "amounttype" on "schregacdmicrul"."amounttype" = "amounttype"."schpaymenttypeid"
	left join "dbo"."GLmstrfl" "amounttypedebitaccount" on "amounttype"."debitaccount" = "amounttypedebitaccount"."Acc_No"
	left join "dbo"."GLmfbab" "amounttypedebitaccountClsacc_" on "amounttypedebitaccount"."Clsacc_No" = "amounttypedebitaccountClsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "amounttypedebitaccountAcc_Bnd" on "amounttypedebitaccount"."Acc_Bnd" = "amounttypedebitaccountAcc_Bnd"."Acc_Bnd"
	left join "dbo"."GLmstrfl" "amounttypecreditaccount" on "amounttype"."creditaccount" = "amounttypecreditaccount"."Acc_No"
	left join "dbo"."GLmfbab" "amounttypecreditaccountClsacc_" on "amounttypecreditaccount"."Clsacc_No" = "amounttypecreditaccountClsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "amounttypecreditaccountAcc_Bnd" on "amounttypecreditaccount"."Acc_Bnd" = "amounttypecreditaccountAcc_Bnd"."Acc_Bnd"
]]></text>
