	,"cacotractmain"."ModifiedBy" "ModifiedBy"
	,"cacotractmain"."ModifiedOn" "ModifiedOn"
	,"cacotractmain"."CreatedBy" "CreatedBy"
	,"cacotractmain"."CreatedOn" "CreatedOn"
from "dbo"."cacotractmain" "cacotractmain"
	left join "dbo"."contractstatus" "contractstatus" on "cacotractmain"."contractstatus" = "contractstatus"."cntstcode"
]]></text>
    </command>
    <command id="contractidIdentityCommand" type="Text" event="Inserted">
