                         dbo.election5create2016.elwazefa, dbo.election5create2016.elenwan, dbo.election5create2016.esmeldamen, dbo.election5create2016.eldaera, dbo.election5create2016.comenot, dbo.election5create2016.fieldeht, 
                         dbo.election5create2016.fieldeht1, dbo.election5create2016.fieldeht2, dbo.election5create2016.fieldeht3, dbo.election5create2016.fieldeht31, dbo.election5create2016.fieldeht32, 
                         dbo.election5create2016.fieldeht33, dbo.election5create2016.branch, dbo.election5create2016.ModifiedBy, dbo.election5create2016.ModifiedOn, dbo.election5create2016.CreatedBy, 
                         dbo.election5create2016.CreatedOn, dbo.election5create2016.eleclganid, dbo.electionlegantable.mandobname, dbo.electionlegantable.telphonemandob, dbo.electionlegantable.lgna_no, 
                         dbo.electionlegantable.wkel_name, dbo.electionlegantable.madny, dbo.electionlegantable.elhodoor, dbo.electionlegantable.totalaswat
FROM            dbo.electionlegantable RIGHT OUTER JOIN
                         dbo.election5create2016 ON dbo.electionlegantable.lgna_no = dbo.election5create2016.fieldeht  
]]></text>
    </command>
