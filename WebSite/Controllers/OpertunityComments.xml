  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"OpertunityComments"."OpertunityCommentsID" "OpertunityCommentsID"
	,"OpertunityComments"."Modid" "Modid"
	,"Mod"."MODAr" "ModMODAr"
	,"OpertunityComments"."FormDescription" "FormDescription"
	,"OpertunityComments"."OpertunityID" "OpertunityID"
	,"Opertunity"."OpertunityDesc" "OpertunityOpertunityDesc"
	,"Opertunitycall"."customertel1" "Opertunitycallcustomertel1"
	,"OpertunitycallMarketplan"."MarketplanDesc" "OpertunitycallMarketplanMarketplanDesc"
	,"Opertunitycallproducts"."productsname" "Opertunitycallproductsproductsname"
	,"OpertunitycallSlm_"."Slm_Nm" "OpertunitycallSlm_Slm_Nm"
	,"OpertunitycallVisitstatus"."Visitstatusname" "OpertunitycallVisitstatusVisitstatusname"
	,"Opertunitycallvisittype"."visittypeNm" "OpertunitycallvisittypevisittypeNm"
	,"OpertunityOpertunityclass"."Opertunityclass" "OpertunityOpertunityclassOpertunityclass"
	,"OpertunityOpertunitydegree"."Opertunitydegree" "OpertunityOpertunitydegreeOpertunitydegree"
	,"OpertunityCstm_"."Cstm_Nm" "OpertunityCstm_Cstm_Nm"
	,"OpertunityCstm_Area"."Area_name" "OpertunityCstm_AreaArea_name"
	,"OpertunityCstm_Evaluation"."Evaluation_name" "OpertunityCstm_EvaluationEvaluation_name"
	,"OpertunityCstm_acc_"."Acc_Nm" "OpertunityCstm_acc_Acc_Nm"
	,"OpertunityCstm_Cntry_"."Cntry_Nm" "OpertunityCstm_Cntry_Cntry_Nm"
	,"OpertunityCstm_curncy_"."Curncy_Nm" "OpertunityCstm_curncy_Curncy_Nm"
	,"OpertunityCstm_TSNEF"."TSNEF_name" "OpertunityCstm_TSNEFTSNEF_name"
	,"OpertunityCstm_TSNEFNO1"."TSNEF1_name" "OpertunityCstm_TSNEFNO1TSNEF1_name"
	,"OpertunityCstm_Slm_"."Slm_Nm" "OpertunityCstm_Slm_Slm_Nm"
	,"OpertunitySlm_"."Slm_Nm" "OpertunitySlm_Slm_Nm"
	,"OpertunitySlm_masngerclass"."masngerclass" "OpertunitySlm_masngerclassmasngerclass"
	,"Opertunityopenslm_"."Slm_Nm" "Opertunityopenslm_Slm_Nm"
	,"Opertunityopenslm_masngerclass"."masngerclass" "Opertunityopenslm_masngerclassmasngerclass"
	,"OpertunityComments"."TaskEVENT" "TaskEVENT"
	,"OpertunityComments"."TaskCommentsEnterDate" "TaskCommentsEnterDate"
	,"OpertunityComments"."TaskCommentsDelvery" "TaskCommentsDelvery"
	,"OpertunityComments"."CommentsNotes" "CommentsNotes"
	,"OpertunityComments"."CommentsDetailes" "CommentsDetailes"
	,"OpertunityComments"."ModifiedBy" "ModifiedBy"
	,"OpertunityComments"."ModifiedOn" "ModifiedOn"
	,"OpertunityComments"."CreatedBy" "CreatedBy"
	,"OpertunityComments"."CreatedOn" "CreatedOn"
from "dbo"."OpertunityComments" "OpertunityComments"
	left join "dbo"."MODMaster" "Mod" on "OpertunityComments"."Modid" = "Mod"."MODID"
	left join "dbo"."Opertunity" "Opertunity" on "OpertunityComments"."OpertunityID" = "Opertunity"."OpertunityID"
	left join "dbo"."callregister" "Opertunitycall" on "Opertunity"."callid" = "Opertunitycall"."callid"
	left join "dbo"."Marketplan" "OpertunitycallMarketplan" on "Opertunitycall"."MarketplanID" = "OpertunitycallMarketplan"."MarketplanID"
	left join "dbo"."ProductsList" "Opertunitycallproducts" on "Opertunitycall"."productsID" = "Opertunitycallproducts"."productsid"
	left join "dbo"."Stslsman" "OpertunitycallSlm_" on "Opertunitycall"."Slm_No" = "OpertunitycallSlm_"."Slm_No"
	left join "dbo"."Visitstatuslist" "OpertunitycallVisitstatus" on "Opertunitycall"."Visitstatus" = "OpertunitycallVisitstatus"."VisitstatusID"
	left join "dbo"."visittypelist" "Opertunitycallvisittype" on "Opertunitycall"."visittype" = "Opertunitycallvisittype"."visittypeNo"
	left join "dbo"."Opertunityclasslist" "OpertunityOpertunityclass" on "Opertunity"."OpertunityclassID" = "OpertunityOpertunityclass"."OpertunityclassID"
	left join "dbo"."Opertunitydegreelist" "OpertunityOpertunitydegree" on "Opertunity"."OpertunitydegreeID" = "OpertunityOpertunitydegree"."OpertunitydegreeID"
	left join "dbo"."Stcustmr" "OpertunityCstm_" on "Opertunity"."Cstm_no" = "OpertunityCstm_"."Cstm_No"
	left join "dbo"."AreaList" "OpertunityCstm_Area" on "OpertunityCstm_"."AreaNo" = "OpertunityCstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "OpertunityCstm_Evaluation" on "OpertunityCstm_"."EvaluationNO" = "OpertunityCstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "OpertunityCstm_acc_" on "OpertunityCstm_"."acc_no" = "OpertunityCstm_acc_"."Acc_No"
	left join "dbo"."PYCntry" "OpertunityCstm_Cntry_" on "OpertunityCstm_"."Cntry_No" = "OpertunityCstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "OpertunityCstm_curncy_" on "OpertunityCstm_"."curncy_no" = "OpertunityCstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "OpertunityCstm_TSNEF" on "OpertunityCstm_"."TSNEFNO" = "OpertunityCstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "OpertunityCstm_TSNEFNO1" on "OpertunityCstm_"."TSNEFNO1" = "OpertunityCstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "OpertunityCstm_Slm_" on "OpertunityCstm_"."Slm_No" = "OpertunityCstm_Slm_"."Slm_No"
	left join "dbo"."Stslsman" "OpertunitySlm_" on "Opertunity"."Slm_No" = "OpertunitySlm_"."Slm_No"
	left join "dbo"."masngerclass" "OpertunitySlm_masngerclass" on "OpertunitySlm_"."masngerclassid" = "OpertunitySlm_masngerclass"."masngerclassid"
	left join "dbo"."Stslsman" "Opertunityopenslm_" on "Opertunity"."openslm_no" = "Opertunityopenslm_"."Slm_No"
	left join "dbo"."masngerclass" "Opertunityopenslm_masngerclass" on "Opertunityopenslm_"."masngerclassid" = "Opertunityopenslm_masngerclass"."masngerclassid"
]]></text>
    </command>
