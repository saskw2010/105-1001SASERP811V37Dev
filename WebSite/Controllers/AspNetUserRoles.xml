  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"AspNetUserRoles"."UserId" "UserId"
	,"User"."UserName" "UserUserName"
	,"AspNetUserRoles"."RoleId" "RoleId"
	,"Role"."Name" "RoleName"
	,"AspNetUserRoles"."active" "active"
from "dbo"."AspNetUserRoles" "AspNetUserRoles"
	left join "dbo"."AspNetUsers" "User" on "AspNetUserRoles"."UserId" = "User"."Id"
	left join "dbo"."AspNetRoles" "Role" on "AspNetUserRoles"."RoleId" = "Role"."Id"
]]></text>
