  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"GLacnchr"."Account_No" "Account_No"
	,"GLacnchr"."Acc_Nm" "Acc_Nm"
	,"GLacnchr"."Acc_No" "Acc_No"
	,"Acc_"."Acc_Nm" "Acc_Acc_Nm"
	,"Acc_Clsacc_"."Acc_Nm" "Acc_Clsacc_Acc_Nm"
	,"Acc_Acc_Bnd"."Acc_Nm" "Acc_Acc_BndAcc_Nm"
	,"Acc_Acc_Bndaccclass"."accountclass" "Acc_Acc_Bndaccclassaccountclass"
	,"Acc_Acc_BndAcc_Bab"."Acc_Nm" "Acc_Acc_BndAcc_BabAcc_Nm"
	,"GLacnchr"."sgm" "sgm"
	,"sgm"."sgm_Nm" "sgmsgm_Nm"
	,"sgmOpco"."OpcoName" "sgmOpcoOpcoName"
	,"GLacnchr"."branch" "branch"
	,"Thebranch"."Desc1" "ThebranchDesc1"
	,"Thebranchsgm"."sgm_Nm" "Thebranchsgmsgm_Nm"
	,"ThebranchsgmOpco"."OpcoName" "ThebranchsgmOpcoOpcoName"
	,"ThebranchGender"."Gender" "ThebranchGenderGender"
	,"Thebranchstage"."ShortDesc1" "ThebranchstageShortDesc1"
	,"Thebranchschtyp"."schtypDesc" "ThebranchschtypschtypDesc"
	,"GLacnchr"."pronsabilites" "pronsabilites"
	,"GLacnchr"."ModifiedBy" "ModifiedBy"
	,"GLacnchr"."ModifiedOn" "ModifiedOn"
	,"GLacnchr"."CreatedBy" "CreatedBy"
	,"GLacnchr"."CreatedOn" "CreatedOn"
from "dbo"."GLacnchr" "GLacnchr"
	left join "dbo"."GLmstrfl" "Acc_" on "GLacnchr"."Acc_No" = "Acc_"."Acc_No"
	left join "dbo"."GLmfbab" "Acc_Clsacc_" on "Acc_"."Clsacc_No" = "Acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Acc_Acc_Bnd" on "Acc_"."Acc_Bnd" = "Acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "Acc_Acc_Bndaccclass" on "Acc_Acc_Bnd"."accclass" = "Acc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "Acc_Acc_BndAcc_Bab" on "Acc_Acc_Bnd"."Acc_Bab" = "Acc_Acc_BndAcc_Bab"."Acc_Bab"
	left join "dbo"."glmulcmp" "sgm" on "GLacnchr"."sgm" = "sgm"."sgmid"
	left join "dbo"."OpCo" "sgmOpco" on "sgm"."OpcoID" = "sgmOpco"."OpcoIDidentity"
	left join "dbo"."schBranch" "Thebranch" on "GLacnchr"."branch" = "Thebranch"."branch"
	left join "dbo"."glmulcmp" "Thebranchsgm" on "Thebranch"."sgm" = "Thebranchsgm"."sgmid"
	left join "dbo"."OpCo" "ThebranchsgmOpco" on "Thebranchsgm"."OpcoID" = "ThebranchsgmOpco"."OpcoIDidentity"
	left join "dbo"."PyGender" "ThebranchGender" on "Thebranch"."Genderid" = "ThebranchGender"."Genderid"
	left join "dbo"."schStage" "Thebranchstage" on "Thebranch"."stagecode" = "Thebranchstage"."Code"
	left join "dbo"."schtyp" "Thebranchschtyp" on "Thebranch"."schtypid" = "Thebranchschtyp"."schtypid"
]]></text>
    </command>
    <command id="Account_NoIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Account_No" />
      </output>
    </command>
  </commands>
