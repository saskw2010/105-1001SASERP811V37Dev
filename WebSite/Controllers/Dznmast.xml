select
	"Dznmast"."countrd" "countrd"
	,"Dznmast"."countrdus" "countrdus"
	,"Dznmast"."Brn_No" "Brn_No"
	,"Brn_"."Brn_Nm" "Brn_Brn_Nm"
	,"Brn_Branch"."Desc1" "Brn_BranchDesc1"
	,"Brn_Branchsgm"."sgm_Nm" "Brn_Branchsgmsgm_Nm"
	,"Brn_BranchGender"."Gender" "Brn_BranchGenderGender"
	,"Brn_Branchstage"."ShortDesc1" "Brn_BranchstageShortDesc1"
	,"Brn_Branchschtyp"."schtypDesc" "Brn_BranchschtypschtypDesc"
	,"Dznmast"."ID_USERM" "ID_USERM"
	,"Dznmast"."subject" "subject"
	,"Dznmast"."azndate" "azndate"
	,"Dznmast"."aznegmaly" "aznegmaly"
	,"Dznmast"."Tarhel" "Tarhel"
	,"Dznmast"."sgm" "sgm"
	,"sgm"."sgm_Nm" "sgmsgm_Nm"
	,"sgmOpco"."OpcoName" "sgmOpcoOpcoName"
	,"Dznmast"."ModifiedBy" "ModifiedBy"
	,"Dznmast"."ModifiedOn" "ModifiedOn"
	,"Dznmast"."CreatedBy" "CreatedBy"
	,"Dznmast"."CreatedOn" "CreatedOn"
	,"Dznmast"."branch" "branch"
	,"Thebranch"."Desc1" "ThebranchDesc1"
	,"Thebranchsgm"."sgm_Nm" "Thebranchsgmsgm_Nm"
	,"ThebranchsgmOpco"."OpcoName" "ThebranchsgmOpcoOpcoName"
	,"ThebranchGender"."Gender" "ThebranchGenderGender"
	,"Thebranchstage"."ShortDesc1" "ThebranchstageShortDesc1"
	,"Thebranchschtyp"."schtypDesc" "ThebranchschtypschtypDesc"
	,"Dznmast"."ID_comp" "ID_comp"
	,"TheID_comp"."ID_USER" "TheID_compID_USER"
	,"TheID_compIDGROUP"."ID_USER" "TheID_compIDGROUPID_USER"
	,"TheID_compSup_"."Item_name" "TheID_compSup_Item_name"
	,"TheID_compid1"."DESCRP1" "TheID_compid1DESCRP1"
	,"TheID_compid2"."DESCRP1" "TheID_compid2DESCRP1"
	,"TheID_compid3"."DESCRP1" "TheID_compid3DESCRP1"
	,"TheID_compid4"."DESCRP1" "TheID_compid4DESCRP1"
	,"TheID_compid5"."DESCRP1" "TheID_compid5DESCRP1"
	,"TheID_compid6"."DESCRP1" "TheID_compid6DESCRP1"
	,"TheID_compid7"."DESCRP1" "TheID_compid7DESCRP1"


	,"Dznmast"."Unit_No" "Unit_No"
	,"Unit_"."Unit_Nm" "Unit_Unit_Nm"	
	,"Dznmast"."Quantity" "Quantity"
	,"Dznmast"."Itm_Cost" "Itm_Cost"	
	,"Dznmast"."Itemprice" "Itemprice"	
from "dbo"."Dznmast" "Dznmast"
	left join "dbo"."Branchcnst" "Brn_" on "Dznmast"."Brn_No" = "Brn_"."Brn_No"
	left join "dbo"."schBranch" "Brn_Branch" on "Brn_"."Branch" = "Brn_Branch"."branch"
	left join "dbo"."glmulcmp" "Brn_Branchsgm" on "Brn_Branch"."sgm" = "Brn_Branchsgm"."sgmid"
	left join "dbo"."PyGender" "Brn_BranchGender" on "Brn_Branch"."Genderid" = "Brn_BranchGender"."Genderid"
	left join "dbo"."schStage" "Brn_Branchstage" on "Brn_Branch"."stagecode" = "Brn_Branchstage"."Code"
	left join "dbo"."schtyp" "Brn_Branchschtyp" on "Brn_Branch"."schtypid" = "Brn_Branchschtyp"."schtypid"
	left join "dbo"."glmulcmp" "sgm" on "Dznmast"."sgm" = "sgm"."sgmid"
	left join "dbo"."OpCo" "sgmOpco" on "sgm"."OpcoID" = "sgmOpco"."OpcoIDidentity"
	left join "dbo"."schBranch" "Thebranch" on "Dznmast"."branch" = "Thebranch"."branch"
	left join "dbo"."glmulcmp" "Thebranchsgm" on "Thebranch"."sgm" = "Thebranchsgm"."sgmid"
	left join "dbo"."OpCo" "ThebranchsgmOpco" on "Thebranchsgm"."OpcoID" = "ThebranchsgmOpco"."OpcoIDidentity"
	left join "dbo"."PyGender" "ThebranchGender" on "Thebranch"."Genderid" = "ThebranchGender"."Genderid"
	left join "dbo"."schStage" "Thebranchstage" on "Thebranch"."stagecode" = "Thebranchstage"."Code"
	left join "dbo"."schtyp" "Thebranchschtyp" on "Thebranch"."schtypid" = "Thebranchschtyp"."schtypid"
	left join "dbo"."ITEMS" "TheID_comp" on "Dznmast"."ID_comp" = "TheID_comp"."ID_comp"
	left join "dbo"."GROUPStk" "TheID_compIDGROUP" on "TheID_comp"."IDGROUP" = "TheID_compIDGROUP"."IDGROUP"
	left join "dbo"."ItemList" "TheID_compSup_" on "TheID_comp"."Sup_No" = "TheID_compSup_"."ItemNo"
	left join "dbo"."PYHldtypsv1" "TheID_compid1" on "TheID_comp"."id1" = "TheID_compid1"."ID1"
	left join "dbo"."PYHldtypsv2" "TheID_compid2" on "TheID_comp"."id2" = "TheID_compid2"."ID2"
	left join "dbo"."PYHldtypsv3" "TheID_compid3" on "TheID_comp"."id3" = "TheID_compid3"."id3"
	left join "dbo"."PYHldtypsv4" "TheID_compid4" on "TheID_comp"."id4" = "TheID_compid4"."id4"
	left join "dbo"."PYHldtypsv5" "TheID_compid5" on "TheID_comp"."id5" = "TheID_compid5"."id5"
	left join "dbo"."PYHldtypsv6" "TheID_compid6" on "TheID_comp"."id6" = "TheID_compid6"."id6"
	left join "dbo"."PYHldtypsv7" "TheID_compid7" on "TheID_comp"."id7" = "TheID_compid7"."id7"
	left join "dbo"."Stunit" "Unit_" on "Dznmast"."Unit_No" = "Unit_"."Unit_No"	
]]></text>
