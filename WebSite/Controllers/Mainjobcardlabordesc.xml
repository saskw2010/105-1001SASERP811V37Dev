  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Mainjobcardlabordesc"."ln_no" "ln_no"
	,"Mainjobcardlabordesc"."jobcardid" "jobcardid"
	,"jobcard"."Major_complains" "jobcardMajor_complains"
	,"jobcardlabor_"."Labor_nm" "jobcardlabor_Labor_nm"
	,"Mainjobcardlabordesc"."Jobcard_datein" "Jobcard_datein"
	,"Mainjobcardlabordesc"."Jobcard_timein" "Jobcard_timein"
	,"Mainjobcardlabordesc"."labor_no" "labor_no"
	,"labor_"."Labor_nm" "labor_Labor_nm"
	,"Mainjobcardlabordesc"."Work_desc" "Work_desc"
	,"Mainjobcardlabordesc"."Labor_cost" "Labor_cost"
	,"Mainjobcardlabordesc"."labor_timehors" "labor_timehors"
	,"Mainjobcardlabordesc"."labor_timemints1" "labor_timemints1"
	,"Mainjobcardlabordesc"."ModifiedBy" "ModifiedBy"
	,"Mainjobcardlabordesc"."ModifiedOn" "ModifiedOn"
	,"Mainjobcardlabordesc"."CreatedBy" "CreatedBy"
	,"Mainjobcardlabordesc"."CreatedOn" "CreatedOn"
from "dbo"."Mainjobcardlabordesc" "Mainjobcardlabordesc"
	left join "dbo"."Mainjobcard" "jobcard" on "Mainjobcardlabordesc"."jobcardid" = "jobcard"."jobcardid"
	left join "dbo"."MainLabors" "jobcardlabor_" on "jobcard"."labor_no" = "jobcardlabor_"."Labor_no"
	left join "dbo"."MainLabors" "labor_" on "Mainjobcardlabordesc"."labor_no" = "labor_"."Labor_no"
]]></text>
    </command>
    <command id="ln_noIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="ln_no" />
      </output>
    </command>
  </commands>
