  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"SoftwareProjectsModuleslist"."sprid" "sprid"
	,"spr"."sprnotes" "sprsprnotes"
	,"sprsprcustm"."sprcustomername" "sprsprcustmsprcustomername"
	,"sprsprcustmsprcustomerclass1"."sprcustomerclass1Desc" "sprsprcustmsprcustomerclass1sprcustomerclass1Desc"
	,"sprsprcustmsprcustomerclass2"."sprcustomerclass2Desc" "sprsprcustmsprcustomerclass2sprcustomerclass2Desc"
	,"sprsprcustmsprcustomerclass3"."sprcustomerclass3Desc" "sprsprcustmsprcustomerclass3sprcustomerclass3Desc"
	,"sprsprcustmsprcustomerclass4"."sprcustomerclass4Desc" "sprsprcustmsprcustomerclass4sprcustomerclass4Desc"
	,"sprsprslm"."sprpersonname" "sprsprslmsprpersonname"
	,"sprsprslmsprjob"."sprjobdesc" "sprsprslmsprjobsprjobdesc"
	,"sprsprleader"."sprpersonname" "sprsprleadersprpersonname"
	,"sprsprleadersprjob"."sprjobdesc" "sprsprleadersprjobsprjobdesc"
	,"sprsprdesigner"."sprpersonname" "sprsprdesignersprpersonname"
	,"sprsprdesignersprjob"."sprjobdesc" "sprsprdesignersprjobsprjobdesc"
	,"sprsprtype"."sprtypeDesc" "sprsprtypesprtypeDesc"
	,"SoftwareProjectsModuleslist"."sprModuleid" "sprModuleid"
	,"sprModule"."sprModuleDesc" "sprModulesprModuleDesc"
	,"sprModulesprModulecat"."sprModulecatDesc" "sprModulesprModulecatsprModulecatDesc"
from "dbo"."SoftwareProjectsModuleslist" "SoftwareProjectsModuleslist"
	left join "dbo"."SoftwareProjects" "spr" on "SoftwareProjectsModuleslist"."sprid" = "spr"."sprid"
	left join "dbo"."Softwareprojectcustomers" "sprsprcustm" on "spr"."sprcustmno" = "sprsprcustm"."sprcustmno"
	left join "dbo"."softwareprojectcustomerclass1" "sprsprcustmsprcustomerclass1" on "sprsprcustm"."sprcustomerclass1id" = "sprsprcustmsprcustomerclass1"."sprcustomerclass1id"
	left join "dbo"."softwareprojectcustomerclass2" "sprsprcustmsprcustomerclass2" on "sprsprcustm"."sprcustomerclass2id" = "sprsprcustmsprcustomerclass2"."sprcustomerclass2id"
	left join "dbo"."softwareprojectcustomerclass3" "sprsprcustmsprcustomerclass3" on "sprsprcustm"."sprcustomerclass3id" = "sprsprcustmsprcustomerclass3"."sprcustomerclass3id"
	left join "dbo"."softwareprojectcustomerclass4" "sprsprcustmsprcustomerclass4" on "sprsprcustm"."sprcustomerclass4id" = "sprsprcustmsprcustomerclass4"."sprcustomerclass4id"
	left join "dbo"."softwareprojectsteam" "sprsprslm" on "spr"."sprslmno" = "sprsprslm"."softwareprojecttid"
	left join "dbo"."sprjobs" "sprsprslmsprjob" on "sprsprslm"."sprjob" = "sprsprslmsprjob"."sprjobid"
	left join "dbo"."softwareprojectsteam" "sprsprleader" on "spr"."sprleader" = "sprsprleader"."softwareprojecttid"
	left join "dbo"."sprjobs" "sprsprleadersprjob" on "sprsprleader"."sprjob" = "sprsprleadersprjob"."sprjobid"
	left join "dbo"."softwareprojectsteam" "sprsprdesigner" on "spr"."sprdesigner" = "sprsprdesigner"."softwareprojecttid"
	left join "dbo"."sprjobs" "sprsprdesignersprjob" on "sprsprdesigner"."sprjob" = "sprsprdesignersprjob"."sprjobid"
	left join "dbo"."softwareprojecttype" "sprsprtype" on "spr"."sprtype" = "sprsprtype"."sprtypeid"
	left join "dbo"."SoftwareModuleslist" "sprModule" on "SoftwareProjectsModuleslist"."sprModuleid" = "sprModule"."sprModuleid"
	left join "dbo"."SoftwareModulescatlist" "sprModulesprModulecat" on "sprModule"."sprModulecatid" = "sprModulesprModulecat"."sprModulecatid"
]]></text>
    </command>
  </commands>
