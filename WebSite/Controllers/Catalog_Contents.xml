  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Catalog_Contents"."catalog_entry_id" "catalog_entry_id"
	,"Catalog_Contents"."catalog_level_number" "catalog_level_number"
	,"Thecatalog_level_number"."catalog_level_name" "Thecatalog_level_numbercatalog_level_name"
	,"Thecatalog_level_numbercatalog_"."catalog_name" "Thecatalog_level_numbercatalog_catalog_name"
	,"Catalog_Contents"."parent_entry_id" "parent_entry_id"
	,"Catalog_Contents"."previous_entry_id" "previous_entry_id"
	,"Catalog_Contents"."next_entry_id" "next_entry_id"
	,"Catalog_Contents"."catalog_entry_name" "catalog_entry_name"
	,"Catalog_Contents"."manufacturer" "manufacturer"
	,"Catalog_Contents"."product_reference_number" "product_reference_number"
	,"Catalog_Contents"."product_description" "product_description"
	,"Catalog_Contents"."photo_filename" "photo_filename"
	,"Catalog_Contents"."price_in_dollars" "price_in_dollars"
	,"Catalog_Contents"."price_in_euros" "price_in_euros"
	,"Catalog_Contents"."price_in_pounds" "price_in_pounds"
	,"Catalog_Contents"."capacity" "capacity"
	,"Catalog_Contents"."length" "length"
	,"Catalog_Contents"."height" "height"
	,"Catalog_Contents"."width" "width"
	,"Catalog_Contents"."other_details" "other_details"
from "dbo"."Catalog_Contents" "Catalog_Contents"
	left join "dbo"."Catalog_Structure" "Thecatalog_level_number" on "Catalog_Contents"."catalog_level_number" = "Thecatalog_level_number"."catalog_level_number"
	left join "dbo"."Catalogs" "Thecatalog_level_numbercatalog_" on "Thecatalog_level_number"."catalog_id" = "Thecatalog_level_numbercatalog_"."catalog_id"
]]></text>
    </command>
    <command id="catalog_entry_idIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="catalog_entry_id" />
      </output>
    </command>
  </commands>
