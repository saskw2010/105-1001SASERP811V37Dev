  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Tbl_MD"."MDID" "MDID"
	,"Tbl_MD"."MD_Name" "MD_Name"
	,"Tbl_MD"."activeflage" "activeflage"
	,"Tbl_MD"."MD_Name1" "MD_Name1"
	,"Tbl_MD"."joindate" "joindate"
	,"Tbl_MD"."usercodename" "usercodename"
	,"Tbl_MD"."branch" "branch"
	
from "dbo"."Tbl_MD" "Tbl_MD"
]]></text>
    </command>
    <command id="MDIDIdentityCommand" type="Text" event="Inserted">
