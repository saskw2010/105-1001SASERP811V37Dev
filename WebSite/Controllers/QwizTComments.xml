    <command id="command1" type="Text">
      <text><![CDATA[
select
	"QwizTComments"."qwizid" "qwizid"
	,"qwiz"."qwizquastion" "qwizqwizquastion"
	,"qwizpragraph"."pragraphtitle" "qwizpragraphpragraphtitle"
	,"qwizcatid1"."cat1name" "qwizcatid1cat1name"
	,"qwizcatid10"."cat10name" "qwizcatid10cat10name"
	,"qwizcatid11"."cat11name" "qwizcatid11cat11name"
	,"qwizcatid12"."cat12name" "qwizcatid12cat12name"
	,"qwizcatid13"."cat13name" "qwizcatid13cat13name"
	,"qwizcatid14"."cat13name" "qwizcatid14cat13name"
	,"qwizcatid2"."cat2name" "qwizcatid2cat2name"
	,"qwizcatid2Cntry_"."Cntry_Nm" "qwizcatid2Cntry_Cntry_Nm"
	,"qwizcatid3"."cat3name" "qwizcatid3cat3name"
	,"qwizcatid3catid2"."cat2name" "qwizcatid3catid2cat2name"
	,"qwizcatid4"."cat4name" "qwizcatid4cat4name"
	,"qwizcatid5"."cat5name" "qwizcatid5cat5name"
	,"qwizcatid6"."cat6name" "qwizcatid6cat6name"
	,"qwizcatid7"."cat7name" "qwizcatid7cat7name"
	,"qwizcatid8"."cat8name" "qwizcatid8cat8name"
	,"qwizcatid9"."cat9name" "qwizcatid9cat9name"
	,"qwizqwizcatgory"."CatgoryTitle" "qwizqwizcatgoryCatgoryTitle"
	,"QwizTComments"."qwizTComments" "qwizTComments"
	,"QwizTComments"."qwizTCommentsid" "qwizTCommentsid"
from "dbo"."QwizTComments" "QwizTComments"
	left join "dbo"."Qwiz" "qwiz" on "QwizTComments"."qwizid" = "qwiz"."qwizid"
	left join "dbo"."pragraphRelated" "qwizpragraph" on "qwiz"."pragraphid" = "qwizpragraph"."pragraphid"
	left join "dbo"."Qwaizcat1" "qwizcatid1" on "qwiz"."catid1" = "qwizcatid1"."catid1"
	left join "dbo"."Qwaizcat10" "qwizcatid10" on "qwiz"."catid10" = "qwizcatid10"."catid10"
	left join "dbo"."Qwaizcat11" "qwizcatid11" on "qwiz"."catid11" = "qwizcatid11"."catid11"
	left join "dbo"."Qwaizcat12" "qwizcatid12" on "qwiz"."catid12" = "qwizcatid12"."catid12"
	left join "dbo"."Qwaizcat13" "qwizcatid13" on "qwiz"."catid13" = "qwizcatid13"."catid13"
	left join "dbo"."Qwaizcat14" "qwizcatid14" on "qwiz"."catid14" = "qwizcatid14"."catid14"
	left join "dbo"."Qwaizcat2" "qwizcatid2" on "qwiz"."catid2" = "qwizcatid2"."catid2"
	left join "dbo"."PYCntry" "qwizcatid2Cntry_" on "qwizcatid2"."Cntry_No" = "qwizcatid2Cntry_"."Cntry_No"
	left join "dbo"."Qwaizcat3" "qwizcatid3" on "qwiz"."catid3" = "qwizcatid3"."catid3"
	left join "dbo"."Qwaizcat2" "qwizcatid3catid2" on "qwizcatid3"."catid2" = "qwizcatid3catid2"."catid2"
	left join "dbo"."Qwaizcat4" "qwizcatid4" on "qwiz"."catid4" = "qwizcatid4"."catid4"
	left join "dbo"."Qwaizcat5" "qwizcatid5" on "qwiz"."catid5" = "qwizcatid5"."catid5"
	left join "dbo"."Qwaizcat6" "qwizcatid6" on "qwiz"."catid6" = "qwizcatid6"."catid6"
	left join "dbo"."Qwaizcat7" "qwizcatid7" on "qwiz"."catid7" = "qwizcatid7"."catid7"
	left join "dbo"."Qwaizcat8" "qwizcatid8" on "qwiz"."catid8" = "qwizcatid8"."catid8"
	left join "dbo"."Qwaizcat9" "qwizcatid9" on "qwiz"."catid9" = "qwizcatid9"."catid9"
	left join "dbo"."QwizCatgory" "qwizqwizcatgory" on "qwiz"."qwizcatgoryid" = "qwizqwizcatgory"."QwizCatgoryid"
]]></text>
