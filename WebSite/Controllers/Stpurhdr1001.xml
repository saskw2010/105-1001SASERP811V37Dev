	,"sgm"."sgm_Nm" "sgmsgm_Nm"
	,"sgmOpco"."OpcoName" "sgmOpcoOpcoName"
	,"Stpurhdr"."acdcode" "acdcode"
	,"acd"."AcademicYear" "acdAcademicYear"
	,"acdGlFinperiod"."Fin_period_info" "acdGlFinperiodFin_period_info"
	,"acdGlFinperiodaccountnumber"."Acc_Nm" "acdGlFinperiodaccountnumberAcc_Nm"
	,"Stpurhdr"."Brn_No" "Brn_No"
	,"Brn_"."Brn_Nm" "Brn_Brn_Nm"
	,"Brn_Cmp_"."Grp_Nm" "Brn_Cmp_Grp_Nm"
	,"Brn_Cmp_Grp_"."Grp_Nm" "Brn_Cmp_Grp_Grp_Nm"
	,"Stpurhdr"."Referanceno" "Referanceno"
	,"Stpurhdr"."Reftxt" "Reftxt"
	,"Stpurhdr"."Pym_No" "Pym_No"
	,"Pym_"."Pymnt_Nm" "Pym_Pymnt_Nm"
	,"Pym_pymnt_acc"."Acc_Nm" "Pym_pymnt_accAcc_Nm"
	,"Pym_pymnt_accClsacc_"."Acc_Nm" "Pym_pymnt_accClsacc_Acc_Nm"
	,"Pym_pymnt_accAcc_Bnd"."Acc_Nm" "Pym_pymnt_accAcc_BndAcc_Nm"
	,"Pym_sgm"."sgm_Nm" "Pym_sgmsgm_Nm"
	,"Pym_sgmOpco"."OpcoName" "Pym_sgmOpcoOpcoName"
	,"Stpurhdr"."Slm_No" "Slm_No"
	,"Slm_"."Slm_Nm" "Slm_Slm_Nm"
	,"Slm_masngerclass"."masngerclass" "Slm_masngerclassmasngerclass"
	,"Stpurhdr"."Doc_Dt" "Doc_Dt"
	,"Stpurhdr"."Cstm_No" "Cstm_No"
	,"Cstm_"."Sup_Nm" "Cstm_Sup_Nm"
	,"Cstm_Acc_"."Acc_Nm" "Cstm_Acc_Acc_Nm"
	,"Cstm_Acc_Clsacc_"."Acc_Nm" "Cstm_Acc_Clsacc_Acc_Nm"
	,"Cstm_Acc_Acc_Bnd"."Acc_Nm" "Cstm_Acc_Acc_BndAcc_Nm"
	,"Cstm_Cntry_"."Cntry_Nm" "Cstm_Cntry_Cntry_Nm"
	,"Cstm_Curncy_"."Curncy_Nm" "Cstm_Curncy_Curncy_Nm"
	,"Stpurhdr"."Cstm_NM" "Cstm_NM"
	,"Stpurhdr"."Cstm_Add" "Cstm_Add"
	,"Stpurhdr"."Cstm_Tel" "Cstm_Tel"
	,"Stpurhdr"."Post" "Post"
	,"Stpurhdr"."Notes" "Notes"
	,"Stpurhdr"."KETAA" "KETAA"
	,"Stpurhdr"."STreet" "STreet"
