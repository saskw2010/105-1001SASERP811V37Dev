	,"Stpurhdr"."ModifiedOn" "ModifiedOn"
	,"Stpurhdr"."CreatedBy" "CreatedBy"
	,"Stpurhdr"."CreatedOn" "CreatedOn"
from "dbo"."Stpurhdr" "Stpurhdr"
	left join "dbo"."schBranch" "Thebranch" on "Stpurhdr"."branch" = "Thebranch"."branch"
	left join "dbo"."glmulcmp" "Thebranchsgm" on "Thebranch"."sgm" = "Thebranchsgm"."sgmid"
	left join "dbo"."OpCo" "ThebranchsgmOpco" on "Thebranchsgm"."OpcoID" = "ThebranchsgmOpco"."OpcoIDidentity"
	left join "dbo"."PyGender" "ThebranchGender" on "Thebranch"."Genderid" = "ThebranchGender"."Genderid"
	left join "dbo"."schStage" "Thebranchstage" on "Thebranch"."stagecode" = "Thebranchstage"."Code"
	left join "dbo"."schtyp" "Thebranchschtyp" on "Thebranch"."schtypid" = "Thebranchschtyp"."schtypid"
	left join "dbo"."glmulcmp" "sgm" on "Stpurhdr"."sgm" = "sgm"."sgmid"
	left join "dbo"."OpCo" "sgmOpco" on "sgm"."OpcoID" = "sgmOpco"."OpcoIDidentity"
	left join "dbo"."schAcademicyear" "acd" on "Stpurhdr"."acdcode" = "acd"."Code"
	left join "dbo"."GlFinanceperiod" "acdGlFinperiod" on "acd"."GlFinperiodID" = "acdGlFinperiod"."GlFinperiodID"
	left join "dbo"."GLmstrfl" "acdGlFinperiodaccountnumber" on "acdGlFinperiod"."accountnumber" = "acdGlFinperiodaccountnumber"."Acc_No"
	left join "dbo"."Branchcnst" "Brn_" on "Stpurhdr"."Brn_No" = "Brn_"."Brn_No"
	left join "dbo"."Pycnstnt" "Brn_Cmp_" on "Brn_"."Cmp_code" = "Brn_Cmp_"."Cmp_No"
	left join "dbo"."Groupcnst" "Brn_Cmp_Grp_" on "Brn_Cmp_"."Grp_No" = "Brn_Cmp_Grp_"."Grp_No"
	left join "dbo"."Pymntype" "Pym_" on "Stpurhdr"."Pym_No" = "Pym_"."Pymnt_No"
	left join "dbo"."GLmstrfl" "Pym_pymnt_acc" on "Pym_"."pymnt_acc" = "Pym_pymnt_acc"."Acc_No"
	left join "dbo"."GLmfbab" "Pym_pymnt_accClsacc_" on "Pym_pymnt_acc"."Clsacc_No" = "Pym_pymnt_accClsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Pym_pymnt_accAcc_Bnd" on "Pym_pymnt_acc"."Acc_Bnd" = "Pym_pymnt_accAcc_Bnd"."Acc_Bnd"
	left join "dbo"."glmulcmp" "Pym_sgm" on "Pym_"."sgm" = "Pym_sgm"."sgmid"
	left join "dbo"."OpCo" "Pym_sgmOpco" on "Pym_sgm"."OpcoID" = "Pym_sgmOpco"."OpcoIDidentity"
	left join "dbo"."Stslsman" "Slm_" on "Stpurhdr"."Slm_No" = "Slm_"."Slm_No"
	left join "dbo"."masngerclass" "Slm_masngerclass" on "Slm_"."masngerclassid" = "Slm_masngerclass"."masngerclassid"
	left join "dbo"."Stsuplir" "Cstm_" on "Stpurhdr"."Cstm_No" = "Cstm_"."Sup_No"
	left join "dbo"."GLmstrfl" "Cstm_Acc_" on "Cstm_"."Acc_No" = "Cstm_Acc_"."Acc_No"
	left join "dbo"."GLmfbab" "Cstm_Acc_Clsacc_" on "Cstm_Acc_"."Clsacc_No" = "Cstm_Acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Cstm_Acc_Acc_Bnd" on "Cstm_Acc_"."Acc_Bnd" = "Cstm_Acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."PYCntry" "Cstm_Cntry_" on "Cstm_"."Cntry_No" = "Cstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Cstm_Curncy_" on "Cstm_"."Curncy_No" = "Cstm_Curncy_"."Curncy_No"
	left join "dbo"."GLSubledgertyplist" "typcmsup" on "Stpurhdr"."typcmsup" = "typcmsup"."Subledgerid"
	left join "dbo"."Stcurncy" "cur" on "Stpurhdr"."curcode" = "cur"."Curncy_No"
]]></text>
    </command>
