from "dbo"."Stcustmr" "Stcustmr"
	left join "dbo"."PYCntry" "Cntry_" on "Stcustmr"."Cntry_No" = "Cntry_"."Cntry_No"
	left join "dbo"."stcustmrtasnef" "TSNEF" on "Stcustmr"."TSNEFNO" = "TSNEF"."TSNEFNO"
	left join "dbo"."AreaList" "Area" on "Stcustmr"."AreaNo" = "Area"."AreaNo"
	left join "dbo"."stcustmrtasnef1" "TheTSNEFNO1" on "Stcustmr"."TSNEFNO1" = "TheTSNEFNO1"."TSNEFNO1"
	left join "dbo"."EvaluationList" "Evaluation" on "Stcustmr"."EvaluationNO" = "Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "acc_" on "Stcustmr"."acc_no" = "acc_"."Acc_No"
	left join "dbo"."GLmfbab" "acc_Clsacc_" on "acc_"."Clsacc_No" = "acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "acc_Acc_Bnd" on "acc_"."Acc_Bnd" = "acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "acc_Acc_Bndaccclass" on "acc_Acc_Bnd"."accclass" = "acc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "acc_Acc_BndAcc_Bab" on "acc_Acc_Bnd"."Acc_Bab" = "acc_Acc_BndAcc_Bab"."Acc_Bab"
	left join "dbo"."Stcurncy" "curncy_" on "Stcustmr"."curncy_no" = "curncy_"."Curncy_No"
	left join "dbo"."Stslsman" "Slm_" on "Stcustmr"."Slm_No" = "Slm_"."Slm_No"
	left join "dbo"."masngerclass" "Slm_masngerclass" on "Slm_"."masngerclassid" = "Slm_masngerclass"."masngerclassid"
]]></text>
