	,"acc_Acc_Bndaccclass"."accountclass" "acc_Acc_Bndaccclassaccountclass"
	,"acc_Acc_BndAcc_Bab"."Acc_Nm" "acc_Acc_BndAcc_BabAcc_Nm"
	,"Stcustmr"."curncy_no" "curncy_no"
	,"curncy_"."Curncy_Nm" "curncy_Curncy_Nm"
	,"Stcustmr"."Slm_No" "Slm_No"
	,"Slm_"."Slm_Nm" "Slm_Slm_Nm"
	,"Slm_masngerclass"."masngerclass" "Slm_masngerclassmasngerclass"
