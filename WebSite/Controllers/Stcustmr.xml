	,"Stcustmr"."workadress" "workadress"
	,"Stcustmr"."Cstm_Rsp" "Cstm_Rsp"
	,"Stcustmr"."Cstm_Email" "Cstm_Email"
	,"Stcustmr"."Cstm_Tel" "Cstm_Tel"
	,"Stcustmr"."Cstm_Tel1" "Cstm_Tel1"
	,"Stcustmr"."Cstm_Tel2" "Cstm_Tel2"
