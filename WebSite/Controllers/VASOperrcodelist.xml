<?xml version="1.0" encoding="utf-8"?>
<dataController name="VASOperrcodelist" conflictDetection="overwriteChanges" label="VAS Operrcodelist" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"VASOperrcodelist"."VASoperatorcode" "VASoperatorcode"
	,"VASOperrcodelist"."VASoperatorcodeDescription" "VASoperatorcodeDescription"
	,"VASOperrcodelist"."Taxs" "Taxs"
	,"VASOperrcodelist"."withdrawalTax" "withdrawalTax"
	,"VASOperrcodelist"."appliedflag" "appliedflag"
	,"VASOperrcodelist"."VasRegioncode" "VasRegioncode"
	,"VasRegion"."VasRegiondescription" "VasRegionVasRegiondescription"
	,"VASOperrcodelist"."VasCurrenceyCode" "VasCurrenceyCode"
	,"VasCurrencey"."VasCurrenceyDescription" "VasCurrenceyVasCurrenceyDescription"
	,"VASOperrcodelist"."VASopCategory" "VASopCategory"
	,"TheVASopCategory"."VASopCategoryDescription" "TheVASopCategoryVASopCategoryDescription"
	,"VASOperrcodelist"."Acc_no" "Acc_no"
	,"Acc_"."Acc_Nm" "Acc_Acc_Nm"
	,"Acc_Clsacc_"."Acc_Nm" "Acc_Clsacc_Acc_Nm"
	,"Acc_Acc_Bnd"."Acc_Nm" "Acc_Acc_BndAcc_Nm"
	,"Acc_Acc_Bndaccclass"."accountclass" "Acc_Acc_Bndaccclassaccountclass"
	,"Acc_Acc_BndAcc_Bab"."Acc_Nm" "Acc_Acc_BndAcc_BabAcc_Nm"
from "dbo"."VASOperrcodelist" "VASOperrcodelist"
	left join "dbo"."VasRegionlist" "VasRegion" on "VASOperrcodelist"."VasRegioncode" = "VasRegion"."VasRegioncode"
	left join "dbo"."VasCurrenceylist" "VasCurrencey" on "VASOperrcodelist"."VasCurrenceyCode" = "VasCurrencey"."VasCurrenceyCode"
	left join "dbo"."VASOperatorCategory" "TheVASopCategory" on "VASOperrcodelist"."VASopCategory" = "TheVASopCategory"."VASopCategory"
	left join "dbo"."GLmstrfl" "Acc_" on "VASOperrcodelist"."Acc_no" = "Acc_"."Acc_No"
	left join "dbo"."GLmfbab" "Acc_Clsacc_" on "Acc_"."Clsacc_No" = "Acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Acc_Acc_Bnd" on "Acc_"."Acc_Bnd" = "Acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "Acc_Acc_Bndaccclass" on "Acc_Acc_Bnd"."accclass" = "Acc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "Acc_Acc_BndAcc_Bab" on "Acc_Acc_Bnd"."Acc_Bab" = "Acc_Acc_BndAcc_Bab"."Acc_Bab"
]]></text>
    </command>
    <command id="VASoperatorcodeIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="VASoperatorcode" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="VASoperatorcode" type="Int64" allowNulls="false" isPrimaryKey="true" label="VA Soperatorcode" readOnly="true" />
    <field name="VASoperatorcodeDescription" type="String" label="VA Soperatorcode Description" length="150" showInSummary="true" />
    <field name="Taxs" type="Decimal" default="((0))" label="Taxs" showInSummary="true" />
    <field name="withdrawalTax" type="Decimal" label="withdrawal Tax" showInSummary="true" />
    <field name="appliedflag" type="Boolean" label="Appliedflag" showInSummary="true" />
    <field name="VasRegioncode" type="Int64" label="Vas Regioncode" showInSummary="true">
      <items style="Lookup" dataController="VasRegionlist" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="VasRegionVasRegiondescription" type="String" readOnly="true" label="Vas Region Vas Regiondescription" length="150" />
    <field name="VasCurrenceyCode" type="Int64" label="Vas Currencey Code">
      <items style="Lookup" dataController="VasCurrenceylist" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="VasCurrenceyVasCurrenceyDescription" type="String" readOnly="true" label="Vas Currencey Description" length="150" />
    <field name="VASopCategory" type="Int64" label="VA Sop Category">
      <items style="Lookup" dataController="VASOperatorCategory" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="TheVASopCategoryVASopCategoryDescription" type="String" readOnly="true" label="VA Sop Category Description" length="150" />
    <field name="Acc_no" type="Int64" label="Acc no">
      <items style="Lookup" dataController="GLmstrfl" newDataView="createForm1" searchOnStart="true" />
    </field>
    <field name="Acc_Acc_Nm" type="String" readOnly="true" label="Acc Nm" length="255" />
    <field name="Acc_Clsacc_Acc_Nm" type="String" readOnly="true" label="Acc Clsacc Acc Nm" length="250" />
    <field name="Acc_Acc_BndAcc_Nm" type="String" readOnly="true" label="Acc Bnd Acc Nm" length="50" />
    <field name="Acc_Acc_Bndaccclassaccountclass" type="String" readOnly="true" label="Acc Bndaccclassaccountclass" length="100" />
    <field name="Acc_Acc_BndAcc_BabAcc_Nm" type="String" readOnly="true" label="Acc Bnd Acc Bab Acc Nm" length="250" />
  </fields>
