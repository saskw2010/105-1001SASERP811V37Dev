            <dataField fieldName="VasCurrenceyCode" aliasFieldName="VasCurrenceyVasCurrenceyDescription" hyperlinkFormatString="~/Pages/VasCurrenceylist.aspx?VasCurrenceyCode={VasCurrenceyCode}&amp;_controller=VasCurrenceylist&amp;_commandName=Select&amp;_commandArgument=editForm1" />
            <dataField fieldName="VASopCategory" aliasFieldName="TheVASopCategoryVASopCategoryDescription" hyperlinkFormatString="~/Pages/VASOperatorCategory.aspx?VASopCategory={VASopCategory}&amp;_controller=VASOperatorCategory&amp;_commandName=Select&amp;_commandArgument=editForm1" />
