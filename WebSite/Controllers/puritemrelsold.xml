  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"puritemrels"."IDpuritemrels" "IDpuritemrels"
	,"puritemrels"."Sup_No" "Sup_No"
	,"Sup_"."Sup_Nm" "Sup_Sup_Nm"
	,"Sup_Acc_"."Acc_Nm" "Sup_Acc_Acc_Nm"
	,"Sup_Acc_Clsacc_"."Acc_Nm" "Sup_Acc_Clsacc_Acc_Nm"
	,"Sup_Acc_Acc_Bnd"."Acc_Nm" "Sup_Acc_Acc_BndAcc_Nm"
	,"Sup_sgm"."sgm_Nm" "Sup_sgmsgm_Nm"
	,"Sup_sgmOpco"."OpcoName" "Sup_sgmOpcoOpcoName"
	,"Sup_Cntry_"."Cntry_Nm" "Sup_Cntry_Cntry_Nm"
	,"Sup_Curncy_"."Curncy_Nm" "Sup_Curncy_Curncy_Nm"
	,"puritemrels"."ID_comp" "ID_comp"
	,"TheID_comp"."ID_USER" "TheID_compID_USER"
	,"TheID_compIDGROUP"."ID_USER" "TheID_compIDGROUPID_USER"
	,"TheID_compSup_"."Item_name" "TheID_compSup_Item_name"
	,"TheID_compid1"."DESCRP1" "TheID_compid1DESCRP1"
	,"TheID_compid2"."DESCRP1" "TheID_compid2DESCRP1"
	,"TheID_compid3"."DESCRP1" "TheID_compid3DESCRP1"
	,"TheID_compid4"."DESCRP1" "TheID_compid4DESCRP1"
	,"TheID_compid5"."DESCRP1" "TheID_compid5DESCRP1"
	,"TheID_compid6"."DESCRP1" "TheID_compid6DESCRP1"
	,"TheID_compid7"."DESCRP1" "TheID_compid7DESCRP1"
	,"puritemrels"."unit_no" "unit_no"
	,"unit_"."Unit_Nm" "unit_Unit_Nm"
	,"puritemrels"."quntit" "quntit"
	,"puritemrels"."quntitb4" "quntitb4"
	,"puritemrels"."opnblnc" "opnblnc"
	,"puritemrels"."talbhad" "talbhad"
	,"puritemrels"."sarfhad" "sarfhad"
	,"puritemrels"."Factor" "Factor"
	,"puritemrels"."shad" "shad"
	,"puritemrels"."Barcodu" "Barcodu"
	,"puritemrels"."Barcodu1" "Barcodu1"
	,"puritemrels"."ModifiedBy" "ModifiedBy"
	,"puritemrels"."ModifiedOn" "ModifiedOn"
	,"puritemrels"."CreatedBy" "CreatedBy"
	,"puritemrels"."CreatedOn" "CreatedOn"
from "dbo"."puritemrels" "puritemrels"
	left join "dbo"."Stsuplir" "Sup_" on "puritemrels"."Sup_No" = "Sup_"."Sup_No"
	left join "dbo"."GLmstrfl" "Sup_Acc_" on "Sup_"."Acc_No" = "Sup_Acc_"."Acc_No"
	left join "dbo"."GLmfbab" "Sup_Acc_Clsacc_" on "Sup_Acc_"."Clsacc_No" = "Sup_Acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Sup_Acc_Acc_Bnd" on "Sup_Acc_"."Acc_Bnd" = "Sup_Acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."glmulcmp" "Sup_sgm" on "Sup_"."sgm" = "Sup_sgm"."sgmid"
	left join "dbo"."OpCo" "Sup_sgmOpco" on "Sup_sgm"."OpcoID" = "Sup_sgmOpco"."OpcoIDidentity"
	left join "dbo"."PYCntry" "Sup_Cntry_" on "Sup_"."Cntry_No" = "Sup_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Sup_Curncy_" on "Sup_"."Curncy_No" = "Sup_Curncy_"."Curncy_No"
	left join "dbo"."ITEMS" "TheID_comp" on "puritemrels"."ID_comp" = "TheID_comp"."ID_comp"
	left join "dbo"."GROUPStk" "TheID_compIDGROUP" on "TheID_comp"."IDGROUP" = "TheID_compIDGROUP"."IDGROUP"
	left join "dbo"."ItemList" "TheID_compSup_" on "TheID_comp"."Sup_No" = "TheID_compSup_"."ItemNo"
	left join "dbo"."PYHldtypsv1" "TheID_compid1" on "TheID_comp"."id1" = "TheID_compid1"."ID1"
	left join "dbo"."PYHldtypsv2" "TheID_compid2" on "TheID_comp"."id2" = "TheID_compid2"."ID2"
	left join "dbo"."PYHldtypsv3" "TheID_compid3" on "TheID_comp"."id3" = "TheID_compid3"."id3"
	left join "dbo"."PYHldtypsv4" "TheID_compid4" on "TheID_comp"."id4" = "TheID_compid4"."id4"
	left join "dbo"."PYHldtypsv5" "TheID_compid5" on "TheID_comp"."id5" = "TheID_compid5"."id5"
	left join "dbo"."PYHldtypsv6" "TheID_compid6" on "TheID_comp"."id6" = "TheID_compid6"."id6"
	left join "dbo"."PYHldtypsv7" "TheID_compid7" on "TheID_comp"."id7" = "TheID_compid7"."id7"
	left join "dbo"."Stunit" "unit_" on "puritemrels"."unit_no" = "unit_"."Unit_No"
]]></text>
    </command>
