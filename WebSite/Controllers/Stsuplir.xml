  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Stsuplir"."Sup_No" "Sup_No"
	,"Stsuplir"."Supcvilid" "Supcvilid"
	,"Stsuplir"."Sup_Nm" "Sup_Nm"
	,"Stsuplir"."Sup_Nme" "Sup_Nme"
	,"Stsuplir"."Sup_Adr" "Sup_Adr"
	,"Stsuplir"."Sup_Rsp" "Sup_Rsp"
	,"Stsuplir"."Sup_Othr" "Sup_Othr"
	,"Stsuplir"."Sup_Email" "Sup_Email"
	,"Stsuplir"."Sup_webUrl" "Sup_webUrl"
	,"Stsuplir"."Sup_Limt" "Sup_Limt"
	,"Stsuplir"."Sup_limitDays" "Sup_limitDays"
	,"Stsuplir"."Sup_Tel" "Sup_Tel"
	,"Stsuplir"."Sup_Tel2" "Sup_Tel2"
	,"Stsuplir"."Sup_Fax2" "Sup_Fax2"
	,"Stsuplir"."Sup_Fax" "Sup_Fax"
	,"Stsuplir"."Cntry_No" "Cntry_No"
	,"Cntry_"."Cntry_Nm" "Cntry_Cntry_Nm"
	,"Stsuplir"."Acc_No" "Acc_No"
	,"Acc_"."Acc_Nm" "Acc_Acc_Nm"
	,"Acc_Clsacc_"."Acc_Nm" "Acc_Clsacc_Acc_Nm"
	,"Acc_Acc_Bnd"."Acc_Nm" "Acc_Acc_BndAcc_Nm"
	,"Acc_Acc_Bndaccclass"."accountclass" "Acc_Acc_Bndaccclassaccountclass"
	,"Acc_Acc_BndAcc_Bab"."Acc_Nm" "Acc_Acc_BndAcc_BabAcc_Nm"
	,"Stsuplir"."Curncy_No" "Curncy_No"
	,"Curncy_"."Curncy_Nm" "Curncy_Curncy_Nm"
	,"Stsuplir"."Fbal_DB" "Fbal_DB"
	,"Stsuplir"."FBal_CR" "FBal_CR"
	,"Stsuplir"."ModifiedBy" "ModifiedBy"
	,"Stsuplir"."ModifiedOn" "ModifiedOn"
	,"Stsuplir"."CreatedBy" "CreatedBy"
	,"Stsuplir"."CreatedOn" "CreatedOn"
	,"Stsuplir"."typ" "typ"
from "dbo"."Stsuplir" "Stsuplir"
	left join "dbo"."PYCntry" "Cntry_" on "Stsuplir"."Cntry_No" = "Cntry_"."Cntry_No"
	left join "dbo"."GLmstrfl" "Acc_" on "Stsuplir"."Acc_No" = "Acc_"."Acc_No"
	left join "dbo"."GLmfbab" "Acc_Clsacc_" on "Acc_"."Clsacc_No" = "Acc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "Acc_Acc_Bnd" on "Acc_"."Acc_Bnd" = "Acc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "Acc_Acc_Bndaccclass" on "Acc_Acc_Bnd"."accclass" = "Acc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "Acc_Acc_BndAcc_Bab" on "Acc_Acc_Bnd"."Acc_Bab" = "Acc_Acc_BndAcc_Bab"."Acc_Bab"
	left join "dbo"."Stcurncy" "Curncy_" on "Stsuplir"."Curncy_No" = "Curncy_"."Curncy_No"
]]></text>
    </command>
