  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"pursalInvoicingDetailes"."pursalInvoicingDid" "pursalInvoicingDid"
	,"pursalInvoicingDetailes"."pursalInvoicingid" "pursalInvoicingid"
	,"pursalInvoicingDetailes"."ID_comp" "ID_comp"
	,"pursalInvoicingDetailes"."Quantity" "Quantity"
	,"pursalInvoicingDetailes"."RevievedAmount" "RevievedAmount"
	,"pursalInvoicingDetailes"."SalAmount" "SalAmount"
	,"pursalInvoicingDetailes"."TotalRevievedAmount" "TotalRevievedAmount"
	,"pursalInvoicingDetailes"."TotalSalAmount" "TotalSalAmount"
	,"pursalInvoicingDetailes"."pursalInvoicingDetailes" "pursalInvoicingDetailes"
	,"pursalInvoicingDetailes"."pursalInvoicingacc_no" "pursalInvoicingacc_no"
	,"pursalInvoicingacc_"."Acc_Nm" "pursalInvoicingacc_Acc_Nm"
	,"pursalInvoicingacc_Clsacc_"."Acc_Nm" "pursalInvoicingacc_Clsacc_Acc_Nm"
	,"pursalInvoicingacc_Acc_Bnd"."Acc_Nm" "pursalInvoicingacc_Acc_BndAcc_Nm"
	,"pursalInvoicingacc_Acc_Bndaccclass"."accountclass" "pursalInvoicingacc_Acc_Bndaccclassaccountclass"
	,"pursalInvoicingacc_Acc_BndAcc_Bab"."Acc_Nm" "pursalInvoicingacc_Acc_BndAcc_BabAcc_Nm"
	,"pursalInvoicingDetailes"."pursalInvoicingTypNO" "pursalInvoicingTypNO"
	,"pursalInvoicingDetailes"."pursalInvoicingCSUPNO" "pursalInvoicingCSUPNO"
	,"pursalInvoicingDetailes"."pursalInvoicingNotes" "pursalInvoicingNotes"
from "dbo"."pursalInvoicingDetailes" "pursalInvoicingDetailes"
	left join "dbo"."GLmstrfl" "pursalInvoicingacc_" on "pursalInvoicingDetailes"."pursalInvoicingacc_no" = "pursalInvoicingacc_"."Acc_No"
	left join "dbo"."GLmfbab" "pursalInvoicingacc_Clsacc_" on "pursalInvoicingacc_"."Clsacc_No" = "pursalInvoicingacc_Clsacc_"."Acc_Bab"
	left join "dbo"."GLmfband" "pursalInvoicingacc_Acc_Bnd" on "pursalInvoicingacc_"."Acc_Bnd" = "pursalInvoicingacc_Acc_Bnd"."Acc_Bnd"
	left join "dbo"."accountclasslist" "pursalInvoicingacc_Acc_Bndaccclass" on "pursalInvoicingacc_Acc_Bnd"."accclass" = "pursalInvoicingacc_Acc_Bndaccclass"."accountclassid"
	left join "dbo"."GLmfbab" "pursalInvoicingacc_Acc_BndAcc_Bab" on "pursalInvoicingacc_Acc_Bnd"."Acc_Bab" = "pursalInvoicingacc_Acc_BndAcc_Bab"."Acc_Bab"
]]></text>
    </command>
    <command id="pursalInvoicingDidIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="pursalInvoicingDid" />
      </output>
    </command>
  </commands>
