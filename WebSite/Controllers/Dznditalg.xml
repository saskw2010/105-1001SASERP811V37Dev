  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Dznditalg"."ln_no" "ln_no"
	,"Dznditalg"."countrdus" "countrdus"
	,"Thecountrdus"."Notes" "ThecountrdusNotes"
	,"ThecountrdusBrn_"."Brn_Nm" "ThecountrdusBrn_Brn_Nm"
	,"ThecountrdusBrn_Branch"."Desc1" "ThecountrdusBrn_BranchDesc1"
	,"Dznditalg"."itm_no" "itm_no"
	,"Dznditalg"."Unit_No" "Unit_No"
	,"Unit_"."Unit_Nm" "Unit_Unit_Nm"
	,"Dznditalg"."quantity" "quantity"
	,"Dznditalg"."Equation" "Equation"
	,"Dznditalg"."ModifiedBy" "ModifiedBy"
	,"Dznditalg"."ModifiedOn" "ModifiedOn"
	,"Dznditalg"."CreatedBy" "CreatedBy"
	,"Dznditalg"."CreatedOn" "CreatedOn"
from "dbo"."Dznditalg" "Dznditalg"
	left join "dbo"."Dznmastg" "Thecountrdus" on "Dznditalg"."countrdus" = "Thecountrdus"."countrdus"
	left join "dbo"."Branchcnst" "ThecountrdusBrn_" on "Thecountrdus"."Brn_No" = "ThecountrdusBrn_"."Brn_No"
	left join "dbo"."schBranch" "ThecountrdusBrn_Branch" on "ThecountrdusBrn_"."Branch" = "ThecountrdusBrn_Branch"."branch"
	left join "dbo"."Stunit" "Unit_" on "Dznditalg"."Unit_No" = "Unit_"."Unit_No"
]]></text>
    </command>
    <command id="ln_noIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="ln_no" />
      </output>
    </command>
  </commands>
