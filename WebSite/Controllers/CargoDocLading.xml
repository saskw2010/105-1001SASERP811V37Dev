	,"CargoDocLading"."ModifiedOn" "ModifiedOn"
	,"CargoDocLading"."CreatedBy" "CreatedBy"
	,"CargoDocLading"."CreatedOn" "CreatedOn"
from "dbo"."CargoDocLading" "CargoDocLading"
	left join "dbo"."CargoBillOFLading" "BillofladingAuto" on "CargoDocLading"."BillofladingAutoID" = "BillofladingAuto"."BillofladingAutoID"
	left join "dbo"."CargoDocList" "BillofladingAutoShip_" on "BillofladingAuto"."Ship_no" = "BillofladingAutoShip_"."Ship_no"
	left join "dbo"."CargoDocList1" "BillofladingAutoConsignee_" on "BillofladingAuto"."Consignee_no" = "BillofladingAutoConsignee_"."Consigneeid"
	left join "dbo"."CargoDocList2" "BillofladingAutoSizetype" on "BillofladingAuto"."Sizetype" = "BillofladingAutoSizetype"."SizetypeID"
	left join "dbo"."CargoDocList3" "BillofladingAutoTrneed" on "BillofladingAuto"."Trneed" = "BillofladingAutoTrneed"."DocID"
	left join "dbo"."CargoDocList4" "BillofladingAutoTmessage" on "BillofladingAuto"."Tmessage" = "BillofladingAutoTmessage"."DocID"
	left join "dbo"."CargoDocList5" "BillofladingAutoTerminalofRelease" on "BillofladingAuto"."TerminalofRelease" = "BillofladingAutoTerminalofRelease"."DocID"
	left join "dbo"."CargoDocList6" "BillofladingAutoPlaceofDchage" on "BillofladingAuto"."PlaceofDchage" = "BillofladingAutoPlaceofDchage"."DocID"
	left join "dbo"."CargoDocList7" "BillofladingAutorequiredFollowup" on "BillofladingAuto"."requiredFollowup" = "BillofladingAutorequiredFollowup"."DocID"
	left join "dbo"."CargoDocList8" "BillofladingAutocourier" on "BillofladingAuto"."courier" = "BillofladingAutocourier"."DocID"
	left join "dbo"."CargoInfo" "BillofladingAutoCargoInfo" on "BillofladingAuto"."CargoInfoID" = "BillofladingAutoCargoInfo"."CargoInfoID"
	left join "dbo"."Stcustmr" "BillofladingAutoCstm_" on "BillofladingAuto"."Cstm_no" = "BillofladingAutoCstm_"."Cstm_No"
	left join "dbo"."AreaList" "BillofladingAutoCstm_Area" on "BillofladingAutoCstm_"."AreaNo" = "BillofladingAutoCstm_Area"."AreaNo"
	left join "dbo"."EvaluationList" "BillofladingAutoCstm_Evaluation" on "BillofladingAutoCstm_"."EvaluationNO" = "BillofladingAutoCstm_Evaluation"."EvaluationNO"
	left join "dbo"."GLmstrfl" "BillofladingAutoCstm_acc_" on "BillofladingAutoCstm_"."acc_no" = "BillofladingAutoCstm_acc_"."Acc_No"
	left join "dbo"."PYCntry" "BillofladingAutoCstm_Cntry_" on "BillofladingAutoCstm_"."Cntry_No" = "BillofladingAutoCstm_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "BillofladingAutoCstm_curncy_" on "BillofladingAutoCstm_"."curncy_no" = "BillofladingAutoCstm_curncy_"."Curncy_No"
	left join "dbo"."stcustmrtasnef" "BillofladingAutoCstm_TSNEF" on "BillofladingAutoCstm_"."TSNEFNO" = "BillofladingAutoCstm_TSNEF"."TSNEFNO"
	left join "dbo"."stcustmrtasnef1" "BillofladingAutoCstm_TSNEFNO1" on "BillofladingAutoCstm_"."TSNEFNO1" = "BillofladingAutoCstm_TSNEFNO1"."TSNEFNO1"
	left join "dbo"."Stslsman" "BillofladingAutoCstm_Slm_" on "BillofladingAutoCstm_"."Slm_No" = "BillofladingAutoCstm_Slm_"."Slm_No"
	left join "dbo"."Stsuplir" "BillofladingAutoLineagent_" on "BillofladingAuto"."Lineagent_no" = "BillofladingAutoLineagent_"."Sup_No"
	left join "dbo"."GLmstrfl" "BillofladingAutoLineagent_Acc_" on "BillofladingAutoLineagent_"."Acc_No" = "BillofladingAutoLineagent_Acc_"."Acc_No"
	left join "dbo"."PYCntry" "BillofladingAutoLineagent_Cntry_" on "BillofladingAutoLineagent_"."Cntry_No" = "BillofladingAutoLineagent_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "BillofladingAutoLineagent_Curncy_" on "BillofladingAutoLineagent_"."Curncy_No" = "BillofladingAutoLineagent_Curncy_"."Curncy_No"
	left join "dbo"."CargoDocList9" "TheDoumenttyp" on "CargoDocLading"."Doumenttyp" = "TheDoumenttyp"."Doumenttyp"
	left join "dbo"."CargoDocList11" "ThedocumetNature" on "CargoDocLading"."documetNature" = "ThedocumetNature"."documetNature"
]]></text>
