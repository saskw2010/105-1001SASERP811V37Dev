	,"BillofladingAutoConsignee_"."Consignee" "BillofladingAutoConsignee_Consignee"
	,"BillofladingAutoSizetype"."SizetypeDesc" "BillofladingAutoSizetypeSizetypeDesc"
	,"BillofladingAutoTrneed"."Docdesc" "BillofladingAutoTrneedDocdesc"
	,"BillofladingAutoTmessage"."Docdesc" "BillofladingAutoTmessageDocdesc"
	,"BillofladingAutoTerminalofRelease"."Docdesc" "BillofladingAutoTerminalofReleaseDocdesc"
	,"BillofladingAutoPlaceofDchage"."Docdesc" "BillofladingAutoPlaceofDchageDocdesc"
	,"BillofladingAutorequiredFollowup"."Docdesc" "BillofladingAutorequiredFollowupDocdesc"
	,"BillofladingAutocourier"."Docdesc" "BillofladingAutocourierDocdesc"
	,"BillofladingAutoCargoInfo"."CargoInfoDesc" "BillofladingAutoCargoInfoCargoInfoDesc"
	,"BillofladingAutoCstm_"."Cstm_Nm" "BillofladingAutoCstm_Cstm_Nm"
	,"BillofladingAutoCstm_Area"."Area_name" "BillofladingAutoCstm_AreaArea_name"
	,"BillofladingAutoCstm_Evaluation"."Evaluation_name" "BillofladingAutoCstm_EvaluationEvaluation_name"
	,"BillofladingAutoCstm_acc_"."Acc_Nm" "BillofladingAutoCstm_acc_Acc_Nm"
	,"BillofladingAutoCstm_Cntry_"."Cntry_Nm" "BillofladingAutoCstm_Cntry_Cntry_Nm"
	,"BillofladingAutoCstm_curncy_"."Curncy_Nm" "BillofladingAutoCstm_curncy_Curncy_Nm"
	,"BillofladingAutoCstm_TSNEF"."TSNEF_name" "BillofladingAutoCstm_TSNEFTSNEF_name"
	,"BillofladingAutoCstm_TSNEFNO1"."TSNEF1_name" "BillofladingAutoCstm_TSNEFNO1TSNEF1_name"
	,"BillofladingAutoCstm_Slm_"."Slm_Nm" "BillofladingAutoCstm_Slm_Slm_Nm"
	,"BillofladingAutoLineagent_"."Sup_Nm" "BillofladingAutoLineagent_Sup_Nm"
	,"BillofladingAutoLineagent_Acc_"."Acc_Nm" "BillofladingAutoLineagent_Acc_Acc_Nm"
	,"BillofladingAutoLineagent_Cntry_"."Cntry_Nm" "BillofladingAutoLineagent_Cntry_Cntry_Nm"
	,"BillofladingAutoLineagent_Curncy_"."Curncy_Nm" "BillofladingAutoLineagent_Curncy_Curncy_Nm"
	,"CargoDocLading"."Doumenttyp" "Doumenttyp"
	,"TheDoumenttyp"."Doumenttypdesc" "TheDoumenttypDoumenttypdesc"
	,"CargoDocLading"."documetNature" "documetNature"
	,"ThedocumetNature"."documetNaturedesc" "ThedocumetNaturedocumetNaturedesc"
