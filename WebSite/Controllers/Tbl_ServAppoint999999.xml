  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Tbl_ServAppoint"."Autoid" "Autoid"
	,"Tbl_ServAppoint"."Rainbow_no" "Rainbow_no"
	,"Rainbow_"."Arabic_Name" "Rainbow_Arabic_Name"
	,"Rainbow_Ref_Kind"."Ref_Kind" "Rainbow_Ref_KindRef_Kind"
	,"Rainbow_collector"."Collector_name" "Rainbow_collectorCollector_name"
	,"Rainbow_collectorcollectmd"."collectmdname" "Rainbow_collectorcollectmdcollectmdname"
	,"Rainbow_Dealer"."Dealer_name" "Rainbow_DealerDealer_name"
	,"Rainbow_DealerMD"."MD_Name" "Rainbow_DealerMDMD_Name"
	,"Rainbow_MD"."MD_Name" "Rainbow_MDMD_Name"
	,"Rainbow_Nationality"."Nationality" "Rainbow_NationalityNationality"
	,"Rainbow_Region"."Region" "Rainbow_RegionRegion"
	,"Rainbow_IDATUTO"."Time" "Rainbow_IDATUTOTime"
	,"Rainbow_IDATUTOCallDep"."CallDepDesc" "Rainbow_IDATUTOCallDepCallDepDesc"
	,"Rainbow_IDATUTORef_Kind"."Ref_Kind" "Rainbow_IDATUTORef_KindRef_Kind"
	,"Rainbow_IDATUTObranch"."Desc1" "Rainbow_IDATUTObranchDesc1"
	,"Rainbow_IDATUTODay"."اليوم" "Rainbow_IDATUTODayاليوم"
	,"Rainbow_IDATUTODealer"."Dealer_name" "Rainbow_IDATUTODealerDealer_name"
	,"Rainbow_IDATUTOMD"."MD_Name" "Rainbow_IDATUTOMDMD_Name"
	,"Rainbow_IDATUTORegion"."Region" "Rainbow_IDATUTORegionRegion"
	,"Rainbow_IDATUTOResult"."ResultDesc" "Rainbow_IDATUTOResultResultDesc"
	,"Rainbow_IDATUTOTele_Mark_Sec"."Tele_Mark_Sec" "Rainbow_IDATUTOTele_Mark_SecTele_Mark_Sec"
	,"Rainbow_IDATUTOTime"."timedesc" "Rainbow_IDATUTOTimetimedesc"
	,"Rainbow_model"."modeldesc" "Rainbow_modelmodeldesc"
	,"Rainbow_modelCatidlist"."Catidlistdesc" "Rainbow_modelCatidlistCatidlistdesc"
	,"Rainbow_Replace"."ReplaceDesc" "Rainbow_ReplaceReplaceDesc"
	,"Tbl_ServAppoint"."Date" "Date"
	,"Tbl_ServAppoint"."Time" "Time"
	,"Tbl_ServAppoint"."Fixed_Date" "Fixed_Date"
	,"Tbl_ServAppoint"."Customer_onCall" "Customer_onCall"
	,"Tbl_ServAppoint"."Dealer_name" "Dealer_name"
	,"Tbl_ServAppoint"."Visit Cause" "VisitCause"
	,"Tbl_ServAppoint"."Result" "Result"
	,"TheResult"."Result1" "TheResultResult1"
	,"Tbl_ServAppoint"."Remark" "Remark"
	,"Tbl_ServAppoint"."Kind_Sales" "Kind_Sales"
	,"Tbl_ServAppoint"."Invoice_no" "Invoice_no"
	,"Tbl_ServAppoint"."Value" "Value"
	,"Tbl_ServAppoint"."Warranty" "Warranty"
	,"Tbl_ServAppoint"."VisitNots" "VisitNots"
	,"Tbl_ServAppoint"."Additional sales" "Additionalsales"
	,"Tbl_ServAppoint"."Other amounts" "Otheramounts"
	,"Tbl_ServAppoint"."ModifiedBy" "ModifiedBy"
	,"Tbl_ServAppoint"."ModifiedOn" "ModifiedOn"
	,"Tbl_ServAppoint"."CreatedBy" "CreatedBy"
	,"Tbl_ServAppoint"."CreatedOn" "CreatedOn"
	,"Tbl_ServAppoint"."calreferanceid" "calreferanceid"
	,"calreferance"."calreferancedescr" "calreferancecalreferancedescr"
from "dbo"."Tbl_ServAppoint" "Tbl_ServAppoint"
	left join "dbo"."TblCustomers" "Rainbow_" on "Tbl_ServAppoint"."Rainbow_no" = "Rainbow_"."Rainbow_no"
	left join "dbo"."Ref_Kindlist" "Rainbow_Ref_Kind" on "Rainbow_"."Ref_KindID" = "Rainbow_Ref_Kind"."Ref_KindID"
	left join "dbo"."Tbl_Collectors" "Rainbow_collector" on "Rainbow_"."collectorid" = "Rainbow_collector"."collectorid"
	left join "dbo"."collectmdlist" "Rainbow_collectorcollectmd" on "Rainbow_collector"."collectmdid" = "Rainbow_collectorcollectmd"."collectmdid"
	left join "dbo"."Tbl_Dealers" "Rainbow_Dealer" on "Rainbow_"."DealerID" = "Rainbow_Dealer"."DealerID"
	left join "dbo"."Tbl_MD" "Rainbow_DealerMD" on "Rainbow_Dealer"."MDID" = "Rainbow_DealerMD"."MDID"
	left join "dbo"."Tbl_MD" "Rainbow_MD" on "Rainbow_"."MDID" = "Rainbow_MD"."MDID"
	left join "dbo"."Tbl_Nationality" "Rainbow_Nationality" on "Rainbow_"."NationalityID" = "Rainbow_Nationality"."NationalityID"
	left join "dbo"."Tbl_Regions" "Rainbow_Region" on "Rainbow_"."RegionID" = "Rainbow_Region"."RegionID"
	left join "dbo"."TblAppointments" "Rainbow_IDATUTO" on "Rainbow_"."IDATUTO" = "Rainbow_IDATUTO"."IDATUTO"
	left join "dbo"."CallDeplist" "Rainbow_IDATUTOCallDep" on "Rainbow_IDATUTO"."CallDepid" = "Rainbow_IDATUTOCallDep"."CallDepid"
	left join "dbo"."Ref_Kindlist" "Rainbow_IDATUTORef_Kind" on "Rainbow_IDATUTO"."Ref_KindID" = "Rainbow_IDATUTORef_Kind"."Ref_KindID"
	left join "dbo"."schBranch" "Rainbow_IDATUTObranch" on "Rainbow_IDATUTO"."branch" = "Rainbow_IDATUTObranch"."branch"
	left join "dbo"."Tbl_Day" "Rainbow_IDATUTODay" on "Rainbow_IDATUTO"."DayID" = "Rainbow_IDATUTODay"."DayID"
	left join "dbo"."Tbl_Dealers" "Rainbow_IDATUTODealer" on "Rainbow_IDATUTO"."DealerID" = "Rainbow_IDATUTODealer"."DealerID"
	left join "dbo"."Tbl_MD" "Rainbow_IDATUTOMD" on "Rainbow_IDATUTO"."MDID" = "Rainbow_IDATUTOMD"."MDID"
	left join "dbo"."Tbl_Regions" "Rainbow_IDATUTORegion" on "Rainbow_IDATUTO"."RegionID" = "Rainbow_IDATUTORegion"."RegionID"
	left join "dbo"."Tbl_Results" "Rainbow_IDATUTOResult" on "Rainbow_IDATUTO"."ResultID" = "Rainbow_IDATUTOResult"."ResultID"
	left join "dbo"."Tbl_Tele_Mark_Sec" "Rainbow_IDATUTOTele_Mark_Sec" on "Rainbow_IDATUTO"."Tele_Mark_SecID" = "Rainbow_IDATUTOTele_Mark_Sec"."Tele_Mark_SecID"
	left join "dbo"."Tbl_Time" "Rainbow_IDATUTOTime" on "Rainbow_IDATUTO"."Time" = "Rainbow_IDATUTOTime"."الوقت"
	left join "dbo"."tblmodellist" "Rainbow_model" on "Rainbow_"."modelno" = "Rainbow_model"."modelno"
	left join "dbo"."tblCatidlist" "Rainbow_modelCatidlist" on "Rainbow_model"."Catidlistid" = "Rainbow_modelCatidlist"."Catidlistid"
	left join "dbo"."tblReplacelist" "Rainbow_Replace" on "Rainbow_"."Replaceid" = "Rainbow_Replace"."Replaceid"
	left join "dbo"."Tbl_ServiceRuslt" "TheResult" on "Tbl_ServAppoint"."Result" = "TheResult"."Result"
	left join "dbo"."Servicecalreferancelist" "calreferance" on "Tbl_ServAppoint"."calreferanceid" = "calreferance"."calreferanceid"
]]></text>
    </command>
    <command id="AutoidIdentityCommand" type="Text" event="Inserted">
