	,"electionmandobfromkeys"."ModifiedOn" "ModifiedOn"
	,"electionmandobfromkeys"."CreatedBy" "CreatedBy"
	,"electionmandobfromkeys"."CreatedOn" "CreatedOn"
	,"electionmandobfromkeys"."cvid" "cvid"
	,"electionmandobfromkeys"."mandobtype" "mandobtype"
	,"electionmandobfromkeys"."type" "type"
from "dbo"."electionmandobfromkeys" "electionmandobfromkeys"
	left join "dbo"."electionlegantable" "eleclegan" on "electionmandobfromkeys"."elecleganid" = "eleclegan"."eleclganid"
	left join "dbo"."eleclgankind" "elecleganlagnahkind" on "eleclegan"."lagnahkind" = "elecleganlagnahkind"."eleclgankindid"
	left join "dbo"."ELectionGadwal" "elecleganGadwal" on "eleclegan"."Gadwalcode" = "elecleganGadwal"."Gadwalcode"
	left join "dbo"."ELectionDAWAER" "elecleganGadwaldwaer" on "elecleganGadwal"."dwaercode" = "elecleganGadwaldwaer"."dwaercode"
	left join "dbo"."electiongender" "elecleganGender" on "eleclegan"."Genderid" = "elecleganGender"."Genderid"
]]></text>
