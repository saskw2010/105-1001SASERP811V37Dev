	,"elecleganlagnahkind"."eleclgankind" "elecleganlagnahkindeleclgankind"
	,"elecleganGadwal"."gadwalname" "elecleganGadwalgadwalname"
	,"elecleganGadwaldwaer"."dwaername" "elecleganGadwaldwaerdwaername"
	,"elecleganGender"."electiogenderdesc" "elecleganGenderelectiogenderdesc"
