<?xml version="1.0" encoding="utf-8"?>
<dataController name="tblappointResult" label="tblappointResult" conflictDetection="overwriteChanges" handler="eZee.Rules.tblappointResultBusinessRules" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"TblAppointments"."HomePhone" "HomePhone"
	,"TblAppointments"."branch" "branch"
	,"Thebranch"."Desc1" "ThebranchDesc1"
	,"TblAppointments"."CallDepid" "CallDepid"
	,"CallDep"."CallDepDesc" "CallDepCallDepDesc"
	,"TblAppointments"."MobilePhone" "MobilePhone"
	,"TblAppointments"."Date" "Date"
	,"TblAppointments"."DayID" "DayID"
	,"Day"."اليوم" "Dayاليوم"
	,"TblAppointments"."Time" "Time"
	,"TblAppointments"."MDID" "MDID"
	,"MD"."MD_Name" "MDMD_Name"
	,"TblAppointments"."Customer_Name" "Customer_Name"
	,"TblAppointments"."Ref_Name" "Ref_Name"
	,"TblAppointments"."Ref_KindID" "Ref_KindID"
	,"Ref_Kind"."Ref_Kind" "Ref_KindRef_Kind"
	,"TblAppointments"."Ref_Phone" "Ref_Phone"
	,"TblAppointments"."RegionID" "RegionID"
	,"Region"."Region" "RegionRegion"
	,"TblAppointments"."Full_DetailsAddress" "Full_DetailsAddress"
	,"TblAppointments"."Fixed_Date" "Fixed_Date"
	,"TblAppointments"."Tele_Mark_SecID" "Tele_Mark_SecID"
	,"Tele_Mark_Sec"."Tele_Mark_Sec" "Tele_Mark_SecTele_Mark_Sec"
	,"Tele_Mark_SecCallDep"."CallDepDesc" "Tele_Mark_SecCallDepCallDepDesc"
	,"TblAppointments"."Customer_onCall" "Customer_onCall"
	,"TblAppointments"."ResultID" "ResultID"
	,"Result"."النتيجة" "Resultالنتيجة"
	,"TblAppointments"."DealerID" "DealerID"
	,"Dealer"."Dealer_name" "DealerDealer_name"
	,"TblAppointments"."Names_No" "Names_No"
	,"TblAppointments"."enter_usre" "enter_usre"
	,"TblAppointments"."old_Names" "old_Names"
	,"TblAppointments"."Postponement_Cause" "Postponement_Cause"
	,"TblAppointments"."Dealer_Remarks" "Dealer_Remarks"
	,"TblAppointments"."ModifiedBy" "ModifiedBy"
	,"TblAppointments"."ModifiedOn" "ModifiedOn"
	,"TblAppointments"."CreatedBy" "CreatedBy"
	,"TblAppointments"."CreatedOn" "CreatedOn"
from "dbo"."TblAppointments" "TblAppointments"
	left join "dbo"."schBranch" "Thebranch" on "TblAppointments"."branch" = "Thebranch"."branch"
	left join "dbo"."CallDeplist" "CallDep" on "TblAppointments"."CallDepid" = "CallDep"."CallDepid"
	left join "dbo"."Tbl_Day" "Day" on "TblAppointments"."DayID" = "Day"."DayID"
	left join "dbo"."Tbl_MD" "MD" on "TblAppointments"."MDID" = "MD"."MDID"
	left join "dbo"."Ref_Kindlist" "Ref_Kind" on "TblAppointments"."Ref_KindID" = "Ref_Kind"."Ref_KindID"
	left join "dbo"."Tbl_Regions" "Region" on "TblAppointments"."RegionID" = "Region"."RegionID"
	left join "dbo"."Tbl_Tele_Mark_Sec" "Tele_Mark_Sec" on "TblAppointments"."Tele_Mark_SecID" = "Tele_Mark_Sec"."Tele_Mark_SecID"
	left join "dbo"."CallDeplist" "Tele_Mark_SecCallDep" on "Tele_Mark_Sec"."CallDepid" = "Tele_Mark_SecCallDep"."CallDepid"
	left join "dbo"."Tbl_Results" "Result" on "TblAppointments"."ResultID" = "Result"."ResultID"
	left join "dbo"."Tbl_Dealers" "Dealer" on "TblAppointments"."DealerID" = "Dealer"."DealerID"
]]></text>
    </command>
  </commands>
  <fields>
    <field name="HomePhone" type="Int32" allowNulls="false" isPrimaryKey="true" label="Home Phone" />
    <field name="branch" type="Int64" label="Branch" showInSummary="true" allowLEV="true" allowNulls="false">
      <items style="DropDownList" dataController="schBranch" newDataView="createForm1" searchOnStart="true" dataView="grid1" dataTextField="Desc1" dataValueField="branch" />
    </field>
    <field name="ThebranchDesc1" type="String" readOnly="true" label="Thebranch Desc1" length="50" />
    <field name="CallDepid" type="Int64" label="Call Depid" showInSummary="true" allowLEV="true" allowNulls="false">
      <items style="DropDownList" dataController="CallDeplist" newDataView="createForm1" searchOnStart="true" dataView="grid1" dataTextField="CallDepDesc" dataValueField="CallDepid" />
    </field>
    <field name="CallDepCallDepDesc" type="String" readOnly="true" label="Call Dep Desc" length="50" />
    <field name="MobilePhone" type="Int32" label="Mobile Phone" showInSummary="true" />
    <field name="Date" type="DateTime" label="Date" showInSummary="true" allowNulls="false" />
    <field name="DayID" type="Int64" label="Day#" showInSummary="true" allowLEV="true" contextFields="Date">
      <items dataController="Tbl_Day" newDataView="createForm1" dataView="grid1" dataTextField="اليوم" dataValueField="DayID" style="DropDownList" />
    </field>
    <field name="Dayاليوم" type="String" readOnly="true" label="Day" length="50" />
    <field name="Time" type="String" label="Time" length="50" allowLEV="true" allowNulls="false">
