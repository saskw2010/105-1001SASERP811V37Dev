    </field>
    <field name="MDID" type="Int64" label="Mdid">
      <items style="AutoComplete" dataController="Tbl_MD" newDataView="createForm1" searchOnStart="true" dataView="grid1" dataTextField="MD_Name" dataValueField="MDID" />
    </field>
    <field name="MDMD_Name" type="String" readOnly="true" label="MDMD Name" length="50" />
    <field name="Customer_Name" type="String" label="Customer Name" length="50" allowNulls="false" />
    <field name="Ref_Name" type="String" label="Ref Name" length="50" allowNulls="false" />
    <field name="Ref_KindID" type="Int64" label="Ref Kind#" allowLEV="true" allowNulls="false">
      <items style="AutoComplete" dataController="Ref_Kindlist" newDataView="createForm1" searchOnStart="true" dataView="grid1" dataTextField="Ref_Kind" dataValueField="Ref_KindID" />
    </field>
    <field name="Ref_KindRef_Kind" type="String" readOnly="true" label="Ref Kind" length="10" />
    <field name="Ref_Phone" type="String" label="Ref Phone" length="15" allowNulls="false" />
    <field name="RegionID" type="Int64" label="Region#" allowNulls="false">
      <items style="AutoComplete" dataController="Tbl_Regions" newDataView="createForm1" searchOnStart="true" dataView="grid1" dataTextField="Region" dataValueField="RegionID" />
    </field>
    <field name="RegionRegion" type="String" readOnly="true" label="Region" length="50" />
    <field name="Full_DetailsAddress" type="String" allowQBE="false" allowSorting="false" label="Full Details Address" allowNulls="false" />
    <field name="Fixed_Date" type="DateTime" label="Fixed Date" allowNulls="false" />
    <field name="Tele_Mark_SecID" type="Int64" label="Tele Mark Sec#" allowNulls="false">
      <items style="AutoComplete" dataController="Tbl_Tele_Mark_Sec" newDataView="createForm1" searchOnStart="true" dataView="grid1" dataTextField="Tele_Mark_Sec" dataValueField="Tele_Mark_SecID" />
    </field>
    <field name="Tele_Mark_SecTele_Mark_Sec" type="String" readOnly="true" label="Tele Mark Sec" length="50" />
    <field name="Tele_Mark_SecCallDepCallDepDesc" type="String" readOnly="true" label="Tele Mark Sec Call Dep Desc" length="50" />
    <field name="Customer_onCall" type="String" label="Customer on Call" length="50" allowNulls="false" />
    <field name="ResultID" type="Int64" label="Result#">
      <items style="DropDownList" dataController="Tbl_Results" newDataView="createForm1" searchOnStart="true" dataView="grid1" dataTextField="النتيجة" dataValueField="ResultID" />
    </field>
    <field name="Resultالنتيجة" type="String" readOnly="true" label="Result" length="50" />
    <field name="DealerID" type="Int64" label="Dealer#">
      <items style="AutoComplete" dataController="Tbl_Dealers" newDataView="createForm1" searchOnStart="true" dataView="grid1" dataTextField="Dealer_name" dataValueField="DealerID" />
    </field>
    <field name="DealerDealer_name" type="String" readOnly="true" label="Dealer name" length="50" />
    <field name="Names_No" type="Int32" label="Names No" />
    <field name="enter_usre" type="String" label="Enter Usre" length="50" />
    <field name="old_Names" type="Int32" label="old Names" />
    <field name="Postponement_Cause" type="String" allowQBE="false" allowSorting="false" label="Postponement Cause" />
    <field name="Dealer_Remarks" type="String" allowQBE="false" allowSorting="false" label="Dealer Remarks" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" />
    <field name="ModifiedOn" type="DateTime" label="Modified On" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="CreatedOn" type="DateTime" label="Created On" />
    <field name="phonesearch" type="String" label="phonesearch" computed="true" showInSummary="true" hidden="false">
      <items letters="false" />
      <formula>CAST(TblAppointments.HomePhone AS nvarchar(9)) + N'N|' + CAST(ISNULL(TblAppointments.MobilePhone, 0) AS nvarchar(9))</formula>
    </field>
    <field name="MD" type="String" allowNulls="false" label="MD" hidden="true" computed="true" readOnly="true" showInSummary="true">
      <items letters="false" />
      <formula>"MD"."MD_Name"</formula>
    </field>
    <field name="Ref_Kind" type="String" allowNulls="false" label="Ref_Kind" allowQBE="false" allowSorting="false" hidden="true" computed="true" readOnly="true" showInSummary="true">
      <items letters="false" />
      <formula>"Ref_Kind"."Ref_Kind"</formula>
    </field>
    <field name="Day" type="String" allowNulls="false" readOnly="true" label="Day" allowQBE="false" allowSorting="false" computed="true" showInSummary="true" hidden="true">
      <items letters="false" />
      <formula>DATENAME(weekday,"TblAppointments"."Date")</formula>
    </field>
    <field name="Dealer_name" type="String" allowNulls="false" readOnly="true" label="Dealer_name" allowQBE="false" allowSorting="false" computed="true" showInSummary="true" hidden="true">
      <items letters="false" />
      <formula>"Dealer"."Dealer_name"</formula>
    </field>
    <field name="Result" type="String" allowNulls="false" readOnly="true" label="Result" allowQBE="false" allowSorting="false" computed="true" showInSummary="true" hidden="true">
      <items letters="false" />
      <formula>"Result"."النتيجة"</formula>
    </field>
    <field name="Region" type="String" allowNulls="false" readOnly="true" label="Region" allowQBE="false" allowSorting="false" computed="true" showInSummary="true" hidden="true">
      <items letters="false" />
      <formula>"Region"."Region"</formula>
    </field>
    <field name="Tele_Mark_Sec" type="String" allowNulls="false" readOnly="true" label="Tele_Mark_Sec" allowQBE="false" allowSorting="false" computed="true" showInSummary="true" hidden="true">
      <items letters="false" />
      <formula>"Tele_Mark_Sec"."Tele_Mark_Sec"</formula>
    </field>
    <field name="Dates" type="DateTime2" allowNulls="false" readOnly="true" label="Dates" allowQBE="false" allowSorting="false" computed="true" showInSummary="true" hidden="true">
      <items letters="false" />
      <formula>"TblAppointments"."Date"</formula>
    </field>
    <field name="daterep" type="String" allowNulls="false" readOnly="true" label="daterep" allowQBE="false" allowSorting="false" computed="true" showInSummary="true" hidden="false">
      <items letters="false" />
      <formula>DATENAME(weekday,"TblAppointments"."Date")</formula>
    </field>
  </fields>
