    <command id="command1" type="Text">
      <text><![CDATA[
select
	"RealstateStock"."Stockunitid" "Stockunitid"
	,"RealstateStock"."StockunitDesc" "StockunitDesc"
	,"RealstateStock"."Floor" "Floor"
	,"RealstateStock"."viewlistid" "viewlistid"
	,"viewlist"."viewlistDesc" "viewlistviewlistDesc"
	,"RealstateStock"."Realstateunittypeid" "Realstateunittypeid"
	,"Realstateunittype"."Realstateunittype" "RealstateunittypeRealstateunittype"
	,"RealstateStock"."Area" "Area"
	,"RealstateStock"."kindlistid" "kindlistid"
	,"kindlist"."kindlistdesc" "kindlistkindlistdesc"
	,"RealstateStock"."Realstateprjid" "Realstateprjid"
	,"Realstateprj"."RealstateprjDesc" "RealstateprjRealstateprjDesc"
	,"Realstateprjsgm"."sgm_Nm" "Realstateprjsgmsgm_Nm"
	,"RealstateprjsgmOpco"."OpcoName" "RealstateprjsgmOpcoOpcoName"
	,"RealstateprjCntry_"."Cntry_Nm" "RealstateprjCntry_Cntry_Nm"
	,"RealstateprjSup_"."Sup_Nm" "RealstateprjSup_Sup_Nm"
	,"RealstateprjSup_Acc_"."Acc_Nm" "RealstateprjSup_Acc_Acc_Nm"
	,"RealstateprjSup_sgm"."sgm_Nm" "RealstateprjSup_sgmsgm_Nm"
	,"RealstateprjSup_Cntry_"."Cntry_Nm" "RealstateprjSup_Cntry_Cntry_Nm"
	
	,"RealstateprjSup_Cntry_"."Cntry_Nm" "RealstateprjSup_Cntry_Cntry_Nm"
	
	,"RealstateprjSup_Curncy_"."Curncy_Nm" "RealstateprjSup_Curncy_Curncy_Nm"
	,"RealstateStock"."countoffloor" "countoffloor"
	,"RealstateStock"."price" "price"
	,"RealstateStock"."price1" "price1"
	,"RealstateStock"."price2" "price2"
	,"RealstateStock"."statusid" "statusid"
	,"status"."statusdesc" "statusstatusdesc"
	,"RealstateStock"."ReservationAmount" "ReservationAmount"
	,"RealstateStock"."ReservationDate" "ReservationDate"
	,"RealstateStock"."ReservationVoucherNo" "ReservationVoucherNo"
	,"RealstateStock"."RealstatypeId" "RealstatypeId"
	,"Realstatype"."Realstatype" "RealstatypeRealstatype"
	,"RealstateStock"."unitowner" "unitowner"
	,"RealstateOwners_"."RealstateOwners" "RealstateOwners"
	,"RealstateStock"."Segment" "Segment"
	,"RealstateStock"."Street" "Street"
	,"RealstateStock"."Avenue" "Avenue"
	,"RealstateStock"."Building_Home" "Building_Home"
	,"RealstateStock"."Appartment" "Appartment"

from "dbo"."RealstateStock" "RealstateStock"
	left join "dbo"."Realstarteviewlist" "viewlist" on "RealstateStock"."viewlistid" = "viewlist"."viewlistid"
	left join "dbo"."Realstateunittypelist" "Realstateunittype" on "RealstateStock"."Realstateunittypeid" = "Realstateunittype"."Realstateunittypeid"
	left join "dbo"."RealstateKindlist" "kindlist" on "RealstateStock"."kindlistid" = "kindlist"."kindlistid"
	left join "dbo"."Realstateproject" "Realstateprj" on "RealstateStock"."Realstateprjid" = "Realstateprj"."Realstateprjid"
	left join "dbo"."glmulcmp" "Realstateprjsgm" on "Realstateprj"."sgm" = "Realstateprjsgm"."sgmid"
	left join "dbo"."OpCo" "RealstateprjsgmOpco" on "Realstateprjsgm"."OpcoID" = "RealstateprjsgmOpco"."OpcoIDidentity"
	left join "dbo"."PYCntry" "RealstateprjCntry_" on "Realstateprj"."Cntry_No" = "RealstateprjCntry_"."Cntry_No"
	left join "dbo"."Stsuplir" "RealstateprjSup_" on "Realstateprj"."Sup_No" = "RealstateprjSup_"."Sup_No"
	left join "dbo"."GLmstrfl" "RealstateprjSup_Acc_" on "RealstateprjSup_"."Acc_No" = "RealstateprjSup_Acc_"."Acc_No"
	left join "dbo"."glmulcmp" "RealstateprjSup_sgm" on "RealstateprjSup_"."sgm" = "RealstateprjSup_sgm"."sgmid"
	
    left join "dbo"."RealstateOwners" "RealstateOwners_" on "RealstateStock"."unitowner" = "RealstateOwners_"."Ownerid"
	
	left join "dbo"."PYCntry" "RealstateprjSup_Cntry_" on "RealstateprjSup_"."Cntry_No" = "RealstateprjSup_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "RealstateprjSup_Curncy_" on "RealstateprjSup_"."Curncy_No" = "RealstateprjSup_Curncy_"."Curncy_No"
	left join "dbo"."Realstatestatuslist" "status" on "RealstateStock"."statusid" = "status"."statusid"
	left join "dbo"."RealstaType" "Realstatype" on "RealstateStock"."RealstatypeId" = "Realstatype"."RealstatypeId"
	
]]></text>
    </command>
