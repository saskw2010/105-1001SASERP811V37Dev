  <businessRules>
    <rule id="r100" commandName="Select " view="grid1" type="Sql" phase="Before"><![CDATA[delete from Barcodeitem   where   Modifiedon=dateadd(day,-1,getdate()) and    ModifiedBy = @BusinessRules_UserName
]]></rule>
    <rule id="r101" commandName="Insert|Update" type="Sql" phase="After"><![CDATA[delete from Barcodeitem   where   autoid<>@autoid  

exec CreateBatchPrint @autoid,@ID_Comp,@Batchid,@usercashid]]></rule>
    <rule id="r102" commandName="Delete" type="Sql" phase="After"><![CDATA[delete from Barcodeitem where autoid=@autoid]]></rule>
