<?xml version="1.0" encoding="utf-8"?>
<dataController name="BarcodeCreate" conflictDetection="overwriteChanges" label="Barcode Create" plugIn="eZee.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"BarcodeCreate"."autoid" "autoid"
	,"BarcodeCreate"."ModifiedBy" "ModifiedBy"
	,"BarcodeCreate"."CreatedBy" "CreatedBy"
	,"BarcodeCreate"."CreatedOn" "CreatedOn"
	,"BarcodeCreate"."ModifiedOn" "ModifiedOn"
	,"BarcodeCreate"."ID_Comp" "ID_Comp"
	,"ID_Comp"."ID_USER" "ID_CompID_USER"
	,"ID_CompIDGROUP"."ID_USER" "ID_CompIDGROUPID_USER"
	,"ID_CompSup_"."Item_name" "ID_CompSup_Item_name"
	,"ID_Compid1"."DESCRP1" "ID_Compid1DESCRP1"
	,"ID_Compid2"."DESCRP1" "ID_Compid2DESCRP1"
	,"ID_Compid3"."DESCRP1" "ID_Compid3DESCRP1"
	,"ID_Compid4"."DESCRP1" "ID_Compid4DESCRP1"
	,"ID_Compid5"."DESCRP1" "ID_Compid5DESCRP1"
	,"ID_Compid6"."DESCRP1" "ID_Compid6DESCRP1"
	,"ID_Compid7"."DESCRP1" "ID_Compid7DESCRP1"
	,"BarcodeCreate"."ID_user" "ID_user"
	,"BarcodeCreate"."Batchid" "Batchid"
	,"Batch"."Barcodubatch" "BatchBarcodubatch"
	,"BatchID_comp"."ID_USER" "BatchID_compID_USER"
	,"BatchID_compIDGROUP"."ID_USER" "BatchID_compIDGROUPID_USER"
	,"BatchID_compSup_"."Item_name" "BatchID_compSup_Item_name"
	,"BatchID_compid1"."DESCRP1" "BatchID_compid1DESCRP1"
	,"BatchID_compid2"."DESCRP1" "BatchID_compid2DESCRP1"
	,"BatchID_compid3"."DESCRP1" "BatchID_compid3DESCRP1"
	,"BatchID_compid4"."DESCRP1" "BatchID_compid4DESCRP1"
	,"BatchID_compid5"."DESCRP1" "BatchID_compid5DESCRP1"
	,"BatchID_compid6"."DESCRP1" "BatchID_compid6DESCRP1"
	,"BatchID_compid7"."DESCRP1" "BatchID_compid7DESCRP1"
	,"BarcodeCreate"."Batchcode" "Batchcode"
	,"BarcodeCreate"."usercashid" "usercashid"
from "dbo"."BarcodeCreate" "BarcodeCreate"
	left join "dbo"."ITEMS" "ID_Comp" on "BarcodeCreate"."ID_Comp" = "ID_Comp"."ID_comp"
	left join "dbo"."GROUPStk" "ID_CompIDGROUP" on "ID_Comp"."IDGROUP" = "ID_CompIDGROUP"."IDGROUP"
	left join "dbo"."ItemList" "ID_CompSup_" on "ID_Comp"."Sup_No" = "ID_CompSup_"."ItemNo"
	left join "dbo"."PYHldtypsv1" "ID_Compid1" on "ID_Comp"."id1" = "ID_Compid1"."ID1"
	left join "dbo"."PYHldtypsv2" "ID_Compid2" on "ID_Comp"."id2" = "ID_Compid2"."ID2"
	left join "dbo"."PYHldtypsv3" "ID_Compid3" on "ID_Comp"."id3" = "ID_Compid3"."id3"
	left join "dbo"."PYHldtypsv4" "ID_Compid4" on "ID_Comp"."id4" = "ID_Compid4"."id4"
	left join "dbo"."PYHldtypsv5" "ID_Compid5" on "ID_Comp"."id5" = "ID_Compid5"."id5"
	left join "dbo"."PYHldtypsv6" "ID_Compid6" on "ID_Comp"."id6" = "ID_Compid6"."id6"
	left join "dbo"."PYHldtypsv7" "ID_Compid7" on "ID_Comp"."id7" = "ID_Compid7"."id7"
	left join "dbo"."itembatch" "Batch" on "BarcodeCreate"."Batchid" = "Batch"."Batchid"
	left join "dbo"."ITEMS" "BatchID_comp" on "Batch"."ID_comp" = "BatchID_comp"."ID_comp"
	left join "dbo"."GROUPStk" "BatchID_compIDGROUP" on "BatchID_comp"."IDGROUP" = "BatchID_compIDGROUP"."IDGROUP"
	left join "dbo"."ItemList" "BatchID_compSup_" on "BatchID_comp"."Sup_No" = "BatchID_compSup_"."ItemNo"
	left join "dbo"."PYHldtypsv1" "BatchID_compid1" on "BatchID_comp"."id1" = "BatchID_compid1"."ID1"
	left join "dbo"."PYHldtypsv2" "BatchID_compid2" on "BatchID_comp"."id2" = "BatchID_compid2"."ID2"
	left join "dbo"."PYHldtypsv3" "BatchID_compid3" on "BatchID_comp"."id3" = "BatchID_compid3"."id3"
	left join "dbo"."PYHldtypsv4" "BatchID_compid4" on "BatchID_comp"."id4" = "BatchID_compid4"."id4"
	left join "dbo"."PYHldtypsv5" "BatchID_compid5" on "BatchID_comp"."id5" = "BatchID_compid5"."id5"
	left join "dbo"."PYHldtypsv6" "BatchID_compid6" on "BatchID_comp"."id6" = "BatchID_compid6"."id6"
	left join "dbo"."PYHldtypsv7" "BatchID_compid7" on "BatchID_comp"."id7" = "BatchID_compid7"."id7"
]]></text>
    </command>
    <command id="autoidIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="autoid" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="autoid" type="Int64" allowNulls="false" isPrimaryKey="true" label="Autoid" readOnly="true" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" showInSummary="true" />
