    <field name="ID_Comp" type="Int64" label="ID Comp" showInSummary="true">
      <items style="Lookup" dataController="ITEMS" newDataView="createForm1" searchOnStart="true" dataView="grid1" dataTextField="ITEM" dataValueField="ID_comp" copy="ID_user=ID_USER" />
    </field>
    <field name="ID_CompID_USER" type="String" readOnly="true" label="ID Comp ID USER" length="50" />
    <field name="ID_CompIDGROUPID_USER" type="String" readOnly="true" label="ID Comp IDGROUPID USER" length="50" />
    <field name="ID_CompSup_Item_name" type="String" readOnly="true" label="ID Comp Sup Item name" length="150" />
    <field name="ID_Compid1DESCRP1" type="String" readOnly="true" label="ID Compid1 DESCRP1" length="255" />
    <field name="ID_Compid2DESCRP1" type="String" readOnly="true" label="ID Compid2 DESCRP1" length="255" />
    <field name="ID_Compid3DESCRP1" type="String" readOnly="true" label="ID Compid3 DESCRP1" length="255" />
    <field name="ID_Compid4DESCRP1" type="String" readOnly="true" label="ID Compid4 DESCRP1" length="255" />
    <field name="ID_Compid5DESCRP1" type="String" readOnly="true" label="ID Compid5 DESCRP1" length="255" />
    <field name="ID_Compid6DESCRP1" type="String" readOnly="true" label="ID Compid6 DESCRP1" length="255" />
    <field name="ID_Compid7DESCRP1" type="String" readOnly="true" label="ID Compid7 DESCRP1" length="255" />
    <field name="ID_user" type="String" label="ID user" length="50" />
    <field name="Batchid" type="Int64" label="Batchid" contextFields="ID_comp=ID_Comp" calculated="true">
      <items style="Lookup" dataController="itembatch" newDataView="createForm1" dataView="grid1" dataTextField="Barcodubatch" dataValueField="Batchid" copy="Batchcode=Expirdate" />
    </field>
    <field name="BatchBarcodubatch" type="String" readOnly="true" label="Batch Barcodubatch" length="50" />
    <field name="BatchID_compID_USER" type="String" readOnly="true" label="Batch ID comp ID USER" length="50" />
    <field name="BatchID_compIDGROUPID_USER" type="String" readOnly="true" label="Batch ID comp IDGROUPID USER" length="50" />
    <field name="BatchID_compSup_Item_name" type="String" readOnly="true" label="Batch ID comp Sup Item name" length="150" />
    <field name="BatchID_compid1DESCRP1" type="String" readOnly="true" label="Batch ID compid1 DESCRP1" length="255" />
    <field name="BatchID_compid2DESCRP1" type="String" readOnly="true" label="Batch ID compid2 DESCRP1" length="255" />
    <field name="BatchID_compid3DESCRP1" type="String" readOnly="true" label="Batch ID compid3 DESCRP1" length="255" />
    <field name="BatchID_compid4DESCRP1" type="String" readOnly="true" label="Batch ID compid4 DESCRP1" length="255" />
    <field name="BatchID_compid5DESCRP1" type="String" readOnly="true" label="Batch ID compid5 DESCRP1" length="255" />
    <field name="BatchID_compid6DESCRP1" type="String" readOnly="true" label="Batch ID compid6 DESCRP1" length="255" />
    <field name="BatchID_compid7DESCRP1" type="String" readOnly="true" label="Batch ID compid7 DESCRP1" length="255" />
    <field name="Batchcode" type="String" label="Batchcode" length="50" />
    <field name="usercashid" type="Int64" default="((30))" label="Usercashid" />
  </fields>
