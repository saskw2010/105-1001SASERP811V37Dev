  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Duty_Shift_Times"."SID" "SID"
	,"Duty_Shift_Times"."Duty_Shift_SID" "Duty_Shift_SID"
	,"Duty_Shift_S"."Shift_Code" "Duty_Shift_SShift_Code"
	,"Duty_Shift_SDuty_S"."Duty_Code" "Duty_Shift_SDuty_SDuty_Code"
	,"Duty_Shift_SDuty_SDuty_Type_S"."Duty_Code" "Duty_Shift_SDuty_SDuty_Type_SDuty_Code"
	,"Duty_Shift_SShift_Type_S"."Shift_Type_Desc" "Duty_Shift_SShift_Type_SShift_Type_Desc"
	,"Duty_Shift_Times"."Start_Time" "Start_Time"
	,"Duty_Shift_Times"."End_Time" "End_Time"
	,"Duty_Shift_Times"."Work_Days" "Work_Days"
	,"Duty_Shift_Times"."IsOff" "IsOff"
from "dbo"."Duty_Shift_Times" "Duty_Shift_Times"
	left join "dbo"."Duty_Shifts" "Duty_Shift_S" on "Duty_Shift_Times"."Duty_Shift_SID" = "Duty_Shift_S"."SID"
	left join "dbo"."Duties" "Duty_Shift_SDuty_S" on "Duty_Shift_S"."Duty_SID" = "Duty_Shift_SDuty_S"."SID"
	left join "dbo"."Duty_Types" "Duty_Shift_SDuty_SDuty_Type_S" on "Duty_Shift_SDuty_S"."Duty_Type_SID" = "Duty_Shift_SDuty_SDuty_Type_S"."SID"
	left join "dbo"."Shift_Types" "Duty_Shift_SShift_Type_S" on "Duty_Shift_S"."Shift_Type_SID" = "Duty_Shift_SShift_Type_S"."SID"
]]></text>
    </command>
    <command id="SIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="SID" />
      </output>
    </command>
  </commands>
