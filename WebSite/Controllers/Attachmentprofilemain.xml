  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Attachmentprofilemain"."AttachmentprofileNo" "AttachmentprofileNo"
	,"Attachmentprofilemain"."profilecode" "profilecode"
	,"profile"."internetCode" "profileinternetCode"
	,"profileCort"."CortName" "profileCortCortName"
	,"profileIdary"."Dwaername" "profileIdaryDwaername"
	,"profilekhesm"."name" "profilekhesmname"
	,"profilekhesmkhosmetype"."khosmetypename" "profilekhesmkhosmetypekhosmetypename"
	,"profilekhesmnationality"."Cntry_Nm" "profilekhesmnationalityCntry_Nm"
	,"profilekhesmreligion"."Reljan_nm" "profilekhesmreligionReljan_nm"
	,"profilelaywer"."laywername" "profilelaywerlaywername"
	,"profileMokel"."name" "profileMokelname"
	,"profileMokp"."name" "profileMokpname"
	,"profilepaymentsType"."Name" "profilepaymentsTypeName"
	,"profileEmpolyee"."name" "profileEmpolyeename"
	,"profileprofiltype"."name" "profileprofiltypename"
	,"Attachmentprofilemain"."Attachmentpath" "Attachmentpath"
	,"Attachmentprofilemain"."AttachmentDescription" "AttachmentDescription"
	,"Attachmentprofilemain"."attachmentNotes" "attachmentNotes"
	,"Attachmentprofilemain"."ModifiedBy" "ModifiedBy"
	,"Attachmentprofilemain"."ModifiedOn" "ModifiedOn"
	,"Attachmentprofilemain"."CreatedBy" "CreatedBy"
	,"Attachmentprofilemain"."CreatedOn" "CreatedOn"
from "dbo"."Attachmentprofilemain" "Attachmentprofilemain"
	left join "dbo"."ProfileMain" "profile" on "Attachmentprofilemain"."profilecode" = "profile"."profilecode"
	left join "dbo"."Corts" "profileCort" on "profile"."CortID" = "profileCort"."CortID"
	left join "dbo"."Dwaer" "profileIdary" on "profile"."IdaryID" = "profileIdary"."Dwaercode"
	left join "dbo"."Khsom" "profilekhesm" on "profile"."khesmcode" = "profilekhesm"."khosmcode"
	left join "dbo"."khosmetypetable" "profilekhesmkhosmetype" on "profilekhesm"."khosmetype" = "profilekhesmkhosmetype"."khosmetype"
	left join "dbo"."PYCntry" "profilekhesmnationality" on "profilekhesm"."nationality" = "profilekhesmnationality"."Cntry_No"
	left join "dbo"."PyReljan" "profilekhesmreligion" on "profilekhesm"."religion" = "profilekhesmreligion"."reljan"
	left join "dbo"."Lawyers" "profilelaywer" on "profile"."laywercode" = "profilelaywer"."laywercode"
	left join "dbo"."Mowakel" "profileMokel" on "profile"."Mokelcode" = "profileMokel"."Mokelcode"
	left join "dbo"."MowakelProperties" "profileMokp" on "profile"."Mokpcode" = "profileMokp"."Mokpcode"
	left join "dbo"."PaymentTypes" "profilepaymentsType" on "profile"."paymentsType" = "profilepaymentsType"."Code"
	left join "dbo"."profileEmployees" "profileEmpolyee" on "profile"."Empolyeecode" = "profileEmpolyee"."Empolyeecode"
	left join "dbo"."ProfileType" "profileprofiltype" on "profile"."profiltype" = "profileprofiltype"."code"
]]></text>
    </command>
    <command id="AttachmentprofileNoIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="AttachmentprofileNo" />
      </output>
    </command>
  </commands>
