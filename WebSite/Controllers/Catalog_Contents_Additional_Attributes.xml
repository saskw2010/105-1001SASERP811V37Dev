    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Catalog_Contents_Additional_Attributes"."catalog_entry_id" "catalog_entry_id"
	,"catalog_entry_"."catalog_entry_name" "catalog_entry_catalog_entry_name"
	,"catalog_entry_catalog_level_number"."catalog_level_name" "catalog_entry_catalog_level_numbercatalog_level_name"
	,"catalog_entry_catalog_level_numbercatalog_"."catalog_name" "catalog_entry_catalog_level_numbercatalog_catalog_name"
	,"Catalog_Contents_Additional_Attributes"."catalog_level_number" "catalog_level_number"
	,"Catalog_Contents_Additional_Attributes"."attribute_id" "attribute_id"
	,"Catalog_Contents_Additional_Attributes"."attribute_value" "attribute_value"
from "dbo"."Catalog_Contents_Additional_Attributes" "Catalog_Contents_Additional_Attributes"
	left join "dbo"."Catalog_Contents" "catalog_entry_" on "Catalog_Contents_Additional_Attributes"."catalog_entry_id" = "catalog_entry_"."catalog_entry_id"
	left join "dbo"."Catalog_Structure" "catalog_entry_catalog_level_number" on "catalog_entry_"."catalog_level_number" = "catalog_entry_catalog_level_number"."catalog_level_number"
	left join "dbo"."Catalogs" "catalog_entry_catalog_level_numbercatalog_" on "catalog_entry_catalog_level_number"."catalog_id" = "catalog_entry_catalog_level_numbercatalog_"."catalog_id"
]]></text>
    </command>
  </commands>
