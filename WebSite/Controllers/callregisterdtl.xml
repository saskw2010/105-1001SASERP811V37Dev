	,"callSlm_"."Slm_Nm" "callSlm_Slm_Nm"
	,"callSlm_masngerclass"."masngerclass" "callSlm_masngerclassmasngerclass"
	,"callVisitstatus"."Visitstatusname" "callVisitstatusVisitstatusname"
	,"callvisittype"."visittypeNm" "callvisittypevisittypeNm"
	,"callregisterdtl"."Slm_No" "Slm_No"
	,"Slm_"."Slm_Nm" "Slm_Slm_Nm"
	,"Slm_masngerclass"."masngerclass" "Slm_masngerclassmasngerclass"
	,"callregisterdtl"."visittype" "visittype"
	,"visittype"."visittypeNm" "visittypevisittypeNm"
	,"callregisterdtl"."Visitstatus" "Visitstatus"
	,"Visitstatus"."Visitstatusname" "VisitstatusVisitstatusname"
	,"callregisterdtl"."Personalname1" "Personalname1"
	,"callregisterdtl"."telphone" "telphone"
	,"callregisterdtl"."Personaljob" "Personaljob"
	,"callregisterdtl"."cutomercomments" "cutomercomments"
	,"callregisterdtl"."callercomments" "callercomments"
	,"callregisterdtl"."mangmentcomments" "mangmentcomments"
	,"callregisterdtl"."startdatetime" "startdatetime"
	,"callregisterdtl"."savedatetime" "savedatetime"
	,"callregisterdtl"."ModifiedBy" "ModifiedBy"
	,"callregisterdtl"."ModifiedOn" "ModifiedOn"
	,"callregisterdtl"."CreatedBy" "CreatedBy"
	,"callregisterdtl"."CreatedOn" "CreatedOn"
from "dbo"."callregisterdtl" "callregisterdtl"
	left join "dbo"."callregister" "call" on "callregisterdtl"."callid" = "call"."callid"
	left join "dbo"."Marketplan" "callMarketplan" on "call"."MarketplanID" = "callMarketplan"."MarketplanID"
	left join "dbo"."ProductsList" "callproducts" on "call"."productsID" = "callproducts"."productsid"
	left join "dbo"."Stslsman" "callSlm_" on "call"."Slm_No" = "callSlm_"."Slm_No"
	left join "dbo"."masngerclass" "callSlm_masngerclass" on "callSlm_"."masngerclassid" = "callSlm_masngerclass"."masngerclassid"
	left join "dbo"."Visitstatuslist" "callVisitstatus" on "call"."Visitstatus" = "callVisitstatus"."VisitstatusID"
	left join "dbo"."visittypelist" "callvisittype" on "call"."visittype" = "callvisittype"."visittypeNo"
	left join "dbo"."Stslsman" "Slm_" on "callregisterdtl"."Slm_No" = "Slm_"."Slm_No"
	left join "dbo"."masngerclass" "Slm_masngerclass" on "Slm_"."masngerclassid" = "Slm_masngerclass"."masngerclassid"
	left join "dbo"."visittypelist" "visittype" on "callregisterdtl"."visittype" = "visittype"."visittypeNo"
	left join "dbo"."Visitstatuslist" "Visitstatus" on "callregisterdtl"."Visitstatus" = "Visitstatus"."VisitstatusID"
]]></text>
