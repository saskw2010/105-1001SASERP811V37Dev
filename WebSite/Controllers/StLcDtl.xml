  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"StLcDtl"."Ln_No" "Ln_No"
	,"StLcDtl"."Doc_No" "Doc_No"
	,"Doc_"."Pur_Wys" "Doc_Pur_Wys"
	,"Doc_brn_"."Brn_Nm" "Doc_brn_Brn_Nm"
	,"Doc_brn_Branch"."Desc1" "Doc_brn_BranchDesc1"
	,"Doc_Pym_"."PaymentTypeName" "Doc_Pym_PaymentTypeName"
	,"Doc_Curncy_"."Curncy_Nm" "Doc_Curncy_Curncy_Nm"
	,"Doc_Sup_"."Sup_Nm" "Doc_Sup_Sup_Nm"
	,"Doc_Sup_Acc_"."Acc_Nm" "Doc_Sup_Acc_Acc_Nm"
	,"Doc_Sup_sgm"."sgm_Nm" "Doc_Sup_sgmsgm_Nm"
	,"Doc_Sup_Cntry_"."Cntry_Nm" "Doc_Sup_Cntry_Cntry_Nm"
	,"Doc_Sup_Curncy_"."Curncy_Nm" "Doc_Sup_Curncy_Curncy_Nm"
	,"StLcDtl"."orderid" "orderid"
	,"StLcDtl"."Unit_No" "Unit_No"
	,"StLcDtl"."Itm_No" "Itm_No"
	,"Itm_"."ID_USER" "Itm_ID_USER"
	,"Itm_"."ITEM" "ITEM"
	
	,"StLcDtl"."Unitprice" "Unitprice"
	,"StLcDtl"."Itm_CurnPrice" "Itm_CurnPrice"
	,"StLcDtl"."Titm_Purpric" "Titm_Purpric"
	,"StLcDtl"."Discount" "Discount"
	,"StLcDtl"."Quantity" "Quantity"
	,"StLcDtl"."Curncy_No" "Curncy_No"
	,"Curncy_"."Curncy_Nm" "Curncy_Curncy_Nm"
	,"StLcDtl"."Curncy_Rate" "Curncy_Rate"
	,"StLcDtl"."Titm_CurnPric" "Titm_CurnPric"
	,"StLcDtl"."Titm_Cost" "Titm_Cost"
	,"StLcDtl"."Itm_Cost" "Itm_Cost"
	,"StLcDtl"."Rec_Qty" "Rec_Qty"
	,"StLcDtl"."Titm_Cost1" "Titm_Cost1"
	,"StLcDtl"."Itm_Cost1" "Itm_Cost1"
	,"StLcDtl"."ModifiedBy" "ModifiedBy"
	,"StLcDtl"."ModifiedOn" "ModifiedOn"
	,"StLcDtl"."CreatedBy" "CreatedBy"
	,"StLcDtl"."CreatedOn" "CreatedOn"
from "dbo"."StLcDtl" "StLcDtl"
	left join "dbo"."StLcHdr" "Doc_" on "StLcDtl"."Doc_No" = "Doc_"."Doc_No"
	left join "dbo"."Branchcnst" "Doc_brn_" on "Doc_"."brn_no" = "Doc_brn_"."Brn_No"
	left join "dbo"."schBranch" "Doc_brn_Branch" on "Doc_brn_"."Branch" = "Doc_brn_Branch"."branch"
	left join "dbo"."PaymentTypes" "Doc_Pym_" on "Doc_"."Pym_No" = "Doc_Pym_"."PaymentTypeID"
	left join "dbo"."Stcurncy" "Doc_Curncy_" on "Doc_"."Curncy_No" = "Doc_Curncy_"."Curncy_No"
	left join "dbo"."Stsuplir" "Doc_Sup_" on "Doc_"."Sup_No" = "Doc_Sup_"."Sup_No"
	left join "dbo"."GLmstrfl" "Doc_Sup_Acc_" on "Doc_Sup_"."Acc_No" = "Doc_Sup_Acc_"."Acc_No"
	left join "dbo"."glmulcmp" "Doc_Sup_sgm" on "Doc_Sup_"."sgm" = "Doc_Sup_sgm"."sgmid"
	left join "dbo"."PYCntry" "Doc_Sup_Cntry_" on "Doc_Sup_"."Cntry_No" = "Doc_Sup_Cntry_"."Cntry_No"
	left join "dbo"."Stcurncy" "Doc_Sup_Curncy_" on "Doc_Sup_"."Curncy_No" = "Doc_Sup_Curncy_"."Curncy_No"
	left join "dbo"."Stcurncy" "Curncy_" on "StLcDtl"."Curncy_No" = "Curncy_"."Curncy_No"
	left join "dbo"."ITEMS" "Itm_" on "StLcDtl"."Itm_No" = "Itm_"."ID_comp"
]]></text>
    </command>
    <command id="Ln_NoIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Ln_No" />
      </output>
    </command>
  </commands>
